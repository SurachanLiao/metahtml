        document.getElementById("mnBotHomeLink").className = "mnMainNavLink";
        document.getElementById("mnBotEuropeLink").className = "mnMainNavLinkSel";
    }
    else if (window.location.pathname.toLowerCase().indexOf("/asia") >= 0) {
        document.getElementById("mnHomeLink").className = "mnMainNavLink";
        document.getElementById("mnAsiaLink").className = "mnMainNavLinkSel";
        document.getElementById("mnBotHomeLink").className = "mnMainNavLink";
        document.getElementById("mnBotAsiaLink").className = "mnMainNavLinkSel";
    }
    else if (window.location.pathname.toLowerCase().indexOf("/middleeast") >= 0) {
        document.getElementById("mnHomeLink").className = "mnMainNavLink";
        document.getElementById("mnMiddleEastLink").className = "mnMainNavLinkSel";
        document.getElementById("mnBotHomeLink").className = "mnMainNavLink";
        document.getElementById("mnBotMiddleEastLink").className = "mnMainNavLinkSel";
    }
    else if (window.location.pathname.toLowerCase().indexOf("/africa") >= 0) {
        document.getElementById("mnHomeLink").className = "mnMainNavLink";
        document.getElementById("mnAfricaLink").className = "mnMainNavLinkSel";
        document.getElementById("mnBotHomeLink").className = "mnMainNavLink";
        document.getElementById("mnBotAfricaLink").className = "mnMainNavLinkSel";
    }
