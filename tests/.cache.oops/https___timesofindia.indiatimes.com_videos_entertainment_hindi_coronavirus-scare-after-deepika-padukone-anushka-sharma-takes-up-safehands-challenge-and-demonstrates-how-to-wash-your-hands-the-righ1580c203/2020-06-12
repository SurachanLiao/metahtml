            function getSectionFromUrl(u){
                    var u = u.replace(".cms","");
                    if(u.indexOf("https") >= 0){
                        var u = (u.indexOf("?") >=0 ) ? u.split("?")[0] : u;
                        var sourceString = u.replace('http://','').replace('https://','').split(/[/?#]/)[0];
                        var domain = "https://" + sourceString + "/";
                        return u.replace(domain,"").split("/")[0];
                    }
                    else if(u.indexOf("/") == 0){
                        return u.split("/")[1];
                    }
                    else{
                        return u.split("/")[0];
                    }
                } 
            function hitCustomGrxPageview(url){
                    if (typeof(grx) === "function") {
                        grx('track', 'page_view', {url: url , subProject:'etimes', section:getSectionFromUrl(url)});
                    }
                }
         
