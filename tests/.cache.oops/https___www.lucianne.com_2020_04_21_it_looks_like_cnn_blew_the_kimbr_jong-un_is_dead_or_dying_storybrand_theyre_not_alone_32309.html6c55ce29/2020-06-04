                opxsTK1.async = true;
                opxsTK1.src = "//a.publir.com/platform/396.js";
                var opxsTargetTK1 = document.getElementsByTagName("head")[0];
                opxsTargetTK1.insertBefore(opxsTK1, opxsTargetTK1.firstChild);
            })();

            // taboola
            window._taboola = window._taboola || [];
