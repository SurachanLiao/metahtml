                    <a class="event-title" :href="urlForEvent(firstUpcomingEvent)">
                        <span class="time">{{getDateFormat(firstUpcomingEvent.begin)}}&nbsp; | </span>
                        <b class="event-title-title">{{firstUpcomingEvent.title}}</b>
                    </a>
                </div>
