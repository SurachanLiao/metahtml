                           if (window.dcPath.indexOf("3456.Magazine") > -1 && isMobile1) {
                            document.querySelector('.cc-grower').style.top = 0;
                            document.querySelector('.cc-grower').style.marginBottom = 0;
                           }
                            var i;
                            for (i = 0; i < adverIds.length; i++) {
                                if (document.getElementById(adverIds[i]) != null)
                                    document.getElementById(adverIds[i]).style.top =
                                        parseInt(document.querySelector(
                                            '.cc-grower').style.maxHeight) + 6 + 'px';
                            }

                        },
                        onPopupClose: function () {
                            var i;
                            for (i = 0; i < adverIds.length; i++) {
                                if (document.getElementById(adverIds[i]) != null)
                                    document.getElementById(adverIds[i]).style.top = '0px';
                            }
                        }
                    })
                });
            }
            </script>
            <script src='' async id='3083'></script>
        
</body>
</html>