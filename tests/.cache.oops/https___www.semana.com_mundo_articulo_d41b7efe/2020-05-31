<script id="specialized-template" type="text/template" container-ids="esp-filters">
    {%
    window.espfacets = request.params.get("facet") || new Array();
    _.each(response.facets, function(facet) {

    var allowed_filters = new Array(["type", "cine"], ["author", ""], ["type", "teatro"]);
    var must_continue = false;
    for (var idx=0; idx < allowed_filters.length; idx++){
    if (facet.root.path[0].toLowerCase().startsWith(allowed_filters[idx][0]))
    must_continue = true;
    }
    if (!must_continue) return;

    var liFiltered = '<span class="icon-semanacross"></span>';
    var liUnfiltered = '<span class="icon-semanaarrow-right"></span>';
    var esphtml = "";

    if (facet.root.children.length==0){
        filtered = $.inArray(facet.root.path, espfacets) >=0 ;
        var bullet = filtered ? liFiltered : liUnfiltered;
        var nodes = facet.root.path;
        var espvalue = nodes[0].startsWith('author') ? 'author' : nodes[0].concat(nodes[1] ? "/" + nodes[1] : "");
        var esptext = nodes[0].startsWith('author') ? 'Autor' : nodes[2] || nodes[1];
        esphtml += '<div class="author-filter"><h6 class="subheader" esptype="facet" espvalue="author">'.concat(esptext).concat('</h6><hr><ul class="side-nav author-l">');
        esphtml += '<li><span class="icon-semanaarrow-right"></span><a esptype="facet" class="author-name left" espvalue="'+espvalue+'">'+bullet+facet.root.label+'</a>';
        esphtml += '<a class="author-count right" esptype="facet" espvalue="'+espvalue+'">('+facet.root.value+')</a></li>';
        esphtml += '</ul></div>';
    } else {
        for(var i=0; i < facet.root.children.length; i++){
        var child=facet.root.children[i];
        filtered=$.inArray(child.path, espfacets)>=0 ;
        var bullet = filtered ? liFiltered : liUnfiltered;

        if (child.children.length==0){
        var nodes = child.path;
        var esptext = nodes[0].startsWith('author') ? 'Autor' : nodes[2] || nodes[1];
        if (i==0) esphtml += '<div class="author-filter"><h6 class="subheader" esptype="facet" espvalue="author">'.concat(esptext).concat('</h6><hr><ul class="side-nav author-l">');
                var espvalue = filtered ? child.path[0].startsWith("type") : child.path[0] + '/' + child.path[1];
                esphtml += '<li><span class="icon-semanaarrow-right"></span><a esptype="facet" class="author-name left" espvalue="'+espvalue+'">'+bullet+child.label+'</a>';
                esphtml += '<a class="author-count right" esptype="facet" espvalue="'+espvalue+'">('+child.value+')</a></li>';
                if (i==facet.root.children.length-1) 
                    esphtml+='</ul><hr></div>';
        }
        else{
            esphtml +='<div class="author-filter"><h6 class="subheader" esptype="facet" espvalue="author">'.concat(child.label).concat('</h6><hr><ul>');
            for(var j=0; j< child.children.length; j++){
                var gchild = child.children[j];
                filtered = $.inArray(gchild.path, espfacets) >=0 ;
                espvalue = filtered ? gchild.path[0].startsWith("type") : gchild.path[0] + '/' + gchild.path[1];
                esphtml += '<li><a esptype="facet" class="author-name left" espvalue="'+espvalue+'">'+bullet+gchild.label[0]+'</a>';
                esphtml += '<a class="author-count right" esptype="facet" espvalue="'+espvalue+'">('+gchild.value[0]+')</a></li>';
            }
            esphtml+='</ul></div>';
        }
    }
    }  %}

    {%=esphtml %}

    {% }); %}
</script>
<script id="other-posts" type="text/template" container-ids="other-posts-semana,other-posts-dinero,other-posts-jetset,other-posts-soho,other-posts-finanzaspersonales,other-posts-revistaarcadia,other-posts-revistafucsia">
{%
    var query = encodeURIComponent( request.params.get("query") );
    var targetUrl = response.documents[0].fields.posturl[0].toString();
    targetUrl = targetUrl.concat("/Buscador.aspx?busqueda=");
    var postDomain = response.documents[0].fields.posturl[0].toString();
    postDomain = postDomain.replace(/.*?:\/\//g, "").replace(/www./gi, "");
    var post = postDomain.replace(/.com/gi, "").replace(/.co/gi, "");
    var utmParams = "?utm_source=semana.com&utm_medium=referral&utm_campaign=otras-publicaciones-busqueda";
    %}

    <div class="results-widget {%= post %}">
        <h6>{%= postDomain %}<a href="{%= targetUrl %}" rel="noopener" class="counter">({%= response.totalCount %})+</a></h6>
    </div>
    <ul class="side-nav">
        {% for(var i=0; i < response.documents.length ; i++) { %}
            <li>
                <a class="related-link" href="{%= response.documents[i].fields.posturl[0] + (response.documents[i].fields.url[0] + utmParams || " ") %}"> {%= response.documents[i].fields.title[0] %}</a>
            </li>
        {%  } %}
    </ul>

</script>



                
