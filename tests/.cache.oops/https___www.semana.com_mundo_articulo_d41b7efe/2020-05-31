<script id="order-template" type="text/template" container-ids="order-results">
    <dt>Ordenar por: </dt>
    {%
    var sortOptions = new Array();
    sortOptions.push( { label:"Relevancia", value:"relevance" } );
    sortOptions.push( { label:"Más antiguo", value:"older" } );
    sortOptions.push( { label:"Más reciente", value:"newer" } );
    var sort = request.params.get("sort");

    for(var i=0; i < sortOptions.length; i++){ %}
        <dd class="{%= sortOptions[i].value == sort? 'active' : '' %}"><a esptype="sort" espvalue="{%= sortOptions[i].value %}">{%= sortOptions[i].label %}</a> </dd> {%
        }
    %}
</script>
<script id="pagination-template" type="text/template" container-ids="top-pager,bottom-pager">
    <ul class="pagination right">
        {%
        //determine number of pages:
        var totalCount = response.totalCount;
        var limit = request.params.get("limit") || 10;
        var offset = request.params.get("offset");
        var int_number = parseInt(totalCount/limit);
        var flt_number = parseFloat(totalCount/limit);
        var pages = (flt_number > int_number) ? int_number + 1 : int_number;

        //determine current page index:
        var pageIndex = (offset / limit);

        //determine pages to display:
        var displayingPages = 10;
        var startIndex = (pageIndex > parseInt(displayingPages/2)) ? pageIndex - parseInt(displayingPages/2) : 0;
        var endIndex = startIndex + displayingPages;

        //draw the pager (first, prev):
        if ( startIndex > 0 ){ %}
        <li class="arrow"><a esptype="offset" espvalue="0"><span class="icon-first"></span></a></li>
        <li class="arrow"><a esptype="offset" espvalue="{%=(startIndex * limit) - limit %}"><span class="icon-previous"></span></a></li>
        {% }

        //draw the pager:
        for (var i = startIndex; i < pages && i < endIndex; i++){ %}
        <li class="{%= (i==pageIndex)? 'current': '' %}">
            <a esptype="offset" espvalue="{%= ((i+1)*limit)-limit %}">{%= i+1 %}</a>
        </li>
        {% }

        //draw the pager (next, last):
        if ( (endIndex + 2) < pages ){ %}
        <li><a esptype="offset" espvalue="{%= (endIndex * limit) + limit %}"><span class="icon-next"></span></a></li>
        <li><a esptype="offset" espvalue="{%=((pages-1) * limit) %}"><span class="icon-last"></span></a></li>
        {% }

        %}
    </ul>
</script>
