    var modalSubscriptionValidationEventListener = document.getElementById("modalSubscriptionValidation");
    // Code for Chrome, Safari and Opera
    modalSubscriptionValidationEventListener.addEventListener("webkitTransitionEnd", focusOnfirstElementOnWindow);
    // Standard syntax
    modalSubscriptionValidationEventListener.addEventListener("transitionend", focusOnfirstElementOnWindow);
    function focusOnfirstElementOnWindow() {
        //console.log('termine');
        $('#modalSubscriptionValidation').find('input').first().focus();
    }    
    $('#modalSubscriptionValidation').find('input').first().off('focus').on('focus', function () {
        modalSubscriptionValidationEventListener.removeEventListener('webkitTransitionEnd', focusOnfirstElementOnWindow, false);
        modalSubscriptionValidationEventListener.removeEventListener('transitionend', focusOnfirstElementOnWindow, false);
    });    
