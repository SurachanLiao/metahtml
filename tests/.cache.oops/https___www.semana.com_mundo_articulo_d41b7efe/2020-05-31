    {%
    _.each(espFullResponse.facets, function(facet) {
    if (!facet.root.path[0].startsWith("type")) return;
    var sortedFacets = espFullResponse.getFacetsSorted (facet.root.children, new Array(["type","Evento"], ["type", "Sitio"], ["type", "Cine"], ["type", "Teatro"]));

    for(var i=0; i< sortedFacets.length; i++){
    var type = sortedFacets[i];
    var fullFacet = response.getFacetsByPath(type.path);
    var selected = (fullFacet.length > 0) ? "active" : "";
    %}

    <li class="{%=selected %}">
        <a class="button" esptype="facet" espvalue="{%=type.path[0] + '/' + type.path[1] %}">
            {%=
            type.label == "Multimedia" ? "Videos" :
            type.label == "Galería" ? "Fotos" :
            type.label == "Artículo" ? "Artículos" :
            type.label == "Evento" ? "Eventos" :
            type.label == "Sitio" ? "Sitios" :
            type.label
            %}
            <span class='counter'>({%= type.value %})</span>
        </a>
    </li>

    {% }
    });
    %}
</script>
