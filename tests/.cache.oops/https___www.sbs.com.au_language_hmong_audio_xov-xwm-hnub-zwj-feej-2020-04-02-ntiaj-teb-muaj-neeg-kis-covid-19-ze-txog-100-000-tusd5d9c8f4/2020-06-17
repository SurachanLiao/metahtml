  //populate page title from <title> tag
  var pageTitle = (document.getElementsByTagName("title")[0] && document.getElementsByTagName("title")[0].innerHTML) ? document.getElementsByTagName("title")[0].innerHTML : "embed";
  digitalData.page.pageInfo["title"] = pageTitle;

  var trackingCode = window.location.search.split('cid=').splice(1).join('').split('&')[0];
  if (trackingCode !== undefined && digitalData.page.pageInfo.campaign !== undefined) {
    digitalData.page.pageInfo.campaign["trackingCode"] = trackingCode;
  }

  // Populate previousPageURL and previousPageName from sessionStorage when available
  if (document.referrer !== "" && document.referrer.match(/amp.sbs.com.au/)) {

    // Now we need to manually construct the AMP page name from the URL... ¯\_(ツ)_/¯
    var path = document.referrer.match(/(amp.sbs.com.au\/).*(?=(\?|$))/);
    if (path[0]) {

      // Splitting the path should yield something like:
      //  * 0 = amp.sbs.com.au
      //  * 1 = v1 (? maybe)
      //  * 2 = <site> eg: news | language | etc.. (? maybe)
      //  * 3 = <content_type> eg: article | live blog | etc..
      //  * 4 = <slug>
      //  * 5 = <uuid>
      var parts = path[0].split('/');

      // Remove the unwanted bits (amp.sbs.com.au and UUID)
      parts = parts.slice(2, parts.length-1);

      // Check if there is a site part, if not add news as the default
      if (parts.length === 2) {
        parts.unshift('news');
      }

      // These are AMP enabled sites only (my require updates as more sites support AMP)
      var sites = ['news', 'language'];

      // The name begins with 's:' and we need to append the site and slug
      var result = 's:';

      // Now reconstruct the name
      parts.forEach(function(p, i) {
        if (sites.includes(p)) {

          // Found site so append it
          result += p + ':';
        }
        if (i === parts.length-1 && p.match(/(.*-.*)/)) {

          // Found slug so append it
          result += p;
        }
      });

      // Add the values to the dataLayer
      digitalData.page.pageInfo["previousPageURL"] = document.referrer;
      digitalData.page.pageInfo["previousPageName"] = result;
    }
  } else {
    digitalData.page.pageInfo["previousPageURL"] = sessionStorage.getItem("pageURL") || "";
    digitalData.page.pageInfo["previousPageName"] = sessionStorage.getItem("pageName") || "";
  }

  // add in the user agent string
  if (digitalData.environment.browser !== undefined) {
    digitalData.environment.browser["userAgent"] = navigator.userAgent;
  } else {
    digitalData.environment.browser = {};
    digitalData.environment.browser["userAgent"] = navigator.userAgent;
  }

  //create the platform object to set the sourceType attribute
  digitalData.environment.platform = {};
  digitalData.environment.platform["sourceType"] = "web";

  //save current page URL and name so we can use it in previous page properties if user clicks to another page
  if (digitalData.page.pageInfo.hasOwnProperty("destinationURL")) {
    sessionStorage.setItem("pageURL", digitalData.page.pageInfo.destinationURL);
  }
  if (digitalData.page.pageInfo.hasOwnProperty("pageName")) {
    sessionStorage.setItem("pageName", digitalData.page.pageInfo.pageName);
  }

  //setting the referrer properties
  if (document.referrer !== "" && !document.referrer.match(/^(?!.*amp).*(sbs.com.au)$/)) {
    digitalData.page.pageInfo["referralURL"] = document.referrer;

    if (localStorage.getItem("originalReferrerURL") === null) {

      //this is the first time user visits our site so we save the referrer as original
      localStorage.setItem("originalReferrerURL", document.referrer);
    }
  }

  //populate originalReferrerURL from sessionStorage when available
  digitalData.page.pageInfo["originalReferrerURL"] = localStorage.getItem("originalReferrerURL") || "";

</script>
