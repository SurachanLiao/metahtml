<script type="text/javascript">
	jQuery(document).ready(function($) {

		var $widget = jQuery(".widget-herramientas"),
        	$content = jQuery(".barra-lateral"),
        	y = $content.offset().top, // coor.y del widget respecto a su barra, referencia: (0, 0)
        	min_offset = 1,
        	css_fixed = "fixed-widget",
        	css_fixed_bottom = "bottom",
        	limit_bottom = calculate_bottom_limit();

        function calculate_bottom_limit() {
        	var h_body = jQuery("body").height();
        	var h_header = jQuery(".header-wrapper").height(); // Altura que toma el header en este momento
        	var h_embellecedor = jQuery(".embellecedor-wrapper").height();
        	var h_footer = jQuery(".site-footer").height();
        	var h_widget = $widget.height();
        	var offset_y_widget_top = parseInt($widget.css("top"), 10); // propiedad CSS top en .widget-herramientas
        	var offset_y_widget_bottom = 22;
        	var limit_bottom = h_body - h_header - h_embellecedor - h_footer - h_widget - offset_y_widget_top - offset_y_widget_bottom;
        	return (limit_bottom);
        }

	    window.addEventListener('scroll', function(event) {
    		var w = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
    		if (w >= 1186)
        		scroll_herramientas();
        });

	    function scroll_herramientas() {
	        y = $content.offset().top - jQuery(document).scrollTop();
	        // Limit por arriba:
	        (y < min_offset ? $widget.addClass(css_fixed) : $widget.removeClass(css_fixed));
	        // Limit por abajo:
			(y < -limit_bottom ? $widget.addClass(css_fixed_bottom) : $widget.removeClass(css_fixed_bottom));
	    }

	    scroll_herramientas();
	});
</script>
