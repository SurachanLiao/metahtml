    <script type='text/javascript'>
    (function () {
        //returns current break point level - used to refresh ads for the new slot size
        var pageMargins = 80;
        var sizeDesktopLarge = 970 + pageMargins, //larger sized desktop ads
            sizeDesktopSmall = 728 + pageMargins, //normal desktop ads
            sizeMobile = 0;

        function getBreakPoint() {
            var width = window.innerWidth;
            if (width <= sizeDesktopSmall) {
                return 0;
            } else if (width <= sizeDesktopLarge) {
                return 1;
            }
            return 2;
        }

        currentBreakPoint = getBreakPoint();
        var adSlots = []; //hold ad slots to refresh

        //builds and appends ad slot
        function buildAd(size, slotName, dimensions, bannerId, sizeMapping) {
            if (size == 'MobileOnlyBanner' && !(new MobileDetect(navigator.userAgent)).mobile()) {
                return;
            }
            if (sizeMapping) {
                adSlots.push(googletag.defineSlot(slotName, dimensions, bannerId).defineSizeMapping(sizeMapping).addService(googletag.pubads()));
            } else {
                adSlots.push(googletag.defineSlot(slotName, dimensions, bannerId).addService(googletag.pubads()));
            }
        }

        var bannerSizeMapping = googletag.sizeMapping().
            addSize([sizeDesktopLarge, 0], [[970, 90], [970, 250], [728, 90]]).
            addSize([sizeDesktopSmall, 0], [[728, 90], [300, 50], [320, 50]]).
            addSize([sizeMobile, 0], [[300, 50], [320, 50]]).
            build();

        var blockSizeMapping = googletag.sizeMapping().
            addSize([sizeDesktopSmall, 0], [[300, 250], [300, 600]]).
            addSize([sizeMobile, 0], [[300, 250], [300, 50], [320, 50]]).
            build();

        var homeBlockSizeMapping = googletag.sizeMapping().
            addSize([sizeDesktopSmall, 0], [[300, 250]]).
            addSize([sizeMobile, 0], [[300, 250], [300, 50], [320, 50]]).
            build();

        //example:
        // addSize([1020, 0], [970, 90]). //first param is [viewportWidth, viewportHeight]. second param is [adWidth, adHeight]
        // addSize([768, 0], [728, 90]).
        // this means that starting at width 768px inclusive, the 728x90 ad displays if it exists, and starting at 1020px inclusive the 970x90 ad displays if it exists

            buildAd('MobileOnlyBanner', '/36117602/htv-wbalradio.com/WBAL_Article_MobileUnit', [[320, 50],[300,50]], 'dfp-article-mobile', );
            buildAd('PageBlock', '/36117602/htv-wbalradio.com/300x250_Article', [[300, 250], [300, 600]], 'dfp-article-rightcol', blockSizeMapping);
            buildAd('PageBanner', '/36117602/htv-wbalradio.com/728x90_Article', [[970, 90], [970, 250], [728, 90]], 'dfp-article-leaderboard', bannerSizeMapping);

        googletag.pubads().enableAsyncRendering();
        googletag.pubads().enableSingleRequest();
        googletag.enableServices();

        jQuery(function () {
            
                AdjustGoogleAd('dfp-article-mobile');
                AdjustGoogleAd('dfp-article-rightcol');
                AdjustGoogleAd('dfp-article-leaderboard');

            // Set timer to fire function after resizing is complete
            var resizeTimer;
            jQuery(window).on('resize', function (e) {
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(function () {
                    //only refresh if we hit a new break point
                    var newBreakPoint = getBreakPoint();
                    if (newBreakPoint != currentBreakPoint) {
                        currentBreakPoint = newBreakPoint;
                        //refresh ads to update sizes
                        //once ads are refreshed the 'slotRenderEnded' event will resize ads
                        googletag.pubads().refresh(adSlots);
                    }

                }, 250);

            });
        });
    })()
</script>



