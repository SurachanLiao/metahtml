    <!-- MediaOS Video -->
    <script>window.HRST = { site: { dartZone: 36117602, adUnit: "htv-wbalradio.com/WBAL_OnDemandVideo_Preroll" } };</script>
    <!-- AnythingSlider initialization -->
    <script type="text/javascript">

        var tag = document.createElement('script');
        tag.src = "https://www.youtube.com/iframe_api";
        var firstScriptTag = document.getElementsByTagName('script')[0];
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

        function onPlayerStateChange(event) {
            switch (event.data) {
                case YT.PlayerState.PLAYING:
                    articleSlider.pause();
                    break;
                case YT.PlayerState.ENDED:
                    articleSlider.flexslider('next');
                    articleSlider.play();
                    break;
            }
        }

        var articleSlider;
        var players = [];
        // SEtup Flex Slider
        jQuery(function () {
            var loadVideoPlayer = function () {
                $('.yt-video').each(function () {
                    players.push(new YT.Player($(this).attr('id'),
                        {
                            width: '778',
                            height: '385',
                            videoId: $(this).attr('data'),
                            events: {
                                'onStateChange': onPlayerStateChange
                            }
                        }));
                });
            };

            //delays loading of YT player if YT has not yet been defined
            if (typeof (YT) == 'undefined' || typeof (YT.Player) == 'undefined') {
                window.onYouTubeIframeAPIReady = function () {
                    loadVideoPlayer();
                };
            } else {
                loadVideoPlayer();
            }


            jQuery('#article-slider').flexslider({
                animation: "slide",
                video: true,
                touch: true,
                useCSS: true,
                smoothHeight: true,
                slideshowSpeed: 5000,
                start: function (slider) {
                    //Create a global slider variable that you can reference anywhere
                    //mySlider.pause();
                    articleSlider = slider;
                }
            });

            // Setup Fresco for Images
            jQuery("a.fresco").on('click', function () {
                ga('send', 'event', 'Image', 'Full Screen');
                articleSlider.pause();
            });

            // Setup Media Player
            jQuery('audio').mediaelementplayer({
                audioWidth: '100%',
                success: function (media) {
                    media.addEventListener('play', function () {
                        ga('send', 'event', 'Audio', 'Play');
                        articleSlider && articleSlider.pause && articleSlider.pause();
                    }, true);
                    media.addEventListener('pause', function () {
                        ga('send', 'event', 'Audio', 'Pause');
                        articleSlider && articleSlider.pause && articleSlider.pause();
                    }, true);
                    media.addEventListener('ended', function () {
                        ga('send', 'event', 'Audio', 'Played To End');
                    }, true);
                    media.addEventListener('seeked', function () {
                        ga('send', 'event', 'Audio', 'Seeked');
                    }, true);
                }
            });


            //Enable Google Analytics Tracking
            if (BrowserDetect.IsIe && BrowserDetect.Version() <= 9) { //hide player for ie9
                jQuery(".soundcloud-player").hide();
                jQuery(".soundcloud-noplayer").show();
            } else {
                jQuery(".soundcloud-player").each(function () {
                    var player = jQuery(this)[0];

                    try {
                        //throw "";
                        var widget = SC.Widget(player);
                        try {
                            widget.bind(SC.Widget.Events.READY, function () {
                                widget.bind(SC.Widget.Events.PLAY, function () {
                                    ga('send', 'event', 'Soundcloud', 'Play');
                                    articleSlider.pause();
                                });
                                widget.bind(SC.Widget.Events.PAUSE, function () {
                                    ga('send', 'event', 'Soundcloud', 'Pause');
                                    articleSlider.pause();
                                });
                                widget.bind(SC.Widget.Events.FINISH, function () {
                                    ga('send', 'event', 'Soundcloud', 'Played To End');
                                });
                                widget.bind(SC.Widget.Events.SHARE, function () {
                                    ga('send', 'event', 'Soundcloud', 'Shared');
                                });
                            });
                        } catch (e) {

                        }
                    } catch (e) { //if sound cloud fails - switch to link view
                        jQuery(".soundcloud-player").hide();
                        jQuery(".soundcloud-noplayer").show();
                    }
                });
            }

            jQuery(".audio-archive-download").on('click', function () {
                ga('send', 'event', 'Audio', 'Download');
                var href = $(this).attr('href');
                setTimeout(function () { document.location.href = href }, 100);
                articleSlider.pause();
                return false;
            });

            jQuery(".file-download").on('click', function () {
                ga('send', 'event', 'File', 'Download');
                var href = $(this).attr('href');
                setTimeout(function () { document.location.href = href }, 100);
                articleSlider.pause();
                return false;
            });


            // Update videos on load
            UpdateNdnVideos();
            UpdateOtCloudVideos();

            // update videos on window resize
            $(window).smartresize(function () {
                UpdateNdnVideos();
                UpdateOtCloudVideos();
            });

        });

        $(window).load(function () {
            UpdateNdnVideos();
            UpdateOtCloudVideos();
        });

        function UpdateNdnVideos() {
            jQuery("li.video .video-ndn iframe").each(function () {
                var id = jQuery(this).attr('id').replace('ndn-', '');
                var height = jQuery(this).height();
                var width = Math.floor((height / 9) * 16);
                var url = 'https://embed.newsinc.com/Single/iframe.html?WID=2&VID=' + id + '&freewheel=90622&sitesection=wbal&height=' + height + '&width=' + width;
                jQuery(this).attr('src', url);
            });
        }


        function UpdateOtCloudVideos() {
            jQuery("li.video .video-otcloud iframe").each(function () {
                var id = jQuery(this).attr('id').replace('otcloud-', '');
                var height = jQuery(this).height();
                var width = Math.floor((height / 9) * 16);
                var url = 'https://player.olympusattelecom.com/player.php?p=' + id + '&h=' + height + '&w=' + width + '&wmode=transparent';
                jQuery(this).attr('src', url);
            });
        }


    </script>




