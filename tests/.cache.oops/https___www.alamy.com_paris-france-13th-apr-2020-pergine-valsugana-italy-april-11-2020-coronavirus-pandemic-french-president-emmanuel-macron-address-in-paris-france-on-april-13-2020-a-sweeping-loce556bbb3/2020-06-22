    }
    $(document).ready(function () {
       
        $(".drp").click(function () {
            if ($(window).width() < 400) {
                $(this).siblings().slideToggle();
                $(this).toggleClass('active');
            }
        });
        
        setLanguageForSelector();
    });
    function switchLanguages(lanCode) {
        var evt = this.window.event || arguments.callee.caller.arguments[0];
        if (evt) {
            evt.preventDefault();
        }

        var linkerParam = "";

        if (typeof ga !== "undefined" && !IsBotrequest) {
            if (window.location.href.indexOf("_ga=") == -1) {
                ga(function () {
                    var trackers = ga.getAll();
                    if (trackers.length) {
                        linkerParam = trackers[0].get('linkerParam');
                    }
                });
            }
        }
        

        var intranetmode = "0";
        if (IsIntranet == "true" || IsIntranet == "1") intranetmode = "1";
        var string = " ";
        var newString = " ";
        if (lanCode == "com" || lanCode == "de") {
            if (window.location.href.toLowerCase().indexOf("themes.aspx") != -1) {
                string = "themes.aspx";
                newString = "themes.asp";
            }

            if (window.location.href.toLowerCase().indexOf("order-download-purchasehistory.aspx") != -1) {
                string = "order-download-purchasehistory.aspx";
                newString = "order-download-purchasehistory.asp";
            }
        }

        if (window.location.href.toLowerCase().indexOf("contactus.aspx") != -1 || window.location.href.toLowerCase().indexOf("contactus/") != -1) {
            string = currentDomain == "com" ? window.location.href.substring(window.location.href.indexOf("contactus/"), window.location.href.length) : "contactus.aspx";
            newString = lanCode == "com" ? "contactus/uk.aspx" : "contactus.aspx";
        }

        if (window.location.href.toLowerCase().indexOf("terms/") != -1) {
            string = window.location.href.substring(window.location.href.indexOf("terms/"), window.location.href.length);
            newString = (lanCode == "com") ? "terms/uk.aspx" : "terms/terms.aspx";
        }

        if ((intranetmode == "1") || intranetmode == "0") {
            var exp = '';
            var now = new Date();
            var then = now.getTime() + (30 * 24 * 60 * 60 * 1000);
            now.setTime(then);
            exp = '; expires=' + now.toGMTString();
            document.cookie = "userLanguage=en;Secure=true;path= /;domain= " + cookieDomain + exp;
            var slinkparam = "";
            if (linkerParam != "" && linkerParam != null) {
                if (window.location.href.indexOf("?") != -1)
                    slinkparam = "&" + linkerParam
                else
                    slinkparam = "?" + linkerParam
            }
            
            if (window.location.href.indexOf("/" + compurl_foldername + "-") != -1 && window.location.href.indexOf("?returnurl") == -1) {
                var url = "";
                url = 'https://www.alamy.com'.replace(cookieDomain, (lanCode == "fr") ? ".alamyimages." + lanCode : ".alamy." + lanCode) + '/mediacomp/imagedetails.aspx?ref=' + '2BEJY07';
                var queryString = removeQueryStringParam("ref", window.location.href);
                if (queryString.length > 0) {
                    queryString = "&" + queryString;
                }

                if (url.indexOf("?") != -1) {
                    if (linkerParam == "" && queryString == "")
                        window.location.href = url
                    else
                        window.location.href = url + "&" + linkerParam + queryString;
                }
                else {
                    window.location.href = url + "?" + linkerParam + queryString;
                }
            }
            else if (lanCode == "fr") {
                window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamyimages." + lanCode).replace(string, newString) + slinkparam;
            }
            else {
                if (lanCode == "de" && window.location.href.toLowerCase().indexOf("/footage-gallery.aspx") != -1)
                    window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamy." + lanCode).replace("footage-gallery.aspx", "video-gallery.aspx") + slinkparam;
                else
                    window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamy." + lanCode).replace(string, newString) + slinkparam;
            }
            
        }
    }
    function removeQueryStringParam(key, sourceURL) {
        var returnUrl = sourceURL.split("?")[0],
        param,
        params_arr = [],
        queryString = (sourceURL.indexOf("?") !== -1) ? sourceURL.split("?")[1] : "";
        if (queryString !== "") {
            params_arr = queryString.split("&");
            for (var i = params_arr.length - 1; i >= 0; i -= 1) {
                param = params_arr[i].split("=")[0];
                if (param === key) {
                    params_arr.splice(i, 1);
                }
            }
            return params_arr.join("&");
        }
        return "";
    }
    var setLanguageForSelector = function () {
        var language = "";
        switch ('en') {
            case 'en':
                language = footerResources.EnglishNoTranslate;
                break;
            case 'de':
                language = footerResources.DeutschNoTranslate;
                break;
            case 'es':
                language = footerResources.EspaÃ±olNoTranslate;
                break;
            case 'fr':
                language = footerResources.FranÃ§aisNoTranslate;
                break;
            case 'it':
                language = footerResources.ItalianoNoTranslate;
                break;
            default:
                language = footerResources.EnglishNoTranslate;
        }
        $("#automationHttpCulture").html(language);
    };
    $("#footer_Normal a").click(function () {
        if (!$(this).hasClass('lang-selector') && $(this).attr('id') != "automationGetCreditAccount")
        {
            objGTM1 = new GTM1();
            var eventAction = 'Internal click';
            var href = $(this).attr('href').split("?")[0];
            if ($(this).parents('#footerSocial').length > 0) eventAction = 'External Click';
            else if ($(this).attr('id') == "lblTel") eventAction = 'Phone Number Click';
            else if ($(this).parents('.lang-dd').length > 0) eventAction = 'Language Selector Dropdown Click';
            objGTM1.setValueswithCategory('Footer Interactions', eventAction,href, "");
        }
        return true;
    });
</script>

