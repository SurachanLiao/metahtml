      <div class="block block-system" id="block-system-main">
    <h2 class="title"></h2>
    <div class="content">  <div class="node">
    <h1 class="title">today</h1>            <div class="taxonomy"></div>
        <span class="print-link"><span class="print_html"><a href="http://www.operationworld.org/print/96" title="Display a printer-friendly version of this page." class="print-page" onclick="window.open(this.href); return false" rel="nofollow"><img class="print-icon print-icon-margin" src="http://www.operationworld.org/sites/all/modules/print/icons/print_icon.png" width="16px" height="16px" alt="Printer-friendly version" title="Printer-friendly version" />Printer-friendly version</a></span></span><div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even"><p><strong>Please wait...this page will redirect to country to pray for today.</strong></p>
<script language="JavaScript">
<!--//--><![CDATA[// ><!--

var today = new Date();
var day = today.getDate();
var month = today.getMonth();
var pgname;
var days;
switch(month)
{
case 0: month = "jan"; break;
case 1: month = "feb"; break;
case 2: month = "mar"; break;
case 3: month = "apr"; break;
case 4: month = "may"; break;
case 5: month = "jun"; break;
case 6: month = "jul"; break;
case 7: month = "aug"; break;
case 8: month = "sep"; break;
case 9: month = "oct"; break;
case 10: month = "nov"; break;
case 11: month = "dec"; break;
default: month = "prayer-calendar"; break;
}
switch(day)
{
case 0: days = ""; break;
