<script type="text/javascript">
    function ocultaMuestraCapa(divId) {
        if (document.getElementById(divId).style.display == 'none') {
            document.getElementById(divId).style.display = 'block';
        } else {
            document.getElementById(divId).style.display = 'none';
        }
    };
    (function($) {
        if (typeof $ !== 'function') {
            return false;
        }
        var tpl = (
                '<a href="#" class="leer-mas-btn">' +
                    '<span class="gradient"></span>' +
                    '<span class="txt js-more-button-text">Leer más</span>' +
                '</a>'
            ),
            commentsId = '#js_5e9e8b5c268e3e9b4e8b46a8',
            containerSelector = '.aside-comments',
            divSelector = '.js-more-button',
            textSelector = '.js-more-button-text'
            appendButton = function (container) {
                return $(tpl)
                .click(function (ev) {
                    ev.preventDefault();
                    if (container.css('max-height') !== 'none') {
                        container.css('max-height', 'none');
                        $(this).find(textSelector).html('cerrar');
                    }
                    else {
                        container.css('max-height', '');
                        $(this).find(textSelector).html('Leer más');
                    }
                })
                .appendTo(container);
            };

        $(commentsId + ' ' + containerSelector)
            .find(divSelector)
            .map(function() {
                var lastChildren = $(this).children().last();
                lastChildren.css({'padding-bottom': 0, 'margin-bottom': 0});
                if (this.clientHeight < this.scrollHeight) {
                    appendButton($(this));
                }
                lastChildren.css({'padding-bottom': '', 'margin-bottom': ''});
            });

    })(jQuery);
</script>
