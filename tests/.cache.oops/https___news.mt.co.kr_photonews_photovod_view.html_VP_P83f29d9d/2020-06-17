		<script type="text/javascript">
					if(9999 == "8000")
			{

				chgSpecial('1');
			}
					function chgSpecial(id)
			{
				for(i=0; i<2; i++)
				{
					document.getElementById('special_tab_'+i).className = "";
					document.getElementById('special_'+i).style.display = "none";
				}

				document.getElementById('special_tab_'+id).className = "on";
				document.getElementById('special_'+id).style.display = "block";
			}


			function changePhoto(lnk,img,title,idx)
			{
				document.getElementById('bigLink_'+idx).href = lnk;
				document.getElementById('bigImg_'+idx).src = img;
				document.getElementById('bigImg_'+idx).alt = title;
				document.getElementById('bigTitle_'+idx).innerHTML = title;
			}

			function changePhoto2(id, idx)
			{
				for(i=0; i<15; i++)
				{
					document.getElementById('special_con'+idx+'_'+i).className = "";
				}

				document.getElementById('special_con'+idx+'_'+id).className = "on";
			}
		</script>
