<div id="container">
    <div style="position:relative;"><!-- 추가 181228 세종 -->
        <div style="margin-right:27px; margin-left:27px;">
        <!-- INCLUDE HEADER AREA END -->
	<link rel="stylesheet" type="text/css" href="https://menu.mt.co.kr/css/mt3_article.css?date=20190711" />
	<script type="text/javascript">
		$(document).ready(function() {
			// 공감, 비공감 %값 표시
			agree_spread('202006171754587580');

			// 맨 처음 로드 후, 이미지 양옆 화살표 처리
			if($('div.ptslide li[id^="photoNewsImg"]').length <= 1) {
				//$('div.ptslide button.prev').addClass('noimg').hide();
				//$('div.ptslide button.next').addClass('noimg').hide();
			}
			
			// 이미지 양옆 화살표 클릭
			$('div.ptslide button').on('click', function() {
				var direction		= $(this).attr('class');
				var lastImgNo		= parseInt($('div.ptslide li[id^="photoNewsImg"]:last').attr('id').replace('photoNewsImg', ''));
				var currentImgNo	= parseInt($('div.ptslide li[id^="photoNewsImg"]:visible').attr('id').replace('photoNewsImg', ''));

				if(direction == 'prev') {
					var targetImgNo = ((currentImgNo - 1) < 1)? 'prevArticle' : (currentImgNo - 1);
				} else {
					var targetImgNo = ((currentImgNo + 1) > lastImgNo)? 'nextArticle' : (currentImgNo + 1);
				}

				if(targetImgNo != 'prevArticle' && targetImgNo != 'nextArticle') {
					$('div.ptslide li[id^="photoNewsImg"]').hide();
					$('#photoNewsImg' + targetImgNo).fadeIn('fast');
					$('#currentPhotoNo').html(targetImgNo);
					$('#photoArticleTitle').html($('#photoTitle'+targetImgNo).val());
					setArticleInfo($('#photoNo'+targetImgNo).val());
				} else {
					var moveArticleNo = $('#' + targetImgNo + 'No').val();

					if(moveArticleNo != '') {
						moveArticle(moveArticleNo);
					} else {
						alert('마지막 기사입니다.');
					}
				}

//				$(this).blur();
			});


			// 기사정보 설정
			setArticleInfo = function(articleId) {

				var articleInfoStr = getDataByAjax('getArticleInfo', articleId, '', '');

				var infoArr		= articleInfoStr.split('<<end>>');

				var adjacentArr	= infoArr[0].split('|');			
				var articleInfo	= infoArr[1].split('|');
				var contents = articleInfo[3].replace(/&#034;/gi, '"');

				$('#pn_giza_name').text(articleInfo[1]);		// 기사 작성자
				$('#pn_article_date').text(articleInfo[2]);		// 작성일자
				$('#articleSummary').html(contents);			// 요약글 (본문내용)

				$('#pn_giza_search').attr("href","https://search.mt.co.kr?srchFd=N&kwd="+articleInfo[1].replace(" 기자", '')+"&search_type=m")
			}

			getDataByAjax = function(mode, article_id, move_page, idx) {
				var cookie_user_id = '';//$('#cookie_user_id').val(); // 포토슬라이드 정보 가져올 때, 조회수 판단용 아이디

				$.ajax({
					url		: 'getPhotovodInfos.php',
					data	: 'mode=' + mode + '&cate=total&slide_no=202006171754587580&cType=&article_id=' + article_id + '&topPage=' + move_page + '&idx=' + idx + '&cookie_user_id=' + cookie_user_id,
					async	: false,
					dateType: 'html',
					error	: function(request, status, error) { //통신 에러 발생시 처리
						returnStr = error;
												alert(returnStr);
					},
					success : function(response, status, request) { //통신 성공시 처리
						returnStr = response;

					},
					complete: function() { //통신이 완료된 후 처리
					}
				});
				return returnStr;
			}

			setArticleInfo($('#photoNo1').val()); // 슬라이드 내의 첫번째 기사 정보 출력
		});
		
		function moveArticle(no) {
			location.href = './photovod_view.html?no=' + no + '&cate=total&cType=';
		}

		function agree(param, gisa_id){
				var url = 'agree_proc_new.php?param='+param+'&gisa_id='+gisa_id+'&outdate=';
				$(function() {
				$.ajax({
						type:"GET",
						url:url,
						data:{query:"jquery"},
						success: function(data, textStatus){ 
							var serverData = data.split('$');			
							if (serverData=="fail"){
								alert('잠시후에 다시 시도해주세요.')
							}else{
								$("#positveCnt").html(serverData[0]+'<span>%</span>');
								$("#negativeCnt").html(serverData[1]+'<span>%</span>');
							}
						}
					});
				})
		}

		function agree_spread(no){
		//	console.log(no);
			var url = 'agree_spread_new.php?no='+no;
			$(function() {
			$.ajax({
					type:"GET",
					url:url,
					data:{query:"jquery"},
					success: function(data, textStatus){ 
						var serverData = data.split('$');
						$("#positveCnt").html(serverData[0]+'<span>%</span>');
						$("#negativeCnt").html(serverData[1]+'<span>%</span>');
					}
				});
			})
		}

	</script>
	<script src="https://developers.kakao.com/sdk/js/kakao.min.js"></script>
	<script type="text/javascript">

		function share(type, kind)
		{
			var popName = '';
			var popWidth = 0;
			var popHeight = 0;

			var txt = '＇2020 인구이야기 팝콘＇ COVID-19가 앞당긴 미래 #MT_Photo';
			var arg = 'https://news.mt.co.kr/photonews/photovod_view.html?no=202006171754587580&cate=total';
			var option = 'width=486, height=286';

			switch(type)
			{
				case "fb":
					var popName = 'facebookPop';
					option = "width=486, height=286";
					break;
				case "tw":
					var popName = 'tweetPop';
					option = "width=486, height=286,scrollbars=yes";
					break;
				case "ks":
					var popName = 'kakaoPop';
					option = "width=600, height=460, resizable=no, scrollbars=yes, status=no";
					break;
				case "nb":
					var popName = 'naverPop';
					txt = '＇2020 인구이야기 팝콘＇ COVID-19가 앞당긴 미래';
					option = "width=486, height=486,scrollbars=yes";
					break;
				case "tg":
					var popName = 'Telegram';
					option = "width=600, height=520, resizable=no, scrollbars=yes, status=no";
				default:
					var popName = '';
					break;
			}

			window.open("https://news.mt.co.kr/share.php?url="+encodeURIComponent(arg)+"&title="+encodeURIComponent(txt)+"&type="+type+"&kind="+kind, popName, option);
		}

	</script>


