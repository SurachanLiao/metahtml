function GetEventsPreferencesBySamAccountName(callback){
var samAccountName = FormsPrincipal.SamAccountName;
$.ajax({
	type: "POST",
	url: "/library/MailingService.aspx/GetEventsPreferencesBySamAccountName",
	contentType: "application/json; charset=utf-8",
	data: "{ samaccountname: '"+samAccountName+"'}",
	dataType: "json",
	async: "false",
	cache: "false",
	success: function (response) {
		var r = response.d;
		var arr = JSON.parse(r);
		callback(arr.EventsPreferences);
		
	},
	error: function (result) {
		console.log("Error " + result.status);
	},
	failure: function (response) {
		console.log(response.d);
	}
	
});	
}

var filterMeetings = new Array();
var filterEvents = new Array();
var CookieMeetings = "";
var CookieEvents = "";	

//IF user Logged IN	
if(FormsPrincipal.IsAuthenticated){
	var Preferences = "";
	GetEventsPreferencesBySamAccountName(function (data) {
    	Preferences = data;
		if(Preferences !== null){
			if(Preferences.Eventspreferences != ""){
				filterEvents = Preferences.Eventspreferences.split(',');
			}
			if(Preferences.Meetingspreferences != ""){
				filterMeetings = Preferences.Meetingspreferences.split(',');	
			}
			var filterEventsUnique = [];
			jQuery.each(filterEvents, function(i, el){
				if(jQuery.inArray(el, filterEventsUnique) === -1) filterEventsUnique.push(el);
			});
			if(filterEventsUnique.length>0){
				if ($.inArray("Other", filterEventsUnique) >= 0){
					filterMeetings.push("Other");
				}
				if ($.inArray("Workshop", filterEventsUnique) >= 0){
					filterMeetings.push("Workshop");
				}
				if ($.inArray("Book launch", filterEventsUnique) >= 0){
					filterMeetings.push("Book launch");
				}
				if ($.inArray("Public Forum", filterEventsUnique) >= 0){
					filterMeetings.push("Public Forum");
				}
			}
			loadYrCalendar_byParameter_HomePage(now.getFullYear(),filterMeetings,filterEvents);
		}
		//START: IF no Local Storage is found
		else{
			loadYrCalendar_HomePage(now.getFullYear());
		}
	});
}
//IF User not logged in	
else{
	loadYrCalendar_HomePage(now.getFullYear());
}

