<script  language="javascript1.2" type="text/javascript">
$.ajax({
	url: '/library/webcast/webcast_data.json',
    dataType: 'json',
    success: function(data) {
		var futureWebcastArray = [];
		var futureContentHtml ="";
		if(data.length > 0){
			
			$.each(data, function(index) {
				var webcast_date = new Date(parseInt(data[index].webcast_date.substr(6)));
				var todaysDate = new Date();
				webcast_date = new Date(webcast_date.getFullYear(),webcast_date.getMonth(),webcast_date.getDate());
				todaysDate = new Date(todaysDate.getFullYear(),todaysDate.getMonth(),todaysDate.getDate());
				if (webcast_date >= todaysDate && (data[index].vod_url == "")){
					futureWebcastArray.push(data[index]);
				}
			});
			
			futureWebcastArray.sort(function(a, b) {
				var textA = a.title_en.toUpperCase();
				var textB = b.title_en.toUpperCase();
				return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;
			});
			futureWebcastArray.sort(function(a, b){
				return a.id - b.id;
			});
			
			if(futureWebcastArray.length > 0){
				futureWebcastArray.sort(function (a, b) {
    				return a.start_time.localeCompare(b.start_time);
				});
				futureWebcastArray.sort(function (a, b) {
    				return a.webcast_date.localeCompare(b.webcast_date);
				});
				console.log(futureWebcastArray);
				$.each(futureWebcastArray, function (index_future) {
					var w_date = new Date(parseInt(futureWebcastArray[index_future].webcast_date.substr(6)));
					var videoDateSTR = w_date.getDate() + " " +month_array_short[w_date.getMonth()+1];
					var webcastTitle = futureWebcastArray[index_future].title_en;
					if(webcastTitle.length > 100){
						webcastTitle = webcastTitle.substr(0,100);
						webcastTitle = webcastTitle + "...";
					}
					futureContentHtml += '<p><b>'+videoDateSTR+':</b> <a href="/english/res_e/webcas_e/live_webcast_e.htm" class="link-blue">'+webcastTitle+'</a></p>';
					futureContentHtml += '<a href="/english/res_e/webcas_e/live_webcast_e.htm" class="link-blue">View all live webcasts <i class="icon-r-arrow"></i></a>';
					return false;
				});
			}
			if(futureContentHtml == ""){
				$("#liveNowDiv").html('<p class="text-danger">Currently no live sessions available.</p>');
			}
			else{
				$("#liveNowDiv").html(futureContentHtml);
			}
		   }
		   else{
				console.log('<h4>No webcasting data found.</h4>');
		   }
       },
      statusCode: {
         404: function() {
           alert('There was a problem with the server.  Try again soon!');
         }
       }
		
    }); 	
if (localStorage.getItem('GenericAccCookie') === null){
	setTimeout(function() {
		loadPopup();
	},1000);	  
}
function loadPopup(){
	if(FormsPrincipal.IsAuthenticated){
		if (verifyUserRoles('DOL-R-WTO#Secretariat#DOL-R-OBS')){
			var SamAccountName = FormsPrincipal.SamAccountName.toLowerCase();
			if (SamAccountName.indexOf("norm") != -1 || SamAccountName.indexOf("obs") != -1 || FormsPrincipal.Email == "") {
				$.ajax({
					url: '/library/xlibrary/del_coordinator.json',
					dataType: 'json',
					success: function(data_del) {
						if(data_del.length > 0){
							if(SamAccountName.toUpperCase() == "USANORMTEST"){
								SamAccountName = "USANORM";
							}
							var delgateArr = $.grep(data_del, function(obj) {
								return obj.SamAccountName == SamAccountName;
							});
							if(delgateArr.length > 0){
								delgateArr.sort(function(a, b){
									return a.Type - b.Type;
								});	
								console.log(delgateArr);
								var delCoordinatorHTML = "";
								var delCoordinatorMainHTML = "";
								var delCoordinatorAlterHTML = "";
								var countryName = "";
								$.each(delgateArr, function(index_del) {
									if(delgateArr[index_del].Type == "Main"){
										delCoordinatorMainHTML += "<br>"+ delgateArr[index_del].Salutation + " " + delgateArr[index_del].FirstName + " " + delgateArr[index_del].LastName + " <a href='mailto:"+delgateArr[index_del].email+"' class='blue'>" + delgateArr[index_del].email+"</a> ("+delgateArr[index_del].Type+")";
									}
									else{
										delCoordinatorAlterHTML += "<br>"+ delgateArr[index_del].Salutation + " " + delgateArr[index_del].FirstName + " " + delgateArr[index_del].LastName + " <a href='mailto:"+delgateArr[index_del].email+"' class='blue'>" + delgateArr[index_del].email+"</a> ("+delgateArr[index_del].Type+")";
									}
									//countryName = delgateArr[index_del].Country;
									countryCode = delgateArr[index_del].DOL_GROUP;
									countryCode = countryCode.substring(4, 7);
								});
								delCoordinatorHTML = delCoordinatorMainHTML + delCoordinatorAlterHTML;
								$("#delCord").html(delCoordinatorHTML);
								$("#acc_country").html(getCountryArticle(countryCode));
								$("#popup-click").trigger('click');
							}
							
						}
					}
				});
				
			}
		}
	}   
}	
function getCountryArticle(countryCode){
	var returnCountryArticle = "";
	if(countryCode == "TPK"){
		countryCode = "CHT";
	}
	if(countryCode == "EUC"){
		countryCode = "EEC";
	}
	for(var cc=0; cc < country.length; cc++){
		if(country[cc].code == countryCode){
			returnCountryArticle = "<b>" + country[cc].name + "</b>";
			break;
		}
	}
	return returnCountryArticle;
}	
$("#genericPopup").on('hide.bs.modal', function(){
	if($("input[id='dontShowChk']:checked").val()){
		localStorage.setItem("GenericAccCookie", "dontShowChk");	
	}
});
function UserComment(){
	var samAccountName = FormsPrincipal.SamAccountName;
	var userComments = $("#txtComment").val();
	$.ajax({
		type: "POST",
		url: "/library/MailingService.aspx/InsertShortSurvey",
		contentType: "application/json; charset=utf-8",
		data: "{ language: '"+wto_language+"', responses: '"+userComments+"', surveyid: 'GenericAccount', samaccountname: '"+samAccountName+"'}",
		dataType: "json",
		async: "false",
		cache: "false",
		success: function (response) {
			var r = response.d;
			var arr = JSON.parse(r);
			console.log(arr);
			var modal = $('#genericPopup');
			modal.modal('hide');
		},
		error: function (result) {
			alert("Error " + result.status);
		},
		failure: function (response) {
			alert(response.d);
		}
	});
}	
$("#btnSubmit").click(function() {
	UserComment();
});	
