	<!-- BREAKING NEWS BAR JS -->

    <script type="text/javascript" nopack="1">
    canTick=false;
    </script>
		
	<script src="//ecdn6.wn.com/or/assets/assets/avp/ticker01.js"></script>
	<link rel="stylesheet" href="//ecdn5.wn.com/or/assets/assets/avp/css/ticker02.css" /> 
	

    
    <script type="text/javascript" nopack="1">
    var articleId = 257874457;
    var HOST = document.location.hostname;
    var page_url =  window.location.href;
    
    $(function(){ 
    
    	$('#article-url').attr('data-io-article-url', page_url);
    	
        $.ajax({
            url: 'https://'+HOST+'/api/upge/cheetah-article/breaking',
            dataType: 'json',
            callback: 'breakingNews',
            cache: true,
            success: function(result){
                initTicker(result);
            }
        });
    });
    function animateTickerElement() {
		var speed = 5000;
        var active_li = $('.ticker-container ul div.ticker-active li');
        var active_div = $('.ticker-container ul div.ticker-active');
        var container = $('.ticker-container ul div');
			if ($(window).width() < 500) {
            if (active_li.width() > $(window).width()) {
					setTimeout(function() {
                    active_li.animate({
                        'margin-left': '-' + ((active_li.width() - $(window).width()) + 60)
						}, speed - (speed / 5), 'swing', function() {
							setTimeout(function() {
                            container.find('li').css('margin-left', 0);
							}, ((speed / 5) / 2));
						});
					}, ((speed / 5) / 2));
				}
			} else {
            if (active_li.width() > ($(window).width() - parseInt(active_div.css('left')))) {
					setTimeout(function() {
                    active_li.animate({
                        'margin-left': Math.abs(active_li.width() - ($(window).width() - parseInt(active_div.css('left'))) + 15) * -1
						}, speed - (speed / 5), 'swing', function() {
							setTimeout(function() {
                            container.find('li').css('margin-left', 0);
							}, ((speed / 5) / 2));
						});
					}, ((speed / 5) / 2));
				}
			}
		}
    function initTicker(breaking_news){
        var r = [];
        for(var sid in breaking_news){
            var s = breaking_news[sid];
            if(s.id != articleId){
                r.push('<div><li><span><a target="_blank" href="'+s.url+'">'+s.title+'</a></li></span></div>');
            }
        }
        if(r.length){
          $('.ticker-goes-here').html('<div class="bn-close pull-right"><i class="fa fa-times"></i></div><div class="ticker-container">'+
            '<div class="ticker-caption"><p><span data-trans="Breaking News">Breaking News</span></p></div><ul>'+r.join('')+'</ul></div>').addClass('ticker');
            if(r.length>1){
                canTick = true;
	window.setInterval(function(){
		animateTickerElement();
	}, 10000);
            }
        }
    }
	</script>

