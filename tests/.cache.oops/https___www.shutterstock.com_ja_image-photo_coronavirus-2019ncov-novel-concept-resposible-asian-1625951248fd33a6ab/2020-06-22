</script><script charSet="UTF-8">
    var previousOnError = window.onerror;
    window.onerror = function(msg, url, line, column, err) {
      var scriptSource = url || 'untrusted external vendor script';
      var message = 'UncaughtException in ' + scriptSource + ' l:' + line + ' c:' + column + ' reason: ' +  msg;

      if (!err || msg === 'Script error.') {
        var scriptError = new Error(message);
        scriptError.name = 'VendorScriptError';
        if (!url || !url.includes('.shutterstock.com')) {
          return true;
        }
        window.NREUM && window.NREUM.noticeError && window.NREUM.noticeError(scriptError, { msg: msg });
        return true;
      }

      if (previousOnError) return previousOnError(message, scriptSource, line, column, err);
      throw err;
    };

    if (window.NREUM && window.NREUM.setErrorHandler) {
      window.NREUM.setErrorHandler(function (err) {
        if (err.message === 'Script error.') return true;
      });
    }
    </script><script charSet="UTF-8">if (window.NREUM && window.NREUM.setCurrentRouteName) {
                window.NREUM.setCurrentRouteName('image-:imageType/:slug');
