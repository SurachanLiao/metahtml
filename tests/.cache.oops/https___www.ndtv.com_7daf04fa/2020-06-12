</div></div></div></div><script type="text/javascript">
function changetabcorona(str) {

if (str == "graph") {
                  
if (document.getElementById("coronagraph").src != "https://edata.ndtv.com/coronavirus/daily/india_color.html?desktop=2")
{
document.getElementById("coronagraph").src = "https://edata.ndtv.com/coronavirus/daily/india_color.html?desktop=2";
}


} 
else if (str == "spread") 
{   
               
if (document.getElementById("spread").src != "https://edata.ndtv.com/coronavirus/graphGrowth.html?day=day")
{
document.getElementById("spread").src = "https://edata.ndtv.com/coronavirus/graphGrowth.html?day=day";
}

}
else if (str == "districts") {

    if (document.getElementById("districts").src != "https://edata.ndtv.com/coronavirus/table/district_table.html?fexpand=1&hidetop=1") {
        document.getElementById("districts").src = "https://edata.ndtv.com/coronavirus/table/district_table.html?fexpand=1&hidetop=1";
    }

}
else if (str == "indiatable") 
{         
         
if (document.getElementById("indiatable").src != "https://edata.ndtv.com/coronavirus/graph.html?tabs=1&type=daily&height=240")
{
document.getElementById("indiatable").src = "https://edata.ndtv.com/coronavirus/graph.html?tabs=1&type=daily&height=240";
}

}
else if (str == "dailynew") {

    if (document.getElementById("dailynew").src != "https://flo.uri.sh/visualisation/1747497/embed") {
        document.getElementById("dailynew").src = "https://flo.uri.sh/visualisation/1747497/embed";
    }

}
else {
             
if ( document.getElementById("coronaworld").src != "https://edata.ndtv.com/coronavirus/table/world_table.html")
{
document.getElementById("coronaworld").src = "https://edata.ndtv.com/coronavirus/table/world_table.html";
}

} 

}
</script>
