    <section id="block-system-main" class="block block-system clearfix">

      
  
    <h1>New Jersey News</h1>
    <section  class="bg-light card card-article data-item border mb-3 mb-sm-4 rounded-m shadow-slot article_type article-nid-28777225 " data-nid="28777225" data-item_type="article" data-promoted="" data-uk="article_nid-28777225" data-slot_weight="0">
    <!-- Content -->
    <section v-pre class='pt-3 pt-sm-4 pb-2 pb-sm-3'>
              <!-- Mobile Badge -->
        <section class='d-sm-none px-3 px-sm-4'>
          <h6 class='text-black-50 lineheight-1 m-0 pb-2'>
