    .map(function(e) {
        return e.getAttribute('id');
    });
var divId = '';
data.adSlots = data.adSlots.filter(function(adSlot){
    divId = data.divId + '_' + adSlot.customTargeting.p.join('_');
    divId += '_' + '566a968a268e3ece348b4585'
