<script type="text/javascript">
  (function(){
    dfpController.enableDebugMode();
    dfpController.loadGPTScript();

    // Set placement specific mappings
    (function(){
        var size_mapping = {};
        
        size_mapping = {
          "bottom_1x1": [1,1],
          "entry_paragraph_1": [[300,250], [336,280]],
          "entry_paragraph_2": [300,250],
          "entry_paragraph_3": [[300, 600], [300, 250]],
          "entry_paragraph_4": [300,250],
          "leaderboard_bottom": [728,90],
          "leaderboard_flex": [[728,90], [970,250]],
          "leaderboard_flex_middle": [[728,90], [970,250]],
          "leaderboard_flex_bottom": [[728,90], [970,250]],
          "right_rail_300x250_lower": [[300,600], [300,250]],
          "right_rail_flex": [[300,600], [300,250]],
          "right_rail_lower": [[300,600], [300,250]],
          "right_rail_upper": [300,250],
          "sharebox_260x60": [260,60],
          "slideshow_300x250_req": [300,250],
          "slideshow_728x90_req": [728,90],
          "top_1x1": [1,1]
        };
        
        dfpController.setSizeMapping(size_mapping);
        return;
    })();

    // Set targeting
    (function(){
      var targeting = {};
      googletag.cmd.push(function() {
        for (var k in targeting) {
          if (targeting[k] !== null) {
            dfpController.log("Set Targeting: " + k + " -> " + targeting[k]);
            googletag.pubads().setTargeting(k, targeting[k]);
          }
        }
      });
    })();

    // Some DFP settings
    (function(){
      googletag.cmd.push(function() {
        googletag.pubads().enableAsyncRendering();
        googletag.pubads().collapseEmptyDivs(true);
        googletag.pubads().setCentering(true);
        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
      });
    })();

    dfpController.setSlotRenderEvents();
    dfpController.setCustomBidder(window.dfpBidder);
    //dfpController.loadAdWhenSlotInView();
    return;
  })();
</script>
