<div id="modulous_right_rail_flex" class="ad_spot desktop-only">
    <script type="text/javascript">
  if ( "function" === typeof(adSetSyncDelay) ) {
    adSetSyncDelay('125');
  }
</script>
<div id="ad_right_rail_flex" class="ad_wrapper_top"><!-- TEMPLATE CODE -->
<div id="dfp_300x250" class="dfp clearfix ad-right_rail_flex">
  <script type="text/javascript">
    (function(){
      var dfp_placement_slug = 'right_rail_flex';
      var dfp_slot_id = '300x250';
      var dfp_div_id = "dfp_" + dfp_slot_id;
      var dfp_div_el = document.getElementById(dfp_div_id);

      var dfp_prefix = "" || dfpController.getDfpPrefix() || "";

      

      var display_in_template = false;
      
      

      var out_of_page_slot = false;
      try {
        var size_mapping_str = '';
        var placement_size = dfpController.getSizeByPlacement(dfp_placement_slug);
        if (placement_size.length) {
          size_mapping_str = " | Sizes: " + JSON.stringify(placement_size);
        }
        dfpController.log("Slot: " + dfp_placement_slug + " -> " + dfp_prefix + "_" + dfp_slot_id + size_mapping_str);
      } catch(e) {
      }

      googletag.cmd.push(function(){
        var dfp_slot = null;
        if (out_of_page_slot) {
          dfp_slot = googletag.defineOutOfPageSlot(dfp_prefix + "_" + dfp_slot_id,
            dfp_div_id).addService(googletag.pubads());
        } else {
          dfp_slot = googletag.defineSlot(dfp_prefix + "_" + dfp_slot_id,
            dfpController.getSizeByPlacement(dfp_placement_slug),
            dfp_div_id).addService(googletag.pubads());
        }

        if ( display_in_template ) {
          if ( !dfpController.hasCustomBidder() ) {
            dfpController.displaySlot(dfp_slot);
          } else if ( dfpController.getCustomBidder().completed() ) {
            dfpController.displaySlot(dfp_slot, true);
          }
        }

        dfpController.attachAdReloadEvent(dfp_div_el, dfp_slot);
        return;
      });
      return;
    })();
  </script>
</div>
<!-- TEMPLATE CODE ENDS --></div>
