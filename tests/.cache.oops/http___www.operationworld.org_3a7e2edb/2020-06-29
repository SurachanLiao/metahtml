    (function($){
        $(document).ready(function() {
            $('#banner-rotator').rotator({delay: 5000});
            $('.left_nav ul').hide();
            $('.left_nav_active').show();
            $('.left_nav li').click(function(){
                $('.left_nav ul').not(this).slideUp('fast');
                // $('.left_nav ul').slideUp('fast');
                if ($(this).find('ul').css("display")=="block"){
                    $(this).find('ul').slideUp('fast');
                } else {
                    $(this).find('ul').slideDown('fast');
                }
            });
            $('#slider').easySlider({
                prevText: '',
                nextText: ''
            });
        });
    })(jQuery);
    </script>

