    }).on('changeDate', function(ev){
      var url = "/archive/content" + ev.format('/yyyy/mm/dd') + "/";
      if ('false' == 'true') {
        window.open(url, '_blank');
      } else {
        window.open(url, '_self');
      }
    });
  });
</script>
