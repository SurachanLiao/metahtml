<script>
    var base_url = 'https://www.wikizero.com/';
    var lng = 'en';

    // var key = 'pk_test_S1dIfHKmXr3nbq6sHXASrgQP001ACohT7c';
    var key = 'pk_live_QFWXKO2Cf6YlXApVyoTaRv2500uibY0nor';
    
    window.onload = function(){
        
        if(window.innerWidth < '768'){
            
            toastr.options = {
                "closeButton": true,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-top-center",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }
        } else{

            toastr.options = {
                "closeButton": true,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-top-right",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }
        }

    }
    
</script>
<script src="https://www.wikizero.com/assets/js/profile.js"></script>
<script src="https://www.wikizero.com/assets/js/uikit_custom.js"></script>
<script src="https://js.stripe.com/v3/"></script>
<script src="https://www.wikizero.com/assets/js/cart.js"></script>
<script>

$(document).ready(function (){ 
    // page modified script
    $(".mw-parser-output").prepend('<h2 style="display: none;">Top View</h2>');
    $("#page_view p sup").remove();$("#page_view h2 .mw-editsection").remove();
    $("#page_view sup").remove();

    $('#page_view h2').each(function(index, value){
        var cont = $(this).text();
        var titles = cont.replace(/\s+/g, '_').toLowerCase();
        var ind = index + 1;
        $(this).nextUntil('#page_view h2').addBack().wrapAll('<div id="page_articles" class="section_'+titles+' page_content"></div>');
                $(this).append('<button type="button" class="btn btn-lg fas fa-play-circle" data-toggle="modal" data-target="#signUpPopup"></button>');
            });

    // wrap each sub headings
    $('#page_articles h3').each(function(index, value){
        var title = $(this).find('span').text();
        var tit = title.replace(/[^\w\s]/gi, '_').toLowerCase();
        var titl = tit.replace(/[\])}[{(]/g, '_').toLowerCase();
        var titles = titl.replace(/\s+/g, '_').toLowerCase();
        // console.log(title);
        var ind = index + 1;
        $(this).nextUntil('#page_articles h3').addBack().wrapAll('<div id="sub_articles" class="sub_section_'+titles+'"></div>');
                $(this).append('<button type="button" class="btn btn-lg fas fa-play-circle" data-toggle="modal" data-target="#signUpPopup"></button>');
            });

    // under main heading wrap each ul tags in p tags
    $('#page_articles ul').each(function(index, value){
        $(this).nextUntil('#page_articles ul').addBack().wrapAll('<p>');
    });

     // under main headings wrap each table tags in p tags
    $('#page_articles table').each(function(index, value){
        $(this).nextUntil('#page_articles table').addBack().wrapAll('<p>');
    });

     $('#page_view .infobox').addClass("table table-responsive bg-secondary");        
     $('#page_articles .toccolours').addClass("table table-dark table-responsive");        
     $('#page_articles .wikitable').addClass("table table-dark table-responsive");        
     $('#page_articles .vertical-navbox').addClass("table bg-secondary table-responsive");        

    // wrap each sub level headings
    $('#page_articles h4').each(function(index, value){
        var title = $(this).find('span').text();
        var tit = title.replace(/[^\w\s]/gi, '_').toLowerCase();
        var titl = tit.replace(/[\])}[{(]/g, '_').toLowerCase();
        var titles = titl.replace(/\s+/g, '_').toLowerCase();
        // console.log(title);
        var ind = index + 1;
        $(this).nextUntil('#page_articles h4').addBack().wrapAll('<div id="sub_articles" class="sub_section_'+titles+'"></div>');
                $(this).append('<button type="button" class="btn btn-lg fas fa-play-circle" data-toggle="modal" data-target="#signUpPopup"></button>');
            });

    // play audio if the user subscribe to premium account
    $("#btn_play_audio").each(function(){
        $(document).on("click", "#btn_play_audio", function(){
            $(this).attr('id','btn_pause_audio');
            $(this).removeClass('fa-play-circle');
            $(this).addClass('fa-pause-circle');
            var title = $(this).data('title-class');
            responsiveVoice.cancel();
            responsiveVoice.speak($(".section_"+title+ " P").text(), "US English Female");
            responsiveVoice.speak($(".sub_section_"+title+ " p").text(), "US English Female");
            responsiveVoice.enableEstimationTimeout = false;
        });
    }); 

    // pause button
    $(document).on("click", "#btn_pause_audio", function(){ 
        $(this).removeClass('fa-pause-circle');
        $(this).addClass('fa-play-circle');
        $(this).attr('id','btn_play_audio'); 
        responsiveVoice.pause(); 
    });

    $(function () {
        $('[data-toggle="popover"]').popover();
    });


    window.onscroll = function() { favsScroll() };

    var header = document.getElementById("listIcons");
    var sticky = header.offsetTop;

    function favsScroll() {
        if (window.pageYOffset > sticky) {
            header.classList.add("sticky");
        } else {
            header.classList.remove("sticky");
        }
    }

    
    var owl = $('.owl-carousel');
    owl.owlCarousel({
        rtl: true,
        margin: 10,
        dots: false,
        nav: true,
        loop: true,
        responsive: {
            0: {
                items: 1
            },
            600: {
                items: 1
            },
            1000: {
                items: 1
            }
        }
    });

});

</script>
