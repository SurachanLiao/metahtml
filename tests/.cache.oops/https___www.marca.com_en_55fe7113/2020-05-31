function getNotLazyLoad(data) {
    var a = data.adSlots[0].adUnit.split('/');
    var s = a[a.length - 1];
    if (s == 'portada') {
        return ['m', 'r', 'si', 'sd', 'r-nata', 'b300x100', 'b300x100b', 'b300x100c', 'c990x50_320x53b', 'f'];
    } else if ((s == 'n') || (s == 'o') || (s == 'cro')) {
        return ['m', 'r', 'sd', 'si', 'f', 'f2', 'f3', '660x50', 'nata', 'natb'];
    } else if (s == 'dir') {
        return ['m', 'r', 'sd', 'si', 'f2', 'f3'];
    }
    return [];
}

function getrootMargin(data) {
    var p = '25';
    var a = data.adSlots[0].adUnit.split('/');
    var s = a[a.length - 1];
    if (s == 'portada') {
        p = '200';
    }
    data.customTargeting.llrm = [p];

    return p + '% 0px ' + p + '% 0px';
}

