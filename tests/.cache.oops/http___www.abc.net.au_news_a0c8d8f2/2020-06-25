<script>
// Add a class so we can load a placeholder.
(function() {
    var el = document.querySelectorAll('[data-player-init]');
    if (!el) return;
    var isMobile = document.body.classList.contains('platform-mobile');
    for (var i = 0; i < el.length; i++) {
        var container = el[i];
        for(j = 0; j < 5; j++){
            container = container.parentNode;
            if (['inline-content wysiwyg', 'section promo', 'inline-content wysiwyg right', 'embed-wysiwyg richtext'].indexOf(container.className) !== -1) {
                container.classList.add('player-promo');
            }
        }
    }
})();
</script>
