h=0;g&&(h=g.offsetHeight),d.classList.contains("sticky-rail")&&0<h&&(d.classList.remove("sticky-unsupported"),d.classList.remove("fixed"),d.classList.remove("width-100"),DOMHelpers2.debounce(window,function(){window.pageYOffset<f||window.pageYOffset>h?d.setAttribute("style",""):window.pageYOffset>=f&&d.setAttribute("style","position: fixed; left: 36px; top: 80px; max-width: 30%;")},"scroll",100))}}},{key:"reSetHeight",value:function(){if(!this.supported){var a=document.getElementById("app-bar-sticky"),
