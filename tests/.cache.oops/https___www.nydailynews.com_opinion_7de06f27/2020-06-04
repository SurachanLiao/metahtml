            } else {
              oPageUnload('trb_ad_300x600/300x250_1_/opinion/', true);
            }
          }
        
        function loadAd() {
            dfpService.appendAd({
                id: 'opinion_sf_797',
                optimeraId: 'trb_ad_300x600/300x250_1_/opinion/',
                adType: 'cube',
                slotName: '/4011/trb.nydn/opinion/sf',
                displayMobile: true,
                displayDesktop: true,
                isOOP: false,
                is5x1: false,
                dimensionsString: '[[300, 600], [300, 250]]',
                sizemapString: '',
                sizemapRefresh: 'false',
                refreshOnPhotoChange: false,
                adRefreshInterval: 0,
                includeInA9: true,
                includeInIndexExchange: true,
                loadImmediately: true
            }, false, true)
        }

        loadAd();
      
    })()
