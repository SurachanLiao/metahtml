success){iterateFeatures(testId,function(feature){variantLoaded(testId,testId,feature,success)})}function showOtherVariant(testId,winner,timeLeft){var uri=window._context+"/api/v2/render/feature/variant/"+winner+"?rid\x3d"+window._rid+"\x26uri\x3d"+window._uri+"\x26outputType\x3d"+window._outputType;ajax(uri,function(response){iterateFeatures(testId,function(feature){var resources=response.pageResources||{};var parent=feature.parentNode;var div=document.createElement("div");div.innerHTML=response.rendering.trim();
