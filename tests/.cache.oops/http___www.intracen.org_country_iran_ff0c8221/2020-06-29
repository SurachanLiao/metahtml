        $(window).load(function () {
            // $('.nav-tabs').tabdrop();
            if ($(window).width() > 768) {
                //$('.nav-itc .dropdown').hover(function () {
                $('.nav-itc .dropdown').on('mouseenter mouseleave', function () { //according to client requirement in basecamp
                    $(this).find('.dropdown-menu').stop(true, true).delay(100).fadeIn(200).parent().addClass('focus');
                }, function () {
                    $(this).find('.dropdown-menu').stop(true, true).delay(100).fadeOut(200).parent().removeClass('focus');
                });
            }
        });
            $('.menu-main li a').each(function () {
                if ($(this).next.length > 0) {
                    $(this).addClass('hover');
                }

            });

            $('.menu-main li a.hover').click(function () {
                if ($(this).next().is(':hidden')) {
                    $('.menu-main li a').next().slideUp('slow');
                    $(this).next().slideDown('slow');
                }
                else {
                    $(this).next().slideUp('slow');
                };
                // $('.menu-main li a.hover').css('padding-left', '25px');
            });
    </script>
    

