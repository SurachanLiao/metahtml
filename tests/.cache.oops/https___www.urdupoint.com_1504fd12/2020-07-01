    </noscript>
<script type="text/javascript">var loadDeferredStyles=function(){var a=document.getElementById("deferred-styles"),b=document.createElement("div");b.innerHTML=a.textContent,document.body.appendChild(b),a.parentElement.removeChild(a)},raf=requestAnimationFrame||mozRequestAnimationFrame||webkitRequestAnimationFrame||msRequestAnimationFrame;raf?raf(function(){window.setTimeout(loadDeferredStyles,0)}):window.addEventListener("load",loadDeferredStyles);</script>

<script async type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5a64c9496088e557"></script>
<script async type="text/javascript" src="https://cdn.urdupoint.com/eng_includes/js/main.js"></script>
<script type="text/javascript">
	loadScript("https://cdn.urdupoint.com/eng_includes/js/jquery.js",function(){
			(function( $ ){
			   $.fn.elmenu = function( menuOrder ) {
			
				  var theCanvas    = $( '#canvas' );
				  var mainNav      = $( '#elmain' );
				  var mobileNav    = $( '#elmobile' );
				  var toggleOpen   = $( '#elmtoggle' );
				  var toggleClose  = $( '#elmobiletoggle' );
			
				  function mobileClasses(e){
			
					if(document.getElementById("mob_nav_lang_hndlr") && document.getElementById("mob_nav_lang_hndlr").innerHTML == 'urdu')
					{
						$( '#elmobile' ).addClass('urdu_nav_mob');
					}
					
					 $( '#elmobile .elm__list' ).addClass( 'm-elm__list' ).removeClass( 'elm__list' );
					 $( '#elmobile .elm__item' ).addClass( 'm-elm__item' ).removeClass( 'elm__item' );
					 $( '#elmobile .elm__link' ).addClass( 'm-elm__link' ).removeClass( 'elm__link' );
					 $( '#elmobile .elm__list--sub' ).addClass( 'm-elm__list--sub' ).removeClass( 'elm__list--sub' );
					 $( '#elmobile .elm__item--sub' ).addClass( 'm-elm__item--sub' ).removeClass( 'elm__item--sub' );
					 $( '#elmobile .elm__link--sub' ).addClass( 'm-elm__link--sub' ).removeClass( 'elm__link--sub' );
					 $( '#elmobile .elm__list--subsub' ).addClass( 'm-elm__list--subsub' ).removeClass( 'elm__list--subsub' );
					 $( '#elmobile .elm__item--subsub' ).addClass( 'm-elm__item--subsub' ).removeClass( 'elm__item--subsub' );
					 $( '#elmobile .elm__link--subsub' ).addClass( 'm-elm__link--subsub' ).removeClass( 'elm__link--subsub' );
					 addSubTriggers();
				  }
			
				  function addSubTriggers() {
			
					 var elmtarget = $( '#elmobile .hassub > .m-elm__link' );
					 $( elmtarget ).each(function( i ) {
						var addTrigger = '<span class="elmtriggerchild-' + i + '"><img src="https://photo-cdn.urdupoint.com/eng_includes/images/arrow-down.svg" alt="Open Sub Menu"></span>';
						$( this ).after( addTrigger );
					 });
			
					 var elmtargetSub = $( '#elmobile .hassubsub > .m-elm__link--sub' );
			
					 $( elmtargetSub ).each(function( i ) {
						var addSubTrigger = '<span class="elmtriggergrandchild-' + i + '"><img src="https://photo-cdn.urdupoint.com/eng_includes/images/arrow-down.svg" alt="Open Sub Menu"></span>';
						$( this ).after( addSubTrigger );
					 });
			
					 swapMobileMenus();
				  }
				  
				  function swapMobileMenus() {
			
					 var $modules = $( '#elmobile > ul' );
			
					 for ( var i = 0, l = menuOrder.length; i < l; i++ ) {
						$( $modules.get(menuOrder[i]) ).appendTo( mobileNav );
					 }
					 findTriggers();
				  }
			
				  function findTriggers() {
			
					 var elmChildTriggers = $( '*[class^="elmtriggerchild-"]' );
					 $( elmChildTriggers ).each(function( i ) {
						$( elmChildTriggers[i] ).click(function(e) {
						   toggleSubs( this );
						   toggleArrow( this );
						});
					 });
			
					 var elmGrandchildTriggers = $( '*[class^="elmtriggergrandchild-"]' );
					 $( elmGrandchildTriggers ).each(function( i ) {
						$( elmGrandchildTriggers[i] ).click(function(e) {
						   toggleSubs( this );
						   toggleArrow( this );
						});
					 });
				  }
			
				  function toggleNav() {
					 $( 'body' ).toggleClass( 'show-nav' );
				  }
				  
				  function toggleSubs(e) {
					 if( !$(e).hasClass( 'open' ) ) {
						$(e).addClass( 'open' );
						$(e).next( 'ul' ).slideDown( 250 );
					 } else {
						$(e).removeClass( 'open' );
						$(e).next( 'ul' ).slideUp( 250 );
					 }
				  }
			
				  function toggleArrow(e) {
			
					 var theicon = $( e ).children();
			
					 if( $(e).hasClass( 'open' ) ) {
						$( theicon ).addClass( 'rotate' );
					 } else {
			
			
						$( theicon ).removeClass( 'rotate' );
					 }
				  }
			
				  function toggleArrowSub(e) {
			
				  }
				 
				  var hdnav = []
				  hdnav = mainNav.find( '*[class^="nav--"]' );
			
				  for ( var i = 0, l = hdnav.length; i < l; i++ ) {
					 $( hdnav[i] ).clone().appendTo( mobileNav );
					 mobileClasses();
				  }
			
				  toggleOpen.click(function(e) {
					 toggleNav();
				  });
			
				  toggleClose.click(function(e) {
					 toggleNav();
				  });
				  return this;
			   };
			})( jQuery );	
				  
			 var menuOrder = ['0'];
   			 $( '#elmain' ).elmenu( menuOrder );
			 
			 jQuery( document ).ready(function() 
		{
			jQuery.post(site_base+"ajax_namaz.php", { type: "get_namaz"})
			  .done(function( data ) {
				jQuery("#real_time_namaz").html(data);
			  });

					});			  
	});	
	
	</script>
<script async type="text/javascript">
document.onreadystatechange = function () 
{
	if (document.readyState === "complete") 
	{
		if(document.getElementById("art_star_wrap"))
		{
			go_star_rate("art_star_wrap","art_star_wrap_msg");
		}
	}
}
</script>
<script type="text/javascript">
	if(document.getElementById("breaking_box_wrap"))
	{
		loadScript("https://embed.urdupoint.com/rss/json/top/breaking_news.json",function()
		{
			if(up_breaking_news.news_id)
			{
				var bn_id 			= up_breaking_news.news_id;
				var bn_news_title 	= up_breaking_news.news_title;
				var bn_news_image 	= up_breaking_news.news_image;
				var bn_image_alt 	= up_breaking_news.image_alt;
				var bn_news_url 	= up_breaking_news.news_url;
							
				document.getElementById("breaking_box_wrap").innerHTML = '<a target="_blank" class="urdu full fl ar rtl mb10 bsbb breaking_box" href="'+bn_news_url+'?utm_source=Internal&utm_medium=Breaking-News"><span class="txt_white fs20 lh40 fl bn_tag">بریکنگ نیوز</span><img class="bn_img" src="'+bn_news_image+'" alt="'+bn_image_alt+'"><p class="fs17 lh34 txt_white mt10">'+bn_news_title+'</p></a>';
			}
			
		});	
	}
	
	if(document.getElementById("menu_app_icon"))
	{
		var isAndroid = /(android)/i.test(navigator.userAgent);
		if (isAndroid)
		{
			document.getElementById("menu_app_icon").style.display = 'inline-block';
		}
	}
	
</script>
