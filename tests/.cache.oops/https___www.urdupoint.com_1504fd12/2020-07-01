<div id="Side2"></div>
<script>
          var refreshed_Side2 = false;
    
            var pos_Side2 = document.getElementById("Side2").getBoundingClientRect().top;
        
          var adAlmostVisibleScrollValueTop_Side2 = pos_Side2 - 100;
          
          
          //console.log('Side2 must be viewd after : '+adAlmostVisibleScrollValueTop_Side2);
          
          var listener_Side2 = function() {
            if (window.pageYOffset >= adAlmostVisibleScrollValueTop_Side2 && !refreshed_Side2) {
              googletag.cmd.push(function() {
                googletag.pubads().refresh([Side2]);
                
                // googletag.display('Side2');
                
                //console.log('Side2 viewd at : '+window.pageYOffset);
              });
              refreshed_Side2 = true;
    
              window.removeEventListener('scroll', listener_Side2);
            }
          }
          window.addEventListener('scroll', listener_Side2);
          
        </script>
</div>
</div>
<script type="application/javascript">
	if(document.getElementById("desk_side_second"))
	{
		var TopLeft_pos_dside2 = getElementTopLeft("desk_side_second");
		
		var pos_dside2= TopLeft_pos_dside2.top;
		
		pos_dside2_max = pos_dside2 + 1200;

		var listener_dside2 = function() {
			if (window.pageYOffset >= pos_dside2 && window.pageYOffset < pos_dside2_max) 
			{
				document.getElementById("desk_side_second").className = "stick_pos";
			}
			else
			{
				if(document.getElementById("desk_side_second").className == 'stick_pos') 
				{
					document.getElementById("desk_side_second").className = "";
				}
			}
		}
		window.addEventListener('scroll', listener_dside2);
	}
</script>
