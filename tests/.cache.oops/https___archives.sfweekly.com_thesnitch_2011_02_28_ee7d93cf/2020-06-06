      <script>
        (function ($) {
          function togglePanel(container, index) {
            $(container).children('.panels').children('li').hide();
            $(container).children('.panels').children('li[value="' + index + '"]').show();
            $(container).children('.tabs').children('li').removeClass('selected');
            $(container).children('.tabs').children('li[value="' + index + '"]').addClass('selected');

            return true;
          }

          togglePanel($('#PopularStories>.componentBody'), 1);

          $('#PopularStories>.componentBody>.tabs>li').click(function(v) {
            var index = $(this).attr('value');
            if (index) {
              togglePanel($(this).closest('.componentBody'), index);
            }
            v.preventDefault();
          });
        })(jQuery);
      </script>
    </div>
