			var elAdUrl = elVast.getAttribute(attr);
			var elAdUnit = elAdUrl.match(/iu=([^&]*)/)
			elAdUnit = elAdUnit.length ? elAdUnit[1] : false
			if (elAdUnit && elAdUnit  === from) {
				var newAdUnit = elAdUrl.replace(/(iu=)[^\&]+/, '$1' +to)
				elVast.setAttribute(attr, newAdUnit)
            }
        });
    }
	function cambia (from, to) {
		if (from && to) {	
			attrRegular.forEach(function(attr) {
				cambiaRegular(from, to, attr);
            })
			attrVast.forEach(function(attr) {
				cambiaVast(from, to, attr);
            })
			
        }
    }
	function info () {
		var elAd = document.querySelector(`[${attrRegular}]`);
		if (elAd) {
			return elAd.getAttribute(attrRegular);
        }
    }
	return {
		cambia: cambia,
		info: info
    }
})()*/</script><script>document.addEventListener('DOMContentLoaded', function () {
