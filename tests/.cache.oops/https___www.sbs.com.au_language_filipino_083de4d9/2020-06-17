      <div class="block block--feature" data-module="block_module" data-edit="1" data-slug="filipino-pinoy-foodie">
              
  <h2 class="page__section-title">Pinoy Foodie</h2>

            <div class="block__container feature" data-count="5">
                              
                  
            
                                                              

<div class="preview preview--feature">
  <div class="preview__wrap">
                            
  
  


        

      
      
  
    
  
  
        
    
    

<div class="media-image media-image--16-9 preview__image">
  <a class="media-image__link" href="/language/filipino/audio/isang-server-binigay-ang-shifts-sa-mga-katrabaho-naging-bao-supplier" title="Isang server binigay ang shifts sa mga katrabaho, naging bao supplier">                  
              <picture class="media-image__picture" data-lazyload='[{ "srcset": "https://sl.sbs.com.au/public/image/file/7f0c9e89-e364-4819-b179-a3456dba0db3/crop/16x9_large", "media": "(min-width: 1023px)" },
