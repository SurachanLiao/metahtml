					function latest_lm(lid, t) {
						jQuery.ajax({
							type: 'POST', cache: false, 
							url: '/upi/dw/m/r/latest_headlines',  
							data: 'more='+lid, 
							dataType: 'html', 
							success: function(d) {
								jQuery(t).remove();
								jQuery('#latest_lm').append(d);
							}
						});
