<script type="text/javascript">
var screenw = $(window).width();
	try {
        var topDiv = document.getElementById("topdivad");
	var mobadDiv = document.getElementById("mobiletopad");
	var midDiv = document.getElementById("toprightlongad");
	var BottomDiv = document.getElementById("bottomsmallad");
	if (screenw > 767) {
	topDiv.innerHTML = document.getElementById("topad").innerHTML;
	}
	 if (screenw < 768) {
	mobadDiv.innerHTML = document.getElementById("topad").innerHTML;
	}  
	midDiv.innerHTML = document.getElementById("midad").innerHTML;  
        BottomDiv.innerHTML = document.getElementById("bottomsmallads").innerHTML;               
        document.getElementById("topad").innerHTML = "";
	document.getElementById("rightlongad").innerHTML = "";
	document.getElementById("bottomsmallads").innerHTML = "";
	}
	catch(err) {
    		
	}
</script>

