  var aiNextSlotId = 1;
 
 function aiGenerateNextSlotName() {
   var id = aiNextSlotId ++;
   return 'ai-slot-' + id;
 }
 
 function aiLoadBlock(adUnitPath, size, map) {
  var slotName = aiGenerateNextSlotName();
  document.getElementById ("ai-block").id = slotName;
  googletag.cmd.push(function() {
    if (map === 'mapLeader'){
      map = googletag.sizeMapping().
      addSize([980, 0], [970, 90]).
      addSize([768, 0], [728, 90]).
      addSize([320, 0], [320, 50]).
      build();
    }
    else if (map === 'mapLeaderSmall'){
      map = googletag.sizeMapping().
      addSize([768, 0], [728, 90]).
      addSize([320, 0], [320, 50]).
      build();
    }
    else if (map === 'mapBillboard'){
      map = googletag.sizeMapping().
      addSize([980, 0], [970, 250]).
      addSize([768, 0], [728, 90]).
      addSize([320, 0], [320, 50]).
      build();
    }
    else {
      map = false
    }

    if (map){
      var slot = googletag.defineSlot(adUnitPath, size, slotName).defineSizeMapping(map).addService(googletag.pubads());
    } else {
      var slot = googletag.defineSlot(adUnitPath, size, slotName).addService(googletag.pubads());
    }
    googletag.display(slotName);
    googletag.pubads().refresh([slot]);
  });
}
