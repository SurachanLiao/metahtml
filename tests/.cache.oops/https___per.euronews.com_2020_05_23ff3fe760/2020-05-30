\u0627\u0646\u062f\u060c \u0647\u0645\u0647 \u0641\u0631\u06cc\u0627\u062f \u0645\u06cc\u200c\u0632\u062f\u0646\u062f \u0648 \u06af\u0631\u06cc\u0647 \u0645\u06cc\u200c\u06a9\u0631\u062f\u0646\u062f \u0647\u0645\u06af\u06cc \u0645\u0634\u063a\u0648\u0644 \u062f\u0639\u0627 \u06a9\u0631\u062f\u0646 \u0628\u0648\u062f\u0646\u062f.\u00bb \n\n\u0632\u06cc\u0628\u0631 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u067e\u0631\u0648\u0627\u0632 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u0627\u0628\u062a\u062f\u0627\u0621 \u0639\u0627\u062f\u06cc \u0628\u0647 \u0646\u0638\u0631 \u0645\u06cc\u200c\u0631\u0633\u06cc\u062f \u0648 \u06a9\u0633\u06cc \u06af\u0645\u0627\u0646 \u0646\u0645\u06cc\u200c\u06a9\u0631\u062f \u06a9\u0647 \u0633\u0642\u0648\u0637 \u06a9\u0646\u062f \u0627\u0645\u0627 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u06f1\u06f0 \u062a\u0627 \u06f1\u06f5 \u062f\u0642\u06cc\u0642\u0647 \u0628\u0639\u062f \u0627\u0632 \u0641\u0631\u0648\u062f \u0646\u0627\u0645\u0648\u0641\u0642 \u0628\u0631 \u0631\u0648\u06cc \u0628\u0627\u0646\u062f\u060c \u0633\u0642\u0648\u0637 \u06a9\u0631\u062f. \u0627\u0648 \u0645\u06cc\u200c\u0627\u0641\u0632\u0627\u06cc\u062f: \u00ab\u0648\u0642\u062a\u06cc \u0628\u0647 \u0647\u0648\u0634 \u0622\u0645\u062f\u0645\u060c \u062a\u0645\u0627\u0645 \u0622\u0646\u0686\u0647 \u06a9\u0647 \u0645\u06cc \u062a\u0648\u0627\u0646\u0633\u062a\u0645 \u0628\u0628\u06cc\u0646\u0645 \u0648 \u0628\u0634\u0646\u0648\u0645 \u0641\u0642\u0637 \u0622\u062a\u0634 \u0628\u0648\u062f \u0648 \u062c\u06cc\u063a \u0648 \u0641\u0631\u06cc\u0627\u062f. \u0634\u0645\u0627\u0631\u0647 \u0635\u0646\u062f\u0644\u06cc \u0645\u0646 \u0647\u0634\u062a\u200c-\u0627\u0641 \u0628\u0648\u062f. \u06a9\u0645\u0631\u0628\u0646\u062f\u0645 \u0631\u0627 \u0628\u0627\u0632 \u06a9\u0631\u062f\u0645 \u0648 \u0646\u0648\u0631\u06cc \u062f\u06cc\u062f\u0645. \u0627\u0632 \u0627\u0631\u062a\u0641\u0627\u0639\u06cc \u0628\u0647 \u0627\u0646\u062f\u0627\u0632\u0647 \u0633\u0647 \u0645\u062a\u0631 \u0628\u0647 \u067e\u0627\u06cc\u06cc\u0646 \u067e\u0631\u06cc\u062f\u0645. \u0628\u0639\u062f\u062a\u0631 \u0628\u0648\u062f \u06a9\u0647 \u0622\u0645\u0628\u0648\u0644\u0627\u0646\u0633\u200c\u0647\u0627 \u0631\u0633\u06cc\u062f\u0646\u062f \u0648 \u0645\u0631\u0627 \u0628\u0647 \u0628\u06cc\u0645\u0627\u0631\u0633\u062a\u0627\u0646 \u062c\u0646\u0627\u062c \u0622\u0648\u0631\u062f\u0646\u062f.\u00bb \n\n\u0639\u0644\u062a \u0633\u0642\u0648\u0637 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u0686\u0647 \u0628\u0648\u062f\u061f \n\n\u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627\u06cc \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u06cc\u06a9 \u0641\u0627\u06cc\u0644 \u0635\u0648\u062a\u06cc \u0631\u0627 \u0645\u0646\u062a\u0634\u0631 \u06a9\u0631\u062f\u0647\u200c \u0648 \u0645\u06cc\u200c\u06af\u0648\u06cc\u0646\u062f \u06a9\u0647 \u0645\u06a9\u0627\u0644\u0645\u0647 \u062e\u0644\u0628\u0627\u0646 \u0628\u0627 \u0628\u0631\u062c \u06a9\u0646\u062a\u0631\u0644 \u0627\u0633\u062a. \u062f\u0631 \u0627\u06cc\u0646 \u0641\u0627\u06cc\u0644 \u0635\u0648\u062a\u06cc \u06a9\u0647 \u0647\u0646\u0648\u0632 \u0647\u06cc\u0686 \u0645\u0631\u062c\u0639 \u0631\u0633\u0645\u06cc \u0622\u0646 \u0631\u0627 \u062a\u0627\u06cc\u06cc\u062f \u0646\u06a9\u0631\u062f\u0647 \u0627\u0633\u062a \u062e\u0644\u0628\u0627\u0646 \u0628\u0647 \u0628\u0631\u062c \u0645\u0631\u0627\u0642\u0628\u062a \u0641\u0631\u0648\u062f\u06af\u0627\u0647 \u06a9\u0631\u0627\u0686\u06cc \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u06a9\u0647 \u0647\u0631 \u062f\u0648 \u0645\u0648\u062a\u0648\u0631 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u0627\u0632 \u06a9\u0627\u0631 \u0627\u0641\u062a\u0627\u062f\u0647 \u0627\u0633\u062a. \u0628\u0639\u062f \u0635\u062f\u0627\u06cc \u0645\u0627\u0645\u0648\u0631 \u0628\u0631\u062c \u0645\u0631\u0627\u0642\u0628\u062a \u0634\u0646\u06cc\u062f\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u0627\u0632 \u062e\u0644\u0628\u0627\u0646 \u0645\u06cc\u200c\u067e\u0631\u0633\u062f \u0622\u06cc\u0627 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u0631\u0627 \u0628\u0631 \u0633\u06cc\u0646\u0647 \u0628\u0631 \u0628\u0627\u0646\u062f \u0628\u0646\u0634\u0627\u0646\u062f.  \n\n\u06cc\u06a9 \u0645\u0642\u0627\u0645 \u0633\u0627\u0632\u0645\u0627\u0646 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u06cc\u06cc \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u0628\u0647 \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u0631\u0648\u06cc\u062a\u0631\u0632 \u06af\u0641\u062a\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0638\u0627\u0647\u0631\u0627 \u0686\u0631\u062e\u200c\u0647\u0627\u06cc \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u0628\u0627\u0632 \u0646\u0634\u062f\u0647\u200c\u0627\u0646\u062f \u0648 \u0628\u0647 \u0647\u0645\u06cc\u0646 \u062f\u0644\u06cc\u0644 \u0628\u0631\u062c \u0645\u0631\u0627\u0642\u0628\u062a \u0627\u0632 \u062e\u0644\u0628\u0627\u0646 \u067e\u0631\u0633\u06cc\u062f\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0622\u06cc\u0627 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u0628\u0631\u0627\u06cc \u00ab\u0641\u0631\u0648\u062f \u0628\u062f\u0648\u0646 \u0686\u0631\u062e\u00bb \u062a\u0644\u0627\u0634 \u06a9\u0646\u062f \u06cc\u0627 \u0646\u0647.   \n\n\u0628\u062f\u0648\u0646 \u062a\u0631\u062f\u06cc\u062f \u062a\u0639\u06cc\u06cc\u0646 \u0639\u0644\u062a \u062f\u0642\u06cc\u0642 \u0627\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u0645\u0631\u06af\u0628\u0627\u0631 \u0628\u0627 \u0628\u0631\u0631\u0633\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u062c\u0639\u0628\u0647 \u0633\u06cc\u0627\u0647 \u0648 \u0646\u06cc\u0632 \u0645\u06a9\u0627\u0644\u0645\u0627\u062a \u062f\u0627\u062e\u0644 \u06a9\u0627\u0628\u06cc\u0646 \u062e\u0644\u0628\u0627\u0646 \u0645\u0645\u06a9\u0646 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \n\n\u00ab\u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u0627\u06cc\u0646\u062a\u0631\u0646\u0634\u0646\u0627\u0644 \u0627\u06cc\u0631\u0644\u0627\u06cc\u0646\u0632\u00bb \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u06cc \u0627\u06cc\u0631\u0628\u0627\u0633 \u06a9\u0647 \u062f\u0686\u0627\u0631 \u062d\u0627\u062f\u062b\u0647 \u0634\u062f \u0648 \u0633\u0642\u0648\u0637 \u06a9\u0631\u062f \u0627\u0632 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f4 \u0645\u06cc\u0644\u0627\u062f\u06cc \u0628\u0647 \u0646\u0627\u0648\u06af\u0627\u0646 \u0647\u0648\u0627\u06cc\u06cc \u0627\u06cc\u0646 \u0634\u0631\u06a9\u062a \u067e\u06cc\u0648\u0633\u062a\u0647 \u0648 \u0631\u0648\u0646\u062f \u0628\u0627\u0632\u0631\u0633\u06cc \u0627\u06cc\u0645\u0646\u06cc \u0633\u0627\u0644\u0627\u0646\u0647 \u0622\u0646 \u062f\u0631 \u0645\u0627\u0647 \u0646\u0648\u0627\u0645\u0628\u0631 \u06af\u0630\u0634\u062a\u0647 \u0627\u0646\u062c\u0627\u0645 \u0648 \u062a\u0627\u06cc\u06cc\u062f \u0634\u062f\u0647 \u0627\u0633\u062a.   \n\n\u062d\u0648\u0627\u062f\u062b \u0647\u0648\u0627\u06cc\u06cc \u06f1\u06f0 \u0633\u0627\u0644 \u0627\u062e\u06cc\u0631 \u062f\u0631 \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \n\n\u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u062f\u0631 \u06f1\u06f0 \u0633\u0627\u0644 \u0627\u062e\u06cc\u0631\u060c \u0634\u0627\u0647\u062f \u0633\u0647 \u062d\u0627\u062f\u062b\u0647 \u0647\u0648\u0627\u06cc\u06cc \u0628\u0632\u0631\u06af \u062f\u06cc\u06af\u0631 \u0628\u0648\u062f\u0647 \u0627\u0633\u062a. \u062f\u0631 \u0645\u0631\u06af\u0628\u0627\u0631\u062a\u0631\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u0647\u0648\u0627\u06cc\u06cc \u062a\u0627\u0631\u06cc\u062e \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f0 \u0645\u06cc\u0644\u0627\u062f\u06cc\u060c \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u06cc \u0645\u062a\u0639\u0644\u0642 \u0628\u0647 \u0634\u0631\u06a9\u062a \u062e\u0635\u0648\u0635\u06cc \u0627\u06cc\u0631\u0628\u0644\u0648 \u062f\u0631 \u0646\u0632\u062f\u06cc\u06a9\u06cc \u0627\u0633\u0644\u0627\u0645\u200c\u0622\u0628\u0627\u062f \u0633\u0642\u0648\u0637 \u06a9\u0631\u062f \u0648 \u062a\u0645\u0627\u0645\u06cc \u06f1\u06f5\u06f2 \u0633\u0631\u0646\u0634\u06cc\u0646 \u0627\u06cc\u0646 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627 \u06a9\u0634\u062a\u0647 \u0634\u062f\u0646\u062f. \u06cc\u06a9 \u0641\u0631\u0648\u0646\u062f \u0628\u0648\u0626\u06cc\u0646\u06af \u0646\u06cc\u0632 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f2 \u0645\u06cc\u0644\u0627\u062f\u06cc \u0628\u062f\u0644\u06cc\u0644 \u0634\u0631\u0627\u06cc\u0637 \u0628\u062f \u062c\u0648\u06cc \u062f\u0631 \u0631\u0627\u0648\u0644\u067e\u0646\u062f\u06cc \u0633\u0642\u0648\u0637 \u06a9\u0631\u062f \u0648 \u062a\u0645\u0627\u0645 \u06f1\u06f2\u06f1 \u0645\u0633\u0627\u0641\u0631 \u0648 \u0634\u0634 \u062e\u062f\u0645\u0647 \u0622\u0646 \u062c\u0627\u0646 \u0628\u0627\u062e\u062a\u0646\u062f \u0648 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f6 \u06cc\u06a9 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u06cc \u00ab\u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u0627\u06cc\u0646\u062a\u0631\u0646\u0634\u0646\u0627\u0644 \u0627\u06cc\u0631\u0644\u0627\u06cc\u0646\u0632\u00bb \u06a9\u0647 \u0645\u0642\u0635\u062f\u0634 \u0627\u0633\u0644\u0627\u0645 \u0622\u0628\u0627\u062f\u060c \u067e\u0627\u06cc\u062a\u062e\u062a \u0628\u0648\u062f\u060c \u0622\u062a\u0634 \u06af\u0631\u0641\u062a \u0648 \u06f4\u06f7 \u0646\u0641\u0631 \u062c\u0627\u0646 \u062e\u0648\u062f \u0631\u0627 \u0627\u0632 \u062f\u0633\u062a \u062f\u0627\u062f\u0646\u062f.   \n\n## \n\n&quot;,&quot;htmlText&quot;:&quot;&lt;h2&gt;\u0622\u062e\u0631\u06cc\u0646 \u06af\u0632\u0627\u0631\u0634 \u0647\u0627\u06cc \u0631\u0633\u06cc\u062f\u0647 \u0627\u0632 \u067e\u0627\u06a9\u0633\u062a\u0627\u0646 \u062d\u0627\u06a9\u06cc \u0627\u0633\u062a \u06a9\u0647 \u062d\u062f\u0627\u0642\u0644 \u062f\u0648 \u06cc\u0627 \u0633\u0647 \u0646\u0641\u0631 \u0627\u0632 \u0633\u0631\u0646\u0634\u06cc\u0646\u0627\u0646 \u067e\u0631\u0648\u0627\u0632 \u0646\u0627\u062a\u0645\u0627\u0645 PK8303