<script>
Kakao.init('6158b6b574f5c2663c58e3d4bc6f0cb9');
function snsShare(snsTitle, snsSummary, snsImage, snsUrl, NewsGID, NewsDate, ProductID, device, host, snsKey, snsNo )
{
    var post = new Object() ;
    post.p0 = snsTitle;
    post.p1 = snsSummary;
    post.p2 = snsImage;
    post.p3 = snsUrl;
    post.p4 = NewsDate;
    post.p5 = ProductID;
    post.p6 = device;
    post.p7 = host;
    post.p8 = snsKey;
    post.p9 = snsNo;
    post.gid = NewsGID;

    var targetUrl = "/HUB/common/snssharebox.php" ;

    $.ajax({
        type : 'POST' ,
        url : targetUrl ,
        data : post ,
        dataType : 'html' ,
        error : function(result) { alert('error') ; } ,
        success : function(result) {
            $('.share_list').html(result) ;
            sharePopupObj.show();
        }
    });

    return false;
}
</script>

