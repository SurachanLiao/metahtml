            <script>
var year = 2016-1 ;

function  getList()
{
    var targetUrl = "?p0=j&p2="+year;

    $.ajax( {
        type: "GET",
        url: targetUrl,
        dataType: "html",
        success: function (result) {

            $('#newsList').append(result);
            year = year - 5;
            if ( year < 2002 ) $('#btn_more').hide() ;

        },
        error:function(e) {
            alert('잠시후 다시 이용해주세요.');
            return false;
        }
    });

    return false;
}

