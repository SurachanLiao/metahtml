  <script>
    googletag.cmd.push(function() {
        var timesBannerRefreshed = 0;
        var afterViewTimerElapsed = false;
        var afterRenderTimerElapsed = false;

        var refresher = function(slot) {
          console.log("Refreshing cont_bn_sticky_sky");
          timesBannerRefreshed++;
          afterRenderTimerElapsed = false;
          afterViewTimerElapsed = false;
          googletag.cmd.push(function() {
            googletag.pubads().refresh([slot]);
          });
        }

        var checkRefresh = function(slot) {
          console.log("Checking cont_bn_sticky_sky for refresh");
          if (afterRenderTimerElapsed && afterRenderTimerElapsed) {
            if (timesBannerRefreshed >= 5) {
              console.log("Reached the refresh limit for cont_bn_sticky_sky");
            } else {
              refresher(slot);
            }
          }
        }

        var afterRenderTimer = function(slot) {
          console.log("Timer elapsed");
          afterRenderTimerElapsed = true;
          checkRefresh(slot);
        }

        var afterViewTimer = function(slot) {
          console.log('Viewed and additional timer elapsed');
          afterViewTimerElapsed = true;
          checkRefresh(slot);
        }

        googletag.pubads().addEventListener('impressionViewable', function(event) {
          if (event.slot.getSlotElementId() === 'cont_bn_sticky_sky') {
            setTimeout(afterViewTimer.bind(null, event.slot), 5000);
          }
        });

        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
          if (event.slot.getSlotElementId() === 'cont_bn_sticky_sky') {
            console.log('Render ended for cont_bn_sticky_sky');
            console.log(event.slot);
            setTimeout(afterRenderTimer.bind(null, event.slot), 30000);
          }
        });
      googletag.display('cont_bn_sticky_sky');
    });
  </script>
