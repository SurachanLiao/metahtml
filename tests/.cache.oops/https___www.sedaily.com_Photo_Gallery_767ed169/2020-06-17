<script type="text/javascript">        
	$(window).load(function () {
        cateCheck();
    });
    $(window).resize(function () {        
        cateCheck();
    });
    $(window).scroll(function () {
        cateCheck();
    });
    function cateCheck() {
        var _y, _x, _pos;
        var npos = $(window).scrollTop();        
        var _cy = $('#contents-article-view').offset().top - 105;
        var maxtop = 0;
        if ($(".photo_bottom").length != 0) {
            maxtop = $(".photo_bottom").offset().top;
        }
        else {
            maxtop = $(".menu_cover").offset().top;
        }
        if (npos > _cy) {            
            if (maxtop - $(".header_bottom").offset().top < $('.view_cate2').innerHeight() + 60) {
                _y = maxtop - $('.view_cate2').innerHeight() - 100;
                _x = 0;
                _pos = 'absolute';                                
            }
            else {
                _y = 100;
                _x = $('#container').offset().left;
                _pos = 'fixed';                
            }
        }
        else {
            _y = 200;
            _x = 0;
            _pos = 'absolute';                      
        }
        $('.view_cate2').css({ 'position': _pos, 'top': _y, 'left': _x });        
    }       

    $(document).ready(function () {
        if (window.location.href.toLowerCase().indexOf("interactive") != -1) {
            $("#spCateName").text("인터랙티브");
        }
        else if (window.location.href.toLowerCase().indexOf("news") != -1) {
            $("#spCateName").text("포토 뉴스");
        }
        else {
            $("#spCateName").text("포토 갤러리");
        }
    });   
</script>
