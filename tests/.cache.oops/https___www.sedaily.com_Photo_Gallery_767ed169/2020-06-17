<script src="/Inc/Js/Wookmark/imagesloaded.pkgd.js"></script>
<script src="/Inc/Js/Wookmark/wookmark.min.js"></script>
<script type="text/javascript">
    var curPage = 1;
    var isLast = false;
    var gClass = '';
    var sType = 0;
    var sPType = 0;
    var sKeyword = "";

    var requestRunning = false;
    var wookmark,
    container = '#divGData',
    $container = $(container),
    $window = $(window),
    $document = $(document);

    function SetWookMark(){
        var itemWidth;
        var contentWidth = $("#gallery_container").outerWidth();
        if(window.innerWidth <= 900){
            //무조건 2줄
            itemWidth = contentWidth / 2 - 50;
        }
        else{
            //무조건 3줄
            itemWidth = contentWidth / 3 - 50;
        }

        imagesLoaded('#divGData', function () {
            wookmark = new Wookmark('#divGData', {
                autoResize: false,
                itemWidth: itemWidth,
                outerOffset: 2,
                //outerOffset: 5,
                flexibleWidth: function () {
                    return '50%';
                },
                onLayoutChanged  : function(){
                    requestRunning = false;
                }
            });
            $('.dotdot').dotdotdot();
        });
    }

    function Search(val){
        if(val == ""){
            alert("검색어를 입력해주세요.");
            $("#txtKeyword").focus();
        }
        else{
            location.href = "/Photo/Gallery/?gClass=" + gClass + "&sType=" + sType + "&sPType=" + sPType + "&sKeyword=" + encodeURIComponent(val);
        }
    }

    $(window).resize(function () {
        SetWookMark();
    });

    $(document).ready(function () {
        $("#txtMKeyword, #txtKeyword").keypress(function (event) {
	        if (event.which == 13) {
	            Search($(this).val());
	        }
	    });        

	    GetGalleryData = function () {
	        if (requestRunning) {
	            return;
	        }
	        var ajaxOpts = {
	            type: "POST",
	            url: "/Photo/Gallery/GetGalleryData?gClass=" + gClass + "&sType=" + sType + "&sPType=" + sPType + "&sKeyword=" + encodeURIComponent(sKeyword) + "&page=" + curPage,
	            datatype: "json",
	            success: function (json) {
	                isLast = json.isLast;
	                if (json.dataList.length > 0) {
	                    var rowHtml = "";
	                    for (var i = 0; i < json.dataList.length; i++) {
	                        var data = json.dataList[i];
	                        var imgPath = ""
	                        if (data.GImg != "") {
	                            imgPath = "https://img.sedaily.com/Photo/Gallery/" + data.GDate.substring(0, 4) + "/" + data.GDate.substring(4, 6) + "/" + data.GImg;
	                        }
	                        rowHtml += "<div class=\"cardout\" style='display:none;'>";
	                        rowHtml += "<div class=\"cardcontent\">";
	                        rowHtml += "<a href=\"/Photo/Gallery/Viewer/" + data.Seq + "\" class=\"mi_wrap_i\">";
	                        rowHtml += "<div class=\"card\">";
	                        rowHtml += "<div class=\"outline\">";
	                        rowHtml += "<p class=\"thumb\"><img src=" + imgPath + " alt=\"" + data.GName + "\"  onmouseover=\"$(this).stop().animate({ scale: 1.2 }, 200);\" onmouseout=\"$(this).stop().animate({ scale: 1 }, 200);\"></p>";
	                        rowHtml += "<div class=\"p_g_gallery_text\">";
	                        rowHtml += "<strong>" + data.GName + "</strong>";
	                        rowHtml += "<span class=\"p_g_con dotdot\">" + data.GComment;
	                        rowHtml += "</span>";
	                        rowHtml += "<span class=\"p_g_info\">" + DateFormat(data.UpUdate, "yyyy-MM-dd") + "</span>";
	                        rowHtml += "<span class=\"p_g_num\"><img src=\"https://img.sedaily.com/Html/Photo/photo_more_big.png\" alt=\"\" />+ " + data.GPhotoNum + "</span>";
	                        rowHtml += "</div>";
	                        rowHtml += "</div>";
	                        rowHtml += "</div>";
	                        rowHtml += "</a>";
	                        rowHtml += "</div>";
	                        rowHtml += "</div>";
	                    }
	                    var $items = $.parseHTML(rowHtml);
	                    $container.append($items);
	                    SetWookMark();
	                }
	                else{
	                    if(sKeyword != ""){
	                        $("#spSKeyword").text(sKeyword);
	                        $("#aSearch").attr("href","/Search/?scText=" + encodeURIComponent(sKeyword) + "&scPeriod=6m")
	                        $("#aDSearch").attr("href","/Search/?scDetail=detail&scText=" + encodeURIComponent(sKeyword) + "&scPeriod=6m")
	                        $("#divEmpty").show();
	                    }
	                }	                
	            },
	            error: function () {
	                alert("서버통신 오류입니다. 잠시 후 시도해 주세요.");
	            },
	            complete: function () {	                
	                curPage++;
		        }
	        };	        
	        requestRunning = true;
	        $.ajax(ajaxOpts);
	    };

        GetGalleryData();
    });
    $(window).scroll(function () {
        if ($(window).scrollTop() + $(window).height() > $(document).height() - 100)
        {
            if (!isLast && !requestRunning) {                
                GetGalleryData();
            }
        }
    });
</script>	



