<!-- Start Sailthru tag -->
<script>
(function() {
    script = document.createElement('script');
    script.type = 'text/javascript';
    script.async = true;
    script.src = "https://ak.sail-horizon.com/spm/spm.v1.min.js";
    document.body.appendChild(script);
    script.addEventListener('load', function(event) {
      if (mpscall.content_id == "101"){
          Sailthru.init({
            "customerId": '3eba32054e8bb0ab42143893885f95b2',
            "iscustom": true,
            "autoTrackPageview": false,
            "useStoredTags": false
          });
          function getParameterByName(name) {
              var url = window.location.href,
                  value = name.replace(/[\[\]]/g, "\\$&"),
                  regex = new RegExp("[?&]" + value + "(=([^&#]*)|&|#|$)"),
                  results = regex.exec(url);
              if (!results) return null;
              if (!results[2]) return '';
              return decodeURIComponent(results[2]);
          }
          var symbols = getParameterByName('symbol');
          var symbols2 = symbols.replace(/[^a-zA-Z0-9,-]/g, "");
          var symbols3 = symbols2.split(',');
          for (var i=0;i<symbols3.length;i++){
            symbols3[i]='symbol-'+symbols3[i];
          }
          var symbols4 = symbols3.join(',')
          Sailthru.track('pageview', {
             url: document.querySelector("link[rel='canonical']").href,
             tags: symbols4,   // The prefix of "symbol-" followed by the ticker symbol, alphanumeric only.  Oil (@CL.1) would be "symbol-cl1".  Dow (.DJI) would be "symbol-dji". Telefonaktiebolaget LM Ericsson (ERIC.B-SE) would be “symbol-ericb-se”.
          });
      } else {
          Sailthru.init({ "customerId": '3eba32054e8bb0ab42143893885f95b2' });
      }
    });
})();
</script>
<!-- End Sailthru tag -->

