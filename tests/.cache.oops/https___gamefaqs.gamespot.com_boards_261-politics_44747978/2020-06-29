<script type="text/javascript">
var timer, lastsearch;

$('body :not(.gamespace_search)').bind('click touch', function(e)
{
	if($('.gamespace_search').css('display')=='block')
	{
		e.stopPropagation();
		if($(this).attr('class')!='gamespace_search' && $(this).attr('id')!='gamespace_search' && $(this).attr('id')!='gamespace_search_input' && $(this).attr('class')!='head')
		{
			dismiss_search_results(0);
		}
	}
});

$('#gamespace_search_input').val('');

$('#gamespace_search_input').click(function()
{
	$('#gamespace_search_module').addClass('fixed');
		$('#gamespace_search_input').focus();
});

$('#gamespace_search_input').keyup(function(e)
{
	if(e.keyCode==13)
	{
		e.preventDefault();
		var query = $('#gamespace_search_input').val();
		$.ajax(
		{
			type: 'POST',
			url: '/ajax/log_search_click',
			data: { t:6086, p:261, s:query, c:'See All Topic Results', x:99, r:99, key:'769c1b6b' },
			success: function(response)
			{
				window.location ='/boards/261-politics?search=' + escape(query);
			}
		});
	}
});

$('#gamespace_search_input').autocomplete(
{
	minLength: 1,
	delay: 150,
	classes: { 'ui-autocomplete':'gamespace_search' },
	source: '/ajax/board_search?id=261&term=' + $('#gamespace_search_input').val() + '&group=' + 'forum',
	focus: function(){ return false; },
	close: function(event, ui)
	{
		if($('#gamespace_search_input').val())
		{
			$('.gamespace_search').show();
			if($('.gamespace_search').has('li').length===0)
				dismiss_search_results(0);
		}
	},
	select: function(event, ui)
	{
		event.preventDefault();
		log_result(ui.item.row_num);
	}
})
.data('ui-autocomplete')._renderItem = function(ul, item)
{
	var list;
	if(item.result)
	{
		list += '<li><a data-row="'+item.row_num+'" data-url="'+item.value+'" data-type="'+item.header_type+'"><span class="result_title">'+item.label+'<br><span class="result_type">'+item.header+'</span></span></a></li>';
	}
	else if(item.topics)
	{
		list += '<li><a data-row="99" data-url="'+item.url+'" data-type="99" class="dismiss_results">See All Topic Results</a></li>';	}
	else
	{
		list += '<li><a class="dismiss_results" onclick="dismiss_search_results(0);">No Results Found - Close</a></li>';
	}
	return $(list).appendTo(ul);
};

function dismiss_search_results(fixed)
{
	$('.gamespace_search').hide();
	if(!fixed)
	{
				$('#gamespace_search_module').removeClass('fixed');
	}
	$('.gamespace_search').empty();
}

function log_result(row)
{
	if($('.gamespace_search a[data-row="'+row+'"]').length>0)
	{
		var query = $('#gamespace_search_input').val();
		var label = $('.gamespace_search a[data-row="'+row+'"] span.result_title').text();
		var type = parseInt($('.gamespace_search a[data-row="'+row+'"]').attr('data-type'));
				window.location = $('.gamespace_search a[data-row="'+row+'"]').attr('data-url');
			}
}
</script>
