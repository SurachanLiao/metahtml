jQuery(".ana-slider .flex-control-nav.flex-control-paging li").live("mouseover", function(event){
    jQuery('.ana-slider .flexslider').flexslider(jQuery(this).index());
});

// Slide linkleri al ve nav'a aktar.
var anaSliderLinks = [];
jQuery('.ana-slider .slides li>a').each(function(index) {
   anaSliderLinks.push( this.href ); 
   var href = this.href;
}); 

jQuery(".ana-slider").one("mouseover", function(event){
    // navigation link
    jQuery(".ana-slider .flex-control-nav.flex-control-paging li a").each(function(index) {
        jQuery(this).attr('onclick', 'window.open("'+anaSliderLinks[index]+'","_blank")');
    });    
});

