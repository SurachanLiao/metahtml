    			<ul id="dulcinea_blog_ul">
                        <script type="text/javascript" lang="javascript">
                                var xmlhttp;
                                xmlhttp=null;
                                var url = "/docroot/dulcinea/jsp/blog_list.jsp";
                                if (window.XMLHttpRequest) {// code for all new browsers
                                        xmlhttp=new XMLHttpRequest();
                                } else if (window.ActiveXObject) {// code for IE5 and IE6
                                        xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
                                }

                                if (xmlhttp!=null) {
                                        xmlhttp.onreadystatechange=get_blog_entries;
                                        xmlhttp.open("GET",url,true);
                                        xmlhttp.send(null);
                                }


                                function get_blog_entries() {
                                        if (xmlhttp.readyState==4) {// 4 = "loaded"
                                                if (xmlhttp.status==200) {// 200 = OK
                                                        var return_val = xmlhttp.responseText.replace(/^\s+|\s+$/g,"");
                                                        if (return_val != "ERR_TRANSFORM") {
                                                                document.getElementById("dulcinea_blog_ul").innerHTML = xmlhttp.responseText;
                                                        }
                                                }
                                        }
                                }

                       </script>
			</ul>
