<div class="adv_search_result">
	
	<table id="example" class="display result-section " cellspacing="0" width="100%"></table><div class="search_count text-center"><p>Your search did not match any content In Live</p></div><div class="pagina"></div><div class="search_count" style="margin-top:5px;text-align:center;"><p><span>More from  :</span></p></div></div>
<script>
function adv_search(){
	var keyword,search_by,row_type_type,url;
	keyword=$('#keyword').val().trim().replace(/[^a-zA-Z ]/g, "");
	search_by=$('#field').val().trim();
	row_type_type=$('#type').val().trim();
	if(keyword==''){
		$('.error_search').show().html(' * Enter a valid keyword');
	}else if(keyword.length < 3){
		$('.error_search').show().html(' * Please Enter more than 2 letters');
	}else{
		if(keyword!='' && search_by=='' && row_type_type==''){
			url='?term='+keyword+'&request=ALL';
		}else{
			url='?term='+keyword+'&type='+search_by+'&row_type='+row_type_type+'&request=MIN';
		}
		window.location.href=url;
	}
	
}
function trigger_event(e){
	if(e.keyCode === 13){
       e.preventDefault();
       $('.adv_search_btn').trigger('click');
    }
}
</script>
