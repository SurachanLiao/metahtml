<script language="javascript">
function slideAjax(page, zone, color, a)
{
	$.ajax({
	  url: page,
	  context: document.body,
	  success: function(data){
		$('#'+zone).html(data);		
		$('.act').removeClass('act');
		$(a).parent().addClass('act');
		$("#menu2 li a").css("border-top","#000 solid 5px");
		$(a).css("border-top",color +" solid 5px");
		
	  }
	});	
}
</script>
