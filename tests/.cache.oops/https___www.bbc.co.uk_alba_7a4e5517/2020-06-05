                getAgeBand: function() {
                    if (user['X-Age-Band']) {
                        return Promise.resolve(user['X-Age-Band']);
                    }
    
                    return bbcuser.isSignedIn().then(function(isSignedIn) {
                        if (isSignedIn) {
