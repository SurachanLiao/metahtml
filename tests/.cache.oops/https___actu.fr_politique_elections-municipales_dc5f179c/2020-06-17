document.addEventListener('DOMContentLoaded', function(){
    if(!actu.context) {
        return;
    }
    var callUrl = actu.disPost.endpoint;
    if(actu.context.marque) {
        callUrl += 'marque-' + actu.context.marque;
    }
    else if(actu.context.region) {
        callUrl += 'region-' + actu.context.region;
    }
    else if(actu.context.departement) {
        callUrl += 'departement-' + actu.context.departement;
    }
    else if(actu.context.commune) {
        callUrl += 'commune-' + actu.context.commune;
    }
    else if(actu.context.category) {
        callUrl += 'category-' + actu.context.category;
    }
    else if(actu.context.typearticle) {
        callUrl += 'typearticle-' + actu.context.typearticle;
    }
    var request = new XMLHttpRequest();
    request.open('GET', callUrl, true);
    request.setRequestHeader('Content-Type', 'application/json');
    request.onload = function() {
        if (200 === this.status) {
            var response = JSON.parse(this.response);
            fillDernieresActus(response.data);
        }
    };
    request.send();

    var dateToday = new Date();
    dateToday.setHours(4, 0, 0);
    function formatDate(date) {
        var dateTest = new Date(date.getTime());
        dateTest.setHours(4, 0, 0);
        var diffDay = (dateToday - dateTest) /  (1000 * 3600 * 24);
        if (diffDay >= 3) {
            return new Intl.DateTimeFormat('fr-FR').format(date);
        }
        if(diffDay >= 2) {
            return 'Avant-hier';
        }
        if(diffDay >= 1) {
            return 'Hier';
        }
        return date.getHours() + ':' + ("0" + date.getMinutes()).slice(-2);
    }

    function fillDernieresActus(data) {
        var chunck = 10;
        var nbPpages  = data.length / chunck;
        var tabs  = document.getElementById('Widget-DernieresActus').querySelectorAll('.tab-content');
        for(var i=0; i < nbPpages; i++) {
            if(!tabs[i]) continue;
            var posts = data.slice(i*chunck, i*chunck+chunck);
            var docFrag = document.createDocumentFragment();
            for(var key in posts) {
                var post = posts[key];
                var meta = document.createElement('div');
                meta.classList.add('post-stack-row_col1', 'meta-light');
                meta.innerHTML = formatDate(new Date(post.date));
                var a = document.createElement('a');
                a.href = post.link;
                a.innerHTML = post.title;
                a.classList.add('post-stack-row_col2');
                var row = document.createElement('div');
                row.classList.add('post-stack-row');
                row.appendChild(meta);
                row.appendChild(a);
                docFrag.appendChild(row);
            }
            tabs[i].innerHTML = '';
            tabs[i].appendChild(docFrag);
        }
    }
});
</script>
