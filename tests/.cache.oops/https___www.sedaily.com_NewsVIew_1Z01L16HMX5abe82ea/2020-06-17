        var Cnt = 0;
        var Top1 = "";
        var Top2 = "";

        for (var i = 0; i < arr.length; i++) {
            if (Cnt <= 9) {
                if (Cnt == 0 && $("#Nid").val() != arr[i].Nid) {    //현재 기사 체크

                    var ChkVideo = false;   //인기 영상 리스트 체크 
                    for (var a = 1; a <= 5; a++) {
                        if ($("#mov_Nid_" + a).val() == arr[i].Nid) {
                            ChkVideo = true;
                            break;
                        }
                    }

                    if (!ChkVideo) {
                        Top1 = "<a href='" + arr[i].Link + "' class='tot_link'>";
                        Top1 += "<div class='new_photo'>";
                        Top1 += "<span><img src='" + arr[i].ImgPath + "'></span>";
                        Top1 += "</div>";
                        Top1 += "<div class='sub_photo'>" + arr[i].NTitle + "</div>";
                        Top1 += "</a>";
                        Top1 += "<ul class=\"text_list\" id=\"NewsTop2\"></ul>";

                        $("#NewsTop1").html(Top1);
                        Cnt++;
                    }
                } else if (0 < Cnt && $("#Nid").val() != arr[i].Nid) {
                    var ChkVideo = false;
                    for (var a = 1; a <= 5; a++) {
                        if ($("#mov_Nid_" + a).val() == arr[i].Nid) {
                            ChkVideo = true;
                            break;
                        }
                    }

                    if (!ChkVideo) {
                        Top2 += "<li><a href='" + arr[i].Link + "'>" + arr[i].NTitle + "</a></li>";
                        Cnt++
                    }
                }
            }
        }
      
        $("#NewsTop2").html(Top2);
    });
