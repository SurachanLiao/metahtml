<script>
jQuery(function($) {
	$("#signup-form").on("click", "#newsletter-signup", function() {
		if (isValidEmail($("#input-email").val())) {
			$.post("http://townhall.com/api/list/register?token=9C8A5D4C-DF99-429B-A353-65299C2BAC57", {
				"FirstName": $("").val(),
				"LastName": $("").val(),
				"PostalCode": $("").val(),
				"Email": $("#input-email").val(),
				"List": "451",
				"ReferralCode": "HotAir",
				"Timestamp": "2020-06-29 10:21:33 pm"
			})
				.done(function(response, status, xhr) {
					//responses: Success, Failed - Duplicate Lead, Failed - Error, Invalid field(s)
					if (status == "success" && xhr.status == 200) {
						switch (response) {
							case "Failed - Error":
							case "Invalid field(s)":
								$('#signup-form').html('<label id="article-signup-label" for="article-signup-email" style="width:400px;"><h4>An error occurred.</h4></label>');
								break;
							case "Success":
								$('#signup-form').html('<label id="article-signup-label" for="article-signup-email" style="width:400px;"><h4>Thanks for signing up!</h4></label>');
								break;
							case "Failed - Duplicate Lead":
								$('#signup-form').html('<label id="article-signup-label" for="article-signup-email" style="width:400px;"><h4>You are already signed up!</h4></label>');
								break;
							default:
								$('#signup-form').html('<label id="article-signup-label" for="article-signup-email" style="width:400px;"><h4>Unknown error occurred.</h4></label>');
						}
					} else {
						$('#signup-form').html('<label id="article-signup-label" for="article-signup-email" style="width:400px;"><h4>An HTTP error occurred</h4></label>');
					}
				});
		} else {
			alert("Please enter a valid email address");
		}
	});
});

function isValidEmail(emailAddress) {
	var pattern = new RegExp(/^(("[\w-+\s]+")|([\w-+]+(?:\.[\w-+]+)*)|("[\w-+\s]+")([\w-+]+(?:\.[\w-+]+)*))(@((?:[\w-+]+\.)*\w[\w-+]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][\d]\.|1[\d]{2}\.|[\d]{1,2}\.))((25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\.){2}(25[0-5]|2[0-4][\d]|1[\d]{2}|[\d]{1,2})\]?$)/i);

	return pattern.test(emailAddress);
}
</script>

