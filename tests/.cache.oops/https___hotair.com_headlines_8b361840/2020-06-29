<script>
	jQuery(document).ready(function($) {
		var _inifinite_scroll_div = $( '#infinite-scroll' );
		if( _inifinite_scroll_div.data( 'disabled' ) ) return;

		var count = 2;
		var	ppp = 20;
		var trig = 3;
				var trigger = $(document).height() / trig ;
		$(window).data('ajaxready', true).scroll(function(e) {

			if ( $(window).data('ajaxready') == false || _inifinite_scroll_div.data( 'disabled' ) ) return;

			if ($(window).scrollTop() >= $(document).height() - $(window).height() - trigger) {
				count++;
				trig++;

				loadArticle(count);

			}
		});//end window.data.scroll(function(e){}
		function loadArticle(pageNumber){

			console.log( '[INFO] Infinite scroll triggered' );

			$('a#infiniteLoader').show('fast');

			var data = "action=th_infinite_scroll&infinite_paged=" + pageNumber + "&infinite_posts_per_page=" + ppp + "";
			$(window).data('ajaxready', false);

			$.ajax({

				url: "https:\/\/hotair.com\/?" + data,
				type: 'GET',
				success: function(html) {

					if (html) {
						_inifinite_scroll_div.append( html );
						console.log( '[INFO] Infinite scroll posts added' );
						$('a#infiniteLoader').hide('1000');
						$(window).data('ajaxready', true);
					} else {
						console.log( '[INFO] Infinite scroll ended' );
						_inifinite_scroll_div.attr( 'data-disabled', true );
					}

				}

			});
		}//end loadArticle


	});//end document.ready
</script>
