function setupAdUnits(){var utm_campaign_id=getParameterByName("utm_campaign");var utm_source_id=getParameterByName("utm_source");var utm_medium_id=getParameterByName("utm_medium");var utm_content_id=getParameterByName("utm_content");var utm_term_id=getParameterByName("utm_term");var ad_units_keys=Object.keys(all_ad_units);var abTestId=Cookies.get("STYXKEY_PATCH_CONFIG");slot_container._lazyload=[];slot_container._instantload=[];var container_name='';for(var j=0;j<ad_units_keys.length;j++){var slot_name=ad_units_keys[j];var indiv_unit=all_ad_units[slot_name];if(indiv_unit.ad_media===deviceType){container_name='_instantload';if(indiv_unit.ad_target==='postartcl_1'||indiv_unit.ad_target==='postartcl_m1'){container_name='_lazyload';}
var slot=googletag.defineSlot('/270360483/'+indiv_unit.ad_unit,indiv_unit.ad_size_arr,indiv_unit.ad_target).setTargeting('slot',indiv_unit.ad_slot).setTargeting('utm_campaign',utm_campaign_id).setTargeting('utm_source',utm_source_id).setTargeting('utm_medium',utm_medium_id).setTargeting('utm_content',utm_content_id).setTargeting('utm_term',utm_term_id).addService(googletag.pubads());slot_container[container_name].push(slot);if(abTestId){slot.setTargeting('patch_config',abTestId);}}}}
function constructPrebidAdUnits(ad_units){var adUnits=[];var ad_units_keys=Object.keys(ad_units);for(var j=0;j<ad_units_keys.length;j++){var slot_name=ad_units_keys[j];var indiv_unit=ad_units[slot_name];if(indiv_unit.ad_media===deviceType){var adUnit={};var bids=[];adUnit.code=indiv_unit.ad_target;adUnit.mediaTypes={banner:{sizes:indiv_unit.ad_size_arr}};if(indiv_unit.hasOwnProperty('openx')){if(indiv_unit.openx.hasOwnProperty(slot_name)){var openx_unit=indiv_unit.openx[slot_name];bids.push({bidder:'openx',params:{delDomain:openx_unit.del_domain,unit:openx_unit.unit}});}}
if(indiv_unit.hasOwnProperty('appnexus')){if(indiv_unit.appnexus.hasOwnProperty(slot_name)){var appnexus_unit=indiv_unit.appnexus[slot_name];bids.push({bidder:'appnexus',params:{placementId:appnexus_unit.placementId,}});}}
if(indiv_unit.hasOwnProperty('index_exchange')){if(indiv_unit.index_exchange.hasOwnProperty(slot_name)){var index_exchange_unit=indiv_unit.index_exchange[slot_name];for(var i=0;i<indiv_unit.ad_size_arr.length;i++){var bid={bidder:'ix',params:{siteId:index_exchange_unit.siteId,size:indiv_unit.ad_size_arr[i]}};bids.push(bid);}}}
if(indiv_unit.hasOwnProperty('triplelift')){if(indiv_unit.triplelift.hasOwnProperty(slot_name)){var triplelift_unit=indiv_unit.triplelift[slot_name];bids.push({bidder:'triplelift',params:{inventoryCode:triplelift_unit.inventoryCode,}});}}
if(indiv_unit.hasOwnProperty('yieldmo')){if(indiv_unit.yieldmo.hasOwnProperty(slot_name)){var yieldmo_unit=indiv_unit.yieldmo[slot_name];bids.push({bidder:'yieldmo',params:{placementId:yieldmo_unit.placementId,}});}}
if(indiv_unit.hasOwnProperty('criteo')){if(indiv_unit.criteo.hasOwnProperty(slot_name)){var criteo_unit=indiv_unit.criteo[slot_name];bids.push({bidder:'criteo',params:{networkId:criteo_unit.networkId,}});}}
if(indiv_unit.hasOwnProperty('pubmatic')){if(indiv_unit.pubmatic.hasOwnProperty(slot_name)){var pubmatic_unit=indiv_unit.pubmatic[slot_name];bids.push({bidder:'pubmatic',params:{publisherId:pubmatic_unit.publisherId,adSlot:pubmatic_unit.adSlot,}});}}
if(indiv_unit.hasOwnProperty('rubicon')){if(indiv_unit.rubicon.hasOwnProperty(slot_name)){var rubicon_unit=indiv_unit.rubicon[slot_name];bids.push({bidder:'rubicon',params:{accountId:rubicon_unit.accountId,siteId:rubicon_unit.siteId,zoneId:rubicon_unit.zoneId,sizes:rubicon_unit.sizes[0],position:rubicon_unit.position,}});}}
adUnit.bids=bids;adUnits.push(adUnit);}}
return adUnits;}
function checkMembershipBanner($this){var membershipPopupEnabled=typeof Drupal.settings.fe_membership!=='undefined'?Drupal.settings.fe_membership.enable_popup:null;if(!membershipPopupEnabled){setClientAd($this);return;}
var membershipEnabledInPatch=false;if(typeof Drupal.settings.fe_membership!=='undefined'){membershipEnabledInPatch=Drupal.settings.fe_membership.enabled_in_current_patch;}
if(membershipEnabledInPatch&&isMember!=1){var MEMBERSHIP_BAR_STORAGE_KEY='PATCH/MEMBERSHIP/FOOTERBAR';var wasBarShown=sessionStorage.getItem(MEMBERSHIP_BAR_STORAGE_KEY)||null;if(wasBarShown){setClientAd($this);return;}
setClientAd($this,false);}else{setClientAd($this);}}
function setClientAd($this,flag){flag=(flag===undefined)?true:flag;if(flag){var div_id=$($this).attr('id');var new_id=div_id.replace("_mobile_div","");$($this).attr('id',new_id);$($this).addClass('ad_displayed');}else{$($this).hide();}}
function adjustAdSlots(){if($('#block-client-ads-dfp-client-ad-dfp-mobile-lb').length){$('#block-client-ads-dfp-client-ad-dfp-mobile-lb').addClass('mobile-lb-fixed');}
