<script>function innerRemoteHtml(container,url){var xhttp=new XMLHttpRequest();xhttp.onreadystatechange=function(){if(xhttp.readyState==4&&xhttp.status==200){var remoteHTML=xhttp.responseText;container.innerHTML+=remoteHTML;prepareSupermenuAttributes();}};xhttp.open("GET",url,true);xhttp.send();}
