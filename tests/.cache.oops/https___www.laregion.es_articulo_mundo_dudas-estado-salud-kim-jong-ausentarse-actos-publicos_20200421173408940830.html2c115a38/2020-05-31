<script>
  jQuery(document).ready(function ($) {
    var smartRefreshIds = ['66661', '66662', '66664', '66665', '66666', '66667', '66668', '66669', '66674', '66675', '66676', '66677', '66678', '66679', '66680'];
    setInterval(function () {
      $('.ad-slot').each(function () {
        var id = $(this).find('div').attr('id');
        if (smartRefreshIds.includes(id.substr(4))) {
          sas.refresh(id, {
            forceMasterFlag: true,
            target: targetingCode + ';auto_refresh=1'
          });
        }
      });
    }, 90000);
  }(jQuery));
</script>

<!-- WebView for APP -->
<script>
    if (parseInt(location.search.split('webview=').splice(1).join('').split('&')[0]) >= 1) {
      $(".show_only_webview").removeClass('d-none'); 
      var webviewValue = parseInt(location.search.split('webview=').splice(1).join('').split('&')[0]);
      var a = document.getElementsByTagName('a'), length = a.length;
      for (var i=0; i< length; i++){
        if ((a[i].href.indexOf('#') == -1) && a[i].href.indexOf('whatsapp') == -1) {
            a[i].href += '?webview='+ webviewValue;
        }
      }
    }
</script>
