function ie_check_mobile_device() {
	var mobiledevice = (/Series60|Nokia|SymbianOS|iphone|android|blackberry|mini|windows\sce|palm/i.test(navigator.userAgent.toLowerCase()));
	if (mobiledevice) {
		return true;
	} else {
		return false;
	}
}


