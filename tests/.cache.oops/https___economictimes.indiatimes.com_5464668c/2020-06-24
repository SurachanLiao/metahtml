		    function mfProCheck (e, elem) {
		        try {
    		        var $this = $(elem), parentEle = $this.parents('.slideBox'), clientList = parentEle.attr('data-clientlist');
    		        var schemeid = parentEle.attr('data-schemeid');
    		        if(clientList && clientList.length) {
        		        clientList = JSON.parse(clientList);
        		        var schemeName = parentEle.attr('data-scm');
        		        objInfo = {'scheme_id': schemeid, 'scheme_name':schemeName};
        		        widgetName = $this.parents('.dataWidgetName').attr('data-widgetName');
        	            ga('send', 'event', 'Buy Direct - ' + clientList[0].clientShortName ,'WEB - Click - ' + objInfo.scheme_name + ' - ' + objInfo.scheme_id +  ' - ' +  $this.text() + ' - ' + widgetName, widgetName + ' - ' + window.location.href);
    		        }
		        } catch(e) {}
		    }
		    function schemeClick(e,elem) {
		      var $this = $(elem),widgetName = $this.parents('.dataWidgetName').attr('data-widgetName');
		      ga('send', 'event', 'MF Widget Interactions', 'WEB - Click - ' + $this.text(), widgetName + ' - ' + window.location.href);
		    }
		    
		    
