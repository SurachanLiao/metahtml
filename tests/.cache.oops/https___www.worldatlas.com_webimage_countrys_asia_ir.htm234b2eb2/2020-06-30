<script>
    var currentIndex = 0;
    var sliderArray = document.querySelectorAll(".qaSliderWrapper");

    function swapSlides(oldSlide, slide) {
        oldSlide.className = "qaSliderWrapper";
        slide.className = "qaSliderWrapper qaSliderWrapper-selected";
    }

    function qaSliderChange(step) {
        oldSlide = sliderArray[currentIndex];

        if(currentIndex + step < 0) {
            currentIndex = sliderArray.length-1;
        } else if(currentIndex + step > (sliderArray.length-1)) {
            currentIndex = 0
        } else {
            currentIndex += step;
        }

        slide = sliderArray[currentIndex];

        oldSlide.className += " qaSliderWrapper-fadeOut ";
        slide.className += " qaSliderWrapper-fadeOut ";
        swap = setTimeout(swapSlides, 300, oldSlide, slide);

        var dots = document.querySelectorAll(".qaSliderDot");

        dots.forEach(function(dot) {
            dot.className = "qaSliderDot";
        });

        dots[currentIndex].className += " qaSliderDot-selected";
    }

    function qaSliderSet(slide) {
        oldSlide = sliderArray[currentIndex];
        currentIndex = slide;
        slide = sliderArray[currentIndex];

        oldSlide.className += " qaSliderWrapper-fadeOut ";
        slide.className += " qaSliderWrapper-fadeOut ";
        swap = setTimeout(swapSlides, 300, oldSlide, slide);

        var dots = document.querySelectorAll(".qaSliderDot");

        dots.forEach(function(dot) {
            dot.className = "qaSliderDot";
        });

        dots[currentIndex].className += " qaSliderDot-selected";
    }
</script>
