        <span class="qaSliderButton-prev icon-left-open" onclick="qaSliderChange(-1)"></span>
        <ul class="qaSliderDots">
                                        <li onclick="qaSliderSet(0)" class="qaSliderDot qaSliderDot-selected "></li>
                            <li onclick="qaSliderSet(1)" class="qaSliderDot "></li>
                            <li onclick="qaSliderSet(2)" class="qaSliderDot "></li>
                            <li onclick="qaSliderSet(3)" class="qaSliderDot "></li>
                            <li onclick="qaSliderSet(4)" class="qaSliderDot "></li>
                            <li onclick="qaSliderSet(5)" class="qaSliderDot "></li>
