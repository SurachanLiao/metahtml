	<div id="listado_inmuebles">
	</div>

	<script>
		function shuffle(array) {
			var copy = [], n = array.length, i;
			while (n) {
				i = Math.floor(Math.random() * array.length);
				if (i in array) {
				    copy.push(array[i]);
				    delete array[i];
				    n--;
				}
			}
			return copy;
		}

		function listado_inmuebles(ti, id_div_random){
			$('#'+id_div_random+' .btn_menu').removeClass('active');
			

			$('#'+id_div_random+' .options').removeClass('active');
			$listado_inmuebles=$("#"+id_div_random).find("#listado_inmuebles");
			// $("#"+id_div_random+" .btn_menu, #"+id_div_random+" .options").removeClass("active");

			// document.getElementById("listado_inmuebles").innerHTML = "<span class='loading'>Preparando inmuebles...</span>";
			$listado_inmuebles.html("<span class='loading'>Preparando inmuebles...</span>");
			ti = (typeof ti != "undefined") ? ti : "1";
			// var proxyUrl = 'https://cors-anywhere.herokuapp.com/', targetUrl = 'https://www.areacaribe.co/home-en-el-heraldo/'+ti;
			// console.log(targetUrl);

			// fetch("/sites/all/themes/elheraldo/js/json/json_inmuebles_"+ti+".json?181020170211")
			fetch("https://www.areacaribe.co/home-en-el-heraldo/"+ti)
			.then(blob => blob.json())
			.then(data => {
				$listado_inmuebles.html("<div class='content'></div>");
				$listado_inmuebles.className = "";
				data=shuffle(data);

				num_inmuebles = (typeof num_inmuebles!="undefined") ? num_inmuebles : 4;

				for (i = 0; i < num_inmuebles; i++) {
				    ubicaciones = data[i].field_lugar.split("\n");
					ubicacion=(ubicaciones[2]!="" && typeof ubicaciones[2]!="undefined") ? ubicaciones[2] : "";
					if(ubicacion=="") ubicacion=(ubicaciones[1]!="" && typeof ubicaciones[1]!="undefined") ? ubicaciones[1] : "";
					if(ubicacion=="") ubicacion=(ubicaciones[0]!="" && typeof ubicaciones[0]!="undefined") ? ubicaciones[0] : "";	

					data[i].valor = (data[i].field_valor_venta == "0" || data[i].field_valor_venta == "" ) ? data[i].field_valor_alquiler : data[i].field_valor_venta;	

					html =
                    '<div class="row">'+
						'<figure>'+
							'<a href="https://www.areacaribe.co'+data[i].path+'" target="_BLANK">'+
								'<img src="'+data[i].field_foto_portada.src+'" alt="">'+
							'</a>'+
						'</figure>'+
						'<div class="tb">'+
							'<span class="tipo_inmueble">'+data[i].field_tipo_inmueble+'</span>'+
							'<span class="barrio">'+ubicacion+'</span>'+
						'</div>'+

						'<div class="op">'+
							'<span class="operacion">EN '+data[i].field_tipo_de_operacion+'</span>'+
							'<span class="precio">'+data[i].valor+'</span>'+
						'</div>'+
					'</div>';
					// console.log(html);
					$listado_inmuebles.find(".content").append(html);
					// $("#"+id_div_random+" #listado_inmuebles .content").append(html);
				}

				items = 4;
				if (typeof inmuebles_desde != "undefined") {
					if(inmuebles_desde == "art") {
					    slider = true;
						if (screen.width <= 768) {items=1;}
						if (screen.width > 768) {items=3;}
					}
				} else {
					if (screen.width <= 768) { // tablet movil
						items=1;
						slider=true;
					} else if(screen.width > 768){ // desktop
						items=4;
						// items=1;
						slider=false;
						// slider=true;
					}
				}
				
                if (slider) {
					setTimeout(function(){
						$("#block--1702733670 .content").owlCarousel({
							items: items,
							dots: false,
							nav: true,
							autoplay: true,    
							loop: true, 
								
							// responsive: {
							// 	0: {items: 1},
							// 	480: {items: 2},
							// 	1000: {items: 3}
							// }
                        });
					}, 500);
				}    
            
			})
			.catch(e => {
				console.log(e);
				return e;
			});
		}
		listado_inmuebles(1, 'block--1702733670');
	</script>
</div>  </div>

  
