			<script type="text/javascript">
				function success(m) {
					if (m.status == "enabled") {
						$("#notificaciones-campana").addClass("active");
						$("#notificaciones-campana2").addClass("active");
						console.log('Notifications active!');
					}
				}
				function error(m) {
					console.log(m);
				}
				document.addEventListener("indigitallReady", function() {
					navigator.indigitall.loadIndigitall(function() {
						navigator.indigitall.getStatus(success, error);
					});
				});
			</script>

							<script type="text/javascript" src="/indigitall.min.js?v=3.1.12"></script>
			
