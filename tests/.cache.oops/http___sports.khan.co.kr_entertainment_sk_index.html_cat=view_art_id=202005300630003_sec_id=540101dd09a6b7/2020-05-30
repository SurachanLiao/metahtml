<script type="text/javascript">
    //필요함수 선언
    if( typeof getParameterByName != 'function' ) {
        function getParameterByName(name) {
            if (name !== "" && name !== null && name != undefined) {
                name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
                var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                    results = regex.exec(location.search);
                return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
            } else {
                var arr = location.href.split("/");
                return arr[arr.length - 1];
            }
        }
    }

    // 변수 초기화
    var art_id      =       getParameterByName('artid');
    if(art_id == '' || art_id == null)
        art_id		=		getParameterByName('art_id');
    if(art_id == '' || art_id == null){
        //향이네
        art_id		=		$( 'meta[property="og:url"]' ).attr( 'content' );
        var art_id_arr  = art_id.split('//');
        art_id_arr = art_id_arr[1].split('/');
        art_id = art_id_arr[1];
    }

    var art_title   =       $( 'meta[property="og:title"]' ).attr( 'content' );

    wpts.tag({

        ty: 'item',

        items: [

            {i: art_id, t: art_title }

        ],

        mud: {}      // 필수 항목

    });

    wpts.tag({

        ty: 'articleview', //공백불가

        isForce: true,    // custom 데이터 전달시 필수 항목

        mud: {
            art_id : art_title
        }

    });
</script>
