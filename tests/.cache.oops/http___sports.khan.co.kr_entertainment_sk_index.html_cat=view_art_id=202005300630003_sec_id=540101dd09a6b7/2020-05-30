							<script type="text/javascript">
							window.livereOptions = {
								customMessage: { m_khan : '경향신문' },
								refer : 'sports.khan.co.kr/news/sk_index.html?art_id=202005300630003',
								site : 'http://sports.khan.co.kr/news/sk_index.html?art_id=202005300630003&utm_source=livere&utm_medium=social_share',
							};

							// Hooking
							window.livereHooks = {
								write: function(data) {
									doHooking('write', data);
								},

								modify: function(data) {
									doHooking('modify', data);
								},

								remove: function(data) {
									doHooking('remove', data);
								}
							};

							function doHooking(mode, data)
							{
								var pars = {
									mode: mode,
									comment_seq: data.reply.replySeq
								};

								if (mode == 'write') {
									pars.parent_seq = data.reply.parentSeq;
									pars.content = data.reply.content;
									pars.time = data.reply.regdate;
									pars.member_icon = data.reply.writer.profileImageUrl;
									pars.member_domain = data.reply.writer.strategy;
									pars.is_mobile = data.reply.environment.isMobile;
									pars.member_id = data.reply.writer.snsId;
									pars.name = data.reply.writer.name;
									pars.user_agent = data.reply.environment.agent;
									pars.member_seq = data.reply.writer.id;
									pars.site = data.reply.site.url;
									pars.page = location.href;
									pars.title = $('meta[property="og:title"]').attr('content');
									if (pars.title == '' || pars.title == undefined) pars.title = '[단독] 변수미 고발한 BJ 한미모 “살려달라 애원도 했다” (인터뷰)';
									pars.art_img = $('meta[property="og:image"]').attr('content');
								}
								else if (mode == 'modify') {
									pars.content = data.reply.newContent;
									pars.time = data.reply.editDate;
									pars.parent_seq = data.reply.parentSeq;
									pars.member_icon = data.reply.writer.profileImageUrl;
									pars.member_domain = data.reply.writer.strategy;
									pars.is_mobile = data.reply.environment.isMobile;
									pars.member_id = data.reply.writer.snsId;
									pars.name = data.reply.writer.name;
									pars.user_agent = data.reply.environment.agent;
									pars.member_seq = data.reply.writer.id;
									pars.site = data.reply.site.url;
									pars.page = location.href;
									pars.title = $('meta[property="og:title"]').attr('content');
									if (pars.title == '' || pars.title == undefined) pars.title = '[단독] 변수미 고발한 BJ 한미모 “살려달라 애원도 했다” (인터뷰)';
									pars.art_img = $('meta[property="og:image"]').attr('content');
								}
								else if (mode == 'remove') {
									pars.time = data.reply.deletedDate;
								}

								jQuery.ajax({
									url : "../../../../livere_hook.html",
									type : "POST",
									dataType : "json",
									data  : pars,
									cache : false,
									success: function(json) {},
									error: function(xhr) {}
								});

								console.log('hooking', pars);
							}

