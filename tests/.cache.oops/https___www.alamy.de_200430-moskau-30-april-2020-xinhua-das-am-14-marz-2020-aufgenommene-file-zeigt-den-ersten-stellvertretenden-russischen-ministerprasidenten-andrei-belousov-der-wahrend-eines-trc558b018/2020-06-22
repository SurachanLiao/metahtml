                                  'dimension40': licenceName
                              
                          }]
                      }
                  }
              });
              }
          }
            else{
if (imageHeader.getpicturetype() == "Images") {
                  dataLayer.push({
                      'event': 'detailview',
                      'ecommerce': {
                          'detail': {
                        
                              'products': [{
                                  "name": "",
                                  "id":imgref,
                                  'category': imageHeader.getpicturetype(),                      
                                  "quantity":1,
                                  'dimension14': imageType,
                                  'dimension22': img_Datetaken,
                                  'dimension23': img_Dateuploaded,
                                  'dimension24': img_x,
                                  'dimension25': img_y,
                                  'dimension26': img_orientation,
                                  'dimension27': img_BandW,
                                  'dimension28': img_cutout,
                                  'dimension29': img_people,
                                  'dimension30': img_license,
                                  'dimension31': img_uploadtype,
                                  'dimension32': img_pseudonym,
                                  'dimension33': img_contributortype,
                                  'dimension35': img_exclusivetoalamy,
                                  'dimension36': img_priority,
                                  'dimension37': SIM_V1
                              }]
                          }
                      }
                  });
              } else {
              dataLayer.push({
                  'event': 'detailview',
                  'ecommerce': {
                     'detail': {
                        
                          'products': [{
                              "name": "",
                                  "id":imgref,
                              'category': imageHeader.getpicturetype(),                      
                                  "quantity":1,
                                  'dimension22': img_Datetaken,
                                  'dimension23': img_Dateuploaded,
                                  'dimension24': img_x,
                                  'dimension25': img_y,
                                  'dimension26': img_orientation,
                                  'dimension27': img_BandW,
                                  'dimension28': img_cutout,
                                  'dimension29': img_people,
                                  'dimension30': img_license,
                                  'dimension31': img_uploadtype,
                                  'dimension32': img_pseudonym,
                                  'dimension33': img_contributortype,
                                  'dimension35': img_exclusivetoalamy,
                                  'dimension36': img_priority,
                                  'dimension37': SIM_V1
                              
                          }]
                      }
                  }
              });
              }
                 }
          });

          $(document).ready(function () {

              if (IsMobiledevice == "1" && IsPopularprice == "1"){
             
              window.location.href = requestHeader.href + "mediacomp/pricecalc.aspx?ref=" + imageHeader.imageRef + "&imageid=" + imageHeader.imageId;
             }else
				$(".solid-loader,.solid-loader-bg").hide();
          });
    </script>
        
