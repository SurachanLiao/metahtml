<script type="text/javascript">
    var IsIntranet = '0';
    var email = "";
    var emailflag = 0;
    var GeoIPCntryCode = 'US';
    var HTTP_Path_Root = 'https://www.alamy.com';
    var HTTP_Path_Static = 'https://static.alamy.com';
    var HrefPath = 'https://www.alamy.de/';
    var IsBotrequestEnabled = 'true';
    var footerResourceString = '{"lblCountryChangeText":"Wir nehmen an, dass Sie sich in den @@countryName befinden. Wenn dies nicht richtig ist","lblCountryChangeTextWithoutWordThe":"Wir nehmen an, dass Sie sich in @@countryName befinden. Wenn dies nicht richtig ist","germany":"Deutschland","us":"USA","canada":"Kanada","auAndnz":"Australien & Neuseeland","india":"Indien","uk":"Großbritannien","clickHere":"Hier klicken","emailNotValidText":"Es tut uns Leid. Diese E-Mail-Adresse ist &#146;nicht gültig","optionNotSelecedText":"Bitte wählen Sie mindestens eine der Möglichkeiten","emailSubscriptionSuccessMessage":"Fertig. Sie stehen jetzt auf dem E-Mail Verteiler für unsere Newsletter.","alreadySignUpMessage":"Sie sind bereits auf dem E-Mail Verteiler für unsere Newsletter","JoinOver230000":"Bleiben Sie auf dem Laufenden und lassen sich von Alamy über die neuesten Angebote und einige der handverlesenen Partner der Branche informieren.","EnglishNoTranslate":"English","DeutschNoTranslate":"Deutsch","EspañolNoTranslate":"Español","FrançaisNoTranslate":"Français","ItalianoNoTranslate":"Italiano"}';
    var footerResources = {};
    if (footerResourceString != null) {
        try {
            footerResources = JSON.parse(footerResourceString);
        }
        catch (ex) {
            console.warn("Unable to parse resource string.");
        }
    }
    function windowListener(targetLink) {
        if ('False' == 'True') {
            window.opener.location.href = targetLink;
            window.close();
            return false;
        }
    }

    var footerSetAccountGTMValues = function () {
        objGTM1 = new GTM1();
        objGTM1.setValueswithCategory('Footer Interactions', 'Apply account link click', 'View account application', "");
        return true;
    }

    var orderSummarySetAccountGTMValues = function () {
        objGTM1 = new GTM1();
        objGTM1.setValueswithCategory('Confirmation Page Interactions', 'Apply account link click', 'View account application', "");
        return true;
    }
    $(document).ready(function () {
        $(".drp").click(function () {
        if ($(window).width() < 400) {
                $(this).siblings().slideToggle();
                $(this).toggleClass('active');
            }
        });
        
        setLanguageForSelector();
    });
    function switchLanguages(lanCode) {
        var evt = this.window.event || arguments.callee.caller.arguments[0];
        if (evt) {
            evt.preventDefault();
        }

        var linkerParam = "";

        if (typeof ga !== "undefined" && !IsBotrequest) {
            if (window.location.href.indexOf("_ga=") == -1) {
                ga(function () {
                    var trackers = ga.getAll();
                    if (trackers.length) {
                        linkerParam = trackers[0].get('linkerParam');
                    }
                });
            }
        }
        

        var intranetmode = "0";
        if (IsIntranet == "true" || IsIntranet == "1") intranetmode = "1";
        var string = " ";
        var newString = " ";
        if (lanCode == "com" || lanCode == "de") {
            if (window.location.href.toLowerCase().indexOf("themes.aspx") != -1) {
                string = "themes.aspx";
                newString = "themes.asp";
            }

            if (window.location.href.toLowerCase().indexOf("order-download-purchasehistory.aspx") != -1) {
                string = "order-download-purchasehistory.aspx";
                newString = "order-download-purchasehistory.asp";
            }
        }

        if (window.location.href.toLowerCase().indexOf("contactus.aspx") != -1 || window.location.href.toLowerCase().indexOf("contactus/") != -1) {
            string = currentDomain == "com" ? window.location.href.substring(window.location.href.indexOf("contactus/"), window.location.href.length) : "contactus.aspx";
            newString = lanCode == "com" ? "contactus/uk.aspx" : "contactus.aspx";
        }

        if (window.location.href.toLowerCase().indexOf("terms/") != -1) {
            string = window.location.href.substring(window.location.href.indexOf("terms/"), window.location.href.length);
            newString = (lanCode == "com") ? "terms/uk.aspx" : "terms/terms.aspx";
        }

        if ((intranetmode == "1") || intranetmode == "0") {
            var exp = '';
            var now = new Date();
            var then = now.getTime() + (30 * 24 * 60 * 60 * 1000);
            now.setTime(then);
            exp = '; expires=' + now.toGMTString();
            document.cookie = "userLanguage=de;Secure=true;path= /;domain= " + cookieDomain + exp;
            var slinkparam = "";
            if (linkerParam != "" && linkerParam != null) {
                if (window.location.href.indexOf("?") != -1)
                    slinkparam = "&" + linkerParam
                else
                    slinkparam = "?" + linkerParam
            }
            
            if (window.location.href.indexOf("/" + compurl_foldername + "-") != -1 && window.location.href.indexOf("?returnurl") == -1) {
                var url = "";
                url = 'https://www.alamy.de'.replace(cookieDomain, (lanCode == "fr") ? ".alamyimages." + lanCode : ".alamy." + lanCode) + '/mediacomp/imagedetails.aspx?ref=' + '2BJWH1B';
                var queryString = removeQueryStringParam("ref", window.location.href);
                if (queryString.length > 0) {
                    queryString = "&" + queryString;
                }

                if (url.indexOf("?") != -1) {
                    if (linkerParam == "" && queryString == "")
                        window.location.href = url
                    else
                        window.location.href = url + "&" + linkerParam + queryString;
                }
                else {
                    window.location.href = url + "?" + linkerParam + queryString;
                }
            }
            else if (lanCode == "fr") {
                window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamyimages." + lanCode).replace(string, newString) + slinkparam;
            }
            else {
                if (lanCode == "com" && window.location.href.toLowerCase().indexOf("/video-gallery.aspx") != -1)
                    window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamy." + lanCode).replace("video-gallery.aspx", "footage-gallery.aspx") + slinkparam;
                else
                    window.location.href = window.location.href.toLowerCase().replace(cookieDomain, ".alamy." + lanCode).replace(string, newString) + slinkparam;
            }
            
        }
    }
    function removeQueryStringParam(key, sourceURL) {
        var returnUrl = sourceURL.split("?")[0],
        param,
        params_arr = [],
        queryString = (sourceURL.indexOf("?") !== -1) ? sourceURL.split("?")[1] : "";
        if (queryString !== "") {
            params_arr = queryString.split("&");
            for (var i = params_arr.length - 1; i >= 0; i -= 1) {
                param = params_arr[i].split("=")[0];
                if (param === key) {
                    params_arr.splice(i, 1);
                }
            }
            return params_arr.join("&");
        }
        return "";
    }
    var setLanguageForSelector = function () {
        var language = "";
        switch ('de') {
            case 'en':
                language = footerResources.EnglishNoTranslate;
                break;
            case 'de':
                language = footerResources.DeutschNoTranslate;
                break;
            case 'es':
                language = footerResources.EspañolNoTranslate;
                break;
            case 'fr':
                language = footerResources.FrançaisNoTranslate;
                break;
            case 'it':
                language = footerResources.ItalianoNoTranslate;
                break;
            default:
                language = footerResources.EnglishNoTranslate;
        }
        $("#automationHttpCulture").html(language);
    };
    $("#footer_Normal a").click(function () {
        if (!$(this).hasClass('lang-selector') && $(this).attr('id') != "automationGetCreditAccount")
        {
            objGTM1 = new GTM1();
            var eventAction = 'Internal click';
            var href = $(this).attr('href').split("?")[0];
            if ($(this).parents('#footerSocial').length > 0) eventAction = 'External Click';
            else if ($(this).attr('id') == "lblTel") eventAction = 'Phone Number Click';
            else if ($(this).parents('.lang-dd').length > 0) eventAction = 'Language Selector Dropdown Click';
            objGTM1.setValueswithCategory('Footer Interactions', eventAction, href, "");
        }
        return true;
    });
</script>

