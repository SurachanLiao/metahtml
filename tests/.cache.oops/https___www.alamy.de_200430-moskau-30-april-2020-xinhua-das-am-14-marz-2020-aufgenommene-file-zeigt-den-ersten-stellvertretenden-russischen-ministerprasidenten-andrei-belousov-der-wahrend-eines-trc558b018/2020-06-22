<script type="text/javascript">
    function SendAddToCartDataforGoogle(e) { for (var n = [], i = 0; i < e.length; i++) { var t, o, a, c, r, s, d, m, u, y, l, p, g, h, v, f, G; e[i].category, c = e[i].Currency, t = e[i].id, e[i].id, o = e[i].price, a = e[i].category, quantity = 1, r = e[i].dimension14, s = e[i].dimension22, d = e[i].dimension23, m = e[i].dimension24, u = e[i].dimension25, y = e[i].dimension26, l = e[i].dimension27, p = e[i].dimension28, g = e[i].dimension29, h = e[i].dimension30, v = e[i].dimension31, f = e[i].dimension32, G = e[i].dimension33, "Images" == a || "Vector" == a || "360" == a ? s ? n.push({ id: t, name: "", category: a, price: o, quantity: 1, dimension14: r, dimension22: s, dimension23: d, dimension24: m, dimension25: u, dimension26: y, dimension27: l, dimension28: p, dimension29: g, dimension30: h, dimension31: v, dimension32: f, dimension33: G }) : n.push({ id: t, name: "", category: a, price: o, quantity: 1, dimension14: r }) : n.push({ id: t, name: "", category: a, price: o, quantity: 1 }) } dataLayer.push({ event: "addToCart", ecommerce: { currencyCode: c, add: { products: n } } }) } function SendremoveFromCartDataforGoogle(e) { for (var n = [], i = 0; i < e.length; i++) { var t, o, a, c, r; e[i].category, c = e[i].Currency, t = e[i].id, e[i].name, o = e[i].price, a = e[i].category, quantity = 1, r = e[i].dimension14, "Images" == a ? n.push({ id: t, name: "", category: a, price: o, quantity: 1, dimension14: r }) : n.push({ id: t, name: "", category: a, price: o, quantity: 1 }) } dataLayer.push({ event: "Removefromcart", ecommerce: { currencyCode: c, remove: { products: n } } }) } GTM = function (e) { this.category = e }, GTM1 = function () { }, GTM.prototype.setValues = function (e, n, i) { dataLayer.push({ category: this.category, action: e, label: n, value: i, event: "click" }) }, GTM.prototype.setPackValues = function (e, n, i, t) { dataLayer.push({ licenceSelectedName: n, downloadPackSize: t, category: this.category, action: e, label: n, value: i, event: "click" }) }, GTM.prototype.setLicenseValues = function (e, n, i) { dataLayer.push({ category: this.category, action: e, label: n, value: i, event: "click", licenceSelectedName: n }) }, GTM1.prototype.setValueswithCategory = function (e, n, i, t) { dataLayer.push({ category: e, action: n, label: i, value: t, event: "click" }) }, GTM.prototype.setEventValues = function (e, n, i, t) { dataLayer.push({ category: this.category, action: e, label: n, value: i, event: t }) }, GTM.prototype.clearValues = function () { this.category = "", this.action = "", this.label = "", this.value = "" };
