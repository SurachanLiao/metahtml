      <div class="sign-in-form__inputs">
        
    <div class="input">
      <input name="session_key" class="input__field input__field--with-label" aria-label="พิมพ์อีเมลหรือหมายเลขโทรศัพท์ของคุณ" required="true" autocomplete="username" type="text">

        <label class="input__label" for="session_key">
          อีเมลหรือโทรศัพท์
        </label>

      
      <p class="input__message hidden" for="session_key" role="alert"></p>
    </div>  

        
    <div class="input">
