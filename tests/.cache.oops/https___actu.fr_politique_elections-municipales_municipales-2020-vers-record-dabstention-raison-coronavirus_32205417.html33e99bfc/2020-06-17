window.addEventListener("DOMContentLoaded", function() {
    var currentDateObject = (new Date());
    currentDateObject.setMinutes(0);
    currentDateObject.setSeconds(0);
    var map = document.getElementById('map');

    // Uniquement pour map.php
    if(!map) {
        return true;
    }


    var onClickSpot = function (spot, event) {
        switch (currentType) {
            case 'country':
                window.location.href = '/meteo/' + window.slugs[spot.region.isoCode];
                break;
            case 'region':
                window.location.href = '/meteo/' + window.slugs[spot.department.isoCode];
                break;
            case 'departement':
                window.location.href = '/meteo/' + window.slugs[spot.id];
                break;
        }
        return false;
    };

    var map = new sportrizer.sportysky.Map(
        document.getElementById('map'),
        actu.disMeteo.endpoint,
        {
            iconSize: 22,
            iconSizeTiny: 24,
            iconTinyBefore: 767,
            debug: false, // active or not debug for position
            textWeather: '[AIRTEMPERATURE]°',
            textWind: '[WINDSPEED]',
            textWindGust: '',
            clickSpot: onClickSpot,   // Callback function on Click image
            apiQs: false,
        }
    );

    switch (currentType) {
        default:
            map.displayCountry(currentDateObject, currentIso);
            currentType = 'country';
            break;
    }

    window.onresize = function() {
        map.redraw();
    }
});
</script>
