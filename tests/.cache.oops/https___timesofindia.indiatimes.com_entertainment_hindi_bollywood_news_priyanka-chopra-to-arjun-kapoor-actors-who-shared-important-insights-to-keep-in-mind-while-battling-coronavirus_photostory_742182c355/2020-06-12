        				   {
        				        if(toiprops.cookie.get('playvd') == undefined || (toiprops.cookie.get('playvd') && toiprops.cookie.get('playvd') != 1))
        				        {
        				        	$(".liveS_container").hide();
        				            if (typeof miniTV.clickLoad == "function") miniTV.clickLoad();
        				        }
        				         $("#player").attr("src", "/sponsortv.cms");
        				         $(".livetv_title").html(sponsoredTitle);
        				         $(".livetv_title").attr("title",sponsoredTitleFull);
        				         $("#streaming_box").addClass("sponsoredad");
        				         isSponsorVideoPlaying = true;
        				         registerSponsorVid && registerSponsorVid();
        				   }
        				    else if(miniTVType == "livetvvideo" && (miniTVScope == "etimes" && ((window.parent.toiprops.subsec1!=undefined && entertainmentSections.indexOf(window.parent.toiprops.subsec1) >=0) || (window.parent.channelname != undefined && window.parent.channelname == 233446897))))
        				   {
        				         $("#streaming_box").removeClass("sponsoredad");
        				        isSponsorVideoPlaying = false;
        				        // do nothing
        				        if(miniTVTitle == "")
        				        {
        				        
        				            if(toiprops.cookie.get('playvd') == undefined || (toiprops.cookie.get('playvd') && toiprops.cookie.get('playvd') != 1))
            				        {
            				        	$(".liveS_container").hide();
            				        }
        				        }
        				        
        				   }
        				   else if(miniTVType == "livetvvideo" && miniTVScope == "")
        				   {
        				        $("#streaming_box").removeClass("sponsoredad");
        				        isSponsorVideoPlaying = false;
        				        // do nothing
        				        if(miniTVTitle == "")
        				        {
        				        
        				            if(toiprops.cookie.get('playvd') == undefined || (toiprops.cookie.get('playvd') && toiprops.cookie.get('playvd') != 1))
            				        {
            				        	$(".liveS_container").hide();
            				        }
        				        }
        				   }
        				   else if(miniTVType == "livetvvideo" && miniTVScope == "ptgalso")
        				   {
        				        $("#streaming_box").removeClass("sponsoredad");
        				        isSponsorVideoPlaying = false;
        				        // do nothing
        				        if(miniTVTitle == "")
        				        {
        				        
        				            if(toiprops.cookie.get('playvd') == undefined || (toiprops.cookie.get('playvd') && toiprops.cookie.get('playvd') != 1))
            				        {
            				        	$(".liveS_container").hide();
            				        }
        				        }
        				   }
        				   else
        				   {
        				        //do nothing
        				        $(".liveS_container").hide();
        				        $("#minitv").hide()
        				   }
        				   try{ if(TimesApps.ad_free_nudge==1){$('#streaming_box').addClass("adjust_nudge")}}catch(e){}
					});
				};
			
			$(window).load(function(){miniTV.isOn();});</script><!--/home_topcontent_banner.cms?version=2&pagetype=newpotime:32--><div data-plugin="slidecontent" class="wrapper clearfix" data-slideprocess="true" data-galno="1" data-slideid="74690078" data-nextslideid="74687246"><div class="comments_container" id="comment-section-74690078"><textarea cols="40" rows="5" data-txtbx="dummy-comment-input" class="hide"></textarea><div class="article"><div id="email_verified"></div><div class="comment-section" data-csmsid="74690078"><span data-plugin="facebook-loadjs" class="hide"></span><h2><label for="comment-input">Comments <span class="count_color">(<span data-plugin="comment-count-new"></span>)</span></label><div class="sortby clearfix"><label for="comment_sort">Sort: </label><select id="comment_sort"><option value="">Newest</option><option value="agree">UpVoted</option><option value="oldest">Oldest</option><option value="discussed">Discussed</option><option value="disagree">Down Voted</option></select></div><a href="javascript:void(0)" data-plugin="closecomments" class="close_cmnts photo_sprite">closecomments</a></h2><div data-level="0" data-id="0" data-index="-1" data-plugin="comment"><div data-plugin="comment-form" class="comment-form2"><div class="user-thumbnail"><img alt="userthumb" title="userthumb" data-plugin="user-thumb" src="https://static.toiimg.com/photo/29251859.cms"></div><textarea placeholder="Add your comment here" id="comment-input" data-plugin="comment-input" cols="40" rows="5"></textarea><div class="comment-footer clearfix"><span class="char-count">Count: <span data-maxchar="3000" data-plugin="comment-input-remaining">3000</span></span></div><div data-plugin="comment-form-login" class="login-area clearfix noreg"><p data-plugin="user-isloggedin" class="clear hide"></p><div data-plugin="user-notloggedin" class="social-login hide"><h3 class="alignshare">SIGN IN WITH</h3><span title="Facebook" data-plugin="comment-facebook-post" class="fb"><small>Facebook</small>