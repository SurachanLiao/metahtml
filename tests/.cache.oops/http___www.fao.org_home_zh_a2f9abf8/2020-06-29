<script>
function checkTimeZone() {
  var d = new Date();
  var timeZone = d.getTimezoneOffset()/60;
  
  if(timeZone==-8){
  var isChina = true;
  }
  else{
  var isChina = false;
  }
  console.log(isChina);
  
  return isChina;
}

var isChina = checkTimeZone();

var s = document.createElement('script');

if (isChina) {
  /* notingh, will excute script in body */
} else {
  s.src = "https://code.jquery.com/ui/1.12.1/jquery-ui.js";
  $("head").append(s);
}

</script>

