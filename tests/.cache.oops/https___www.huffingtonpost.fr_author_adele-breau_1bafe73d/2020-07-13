  (function() {
    function load_adscript() {
      var scriptTag = document.createElement('script'),
        firstScriptTag = document.getElementsByTagName('script')[0];
      scriptTag.src = '//wrapper.lemde.fr/glmaw.v0.1.0.js';
      firstScriptTag.parentNode.insertBefore(scriptTag, firstScriptTag);
    }
    
    (function() {
      var use_cmp = true;
      var multiple_slots = document.getElementsByClassName("dfp-slot dfp-multi-slot");
      for (var i = 0; i < multiple_slots.length; i++) {
        var slot = multiple_slots[i];
        slot.id = slot.id + "_" + Math.ceil(Math.random(new Date().getTime()) * 10E9);
      }

      if (use_cmp) {
        waitForGlobal('HPCmp', function(){
          HPCmp.checkCmpScript(function(){
            load_adscript();
          });
        });
      } else {
        load_adscript();
      }
      return;
    })();
  })();
</script>
