el.innerHTML=topicsContent.join("");insertAfter(insertAfterEl,el)}function followUnfollowTopic(status,topic,el){var status=el.getAttribute("data-status");var envEndpoint="dev";var isProdOrStage=window.tgam.env.isProdOrStage;if(isProdOrStage===true)envEndpoint="prd";var action="follow";if(status==="following")action="unfollow";var endpoint="https://myglobe-"+envEndpoint+".theglobeandmail.com/v1/user/topic/"+action;var postBody={"hashId":window.getCookie("gtm_u_info").split("|")[0],"topicSlug":topic};
