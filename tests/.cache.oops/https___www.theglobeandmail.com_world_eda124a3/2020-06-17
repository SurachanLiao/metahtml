  // add newsletter addon
  function showArticleNewsletters() {

    const newsletterSection = {
      'canada': [{
          'newsletterId': '356',
          'newsletterKey': 'LG_Western',
          'name': 'Western Canada',
          'duration': 'Sent Wednesdays and Thursdays',
          'body': 'A comprehensive package of the news about the region.'
        },
        {
          'newsletterId': '113',
          'newsletterKey': 'LG_Politics',
          'name': 'Politics Briefing',
          'duration': 'Sent weekday mornings<br><br>',
          'body': 'Know what is happening in the halls of power.'
        },
        {
