            if (!window.oPageUnload) {
              oAv.push("trb_ad_300x250_1_/espanol/");
              oVa["trb_ad_300x250_1_/espanol/"] = ["NULL"];
            } else {
              oPageUnload('trb_ad_300x250_1_/espanol/', true);
            }
          }
        
        function loadAd() {
            dfpService.appendAd({
                id: 'espanol_1337',
                optimeraId: 'trb_ad_300x250_1_/espanol/',
                adType: 'cube',
