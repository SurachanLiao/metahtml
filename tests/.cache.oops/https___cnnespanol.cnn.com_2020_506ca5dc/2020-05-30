	var src = 'https:' + '//cdn.cnn.com/analytics/cnnespanol/jsmd-prod.min.js',
		loadJsmd = function loadJsmd( src ) {
			var e = document.createElement('script');
			e.setAttribute('id', 'JSMD-script');
			e.setAttribute('src', src);
			e.setAttribute('type', 'text/javascript');
			
			document.body.appendChild(e);

			e.onload = () => {
				/* begin Turner JSMD code */
				var pageURL = window.location.pathname;
				if (pageURL.indexOf("/.element/ssi/ads.iframes/") == -1 && pageURL.indexOf("/doubleclick/dartiframe.html") == -1) {
					try {
						if ( ! jQuery( 'body' ).hasClass( 'search-results' ) ) {
							if ( 'undefined' !== _jsmd ) {
								var jsmd = _jsmd.init();
								jsmd.send();
							}
						}
					} catch (err) {
						console.log("jsmd did not init or send correctly");
						console.log(err);
					}
				}
				/* end Turner JSMD code */
			};
		};
	// append the JSMD script and on load, run jsmd.init() and jsmd.send()
	loadJsmd(src);
</script>
