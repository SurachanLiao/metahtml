    AARP.Everywhere.Header.init();
    LUX.mark("HeaderLinks_End");
    LUX.measure("HeaderLinks", "HeaderLinks_Start","HeaderLinks_End");
    
    function userLoggedIn(){
        return AARP.store.getState().cookie.hasOwnProperty('at');
    }
    var hostname = window.location.hostname.split('.');
    if(hostname.indexOf('jeopardy') > -1){
        AARP.Externals.attachPageJsCallback(function() {
            $aarpe('.aarpe-everywhere-header').find('a.dropdown-toggle').dropdown();
        });
        if(userLoggedIn()){
            AARP.Externals.attachPageJsCallback(function() {
                $aarpe('.aarpe-everywhere-header').find('div.dropdown-toggle').dropdown();
            });
        }
    }
