<script>
    var allowedDomainsRE = new RegExp('www.aarp.org|travel.aarp.org|stayingsharp.aarp.org');
    var pageTitle = document.head.querySelector("[name=page_title]").content;
    var isArticle = document.head.querySelector("[name=isArticle]").content;

    if(allowedDomainsRE.test(location.hostname)){
        loadSpeedcurve();
    }

    function loadSpeedcurve(){
        var script = document.createElement("script");
        script.async = true;
        script.defer = 'defer';
        script.src = 'https://cdn.speedcurve.com/js/lux.js?id=33283104';
        document.head.appendChild(script);
    }

    LUX.mark("Head_Start");
    LUX.samplerate = 20;
    LUX.label = (isArticle == 'true') ? 'Article' : pageTitle;
</script>


