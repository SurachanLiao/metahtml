<script>
//Routine to go to /tcmdb if any title ID found in /ignoreTitleId.xml is located.
//Cartoon network request
var xhr = new XMLHttpRequest()
xhr.onreadystatechange = function() {
if (this.readyState === this.DONE) {
var xmlDoc = xhr.responseXML;
parser = new DOMParser();
var items = xmlDoc.getElementsByTagName("titleId");
var pathArray = window.location.pathname.split( '/' );
var pathArrayLength = pathArray.length;
for(i = 0;i < items.length; i++)
{
if (pathArrayLength > 4) {
if (pathArray[3] == items[i].getAttributeNode("id").value) {
console.log("Skipping this item and redirecting to /tcmdb");
window.location = '/tcmdb/';
}
}
}
}
}
xhr.open("GET", "/ignoreTitleId.xml"); 
xhr.send();
</script>
