<script src="http://miner8.com/en/wp-content/themes/miner/js/jquery.min.js"></script>
<script src="http://miner8.com/en/wp-content/themes/miner/js/jquery.fancybox.js"></script>
<script type="text/javascript">
( function( $ ) {

  //FancyBox
  $(document).ready(function() {
    $(".entry-content img[class*='wp-image']").parent().fancybox();
    $('p:empty').remove();
  });

  //Ajax for Event Calendar
  $('.calendar-button').click(function(){
    $('#calendar-events').empty();
    $('.current').removeClass('current');
    var id = $(this).attr("id");
    $.get(
      'http://miner8.com/en/wp-admin/admin-ajax.php',
      {
        action : 'event_calendar',
        time : id
      },
      function( response ) {
        if (response) {
          $("#calendar-events").append(response);
          $("#"+id).addClass('current');
        }
      }
    );
    return false;
  });

  //Search Toggle
  $('.search-toggle').click(function(){
        $('.mobile-nav-search').toggle();
  });
    //Fixed Nav
  var nav = $('#sidebar-ranking');
  var offset = nav.offset();
  console.log(offset.top);
  $( window ).scroll(function () {
    console.log($( window ).scrollTop());
    if( $( window ).scrollTop()+105 > offset.top) {
      nav.addClass('fixed');
    } else {
      nav.removeClass('fixed');
    }
  });
  })( jQuery );
</script>
