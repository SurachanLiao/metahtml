<script type="text/javascript">
  window.addEventListener('message', receiveMessage, false)
  function receiveMessage(event) {
    var ERregex = /^https:\/\/(\w*\.)?eyereturn\.com/
    var origin = event.origin || event.originalEvent.origin
    if (origin !== 'https://misc.thestar.com' && !ERregex.test(origin)) return
    var data = event.data.resize
    if (data) {
      if (data.height && data.iframe) {
        var elem = (document.getElementById(data.iframe).height = data.height)
      }
    }
  }
</script>

