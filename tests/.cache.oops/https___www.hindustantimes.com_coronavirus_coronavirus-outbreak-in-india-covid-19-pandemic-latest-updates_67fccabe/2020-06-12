var categories = [];
var seriesData = [];

for (var i = 0; i < chartData.length; i++) {
	var dataDate= new Date();
	var currDate = new Date(chartData[i].browserDate);
    if(dataDate.getDate()!==currDate.getDate()){
	    categories.push(chartData[i].dateStr);
	    seriesData.push(chartData[i].newCases);
    }
}
Highcharts.chart('indiaCasesChart', {
    chart: {
        type: 'column'
    },
    title: false,

    xAxis: {
        categories: categories,
        crosshair: true
    },
    yAxis: {
        min: 8,
        visible: false
    },
    tooltip : {
    	backgroundColor: '#fff',
        borderColor: '#7db5ed',
		headerFormat : '<div class="chartToolTip"><span>{point.key}</span><table >',
		pointFormat : '<tr><td>{series.name}: </td>'
				+ '<td>{point.y:if}</td></tr>',
		footerFormat : '</table></div>',
		shared : true,
		useHTML : true
    },
    plotOptions: {
        column: {
            pointPadding: 0,
            borderWidth: 0
        }
    },
    credits: {
        enabled: false
    },
    series: [{
        name: 'New Cases',
        data: seriesData

    }]
});

function showFullTable(ev) {
    ev.parentNode.previousElementSibling.style.height = 'auto';
    ev.parentNode.style.display = 'none';
}
</script><!--#em:content-->
