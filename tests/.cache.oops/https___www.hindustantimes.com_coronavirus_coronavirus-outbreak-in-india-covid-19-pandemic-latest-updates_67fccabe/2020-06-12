		<script>function covidOpen(event, element) {var popState = document.querySelector("#popup-data");popState.style.display = "block";if (window.innerWidth > 767 && event.type != "click") {popState.style.left = (event.clientX - popState.clientWidth / 2) + "px";popState.style.top = (event.clientY + 25) + "px";console.log("desktop");} else if (window.innerWidth <= 767) {popState.style.left = "0";popState.style.top = "0";}$('#covidRange').css('background-color', element.getAttribute("tally-range"));$("#covidStateName").html(element.getAttribute("data-state"));$("#covidTotal").html(element.getAttribute("data-total"));$("#covidActive").html(element.getAttribute("data-active"));$("#covidRecovered").html(element.getAttribute("data-recovered"));$("#covidDeaths").html(element.getAttribute("data-deaths"));}function covidClose(){if (window.innerWidth <= 767) {return;}$('#popup-data').css('display', 'none');$('#popup-data').css('min-width', '360');}function popupClose(){$('#popup-data').css('display', 'none');}$(document).ready(function(){GetTopTally();setInterval(function(){GetTopTally(); }, 60000);});function GetTopTally() { $.ajax({        url: 'https://hindustantimes.com/feed-elections/10s/2019-10/ht_coronawidget.json',        dataType: 'json',        success: function(data) {        bindTally(data);                },        error: function(jqXHR, exception) {          }    });}function bindTally(response) {var element = document.createElement("div");element.innerHTML = response.html;var scriptList = $(element).find('#covid-livetraker').html();$('#covid-livetraker').html(scriptList);}</script>
