            $(this).remove();
            jQuery(window).scrollTop(top);

          });

        }

      }else{
        var height = jQuery(".post-content-container").height();
        var heightMax = 1200;

        if(height > heightMax){
          jQuery(".post-content-container")
          .addClass("dcl_auto_readmore_post")
          .after("<div class = 'dcl_auto_readmore_button'>...&nbsp;Read More</div>");

          jQuery(".dcl_auto_readmore_button").click(function(){
            var top = jQuery(window).scrollTop();

            jQuery(".post-content-container").removeClass("dcl_auto_readmore_post");
            $(this).remove();
            jQuery(window).scrollTop(top);

          });

        }

    }

