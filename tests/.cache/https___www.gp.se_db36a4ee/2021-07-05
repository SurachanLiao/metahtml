(function(){
    var copySploar = function() {
        var sploar = headerScripts.readCookie('sploar');
        var fxtqrtp = headerScripts.readCookie('p_fxtqrtp');
        if (sploar === false) {
            headerScripts.eraseCookie('p_fxtqrtp');
        } else if (sploar !== fxtqrtp) {
            headerScripts.createCookie('p_fxtqrtp', sploar, 86400);
        }
    };
    var originalCheckForFetch = headerScripts.frequency.checkForFetch;
    headerScripts.frequency.checkForFetch = function(articleId, renderedArticlesCount) {
        originalCheckForFetch(articleId, renderedArticlesCount);
        copySploar();
    };
    copySploar();
})()
</script>
