<div id="GUnitVC"></div>
<form id="SnsCountForm" name="SnsCountForm" method="post" target="hframe"></form>
<script src="https://developers.kakao.com/sdk/js/kakao.min.js"></script>
<script type="text/javascript">
    Kakao.init('608b9b47ccae5917682577152d370511');

    var gList = new Array(14);
    var margin = 5;
    var thumWidth = 1554;
    var warpWidth = 0;
    var isFull = false;
    var isStart = false;
    var curLeft = 0;
    var curIndex = 0;
    var arrItemLeft = [0,118,236,354,472,590,659,777,895,1013,1131,1249,1367,1485,1554];
    var command = "slide";

    var unitBxslider;
    var bxSliderOptions = {
        speed: 300,
        auto: false,
        autoHover: true,
        adaptiveHeight: true,
        controls: false,
        slideMargin: 0,
        infiniteLoop: true,
        pager: false,
        video: true,
        useCSS: false,
        onSlideBefore: function ($slideElement, oldIndex, newIndex) {
            stopAutoSlide();
            SlideCore(newIndex);
        },
        onSlideAfter: function ($slideElement, oldIndex, newIndex) {
            unitBxslider.reloadSlider($.extend(bxSliderOptions, { startSlide: newIndex }));
            if (isStart) {
                startAutoSlide();
            }
        }
    }

    function NewsMailSend() {
        viewMessageOpen(0);
        $('html, body').animate({ scrollTop: 0 });
    }

    function CopyURL() {
        viewAddressOpen(location.href);
    }

    function SetWarpWidth() {
        warpWidth = $(".photo_read_g_wrap").outerWidth() - 100;
        if (thumWidth > warpWidth) {
            isFull = true;
        }
        else {
            isFull = false;
        }
    }

    function locationHashChanged() {
        ga('set', { page: location.href.replace(location.protocol + "//" + location.host ,""), title: "물안경 쓰고 입국까지…신종코로나가 만든 풍경" });
        ga('send', 'pageview');
    }

    function supportsWebGl() {
        var support = true;
        try {

            var $canvas = $('<canvas />');
            $('body').append($canvas);
            var canvas = $canvas[0];

            if (canvas.addEventListener) {
                canvas.addEventListener("webglcontextcreationerror", function (event) {
                    console.log('webglcontextcreationerror');
                    support = false;
                }, false);
            }

            var context = create3DContext(canvas);
            if (!context) {

                console.log('No webgl context');

                if (!window.WebGLRenderingContext) {
                    console.log('No WebGLRenderingContext');
                }

                support = false;
            }
        }
        catch (e) {
            console.log(e);
        } finally {
            $canvas.remove();
        }

        return support;
    }

    function create3DContext(canvas) {
        var names = ["webgl", "experimental-webgl", "webkit-3d", "moz-webgl"];
        var context = null;
        for (var ii = 0; ii < names.length; ++ii) {
            try {
                context = canvas.getContext(names[ii]);
            } catch (e) { }
            if (context) {
                break;
            }
        }
        return context;
    }

    function FillUnit(newIndex) {
        var data = gList[newIndex];
        if ($.trim($(".li_" + data.Seq).html()) == "") {
            var rowHtml = "";
            if (data.GUSource == "" && data.GUChkVR != 1) {
                rowHtml += "<span id=\"readViewr_Img\"><img src=\"https://img.sedaily.com" + data.GUDate + data.GImg + "\" alt=\"\" /></span>";
            }
            else if (data.GUChkVR == 1) {
                if (supportsWebGl()) {
                    if ("web" == "web") {
                        rowHtml += "<img src=\"https://img.sedaily.com/Html/Photo/360_read.png\" alt=\"\" class=\"view360\">";
                        rowHtml += "<iframe id=\"ifrmView_" + data.Seq + "\" style=\"width:100%;height:600px;border:0 none;\" scrolling=\"no\" allowfullscreen src=\"\"></iframe>";
                    }
                    else {
                        rowHtml += "<a href=\"" + "https://img.sedaily.com/VR/View.html?image=" +  data.GUDate + data.GImg + "\"><span id=\"readViewr_Img\"><img src=\"https://img.sedaily.com/Html/Photo/360_read.png\" alt=\"\" class=\"view360\"><img src=\"https://img.sedaily.com" + data.GUDate + data.GImg + "\" alt=\"\" /></span></a>";
                    }
                }
                else {
                    rowHtml += "<span id=\"readViewr_Img\"><img src=\"https://img.sedaily.com" + data.GUDate + data.GImg + "\" alt=\"\" /></span>";
                }
            }
            else {
                rowHtml += data.GUSource;
            }
            $(".li_" + data.Seq).html(rowHtml);
        }
    }

    function FillUnitThum(newIndex) {
        var dWith = arrItemLeft[newIndex] + warpWidth;
        var rowNums = "";
        var iL = new Array();
        if(newIndex != 0){
            newIndex = newIndex - 1;
        }
        for (var i = newIndex; i < arrItemLeft.length - 1; i++) {
            var data = gList[i];
            if(data == undefined){
                rowNums += "," + Number(i + 1);
                iL.push(i);
            }
            else{
                if ($("#img_GUnit_Thum_" + i).attr("src") == "") {
                    $("#img_GUnit_Thum_" + i).attr("src", "https://img.sedaily.com" + data.GUDate + data.GUTImg);
                }
            }
            if (dWith <= arrItemLeft[i]) {
                break;
            }
        }
        if(rowNums != ""){
            $.ajax({
                type: 'GET',
                url: "/Photo/Gallery/GetGUnit?gKey=3670&rowNums=" + encodeURIComponent(rowNums.substr(1, rowNums.length - 1)),
                success: function (result) {
                    for(var i = 0; i < result.dataList.length;i++)
                    {
                        var data = result.dataList[i];
                        gList[iL[i]] = data;
                        if ($("#img_GUnit_Thum_" + iL[i]).attr("src") == "") {
                            $("#img_GUnit_Thum_" + iL[i]).attr("src", "https://img.sedaily.com" + data.GUDate + data.GUTImg);
                        }
                    }
                },
                error: function () {
                    alert("서버통신 오류입니다. 잠시 후 시도해 주세요.");
                }
            });
        }
    }

    $(window).resize(function () {
        SetWarpWidth();
        if (isFull) {
            $('#ul_GUnit_Thum').css({ 'left': curLeft + 'px' });
        }
    });

    $(document).ready(function () {
        SetWarpWidth();

        UpdateGUnitVC = function (seq) {
            jAjax("GUnitVC", "/Photo/Gallery/UpdateGUnitViewCount?seq=" + seq);
        };

        SlideCore = function (newIndex) {
            if (command == "slide") {
                var totalWidth = 0;
                var moveIndex = 0;
                for (var i = gList.length - 1; i >= 0; i--) {
                    totalWidth += Number($("#img_GUnit_Thum_" + i).attr("width")) + margin;
                    if (warpWidth == totalWidth) {
                        moveIndex = i;
                        break;
                    }
                    else if (warpWidth < totalWidth) {
                        moveIndex = i + 1;
                        break;
                    }
                }
                var limitS = (arrItemLeft[moveIndex]);
                var limitE = (arrItemLeft[moveIndex]) + warpWidth;
                if (arrItemLeft[newIndex] >= limitS && arrItemLeft[newIndex] <= limitE) {
                    $('#ul_GUnit_Thum').stop(true, true);
                    $('#ul_GUnit_Thum').animate({ 'left': -(limitS) + 'px' }, function () {
                        FillUnitThum(moveIndex);
                    });
                }
                else {
                    $('#ul_GUnit_Thum').stop(true, true);
                    $('#ul_GUnit_Thum').animate({ 'left': -(arrItemLeft[newIndex]) + 'px' }, function () {
                        FillUnitThum(newIndex);
                    });
                }
            }

            $("[id^='li_GUnit_Thum_']").each(function () {
                if ($(this).hasClass("p_r_g1_on")) {
                    $(this).removeClass("p_r_g1_on").addClass("thum_img_opacity");
                }
            });

            var prevIndex = 0;
            var nextIndex = 0;
            if (newIndex - 1 < 0) {
                prevIndex = gList.length - 1;
            }
            else {
                prevIndex = newIndex - 1;
            }
            if (newIndex + 1 >= gList.length) {
                nextIndex = 0;
            }
            else {
                nextIndex = newIndex + 1;
            }

            var rowNums = "";
            var indexArr = new Array();
            var iL = new Array();
            indexArr.push(prevIndex);
            indexArr.push(newIndex);
            indexArr.push(nextIndex);

            for(var i = 0;i< indexArr.length;i++)
            {
                var data = gList[indexArr[i]];
                if(data == undefined){
                    rowNums += "," + Number(indexArr[i] + 1);
                    iL.push(indexArr[i]);
                }
            }

            if(rowNums != ""){
                iL.sort();
                $.ajax({
                    type: 'GET',
                    url: "/Photo/Gallery/GetGUnit?gKey=3670&rowNums=" + encodeURIComponent(rowNums.substr(1, rowNums.length - 1)),
                    async: false,
                    cache: false,
                    success: function (result) {
                        for(var i = 0; i < result.dataList.length;i++)
                        {
                            var data = result.dataList[i];
                            gList[iL[i]] = data;
                        }
                    },
                    error: function () {
                        alert("서버통신 오류입니다. 잠시 후 시도해 주세요.");
                    }
                });
            }

            FillUnit(prevIndex);
            FillUnit(newIndex);
            FillUnit(nextIndex);

            var data = gList[newIndex];
            $("#li_GUnit_Thum_" + data.Seq).removeClass("thum_img_opacity").addClass("p_r_g1_on");
            $("#sp_GUnit_CurPage").text(newIndex + 1);

            $("#sp_GUnit_GUTitle").text(data.GUTitle);
            $("#sp_GUnit_GUComment").text(data.GUComment);

            if ("web" != "web") {
                $("#sp_GUnit_GUComment").dotdotdot();
            }
            if (data.GUChkVR == 1) {
                if (supportsWebGl())
                {
                    if ("web" == "web") {
                        $("#ifrmView_" + data.Seq).attr("src", "https://img.sedaily.com/VR/View.html?image=" + data.GUDate + data.GImg);
                    }
                }
            }

            window.location.hash = "#" + data.Seq;
            UpdateGUnitVC(data.Seq);
            curIndex = newIndex;
            curLeft = -(arrItemLeft[newIndex]);
            command = "slide";
        };

        var startSlide = 0;
        if (gList.length > 0) {
            var seq = window.location.hash.substring(1);
            if(seq != ""){
                $.ajax({
                    type: 'GET',
                    url: "/Photo/Gallery/GetGUnitIndex?gKey=3670&seq=" + seq,
                    async: false,
                    cache: false,
                    success: function (result) {
                        startSlide = Number(result);
                    },
                    error: function () {
                        alert("서버통신 오류입니다. 잠시 후 시도해 주세요.");
                    }
                });
                locationHashChanged();
            }
            SlideCore(startSlide);
        }

        unitBxslider = $('#ul_GUnit').bxSlider($.extend(bxSliderOptions, { startSlide: startSlide }));

        startAutoSlide = function () {
            unitBxslider.startAuto();
        };
        stopAutoSlide = function () {
            unitBxslider.stopAuto();
        };

        MoveThumnail = function (type) {
            command = "slide";
            if (type == 0) {
                unitBxslider.goToPrevSlide();
            }
            else {
                unitBxslider.goToNextSlide();
            }
        };

        PageMoveToSlide = function (slideNumber) {
            unitBxslider.goToSlide(slideNumber);
        };

        ClickThumnail = function (index) {
            PageMoveToSlide(index);
        };

        ClickData = function (index) {
            command = "click";
            ClickThumnail(index);
            if ((-curLeft) - (-1 * $('#ul_GUnit_Thum').position().left) + Number($("#img_GUnit_Thum_" + index).attr("width")) + margin > warpWidth) {
                $('.p_r_g1_btn2').trigger("click");
            }
        };

        if (DeviceCheck() == "web") {
            $(".p_r_g1_btn2").mouseover(function () {
                $("#img_GUThum_Next").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_02_on.png");
            });

            $(".p_r_g1_btn2").mouseout(function () {
                $("#img_GUThum_Next").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_02.png");
            });

            $(".p_r_g1_btn1").mouseover(function () {
                $("#img_GUThum_Prev").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_01_on.png");
            });

            $(".p_r_g1_btn1").mouseout(function () {
                $("#img_GUThum_Prev").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_01.png");
            });

            $("#img_LeftGUnit").mouseover(function () {
                $("#img_LeftGUnit").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_04_on.png");
            });

            $("#img_LeftGUnit").mouseout(function () {
                $("#img_LeftGUnit").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_04.png");
            });

            $("#img_RightGUnit").mouseover(function () {
                $("#img_RightGUnit").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_05_on.png");
            });

            $("#img_RightGUnit").mouseout(function () {
                $("#img_RightGUnit").attr("src", "https://img.sedaily.com/Html/Photo/photo_read_05.png");
            });
        }

        $('.p_r_g1_btn2').click(function () {
            if (thumWidth + $('#ul_GUnit_Thum').position().left > warpWidth) {
                $('#ul_GUnit_Thum').stop(true, true);
                var $newLeft = 0;
                var nexIndex = 0;
                for (var i = 0; i < arrItemLeft.length; i++) {
                    if ($('#ul_GUnit_Thum').position().left > -(arrItemLeft[i])) {
                        $newLeft = -(arrItemLeft[i]);
                        nexIndex = i;
                        break;
                    }
                }
                $('#ul_GUnit_Thum').animate({ 'left': $newLeft + 'px' }, "fast", function () {
                    FillUnitThum(nexIndex);
                });
                return false;
            }
            else {
                if (curIndex == gList.length - 1) {
                    ClickThumnail(0);
                }
            }
        });

        $('.p_r_g1_btn1').click(function () {
            $('#ul_GUnit_Thum').stop(true, true);
            var $newLeft = 0;
            var nexIndex = 0;
            for (var i = arrItemLeft.length - 1; i >= 0; i--) {
                if ($('#ul_GUnit_Thum').position().left < -(arrItemLeft[i])) {
                    $newLeft = -(arrItemLeft[i]);
                    nexIndex = i;
                    break;
                }
            }
            $('#ul_GUnit_Thum').animate({ 'left': $newLeft + 'px' }, "fast", function () {
                FillUnitThum(nexIndex);
            });
            return false;
        });

        $("#img_GUnit_AutoStart").click(function(){
            if($(this).attr("src").indexOf("play") != -1){
                $(this).attr("src", "https://img.sedaily.com/Html/Photo/pause2.png");
                startAutoSlide();
                isStart = true;
            }
            else{
                $(this).attr("src", "https://img.sedaily.com/Html/Photo/play2.png");
                stopAutoSlide();
                isStart = false;
            }
        });

        SnsShare = function (Kind) {
            
		    jAjax("", "/News/NewsView/SnsCount?Nid=" + encodeURIComponent(window.location.pathname) + "&Tool=" + Kind, "SnsCountForm", "Post");

            var Link = encodeURIComponent(window.location);
            var Title = encodeURIComponent(gList[curIndex].GUTitle);
            var Url = "";

            if (Kind == "fb") {
                Url = "http://www.facebook.com/sharer/sharer.php?u=" + Link + "&t=" + Title;
            }
            else if (Kind == "twt") {
                Url = "http://twitter.com/share?url=" + Link + "&text=" + Title;
            }
            else if (Kind == "kko") {
                Url = "https://story.kakao.com/share?url=" + Link;
            }
            else if (Kind == "gplus") {
                Url = "https://plus.google.com/share?url=" + Link;
            }
            window.open(Url, "SnsSharePop", "width=500,height=450");
        };

        SnsShareKaKao = function () {
            
            jAjax("", "/News/NewsView/SnsCount?Nid=" + encodeURIComponent(window.location.pathname) + "&Tool=kko", "SnsCountForm", "Post");

            Kakao.Link.sendTalkLink({
                label: "[서울경제] 물안경 쓰고 입국까지…신종코로나가 만든 풍경",
                image:
                {
                    src: "https://img.sedaily.com" + gList[curIndex].GUDate + gList[curIndex].GImg,
                    width: gList[curIndex].GUImgW,
                    height: gList[curIndex].GUImgH
                },
                webLink: {
                        text: '사진보기',
                        url: location.href
                },
                webButton: {
                    text: '포토갤러리 더보기',
                    url: 'http://www.sedaily.com/v/Photo'
                }
            });
        };

        $("#btnMailSend").click(function () {
            $("#GallerySendMailForm").validationEngine();
            if ($("#GallerySendMailForm").validationEngine('val')) {
                if (confirm("메일을 발송 하시겠습니까?") == true) {
                    $("#GallerySendMailForm").submit();
                }
                else {
                    return false;
                }
            }
        });
    });

    window.onhashchange = locationHashChanged;
</script>

	



