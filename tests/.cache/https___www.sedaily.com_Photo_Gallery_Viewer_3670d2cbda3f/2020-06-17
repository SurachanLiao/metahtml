            $(this).find(".image_scale").eq(0).stop().animate({ scale: 1.2 }, 200);
        });
        $(".mi_wrap_i").mouseout(function () {
            $(this).find(".image_scale").eq(0).stop().animate({ scale: 1 }, 200);

        });
    });
