		    <script>
			    $( document ).ready(function() {
			      addCarouselButtons();
				  $('#pauseButton').click(function() {
				  	//event.stopPropagation();
				    if ( $('#pauseButton').hasClass('playing') ) {
				      $('#pauseButton .sr-only').html('Play');
				      $('.carousel').carousel('pause');
				      $('#pauseButton .fa').toggleClass('fa-pause fa-play')
				    } else {
				      $('#pauseButton .sr-only').html('Pause');
				      $('.carousel').carousel('cycle');
				      $('#pauseButton .fa').toggleClass('fa-play fa-pause');
				    }
				    $(this).toggleClass('playing pause');
				  })
				});
				function addCarouselButtons() {
					// add sr-only stuff for ADA
					$('.carousel').addClass('has-controls');
					$('.carousel .carousel-control.left').append('<span class="sr-only">Previous</span>');
					$('.carousel .carousel-control.right').append('<span class="sr-only">Next</span>');
					var carouselID = $('.carousel .carousel-control.left').attr('href');
					var pausePlayButton = '<button type="button" id="pauseButton" class="playing carousel-control" role="button"><span class="fa fa-pause" aria-hidden="true"></span><span class="sr-only">Pause</span></button>'
					$('.carousel .carousel-control.left').after(pausePlayButton);
				}
				</script>
