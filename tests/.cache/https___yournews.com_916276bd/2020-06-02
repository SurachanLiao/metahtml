      if ( language_name == "English" && english_flag_choice == 'us_flag') {
        var language_name_flag = 'united-states';
      }
      if ( language_name == "Spanish" && spanish_flag_choice == 'mexican_flag') {
        var language_name_flag = 'mexico';
