    function tabImgWide() {
        var root = $("#tabImgUl > li").length;
        $("#tabImgUl").css("width", root * oneImgWide);
        var root = $("#tabImgUl2 > li").length;
        $("#tabImgUl2").css("width", root * oneImgWide);
    }
