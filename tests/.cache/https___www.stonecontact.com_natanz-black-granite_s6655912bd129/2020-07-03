<script type="text/javascript">
    $(function () {
        var materialId = '6655';
        if (materialId != '') {
            writeApiHost = "https://w.stonecontact.com";
            insertVistor({ KeyWord: "ce0d6a15-7b2d-4593-9ff5-fb3a301d39c9", PageId: 5, PageType: 1, CompanyId: "00000000-0000-0000-0000-000000000000" });
            updateClickCount(materialId, { "": 5 });
        }

        jQuery("#sendpasswordbymail").click(function () {
            var username = jQuery("#memberID").val();
            if (username == '') {
                alert("please input your E-mail or member ID!");
                jQuery("#memberID").focus();
                return false;
            }
            username = encodeURIComponent(username).replace(/\+/g, '%2B').replace(/\"/g, '%22').replace(/\'/g, '%27').replace(/\//g, '%2F');
            var url = '/DataManager/StoneContactDataHandler.ashx?action=GetPasswordByEmail&username=' + username;
            submit(url, function (data) {
                data = eval(data)[0];
                switch (data.result) {
                    case "error_arg":
                        alert("please input your E-mail  or member ID!");
                        jQuery("#memberID").focus();
                        return false;
                        break;

                    case "error_wrong":
                        alert("Wrong Email or MemberID!");
                        jQuery("#memberID").focus();
                        return false;
                        break;

                    case "ok":
                        alert("The Password has send to your E-mail,Please Check it!");
                        return false;
                        break
                }
                return false;
            });
        });
    });

    function submit(url, callback) {
        $.ajax({
            type: 'post',
            dataType: 'json',
            contentType: "application/x-www-form-urlencoded; charset=utf-8",
            cache: false,
            url: url,
            success:
                function (result) {
                    if (callback) {
                        callback(result);
                    }
                },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(XMLHttpRequest.responseText + "---åé¢ä¸ºéè¯¯åå®¹ï¼ï¼ï¼");
                alert(textStatus);
            }
        });
    }

    function showdialog(companyName, materialID) {
