<script>
    function PageNextHtml() {
        var page = $("#productrepage").val();
        var key = 'Natanz Black Granite';
        var nextpage = parseInt(page) + 1;

        if (nextpage != "0") {
            $.ajax({
                url: '/Ajax/RelatedProducrHandler.ashx',
                type: 'POST',
                async: 'True',
                data: { page: nextpage, key: key },
                success: function (data) {
                    if (data == "") {
                    } else {
                        $("#productrepage").val(nextpage);
                        $("#MainContainer_Control_RepeaterSupplier").html($("#MainContainer_Control_RepeaterSupplier").html() + data)
                    }
                },
                error: function (data) {
                }
            });
        }
    }
</script></div>
