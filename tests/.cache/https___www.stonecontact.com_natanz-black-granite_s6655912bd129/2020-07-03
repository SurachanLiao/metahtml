                        };
                    }
                    var ajaxUrl = "/Ajax/Request.aspx";//"/Ajax/SendEmail/SendRequestInfo.aspx";
                    jQuery.get(ajaxUrl, data,
                        function (xml) {
                            var isSuccess = $("isSuccess", xml).eq(0).text().trim();
                            var returnText = $("returnText", xml).eq(0).text();
                            if (isSuccess == 1) {
                                $("#product-dese-login-box").dialog("close");
                                alterMsg();

                            }
                            else { alert(returnText); return false; }
                        });
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            },
            close: function () {
            }
        });
    });
    function checkEmpty(id, note) {
        var obj = $("#" + id);
        var v = obj.val();
        if (v == '') {
            alert("Please Input Your " + note + ".");
            obj.focus();
            return false;
        }
        return true;
    }
    function alterMsg() {

        $("#product-desc-send-info-box").hide();
        $("#product-desc-send-info-hidebox").show();

        var title = "Your request has been sent to the supplier. They will quote you the price according to your specs.";
        var src = "/UIControls/Common/CommonPrompt.aspx?title=" + title;
        $("#ifPrompt").attr("src", src);
        $("#ifPrompt").dialog({
            autoOpen: false, width: 550, height: 185,
            bgiframe: true, modal: false, resizable: false,
            title: 'Send a message to the company', position: "center", buttons: null
        });
        $("#ifPrompt").dialog('open');
        $("#ifPrompt").css("width", "555px");
        $("#ifPrompt").css("height", "185px");
    }
