        });
        $("#product-dese-login-box").dialog({
            autoOpen: false,
            height: 440,
            width: 450,
            modal: true,
            title: "Send E-mail",
            buttons: {
                "Send": function () {
                    var data;
                    if (sendType == 0) {
                        if (!checkEmpty("visiterName", "name")) {
                            return false;
                        }
                        if ($("#visiterEmail").val() == "") { alert("Please Input your Email!"); $("#visiterEmail").focus(); return false; }
                        var myreg = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;
                        if (!myreg.test($("#visiterEmail").val())) { alert("Please Check your Email!"); $("#visiterEmail").focus(); return false; }
                        if (!checkEmpty("visiterTel", "Telephone No")) {
                            return false;
                        }
                        if ($("#MainContainer_Control_ddlCountryID").val() == -1) { alert("Please select your country!"); $("#MainContainer_Control_ddlCountryID").focus(); return false; }

                        data = {
                            Action: "RequestSubmit",
                            method: "visiterSend",
                            content: $("#MailContent").val(),
                            name: $("#visiterName").val(),
