    $(function () {
        PicLeftRightMove('.TabImgLeft', '.TabImgRight', '.NaturalStones-desc-tab-img', '#tabImgUl', '#tabImgUl2', '');
        postJson("https://w.stonecontact.com/api/v1/history/inserthistory", JSON.stringify({ "inRequestId": "1231", "DataType": "7", "DataID": "ce0d6a15-7b2d-4593-9ff5-fb3a301d39c9", "From": "2", "Email": "", "Driven": "0" }), function (data) {
            if (data.success) {

            }
        });
        function postJson(url, data, funcSuccess, funcError, funcBeforeSend) {
            $.ajax({
                url: url,
                type: 'POST',
                dataType: 'json',
                data: JSON.stringify(data),
                contentType: 'application/json; charset=utf-8',
                beforeSend: function (xhr) {
                    if (funcBeforeSend) {
                        funcBeforeSend(xhr);
                    }
                },
                success: function (data, status, xhr) {
                    if (funcSuccess) {
                        funcSuccess(data);
                    }
                },
                error: function (xhr, status, error) {
                    if (funcError) {
                        funcError(error, url);
                    }
                }
            });
        }
    })
</script>
