<script type="text/javascript">
    jQuery(document).ready(function () {
        var fileName = "";
        var fileSize = "";
        jQuery("#file_upload_1").uploadify({
            uploader: '/Scripts/uploadify/uploadify.swf',
            script: '/Ajax/ReuquestAttachHandler.ashx',
            cancelImg: '/Scripts/uploadify/cancel.png',
            buttonText: 'Upload',
            fileExt: '*.JPEG;*.PNG; *.JPG;*.GIF; *.TIFF;*.BMP;.xlsx;*.doc;*.xls;*.txt;*.PDF;*.ZIP;*.RAR',
            fileDesc: 'Format: JPEG,JPG, .PNG, .GIF, .TIFF, .BMP, .JPG, .xlsx, .doc, .xls, .txt, .PDF, .ZIP, .RAR;',
            sizeLimit: 3145728,
            multi: false,
            auto: true,
            width: 96,
            height: 32,
            wmode: 'transparent',
            hideButton: true,
            queueSizeLimit: 1,
            queueID: 'myqueue',
            onSelect: function (event, queueID, fileObj) {
                fileName = fileObj.name;
                fileName = fileName.substring(0, fileName.indexOf("."));
                var _fileSize = parseInt(fileObj.size) / 1024;
                _fileSize = parseInt(_fileSize);
                fileSize = "(" + _fileSize + " KB )";
                //å¤æ­æä»¶åç§°æ¯å¦æç¹æ®å­ç¬¦å¦
                if (!IsShowButtom()) {
                    // alert("Only three");
                  
                    $(this).uploadifyCancel(queueID); //åæ¶ä¸ä¼ 
                }
            },
            onComplete: function (event, id, fileObj, response, data) {
                if (response == "0") {
                    alert("Can't not upload .exe,.dll file!");
                } else {
                    for (var i = 1; i <= 3 ; i++) {
                        var attach = jQuery("#MainContainer_Control_UploadFile_hdAttach" + i).val();
                        if (i == 3) {//å½å¾çç­äºä¸ä¸ªçæ¶åéèfileupload
                            $('#fileupload').hide();
                        }
                        if (attach == "") {
                            jQuery("#divAttach" + i).show();
                            jQuery("#MainContainer_Control_UploadFile_hdAttach" + i).val(response);
                            //filename
                            jQuery("#MainContainer_Control_UploadFile_lblAttach" + i).text(fileName);
                            jQuery("#MainContainer_Control_UploadFile_lblhdAttach" + i).val(fileName);

                            //size
                            jQuery("#MainContainer_Control_UploadFile_lblClose" + i).text(fileSize);
                            jQuery("#MainContainer_Control_UploadFile_hdAttachSize" + i).val(fileSize);
                            break;
                        }
                    }


                }
            }
        });
    });
    function deleteAttach(attachId, obj) {
        //æ¸é¤éèæ§ä»¶çå¼
        jQuery("#MainContainer_Control_UploadFile_hdAttach" + attachId).val("");
        jQuery("#" + obj).hide();
        $('#fileupload').show();
    }
    ///ä¸ä¼ ä¸ä¸ªæä»¶ç¦æ­¢ä¸ä¼ 
    function IsShowButtom() {
        var isShow = false;
        for (var i = 1; i <= 3 ; i++) {
            var attach = jQuery("#MainContainer_Control_UploadFile_hdAttach" + i).val();
            if (attach == "") {
                isShow = true;
                break;
            }
        }
        return isShow;
    }
</script>
