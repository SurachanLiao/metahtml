        $("#ifCompanyInfo").attr("src", src);
        $("#ifCompanyInfo").dialog({
            autoOpen: false,
            width: 795,
            height: 535,
            bgiframe: true,
            modal: false,
            resizable: false,
            title: 'Send a message to the company',
            position: "center",
            buttons: null
        });
        $("#ifCompanyInfo").dialog('open');
        $("#ifCompanyInfo").css("width", "795px");
        $("#ifCompanyInfo").css("height", "535px");
    }
    function hideContactSupplier() {
        $("#ifCompanyInfo").dialog("close");
    }
