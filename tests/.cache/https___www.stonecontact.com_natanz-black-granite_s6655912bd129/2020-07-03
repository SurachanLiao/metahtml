                            hdAttach2: $("#MainContainer_Control_UploadFile_hdAttach2").val(),
                            hdAttach3: $("#MainContainer_Control_UploadFile_hdAttach3").val()
                        };
                    }
                    else {
                        if (!checkEmpty("memberID", "ID")) {
                            return false;
                        }
                        if (!checkEmpty("memberPassword", "Password")) {
                            return false;
                        }
                        data = {
                            Action: "RequestSubmit",
                            method: "memberSend", //NaturlStoneByMember
                            content: $("#MailContent").val(),
                            memberID: $("#memberID").val(),
                            memberPassword: $("#memberPassword").val(),
