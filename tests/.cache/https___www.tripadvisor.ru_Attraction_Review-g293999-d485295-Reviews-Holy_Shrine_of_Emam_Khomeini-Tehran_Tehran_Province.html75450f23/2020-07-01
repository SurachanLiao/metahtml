},
termsPopup: function(event, elmt) {
popup.apply(elmt, [event, 'terms', 300, 300, 30, 25]);
return false;
},
openInParent: function(event, elmt, leaveOpen) {
var w = false;
if (window.top && window.top.opener) w = window.top;
else if (window.opener) w = window;
if (!w) return true;
var url = '';
if(elmt.nodeName === 'FORM') {
url = elmt.action + '?' + api.toFormQueryString(elmt);
}
else {
elmt=$(elmt).closest("a")[0];
if (elmt) {
url = elmt.href;
}
}
if(url !== '') {
_preventDefault(event);
w.opener.location = url;
if (! (typeof leaveOpen !== 'undefined' && leaveOpen) ) {
w.close();
} else {
window.blur();
}
}
},
openInParentOrSelf: function(event, elmt) {
var w = false;
if (window.top && window.top.opener) w = window.top;
else if (window.opener) w = window;
var url = '';
if(elmt.nodeName === 'FORM') {
url = elmt.action + '?' + api.toFormQueryString(elmt);
}
else {
elmt=$(elmt).closest("a")[0];
if (elmt) {
url = elmt.href;
}
}
if(url !== '') {
_preventDefault(event);
if (!w || (typeof w.opener == 'undefined') || !w.opener || (typeof w.opener.closed === 'undefined') || w.opener.closed)
{
window.location = url;
}
else
{
w.opener.location = url;
w.close();
}
}
},
photoGallery: function(event, elmt) {
elmt=$(elmt).closest("a")[0];
if (!elmt) {
taError.record(e, 'photoGallery - get anchor elmt');
return false;
}
var url = "";
var id = "1";
if(elmt.className)
{
url = elmt.className.match(/u_([^ ]*)/)[1];
id = elmt.className.match(/#([0-9]+)/)[1];
}
try{
if(typeof cookieDomain == "undefined")
{
var h = window.location.hostname.split('.'), i=0, dd;
while(h[i] && h[i] != "tripadvisor") {
i++;
}
dd = h.slice(i).join('.');
if(dd) {
cookieDomain = "." + dd;
}
}
taCookie.write("PhotoPop", id, null, null, cookieDomain);
}
catch(e) {
taError.record(e, 'photoGallery - cookie set');
}
try {
var w = "";
if(url)
{
w = exports.open(url, 'media', 780, 800);
}
else
{
}
try{
if (w && w.showImage) {
w.showImage(id);
}
}
catch(e) { }
}
catch(e) {
taError.record(e, 'photoGallery - pop window');
}
return false;
},
openAIMLink:function(event,elmt)
{
_preventDefault(event);
var url;
var srcel = $(elmt).closest("span.fkASDF,div.fkASDF,a")[0];
if (!srcel) {
return;
} else if (srcel.nodeName === 'A') {
url = srcel.href;
