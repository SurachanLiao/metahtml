define("ta/servlet/Popup", ['ns-exporter', 'lib/jquery-amd', 'ta/Browser', 'ta/util/Error', 'ta/Core/TA.Record', 'ta/Core/TA.Store', 'ta/util/Cookie', "api-mod"],
function(nsExporter, $, taBrowser, taError, taRecord, taStore, taCookie, api) {
var _index=0;
function _preventDefault(event) {
if (event) {
if (event.preventDefault) {
event.preventDefault();
} else {
event.returnValue = false;
}
}
}
var exports = {
open: function(url, name, width, height, x, y, noScroll, allOptions)
{
try {
if(isNaN(parseInt(width))) {
width = 600;
}
else {
width = parseInt(width);
}
if(isNaN(parseInt(height))) {
height = 400;
}
else {
height = parseInt(height);
}
if(isNaN(parseInt(x))) {
x = 0;
}
else {
x = parseInt(x);
}
if(isNaN(parseInt(y))) {
y = 0;
}
else {
y = parseInt(y);
}
if (url.indexOf("p=HotelsCom") > -1) {
width = Math.max(width, 960);
height = Math.max(
height,
window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight,
600);
}
if (taBrowser.isChrome()) {
noScroll = true;
}
var noScrollVal = 1;
if(noScroll) {
noScrollVal = 0;
}
var menubar = 1;
if ( taBrowser.isIE9orHigher() )
{
var menubar = 0;
}
var winName = false;
if (name && typeof name == "string") {
winName = name;
}
if (!winName) {
var popNum = _index++;
winName = "TripPop" + popNum;
}
var ops = "";
if (allOptions) {
ops = "toolbar=1,resizable=1,location=1,status=1";
}
else {
menubar = 0;
ops = "toolbar=0,resizable=1,location=0,status=0";
}
ops += ",menubar="+menubar;
ops += ",scrollbars="+noScrollVal;
ops += ",width="+width;
ops += ",height="+height;
ops += ",screenX="+x+",left="+x;
ops += ",screenY="+y+",top="+y;
var w = window.open(url, winName, ops);
if (w != null) {
try {
w.focus();
w.opener = self;
} catch (exc) {}
return w;
}
} catch(e) {
taError.record(e, 'Popup.open', null, { name: name, width: width, height: height, x: x, y: y, noScroll: noScroll, allOptions: allOptions});
}
},
openExternalWithDefaultOptions: function(url) {
try {
var width = 475;
var height = 390;
var x = 210;
var y = 5;
if ( screen.width >= 1024 && screen.height >= 768) {
width = 900;
height = 768;
x = 240;
}
else if (screen.width > 1024) {
width = 800;
height = 600;
x = 240;
}
else if (screen.width > 800) {
width = 620;
height = 500;
x = 240;
}
if (url.indexOf("p=HotelsCom") > -1) {
width = Math.max(width, 960);
height = Math.max(
height,
window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight,
600);
}
var noScroll = false;
if (taBrowser.isChrome()) {
noScroll = true;
}
var noScrollVal = 1;
if(noScroll) {
noScrollVal = 0;
}
var menubar = 1;
if ( taBrowser.isIE9orHigher() )
{
var menubar = 0;
}
var popNum = _index++;
var winName = "TripPop" + popNum;
var ops = "toolbar=1,resizable=1,location=1,status=1";
ops += ",menubar="+menubar;
ops += ",scrollbars="+noScrollVal;
ops += ",width="+width;
ops += ",height="+height;
ops += ",screenX="+x+",left="+x;
ops += ",screenY="+y+",top="+y;
var w = window.open(url, winName, ops);
if (w != null) {
try {
w.focus();
w.opener = self;
} catch (exc) {}
return w;
}
} catch(e) {
taError.record(e, 'Popup.open - openExternalWithDefaultOptions', null, { name: name, width: width, height: height, x: x, y: y, noScroll: noScroll});
}
},
openSizedPopup: function(evnt, elmt, noScroll)
{
_preventDefault(evnt);
var width = 600;
var height = 400;
if (sz = elmt.className.match(/sz(\d+)x(\d+)/)) {
width = sz[1];
height = sz[2];
}
exports.open(elmt.href, 'photoTPW', width, height, false, false, ( (typeof noScroll !== 'undefined')? noScroll: true), true);
},
