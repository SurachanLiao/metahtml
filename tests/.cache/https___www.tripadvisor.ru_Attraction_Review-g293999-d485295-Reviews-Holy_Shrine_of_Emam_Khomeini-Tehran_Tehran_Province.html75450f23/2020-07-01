url = elmt.className.match(/u_([^ ]*)/)[1];
}
var navarea = "navarea";
if (ta.meta && ta.meta.link && ta.meta.link.getPhotoNavArea) {
navarea = ta.meta.link.getPhotoNavArea(elmt);
}
url = url.replace("%7Cnavarea%7C", navarea);
var winName = elmt.className.match(/win(.+)/);
if (winName && winName.length>1) {
winName = winName[1];
} else {
winName = null;
}
taStore.store('commerce.AIMWindow', exports.open(url,winName,980,800,240,5,false,true));
},
