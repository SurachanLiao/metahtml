        gap: 0,
        peek: {
            before: 0,
            after: 0
        },
        breakpoints: {
            480: {
                gap: 10,
                    peek: {
                        before: 35,
                        after: 15
                    }
                },
            768: {
                gap: 10,
                peek: {
                    before: 35,
                    after: 15
                }
            },
            992: {
                gap: 0,
                peek: {
                    before: 0,
                    after: 0
                }
            }
        }
    }).mount()
    </script>
            </div>
													








              	                                
