                    <div id="outsiderTop" class="c-ad c-ad__outsider">
                        <script type="text/javascript">
                            adformScripts.defineTag('outsiderTop', {	desktop: {id: '205205', index: 1, sizes: [[250,600]], prebid: true, lazyload: true, offset: 0, label: true}
		,mobile: null
	});
                        </script>
