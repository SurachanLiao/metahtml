    <!-- //미디어 스토리 콘텐츠 -->

    <script>
    (function($){
        var READY_SLIDE = true;

        /* 슬라이드 네비게이션 보이기 */
        $('#sectionMediastory .slide_mstory .btn_prev, #sectionMediastory .slide_mstory .btn_next').css({opacity: 0});
        $('#sectionMediastory .slide_mstory').hover(function(){
            $(this).find('.btn_prev, .btn_next').stop().animate({opacity: 1}, 200);
        }, function(){
            $(this).find('.btn_prev, .btn_next').stop().animate({opacity: 0}, 200);
        });

        /* 이전 컨텐츠 이동 */
        $('#sectionMediastory .slide_mstory .btn_prev').on('click', function(){
            if (READY_SLIDE == true) {
                slideContent(this, 'prev');
            }
        });

        /* 다음 컨텐츠 이동 */
        $('#sectionMediastory .slide_mstory .btn_next').on('click', function(){
            if (READY_SLIDE == true) {
                slideContent(this, 'next');
            }
        });

        /* 컨텐츠 지정 이동 */
        $('#sectionMediastory .slide_mstory div.paging_now button').on('click', function(){
            var $wrap = $(this).closest('div.paging_now');
            var idx = $wrap.find('button').index(this);
            if (READY_SLIDE == true) {
                slideContent(this, idx);
            }
        });

        /**
         * 컨텐츠 슬라이드
         **/
        function slideContent(_this, spot)
        {
            READY_SLIDE = false;
            var $wrap = $(_this).closest('.slide_mstory');
            var $buttons = $wrap.find('div.paging_now button');
            var $list_thumb = $wrap.find('ol.list_thumb');
            var $el = $list_thumb.find('li:eq(0)');
            var cnt = $list_thumb.find('li').length;
            var w = $el.css('width').replace('px', ''); ///< 요소 넓이

            // 다음 컨텐츠
            if (spot == 'next') {
                // 스크롤 이동 종료후 요소 이동
                $list_thumb.animate({left : (w * -1)}, 500, function(){ 
                    // 좌측요소 맨끝이동
                    $list_thumb.css({left: 0}).append($list_thumb.find('li:first'));

                    // 버튼 위치 표시
                    var idx = $list_thumb.find('li:first').data('index');
                    $buttons.removeClass('on').eq(idx).addClass('on');

                    READY_SLIDE = true;
                });
            }

            // 이전 컨텐츠
            else if (spot == 'prev') {
                // 요소먼저 이동후 스크롤
                $list_thumb.css({left: (w * -1)}).prepend($list_thumb.find('li:last'));
                $list_thumb.animate({left : 0}, 500, function(){ 
                    // 버튼 위치 표시
                    var idx = $list_thumb.find('li:first').data('index');
                    $buttons.removeClass('on').eq(idx).addClass('on');

                    READY_SLIDE = true;
                });
            }

            // 직접지정 컨텐츠
            else if ($.isNumeric(spot)) {
                READY_SLIDE = true;

                // 선택된 요소
                var $sel = $list_thumb.find('li[data-index='+ spot +']');

                // 현재 요소 (가변적인)배치중 선택된 요소의 위치값(index)
                var this_idx = $list_thumb.find('li').index($sel);

                // this_idx == 0 : 현재 보여지고 있는 컨텐츠임으로 무시 (즉, 선택요소의 좌측에 요소가 존재하지 않음)
                if (this_idx > 0) {
                    // 보여져야 할 요소보다 좌측에 있는 요소를 모두 잘라 우측으로 이동
                    $list_thumb.append($list_thumb.find('li:lt('+ this_idx +')'));
                    // 버튼 위치 표시
                    var idx = $list_thumb.find('li:first').data('index');
                    $buttons.removeClass('on').eq(idx).addClass('on');
                }
            }
        }
    }(jQuery));
    </script>

