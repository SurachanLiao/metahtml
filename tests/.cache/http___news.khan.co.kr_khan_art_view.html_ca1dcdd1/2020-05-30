                            <script>
                                function emulAcceptCharset(form) {
                                    if (form.canHaveHTML) { // detect IE
                                        document.charset = form.acceptCharset;
                                    }
                                    return true;
                                }
                            </script>
                        </fieldset>
                    </div>
                    <input type="hidden" id="main_top_search_btn_yn2" value="0" />
                </div>
            </div>

            <script>
            jQuery("#main_top_search_btn2").on('click',function()
            {
                search_input_fn2();
            });

            jQuery("#main_top_search_input2").keypress(function(e)
            {
                if (e.which == 13) { search_input_fn2(); }
            });

            function search_input_fn2()
            {
                var main_search_val2 = jQuery("#main_top_search_btn_yn2").val();
                if (main_search_val2 == "0")
                {
                    jQuery("#main_top_search_input2").css("display","inline-block");
                    jQuery("#main_top_search_btn_yn2").val("1");
                    jQuery("#main_top_search_input2").val("").focus();
                }
                else
                {
                    if (jQuery("#main_top_search_input2").val())
                    {
                        jQuery("#main_top_search_input2").css("display","none");
                        jQuery("#main_top_search_btn_yn2").val("0");
                        search2.submit();
                    }
                    else
                    {
                        jQuery("#main_top_search_input2").css("display","none").val("");
                        jQuery("#main_top_search_btn_yn2").val("0");
                    }
                }
            }
            </script>
