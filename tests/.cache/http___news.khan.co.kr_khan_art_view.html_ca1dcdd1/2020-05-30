                            <legend>검색</legend>
                            <form target="_top" action="http://search.khan.co.kr/search.html" onsubmit="emulAccpetCharest(this)" method="get" name="search2" accept-charset="UTF-8">
                                <input type="hidden" value="1" name="sect">
                                <input type="hidden" value="1" name="path">
                                <input type="text" id="main_top_search_input2" style="display:none;" name="q" title="검색어" class="keyword" accesskey="s">
