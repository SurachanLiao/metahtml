                <script type="text/javascript">
                    var scr_width = jQuery(window).width();
                    var rwb_top;
                    var rwb_right;

                    if(scr_width >= 1680) {
                        rwb_top = '15px';
                        rwb_right = '-170px';
                    }
                    else if (scr_width >= 1440 && scr_width < 1680) {
                        rwb_top = '15px';
                        rwb_right = '-170px';
                    }
                    else if (scr_width >= 1280 && scr_width < 1440) {
                        rwb_top = '15px';
                        rwb_right = '-170px';
                    }

                    jQuery("#mainRightWing").css({
                        "top" : rwb_top,
                        "right" : rwb_right
                    }).show();
