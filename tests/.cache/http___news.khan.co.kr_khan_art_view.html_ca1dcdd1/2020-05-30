<script type="text/javascript">
if (document.referrer == "http://newsstand.naver.com/")
{
	document.write("<img height='0' width='0' src='//ad.khan.co.kr/imp/khan_imp.php?dp=3302' style='display:none' alt='이미지'>");
}
else
{
	document.write("<img height='0' width='0' src='//ad.khan.co.kr/imp/khan_imp.php?dp=0101' style='display:none' alt='이미지'>");
}

//function reloading(){
//	location.reload();
//
//}
//tid=setTimeout(reloading,180000); //3분후실행

/**
 * 3분마다 페이지 리로드 되는 상황을 로그로 기록하기 위해...
 **/
(function($){
    var ad_imp_code = 'khan_index_total';
    var is_recognize = false; ///< 사용자가 페이지를 인지하였는지?
    var tid = setTimeout(reloading, 60000 * 3); ///< 3분후실행

    function ad_imp()
    {
        $('#adImpDot').attr('src', '//ad.khan.co.kr/imp/ad_imp.php?m=0&c='+ ad_imp_code);
    }

    // 스크롤 이벤트 감지 (사용자의 인지여부 확인 용도)
    $(window).on('scroll', function(){
        if (is_recognize == false)
        {
            ad_imp_code = 'khan_index_action';
            is_recognize = true;
        }
    });

    // 마우스 move 이벤트 감지(사용자의 인지여부 확인 용도)
    $(window).on('mousemove', function(){
        if (is_recognize == false)
        {
            ad_imp_code = 'khan_index_action';
            is_recognize = true;
        }

        // 스크롤 이벤트 제거
        $(this).off('mousemove');
    });

    // 3분후 페이지 새로고침
    function reloading(){
        if (is_recognize == false)
        {
            // 아무 동작(스크롤)을 하지 않을경우 해당 로그 카운트
            ad_imp_code = 'khan_index_no_action';
        }
        ad_imp();
        location.reload();
    }
})(jQuery);
</script>
</div>

<img id="adImpDot" src="//ad.khan.co.kr/imp/ad_imp.php?m=0&c=khan_index_total" alt="dot image" width="0" height="0" style="display:none;" />

