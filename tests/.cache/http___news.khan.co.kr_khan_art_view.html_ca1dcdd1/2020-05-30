<script>
	var main_sports = Math.ceil(Math.random()*4);
	jQuery("#main_many_art > li:eq("+(main_sports-1)+") ").addClass("on");
	jQuery("#top_article_"+main_sports).show();

	jQuery("#main_many_art > li").mouseover(function(){
		jQuery("#main_many_art > li ").removeClass("on");
		jQuery("#many_list > ul").hide();
		jQuery("#top_article_"+parseInt(jQuery(this).index()+1)).show();
		jQuery("#main_many_art > li:eq("+parseInt(jQuery(this).index())+") ").addClass("on");
	});	

</script>				
					
