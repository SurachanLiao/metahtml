    <div class="gotomobile" style="display:none;" >
        <!--
        <p><a href="http://m.khan.co.kr"><img width="430" height="130" alt="모바일 화면으로 보기" src="http://img.khan.co.kr/spko/sub_2013/btn_gotomobile.png"></a></p>
        -->
        <p><a href="http://m.khan.co.kr" onclick="gomobile(this.href); return false;"><img width="430" height="130" alt="모바일 화면으로 보기" src="http://img.khan.co.kr/spko/sub_2013/btn_gotomobile.png"></a></p>
    </div>

    <script type="text/javascript">
	if (navigator.userAgent.match(/iPhone|BlackBerry|Android|Windows CE/i) && !navigator.userAgent.match(/SHW-M380S|SHW-M180S|SHW-M180L|SCH-I800|SGH-T849|SHW-M480W/i)
	) { jQuery(".gotomobile").show(); }

	// 모바일 화면으로 이동할경우 브라우저 고정 쿠키값을 제거한다._@add 2016-07-19 shyun
	function gomobile(murl)
	{
		document.cookie = "browser_mod2e=; path=/; domain=.khan.co.kr";
		location.href = murl;
	}
    </script>
</div>
