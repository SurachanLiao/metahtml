        .map((s) => s.trim())
        .find((s) => s.startsWith('OptanonConsent'));
    var categoryEnabledRegex = /groups=[^&]*%2C7%3A1/;
    contentSelectionAndMeasurementEnabled = categoryEnabledRegex.test(optanonCookie);
  };
  window._comscore = [
    {
      c1: '2',
      c2: '1000004',
      options: { url_append: 'comscorekw=Politics|2020_Elections' },
      cs_ucfr: contentSelectionAndMeasurementEnabled ? 1 : 0
    }
  ];
