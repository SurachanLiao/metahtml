<script>
            document.addEventListener("DOMContentLoaded", function() {
                setTimeout(function(){ $('#newsletterModal').modal('show'); }, 4000);
            });
        </script>
<script>
        function closeNewsletter(){
            $.ajax({
                headers: {
                  'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
               type: "post",
               url: '/newsletter-close',
               async: false,
              });
            document.getElementById('mobileNewsletter').classList.add('d-none')
            document.getElementById('mobileNewsletter').classList.remove('d-block')
        }

        document.addEventListener("DOMContentLoaded", function(){
            document.getElementById('newsDropdownLink').addEventListener("click",goToNews);
        });

        

        function goToNews(){
            location.href = "https://babylonbee.com/news";
        }

        function submitModalNewsletter(){
            $.ajax({
                headers: {
                  'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: "post",
                url: '/newsletter-send',
                data: { 
                    'email': document.getElementById('modal-newsletter-email').value, 
                },
                dataType: "json",
                success: function (msg) {
                    if(msg.success == false){
                        document.getElementById('modalNewsletterError').classList.remove('d-none');
                        document.getElementById('modalNewsletterError').innerHTML = msg.message;
                    } else {
                        document.getElementById('modalNewsletterInput').classList.add('d-none');
                        document.getElementById('modalNewsletterError').classList.add('d-none');
                        document.getElementById('modalNewsletterError').innerHTML = "";
                        document.getElementById('modalNewsletterSuccess').classList.remove('d-none');
                        document.getElementById('modalNewsletterTitle').innerHTML = '';
                    }
                },
                async: false,
            });
          }

        function submitNewsletter(){
            $.ajax({
                headers: {
                  'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: "post",
                url: '/newsletter-send',
                data: { 
                    'email': document.getElementById('newsletter-email').value, 
                },
                dataType: "json",
                success: function (msg) {
                    if(msg.success == false){
                        document.getElementById('mobileNewsletterError').classList.remove('d-none');
                        document.getElementById('mobileNewsletterError').innerHTML = msg.message;
                    } else {
                        document.getElementById('mobileNewsletterInput').classList.add('d-none');
                        document.getElementById('mobileNewsletterError').classList.add('d-none');
                        document.getElementById('mobileNewsletterError').innerHTML = "";
                        document.getElementById('mobileNewsletterSuccess').classList.remove('d-none');
                        document.getElementById('mobileNewsletterTitle').innerHTML = '';
                    }
                },
                async: false,
            });
          }

        function submitNewsletterFooter(){
            
            
            $.ajax({
                headers: {
                  'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                },
                type: "post",
                url: '/newsletter-send',
                data: { 
                    'email': document.getElementById('footer-EMAIL').value, 
                },
                dataType: "json",
                success: function (msg) {
                    if(msg.success == false){
                        document.getElementById('footerNewsletterError').classList.remove('d-none');
                        document.getElementById('footerNewsletterError').innerHTML = msg.message;
                    } else {
                        document.getElementById('footerNewsletter').classList.add('d-none');
                        document.getElementById('footerNewsletterError').classList.add('d-none');
                        document.getElementById('footerNewsletterSuccess').classList.remove('d-none');
                    }
                },
                async: true,
            });
          }
            
    </script>
