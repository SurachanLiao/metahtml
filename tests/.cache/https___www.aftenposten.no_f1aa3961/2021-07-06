<script>
  var foreldreliv = document.querySelector('.widget-foreldreliv');

  var qs = getElements(foreldreliv, 'question')
  var container = getElements(foreldreliv, 'link-container');
  var images = getElements(foreldreliv, 'image-slide');

  var current = 0;

  // rotate through elements
  switchElements();
  var slideInterval = setInterval(switchElements, 6000);

  function switchElements() {
    qs[current].classList.remove("active");
    images[current].classList.remove("active");
    current = (current + 1) % qs.length;

    foreldreliv.setAttribute('href', qs[current].href);
    qs[current].classList.add("active");
    images[current].classList.add("active");
  }

  function getElements(parent, children) {
    return Array.from(parent.getElementsByClassName(children))
  }
</script>

        </div>

        <div class="content-container top">
          <div class="container _center">
            <div class="block">
  <div class="column col-1">
    <div class="article-container">
      <article
        class="article-medium-crop-16-9 article-body-medium-crop-16-9 section-norge    decoration-standard-bottom presentation-news"
        data-pulse-entity-id="bn9Ojl"
      data-pulse-source="frontpage:jm-a"
      data-pulse-position="16"
      data-pulse-access-level="Meter"
      data-pulse-access-level-name="Meter"
      data-pulse-position-in-level="0"
      
