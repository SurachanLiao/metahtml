    <div class="article-container">
      <article
        class="article-medium-crop-16-9 article-body-medium-crop-16-9 section-sprek   paywall-closed decoration-standard-bottom presentation-news"
        data-pulse-entity-id="oAJnQg"
      data-pulse-source="frontpage:b2b"
      data-pulse-position="36"
      data-pulse-access-level="Paid"
      data-pulse-access-level-name="Paid"
      data-pulse-position-in-level="0"
      
      data-pulse-card-size="medium"
      data-pulse-url="https://www.aftenposten.no/sport/sprek/i/oAJnQg/han-presset-kroppen-for-hardt-og-endte-paa-sykehus-dette-er-faresignal"
      data-pulse-teaser-image="https://premium.vgc.no/ap/images/0542e02e-ddc2-4383-ab2c-828c30a7e99b?fit&#x3D;crop&amp;h&#x3D;383&amp;q&#x3D;80&amp;tight&#x3D;true&amp;w&#x3D;680"
      data-pulse-teaser-title="Han presset kroppen for hardt og endte på sykehus. Dette er faresignalene ved overtrening."
