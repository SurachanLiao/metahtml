          '<li class="nav-subscribe"><a href="https://www.sphsubscription.com.sg/eshop/?r=products/newsubscriptionpackages&pcode=st&utm_campaign=st_subscription&utm_medium=web&utm_source=st&utm_content=subscribetext-header&utm_term=multimedia" target="_blank">Subscribe</a></li>';
          $('.menu-user').html(st_login_info);
          //if($("body.page-home").length==1 || (isArticlePage && _data.contentcat==2)) {
            trySingleSignOnLogin();
          //}
          cX.callQueue.push(['setCustomParameters', {'sph-visitorcat': _data.visitorcat, 'isSubscriber': 'n', 'sph-propenmo': propenmo}]);
        }else{
          _data.visitorcat = 4; // sph annoy
          cX.callQueue.push(['setCustomParameters', {'sph-visitorcat': _data.visitorcat, 'isSubscriber': 'n', 'sph-propenmo': propenmo}]);
