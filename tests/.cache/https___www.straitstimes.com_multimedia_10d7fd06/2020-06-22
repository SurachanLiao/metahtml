    function delete_cookie(name) {
      document.cookie = name +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }
    var lb1_starttime = new Date().getTime();
    /** Get Query string value by name */
    function querystring(key) {
       var re=new RegExp('(?:\\?|&)'+key+'=(.*?)(?=&|$)','gi');
       var r=[], m;
       while ((m=re.exec(document.location.search)) != null) r[r.length]=m[1];
       return r;
    }

    /** DFP Preview */
    var key ='dfp_preview';
    var keycode = querystring(key);

    /** Get cookie by name */
    window.getCookieByName = function(name) {
        match = document.cookie.match(new RegExp(name + '=([^;]+)'));
        if (match) return match[1];
    }

    /*** Render loaded DFP tags for prebid use**/
    var dfp_preview_ids = querystring("dfp_preview");
    var loaded_dfp_tags = '{"imu1":{"adunit":"\/5908\/st\/imu1\/multimedia","slotname":"dfp-ad-imu1","size":"1x1, 10x10, 300x250, 780x300"},"imu2":{"adunit":"\/5908\/st\/imu2\/multimedia","slotname":"dfp-ad-imu2","size":"300x600,300x250"},"lb1":{"adunit":"\/5908\/st\/lb1\/multimedia","slotname":"dfp-ad-lb1","size":"970x90,728x90,970x250,320x50,320x100,320x250"},"catfish1":{"adunit":"\/5908\/st\/catfish\/multimedia","slotname":"dfp-ad-catfish1","size":"1x1"},"prestitial":{"adunit":"\/5908\/st\/prestitial\/multimedia","slotname":"dfp-ad-prestitial","size":"1x1"}}';
    var overlay_validity = 4; 
    var catfish_validity = 4;
    var postitial_validity = 4;
