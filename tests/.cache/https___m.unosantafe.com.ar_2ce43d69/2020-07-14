              var scripts = document.getElementsByTagName('script');
              var index   = scripts.length -1;
              var url     = scripts[index].src;
              var matches = url.match(/^http?\:\/\/([^\/?#]+)(?:[\/?#]|$)/i);
              var domain  = matches && matches[1]; 
              if(domain==null){
                  var matches = url.match(/^https?\:\/\/([^\/?#]+)(?:[\/?#]|$)/i);
                var domain  = matches && matches[1]; 
              }
            
              return domain;
            };
