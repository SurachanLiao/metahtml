<script xmlns:deferer="no"> var zocalo = {};

  // definicion estandar GPT
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];

  googletag.cmd.push(function() {
    "use strict";

    // size mapping normal de GPT. Uso sólo el alto para definir tamaños
    var mapping1 = googletag.sizeMapping().
      addSize([728, 0], [728, 90]).
	  //addSize([970, 0], [970, 90]).
	  build();

    // ruta de ejemplo. ¡Cambiar por lo que corresponde!
    googletag.defineSlot('/21912588402/UnoSantaFe_Desk/Home_Zocalo', [], 'prestitiall_adslot').
      setTargeting('kz', ['startup']).defineSizeMapping(mapping1).
      addService(googletag.pubads());
    googletag.pubads().enableSingleRequest();
    googletag.enableServices();
    document.body.style.overflow="visible";

    // oculto el layer en blanco y cierro el aviso
    // esto se llama en 3 ocasiones:
    // - no hubo aviso
    // - apreté el boton cerrar
    // - timeout 8 segundos 
    zocalo.hide = function() {
      document.getElementById("prestitiall_layer").style.display="none";
      document.getElementById("prestitiall_ad").style.display="none";
      document.body.style.overflow="visible";
    }

    // este bloque puede convivir con otros slotRenderedEnded listeners
    // esta parte es la que hace la magia del aviso Startup
    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
      if(event.isEmpty && event.slot.getSlotElementId()=="prestitiall_adslot") {
        // no hubo aviso
        
        zocalo.hide();

      } else if(!event.isEmpty && event.slot.getSlotElementId()=="prestitiall_adslot") {
        // hay un prestitial para mostrar!

        // coloco el ad slot centrado
        var adWidth = event.size[0];
        var adHeight = event.size[1];
        var adSlot1 = document.getElementById(event.slot.getSlotElementId());
        adSlot1.style.marginLeft = "-"+(adWidth/2)+"px";
        adSlot1.style.marginTop = "-"+(adHeight/2)+"px";
        adSlot1.style.left = "50%";
        adSlot1.style.top = "95%";
        adSlot1.style.position = "fixed";

        var icon1 = document.getElementById("prestitiall_ad_icon");
        icon1.style.marginLeft = ((adWidth/2)-20)+"px";
        icon1.style.marginTop =  (-(adHeight/2)-20)+"px";
        icon1.style.left = "50%";
        icon1.style.top = "96%";

        // preparo el contenedor
        var ad12 = document.getElementById("prestitiall_ad");
        //ad1.style.backgroundColor = "none";
        ad12.style.display = "block";

        // hago scroll al inicio de la ventana. esto es opcional
        //window.scrollTo(0,0);

        //window.setTimeout( zocalo.hide, 5000 );
      }
    });
    // Fin slotRenderedEvent listener

  });
</script>

