var trimmedString = current.title.substr(0, maxLength);
//re-trim if we are in the middle of a word
trimmedString = trimmedString.substr(0, Math.min(trimmedString.length, trimmedString.lastIndexOf(" ")))
//current.title = decodeEntities(current.title);
html += '<article class="item"><figure>\
<a href="'+current.link+'" title="'+current.title+'" target="_blank">\
<img alt=""  src="'+current.thumbnail+'"/>\
</a>\
<figcaption>\
<h4 class="title"><a href="'+current.link+'" title="'+current.title+'" target="_blank">'+trimmedString+'</a></h4>\
<!--<h3 class="publicate_date"></h3>-->\
</figcaption>\
</figure>\
</article>'
}
}
container.innerHTML = html;
}
//   setTimeout(function () {
xhr.send();
// }, 0);
})()
</script><div id="block-block-24" class="block block-block"><div class="content"> <ins data-revive-zoneid="126" data-revive-id="0d9cc458234cbe530fbfe5d10ca53050"></ins></div></div><div id="block-block-45" class="block block-block"><div class="content"><div class="home-block-vda home-block-vda-Bottom2" id="home-block-vda-Bottom2"><div class="vda"><div id="rcsad_Bottom2" class="etichetta-vda" style="display: none;"></div></div></div></div></div><div id="block-block-52" class="block block-block"><div class="content"><div class="home-block-vda home-block-vda-Frame2" id="home-block-vda-Frame2"><div class="vda"><div id="rcsad_Frame2" class="etichetta-vda" style="display: none;"></div></div></div></div></div><div id="block-block-51" class="block block-block"><div class="content"><div class="home-block-vda home-block-vda-Frame2_mobile" id="home-block-vda-Frame2_mobile"><div class="vda"><div id="rcsad_Frame2_mobile" class="etichetta-vda" style="display: none;"></div></div></div></div></div><div id="block-block-56" class="block block-block"><div class="content"><div class="home-block-vda home-block-vda-TopLeft_mobile" id="home-block-vda-TopLeft_mobile"><div class="vda"><div id="rcsad_TopLeft_mobile" style="display:none"></div></div></div></div></div><div id="block-block-55" class="block block-block"><div class="content"><div class="home-block-vda home-block-vda-Outbrain_mobile" id="home-block-vda-Outbrain_mobile"><div class="OUTBRAIN" id="outb_mobile" data-src="" data-widget-id="" data-ob-template="IT_la7.it"></div></div></div></div></div></div></div></div></div><div id="footer-new" class="clearfix"><div id="footerMenu" class="clearfix"><div class="region region-footer"><div id="block-menu-menu-menu-footer-informazione" class="block block-menu"><h2>Informazione</h2><div class="content"><ul class="menu clearfix"><li class="first leaf"><a href="http://www.tgla7.it/listing/cronaca" title="" target="_blank">Cronaca</a></li><li class="leaf"><a href="http://www.tgla7.it/listing/eco