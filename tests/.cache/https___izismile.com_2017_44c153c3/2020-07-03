	$(this).css('top', 175 - $(this).height()/2+'px');
});

$('div.leep5_thumbnail').live('mouseenter', function() {
$(this).find('a').css('text-decoration', 'underline !important');
$(this).find('div.leep5_title_div').fadeTo('normal', 0.6);
});
$('div.leep5_thumbnail').live('mouseleave', function() {
$(this).find('a').css('text-decoration', 'none !important');
$(this).find('div.leep5_title_div').fadeTo('normal', 1);
});


