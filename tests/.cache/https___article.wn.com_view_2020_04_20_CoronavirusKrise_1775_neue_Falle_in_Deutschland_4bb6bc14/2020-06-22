<script>

    $(function(){
		
	        		
			$(".slideshow.TTip span").delay(8000).fadeOut(500);

			jQuery(".rslides.photos").responsiveSlides({
				manualControls: '#photos-pager',
				auto: true,
				speed: 1000,           
  				timeout: 6000, 
				pagination: true,
				nav: true,
				navElements : '#rslides_nav',
				fade: 500
      });



			 $('.toggle-player').click(function() {
						$(this).hide();
						$('.exclusive-player').animate({ 'padding' : '0' }, "slow").html('');
			  });



	});

		var wikisnippet_ex =    jQuery('#wiki-snippet');
		var wikifull =       jQuery('#wiki-full');
		var wikiclose =      jQuery('#load_wiki_short');
		var wikiviewmore =   jQuery('#load_wiki_ex');

    function load_wiki_ex(el, query, main) {
          wikiviewmore.html('Loading... please wait...');
          wikifull.load('/api/upge/cheetah-photo-search/wiki_page?main='+main+'&query='+escape(query), function(){
          wikisnippet_ex.hide();
					wikiclose.show();
					wikifull.show();
         });
         return false;
    }

		function load_wiki_short(el) {
			wikiviewmore.html('View more...');
			wikifull.hide();
			wikiclose.hide();
      wikisnippet_ex.show();
    }
		function triggerFullscreen(){
			$('.header .nav').hide();
			$('.header .chat-hide').hide();
		    $('#playercontainer1').css('position', 'fixed');
		    $('#playercontainer1').css('top', '54px');
		    $('#playercontainer1').css('left', '0px');
		    $('#playercontainer1').css('right', '0px');
		    $('#playercontainer1').css('height', $(window).height() - 54);
		    $('#playercontainer1').css('z-index', '100000');
		    $('#playercontainer1').css('background-color', '#000000');
		    $('#home').show();
		    $('#videos').show();
		    $('body').css('overflow', 'hidden');
		    return false;
		}

		function doneFullscreen(){
			$('.header .nav').show();
			$('.header .chat-hide').show();
		    $('#playercontainer1').css('position', 'relative');
		    $('#playercontainer1').css('top', '0px');
		    $('#playercontainer1').css('height', '360px');
		    $('#playercontainer').css('z-index', '102');
		    $('#home').hide();
		    $('#videos').hide();
		    $('body').css('overflow', 'auto');
		    return false;
		}
		function load_image(img_url, credit) {
		    var img = document.createElement("IMG");
		    img.src = img_url;
		    $('.img-content').find('.img-body').html(img);
			$('.img-content').find('.credit').html(credit);
		}

		function triggerPlayer(element, video_id){
				 	loading_obj = jQuery('.wn-exclusive-related').find('.video-thumb')
					 if (loading_obj.hasClass('player-open')) {
						$('.exclusive-player').next().hide();
						$('.exclusive-player').animate({ 'padding' : '0' }, "slow").hide();
			  			loading_obj.removeClass('player-open');
		  			}
					$(element).parent().addClass('player-open');
					$('.exclusive-player').next().show();
					$('.exclusive-player').show().animate({ 'paddingBottom' : '56.25%' }, "slow").html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1" frameborder="0" allowfullscreen></iframe>');


		}
		$(document).on('keyup',function(evt) {
		    if (evt.keyCode == 27) {
		      doneFullscreen();
		    }
		});
		
</script>




    <script nopack="1" type="text/javascript" src="https://www.youtube.com/iframe_api"></script>
    
    <script nopack="1">    
    $(function() {
		
			$('.play-main-video').click(function(e) {		
						$('.main-image').hide();			
						$('.playHover').hide();	
						var symbol = $("#featured-video")[0].src.indexOf("?") > -1 ? "&" : "?";							
						$("#featured-video").addClass('is-playing');
						$("#featured-video")[0].src += symbol + "autoplay=1";
							
			});			
    
    	var featuredMedia = []
    	var featuredVideo = []
    	var featuredMedia = $("#featured-media"); 
    	var featuredVideo = $("#featured-video"); 
    
    
    	var top = featuredMedia.offset().top; 	
    	var offset = Math.floor( top + ( featuredMedia.outerHeight() / 2 ) );	
    	
    	$(window).on( "resize", function() {
    		   top = featuredMedia.offset().top;
    		   offset = Math.floor( top + ( featuredMedia.outerHeight() / 2 ) );
    	})		 
    	.on( "scroll", function() {			
    			 featuredVideo.toggleClass("is-sticky", $(window).scrollTop() > offset && featuredVideo.hasClass("is-playing"));
    	});
    	
    
    });
    		var onPlayerReady;
    		var player;
    		function onYouTubeIframeAPIReady() {
    		  player = new YT.Player('featured-video', {
    		    events: {
    		      'onStateChange': onPlayerStateChange
    		    }
    		  });
    		} 
    	  
          function onPlayerStateChange(event) {
            if (event.data == YT.PlayerState.PLAYING) {
              setTimeout(addClass, 1000);         
            }        
          }      
          function addClass() {      
            $("#featured-video").removeClass( "is-paused" );
    				$("#featured-video").toggleClass( "is-playing" );
          }
    
    </script>

		
<script nopack="1">
window.audioread = function(pos){
    //$('.main-article').find('style').detach().insertAfter('head');
    //$('.wn-article').find('script').detach().insertAfter('head');
    var paras = [];
    $('.main-article,.wn-article').find('p').each(function(){
        var t = $(this).text();
        if(t.replace(/\s*/,'') && !$(this).closest('#twitter-content').length){
            paras.push(t.replace(/<[^>]*>/g,' ').replace(/\s+/g,' '));
        }
    });
    var sentences = paras.join('\n').replace(/(.{200,}?[.?!])/g, '$1\n').split(/\s*(\n\s*)+/g);
    for(var s=0; s<sentences.length; s++){
        if(s == pos){
            var txt = sentences[s];
            $('.audioblock').html('<audio autoplay="1" onended="audioread('+(s+1)+');" controls="1" src="https://api.wn.com/tts/?t='+escape(txt)+'" />'+txt);
            $('.audioread a').hide();
        }
    }
    if(pos >= sentences.length){
        $('.audioread a').show();
        $('.audioblock').html('');
    }
}
</script>




<script nopack="1">
function update_user_actions(a) {
    a.username && (waitForSelector(".top-navigation .username", function() {
        $(".top-navigation .username").attr('data-trans', '').html(a.username);
    }), waitForSelector(".top-navigation .user-options .dropdown-menu", function() {
        $(".top-navigation .user-options .dropdown-menu").html(a.html)
    }), window.username = a.username), waitForSelector(".top-navigation", function() {
        a.username ? ($(".not_editable").hide(), $(".is_editable").show()) : ($(".not_editable").show(), $(".is_editable").hide())
    })
}
</script>







		
		<!-- The YouTube Right Col video -->	
		<script>			
				var fBox = $('.avp-content-media');	
				var fBoxHeight = $('.avp-content-media').outerHeight();
				var position = fBox.position(); 
        var fixmeTop = fBox.offset().top;        
				var footerHeight =  $('#footer').outerHeight();
								
        $(window).scroll(function() {    
				            
        	var currentScroll = $(window).scrollTop();     
	       	var fixmeBottom = 	$(document).height() - currentScroll - '310';
        
				
					 if ((currentScroll >= fixmeTop) && (fixmeBottom > fBoxHeight)) { 							
                $('.avp-content-media').css({                      
                    position: 'absolute',
                    top: currentScroll-250 + 'px',	
                    left: '0px',
                    right: '15px'
                });
            } else if (fixmeBottom < fBoxHeight) {
								                         
                $('.avp-content-media').css({                    
                    		position: 'absolute',                    											
                    		bottom: footerHeight + 'px',
                    		left: '0px',
                   			right: '15px'
                });
            } else {                                 
                $('.avp-content-media').css({                    
                    	position: 'static',
										  top: '0',
	                    right: '0px'
                });
            }
        
    });
		</script>
		


		<script>	
				function popUp(url) {					
					window.open(url, 'ShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
					 return false;
				}
		
				function share_popup(element, video_title){		
					title = video_title.replace(/[:\.\/_ ]+/g, '_');
					video_url = 'https://wn.com/' + title.replace(' ', '_');
			   		$(element).parent().find('.buttons').show().html('<div class="buttons-close"  onclick="return buttons_close(this)"><i class="fa fa-times"></i></div><a href="javascript:void(0);" id="https://twitter.com/home?status=' +video_url+ '" class="twitter" onclick="return popUp(this.id)"><i class="fa fa-twitter"></i></a><a href="javascript:void(0);" id="https://www.facebook.com/sharer.php?u=' +video_url+ '" class="facebook" onclick="return popUp(this.id)"><i class="fa fa-facebook"></i></a><a id="https://plus.google.com/share?url=' +video_url+ '" href="javascript:void(0);" class="google" onclick="return popUp(this.id)"><i class="fa fa-google-plus"></i></a><a href="javascript:void(0);" id="https://wn.com/api/upge/cheetah-photo-search/send_email?title=' +video_url+ '&url=' +video_url+ '" class="email" onclick="return popUp(this.id)"><i class="fa fa-envelope-o"></i></a></div>');
					$(element).addClass('active');
				}
				function buttons_close(element){		   		
					$(element).parent().hide();
				}
		</script>





<script>
   $(function(){
				$('#draggable').draggable();

			
					
				
				var fullscreenOn = false				
				var playerOn = false						
				
				if ($(window).width() < 768) {
	        
						   
	       $('.play-float-video').click(function(e) {		
		   					var video_id = $(this).attr('id');				
							e.preventDefault();							
							$('.player-container').css('width', '100%').show();
	        				$('.player').animate({ 'paddingBottom' : '56.25%' });						
							$('.player-container').addClass('fs').removeClass('small');	
							$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div>');							
						 });
	         	}
	       else{
				$('.play-float-video').click(function(e) {		
	              		var video_id = $(this).attr('id');				
	           e.preventDefault();						
						 if (playerOn == false) {
							triggerSmallPlayer(video_id);			
						 } else {		
							reloadSmallPlayer(video_id);	
						 }
					});
	        	}	        	
	        			
	       $('.small_player').live("click", function(e){  			
					 	e.preventDefault();		
						smallerPlayer();
						fullscreenOn = false;
						playerOn = true;	
				});				
				$('.enlarge_player').live("click", function(e){  
			 		e.preventDefault();		
					enlargePlayer();
					fullscreenOn = true;
				});			
				$('.remove_player').live("click", function(e){    
					e.preventDefault();	
					$('.player-container').hide('slow');
					$('.player').html('');
					fullscreenOn = false;			
					playerOn = false;
				});			 

			  
			  
	   			$('.popup').click(function(e) {
					        e.preventDefault();
					        window.open($(this).attr('href'), 'fbShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
					        return false;
				});
					
	            $(".search-type a").click(function () {
	                    var search_type = $(this).data('type');
	                    $("#search-selector").val(search_type);
	                        if(!$(this).hasClass("active"))
	                        {
	                            $(".search-type a").removeClass("active");
	                            $(this).addClass("active");
	                        }
	            });
	            $('#mainsearch').submit(function(event) {
	                    var search_type = $("#search-selector").val();
	                    if(search_type == 'videos') {
	                        var search_string = $(".top_query").val();
	                        search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
	                        window.open('https://wn.com/' + search_string, '_blank');
	                        return false; // do not submit the form
	                    }else{
	                        return true;
	                    }
	             });

                $(".gotop").live("click", function(){                	
                    $('html, body').animate({
        				scrollTop: $('#playercontainer1').offset().top
    				}, 1000);      
                });
                $(".logo-bm li").live("click", function(){
                    $(".logo-bm li").removeClass("active");
                    $(this).addClass("active")
                });
                $('.bn-close').click(function () {
                    $(this).parent().hide("slow");
                });
                $( ".nav li" ).click(function() {
                     if ( $( this ).hasClass( "local" ) ) {
                        $('#page-title').hide();
                    }else{
                        $('#page-title').show();
                    }
                });
                
                

            // focus on the email field when a user clicks the subscribe btn
            jQuery('#dLabel').on('click', function(e) {
                jQuery('#subscribe-email').focus();
            });
            jQuery('.emailTool').tooltip();
			jQuery('.main-article a').attr('target', '_blank');

        });
