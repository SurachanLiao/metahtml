<script async="">
var esi_loader = (function () {
  return function (urls, resolve, reject, id) {
    if (typeof Promise === 'undefined') return;
    var promises = urls.map(url => {
      return new Promise(function (ok, error) {
        var element = document.createElement('script');
        var parent = 'head';
        var attr = 'src';
        element.onload = ok;
        element.onerror = error;
        if (id && id !== 'undefined') {
          element.id = id;
        }
        element.async = 'async';
        element[attr] = url;
        document[parent].appendChild(element);
      });
    });
    return Promise.all(promises).then(resolve, reject);
  };
})();
</script>
<script async="">
function esiCmpConsent(vendorID, cb, rejectCb) {
  window.__cmp('getNonIABVendorConsents', [vendorID], function (consent, success) {
    var list = consent.nonIABVendorConsents;
    success || cb && cb();
    success && list && list[vendorID] && cb && cb();
    success && list && !list[vendorID] && rejectCb && rejectCb();
  });
}
function esiCmpLoader(vendorID, cb, rejectCb) {
  window.__cmp('setConsentUiCallback', function() {
    esiCmpConsent(vendorID, cb, rejectCb);
  });
}
</script>
