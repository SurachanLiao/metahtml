     sections = sections.substr(1);
   } else if (path.indexOf('go/london/search') >= 0) {
     sections = 'GO Search';
     title = 'GO Search';
   }

   window._sf_async_config = {};
   const _sf_async_config = window._sf_async_config;
   _sf_async_config.uid = chartbeatUid;
   _sf_async_config.domain = domain;
   _sf_async_config.useCanonical = true;
   _sf_async_config.flickerControl = false;
   _sf_async_config.useCanonicalDomain = true;
   _sf_async_config.title = title;
   _sf_async_config.sections = sections;
   _sf_async_config.authors = authors;
   _sf_async_config.playerdomain = brightcovePlayer;

   var _cbq = window._cbq = (window._cbq || []);

   if (domain === 'independent.co.uk') {
     if (loggedIn && subscriber) {
      _cbq.push(['_acct', 'paid']);
     } else if(loggedIn) {
      _cbq.push(['_acct', 'lgdin']);
     } else {
      _cbq.push(['_acct', 'anon']);
     }
     if (globals.contentType === 'article') {
      _sf_async_config.sections = 'article-type' + (globals.isPremiumArticle ? 'premium' : 'non-premium') + ',' + _sf_async_config.sections;
     }
   } else {
     if (loggedIn) {
      _cbq.push(['_acct', 'lgdin']);
     }
   }
 }
}();
</script>
