lotto_submit = function()
{
  $('#form_lotto').attr('action','/lotto/#'+$('#lotto-number').val());
}

createArticle = function(data,loadBtn,formatType){

  var article_box = "";
  var vdo_class ="";
  var article_type = "";

  if(loadBtn === 'load_more'){
    article_type = "loaded-content";
  }

  if(formatType === 'mediaThumbnail'){
    $.each(data, function(){
      if(this.clip){
        vdo_class = "-vdo";
      }else{
        vdo_class = "";
      }
      if(this.colTitle){
        colTitleObj = "<span class='media-column-heading2'>"+ this.colTitle +"</span>";
      }else{
        colTitleObj = "";
      }
      article_box +=  "<article class='"+ article_type +" content " + vdo_class + "'>" +
                      "<a class='media' title='"+ this.title +"' href='"+ this.detail +"'>" + //todo: add news id to uri
                      "<div class='media-left'>" +
                      "<img class='media-object' title='" + this.title + "' alt='" + this.title + "' src='"+ this.imgPath +"' >" +
                      "</div>" +
                      "<div class='media-body'>" +
                      colTitleObj +
                      "<h3 class='media-heading'>" + this.title + "</h3>" +
                      "</div></a></article>";
    });
  }else if(formatType === 'textOnly'){
    $.each(data, function(){
      if(this.clip){
        vdo_class = "-vdo";
      }else{
        vdo_class = "";
      }
      article_box +=  "<article class='"+ article_type +" content'>" +
                      "<a class='media' title='"+ this.title +"' href='"+ this.detail +"'>" + //todo: add news id to uri
                      "<div class='media-left'>" +
                      "<i class='fa fa-caret-right'></i>" +
                      "</div>" +
                      "<div class='media-body'>" +
                      "<h3 class='media-heading'>" + this.title + "</h3>" +
                      "</div></a></article>";
    });
  }//end if

  return article_box;

}//createArticle

$(function(){
	
     

  $('#highlight-slide').children('.owl-carousel').owlCarousel({
    loop:true,
    items: 1,
    nav:true,
    autoplay: true,
    navText: ['<i class="fa fa-chevron-left"></i>','<i class="fa fa-chevron-right"></i>']
  });

  $('#hot-news').children('.content-box').children('.owl-carousel').owlCarousel({
    loop:true,
    margin:16,
    items: 4,
    nav: true,
    dots: false,
    autoplay: true,
    lazyLoad:true,
    navText: ['<i class="fa fa-chevron-left"></i>','<i class="fa fa-chevron-right"></i>'],
    responsive : {
      // breakpoint from 0 up
      0 : {
          items: 2
      },
      // breakpoint from 640 up
      640 : {
          items: 4
      }
    }
  });

  //switch tab and load content
  var $tabBox = $app.mainContent.find('#middle-section,#bottom-section').find('.content-box.-tab');
  $tabBox.children('.heading').on('click', '.text', function(){

    var $thisBtn = $(this);
    var $thisLink = $thisBtn.children('.link');
    var isLoaded = $thisLink.is('.loaded');
    var targetTab = $thisLink.attr('data-tab');
    var dataFeed =  $thisLink.attr('data-feed');

    $thisBtn.addClass('active').siblings().removeClass('active')
           .closest('.content-box').find('.'+ targetTab).addClass('active')
           .siblings('.tab-content').removeClass('active');

    if(targetTab === '-tab2' && !isLoaded ){

      url = $thisBtn.find('a').attr('data-feed');

      $.ajax({
        type:'GET',
        url: url,
        dataType: 'json',
        // beforeSend: function(){
          // $thisBtn.addClass('hide').siblings('.loading').show();
        // },
        success: function(result){

          $articles = $(createArticle(result.data,'load_tab','mediaThumbnail'));

          $thisLink.addClass('loaded');
          $thisBtn.closest('.content-box').find('.'+ targetTab).addClass('-loaded')
            .children('.content-wrapper').append($articles);

        }//success
      });

    }//end if

    return false;
  });

});
</script>


