    background-image: url("https://bt.bmcdn.dk/users/berlingske/files/logos/lpwl9-chevron_breaking.png?accessToken=0566ec976341e7f01a197b03c537e0ddcbb51c0f5d752c6504559ca7931b0bc1"); }
  .theme-bt .dre-item--sm-half .dre-item__chaser--article .dre-item__chaser-text {
    padding-left: calc(9vw / 3.75);
    padding-right: calc(9vw / 3.75); }
  .theme-bt .dre-item--sm-full .dre-item__chaser {
    padding: calc(10vw / 3.75) calc(20vw / 3.75);
    font-size: calc(14vw / 3.75); }
    .theme-bt .dre-item--sm-full .dre-item__chaser.dre-item__chaser--article {
      padding: 0; }
      .theme-bt .dre-item--sm-full .dre-item__chaser.dre-item__chaser--article .dre-item__chaser-text {
        padding-right: calc(30vw / 3.75); }
  .theme-bt .dre-item--sm-full .dre-item__chaser--tip .dre-item__chaser-text {
    display: block; }
