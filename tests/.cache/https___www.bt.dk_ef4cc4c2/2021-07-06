    clear: left; }
  .theme-bt .dre-item--sm-full-image-left .dre-item__chaser,
  .theme-bt .dre-item--sm-full-image-left .dre-item__chaser--article {
    margin-left: 0;
    padding-left: 2.66667vw; } }

