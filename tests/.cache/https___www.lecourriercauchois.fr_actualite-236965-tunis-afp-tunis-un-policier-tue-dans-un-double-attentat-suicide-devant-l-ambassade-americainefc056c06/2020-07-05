	<script type="text/javascript">
	var onSubmit = function(token) {
		console.log('callback recaptcha !');
		var formData = $("#comment_form").serialize();
		$.ajax({
			type: 'POST',
			url: 'ajax/post_comment.php',
			data: formData
		}).done(function(response) {
			$("#return_info").removeClass('error');
			$("#return_info").addClass('success');
			$("#nom_commentaire").val('');
			$("#email_commentaire").val('');
			$("#msg_commentaire").val('');
			$("#return_info").show();
			$("#return_info").text(response);
		}).fail(function(data) {
			$("#return_info").removeClass('success');
			$("#return_info").addClass('error');
			if (data.responseText !== '') {
				$("#return_info").text(data.responseText);
			} else {
				$("#return_info").text('Erreur lors de l\'envoi, veuillez réessayer.');
			}
			$("#return_info").show();
		});
		grecaptcha.reset();
	};
	
	function validate_commentaire(event) {
		event.preventDefault();
		if (!document.getElementById('nom_commentaire').value || !document.getElementById('email_commentaire').value || !document.getElementById('msg_commentaire').value) {
			alert("Veuillez remplir tous les champs !");
		} else {
			$("#return_info").hide();
			grecaptcha.execute();
		}
	}
</script>
