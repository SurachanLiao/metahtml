  <script>
    fcmsJs.load('fcmsUser_Customer').onLoad(function()
    {
      var controller = new fcmsUser_Customer_LoginLogout_Controller('9807', '9a7f3f271e138bb2201d1320f4773d01');
      
      controller.onLoad(function(userData, ssoData, trackId, accessBy)
      {
        fcmsUser_Customer_Sso.tryExtLogin(userData, ssoData);
                
        if(userData !== null)
        {
          localStorage.setItem("userName", userData.firstname + ' ' + userData.lastname);
        }
        
        if(userData === null || !/\bonline\b/.test(userData.permissions))
        {
          var permissionOnline = document.querySelectorAll('.permission-online');
          
          if(permissionOnline !== null)
          {
            for(var i = 0; i < permissionOnline.length; i++)
            {
              permissionOnline[i].classList.remove('d-none');
            }
          }
        }
        
        var notice = document.querySelector('#email-message');
        
        if(userData !== null && userData.confirmstatus === 0 && notice !== null)
        {
          notice.style.display = "block";
        }
        
        (function(userData, accessBy){
          var payload = {},
              userData = userData || {
                'permissions': '',
                'originalId': '',
                'customerGroups': '',
              };

          try{
            payload.cd = userData.originalId.substring(8);
            payload.cs2 = (userData.originalId.length > 0 ? '' : 'not ') + 'logged in';
            payload.uc3 = userData.permissions.match(/\bonline\b/) ? 'SK Digital Basis' : 'kein Abo';
            payload.uc4 = userData.permissions.match(/\b(onlineepaper|epapertest)\b/) ? 'SK Digital' : 'kein Abo';
            payload.uc23 = (userData.customerGroups.match(/^[A-Za-z]+/)||['BLAU'])[0];
            
            if(accessBy !== null && typeof accessBy === 'object' && accessBy.name == 'ipforfree'){
              payload.cs10 = accessBy.details.ipIdentifier;
              document.body.className += ' permission-online-ip';
            }

            _ti_update(payload);

            if(typeof window.tp_update === 'function'){
              window.tp_update();
            }

            fcmsJs.scripts.load( "extlib_jquery" ).onLoad(function(){
              jQuery(document).ready(function(){
                if(typeof(_ti_push_zones) === "function"){
                  _ti_push_zones();
                }
              });
            });
          }
          catch(e){
            console.log(e); 
          }

        })(userData, accessBy);
      });

      if(typeof fcmsJs.ajaxLoginControllers === 'undefined')
      {
        fcmsJs.ajaxLoginControllers = [];
      }

      fcmsJs.ajaxLoginControllers.push(controller);
    });
  </script>




























