  <script> 
  window._ti_update = function(data){
    for(var key in data){
        if(data.hasOwnProperty(key)){
          window._ti[key] = data[key];
        }
      }
    console.log("wt is not correctly initialized!");
  }
  
  window._ti = {
    "ci": "/regionalsport/",
    "cg1": "skwww",
    "cg2": "Regionalsport",
    "cg3": "",
    "cg4": "",
    "cg5": "",
    "cg6": "Übersichtsseite",
    "cp9": document.title || "",
    "cp10": "skwww",
    "cs2": "not logged in",
    "uc3": "kein Abo",
	"uc4": "kein Abo",
    "uc21": "not supported"
  };
  
  if(typeof Notification != 'undefined' && typeof Notification.permission == 'string'){
  	_ti.uc21 = Notification.permission;
  }
  

if(window.location.host != "www.suedkurier.de" && window.location.host != "alpha.suedkurier.de"){
	window._ti = { };
}
</script>

<script>
  tp = window.tp || [];

  if(window.location.host == "alpha.suedkurier.de"){
    tp.push(["setSandbox", true]);
    tp.push(["setDebug", true]);
  }else{
    tp.push(["setSandbox", false]);
    tp.push(["setDebug", false]);
  }
  
  tp.push(["setContentSection", "Konstanz"]);
  tp.push(["setMaxCookieExpirationInDays", 365]);
  if(typeof _ti.cg6 == "string"){
  	tp.push(["setCustomVariable", "contentType", _ti.cg6.toLowerCase()]);
  }

  
  tp.push(["addHandler", "showTemplate", 
           function(template){
             if(_ti.cp5 == 'paywall'){
               _ti_update({
                 'ct': 'Content',
                 'ck13': ['Paywall~view', template.experienceId, template.templateId].join('~'),
               });
             }
           }
          ]); 
          
  tp.push(["addHandler", "checkoutCustomEvent", 
           function(event){
             var params = JSON.parse(event.params.params || '{}');
             
             if(event.eventName == "paywallConversion"){
               _ti_update({
                 'ct': 'Content',
                 'ck13': ['Paywall~click', params.experienceId, params.templateId].join('~'),
                 'ck14': event.params.product + "~" + event.params.caption
               });
             }else{
               _ti_update({
                 'ct': 'Content',
                 'ck13': ['Piano~' + event.eventName + '~click', params.experienceId, params.templateId].join('~'),
                 'ck14': event.params.product + "~" + event.params.caption
               });
             }
             
           }
          ]);
  

  var tp_update = function(){
    tp.push(["setCustomVariable", "userLoginState", _ti.cs2.indexOf('not') >= 0 ? "false" : "true"]);
    tp.push(["setCustomVariable", "userCustomerGroupSKDigitalBasis", _ti.uc3[0] == 'k'? "false" : "true"]);
    tp.push(["setCustomVariable", "userCustomerGroupSKDigital", _ti.uc4[0] == 'k'? "false" : "true"]);
    
    tp.push(["init", function() {
      tp.experience.init();
    }]);
  }
</script>

