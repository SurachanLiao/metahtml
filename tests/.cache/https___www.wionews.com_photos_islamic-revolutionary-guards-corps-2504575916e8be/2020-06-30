<script>
var previousScroll = 0;
$(window).scroll(function () {
  var currentScroll = $(this).scrollTop();
    if (currentScroll > previousScroll){
	    _up = false;
    } else {
	   _up  = true;
    }
    previousScroll = currentScroll;
    var prevLoc = window.location.pathname;
    var divid = 'div[id^="album-photo"]';
    var cutoff = $(window).scrollTop() + 64;
   if($(divid).length>0){
    $(divid).each(function () {
        if ($(this).offset().top + $(this).height() > cutoff) {
            if (prevLoc != $(this).attr('data-url')) {
                prevLoc = $(this).attr('data-url');
                $('html head').find('title').text($(this).attr('data-title'))
                window.history.pushState("", "", prevLoc);
		if(_up == false){
	          var urlstr = prevLoc;
                  var urlarray = urlstr.split('https://www.wionews.com/');
                  gtag('config', 'UA-78400808-1', {'page_path': urlarray[1]});
	          console.log('***' + urlarray[1]);
	          console.log(prevLoc);
		 }
            }
            return false;
        }
    });
}
});


function fillElementWithAd($el, slotCode, size, targeting) {
    if (typeof targeting === 'undefined') {
        targeting = {};
    } else if (Object.prototype.toString.call(targeting) !== '[object Object]') {
        targeting = {};
    }
    var elId = $el.attr('id');
    googletag.cmd.push(function () {
        var slot = googletag.defineSlot(slotCode, size, elId);
        for (var t in targeting) {
            slot.setTargeting(t, targeting[t]);
        }
        slot.addService(googletag.pubads());
        googletag.display(elId);
        /*googletag.pubads().refresh([slot]);*/
    });
}

var ci = 1;
var pl = $(".jq-lst-Photos").children('div.col-sm-12').length;
var adcode = '/11440465/Wionews_Web/Wionews_PTG_BTF_1_728x90';
var taboola = 0;
if (pl > 3) {
  $(".jq-lst-Photos").children('div.col-sm-12').each(function (i, n) {
    t = this;
    if ($(this).html().length > 10) {
      if ((ci) % 3 == 0 && (i + 1) > 1 && $(this).html().length > 10) {
        if(taboola == 0) {
          d = $("<div id='ad" + ci + "-250457' class='col-sm-12 block adATF' style='height:auto; margin:auto; padding-bottom: 20px;text-align: center;'></div>");
          d.insertAfter(t); fillElementWithAd(d, adcode, [728, 90], {});
          adcode = '/11440465/Wionews_Web/Wionews_PTG_BTF_2_728x90';
          taboola = 1;
        }else {
          taboola = 0;
        }
      }
      ci = ci + 1;
    }
  });
}
</script>

