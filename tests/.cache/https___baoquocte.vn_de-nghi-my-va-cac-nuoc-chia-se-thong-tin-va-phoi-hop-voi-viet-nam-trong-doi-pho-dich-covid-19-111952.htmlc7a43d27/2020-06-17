<script type="8e7d5d76dfc7d27a81f4c1ad-text/javascript">
$(window).load(function() {		
	$(".gotop").hide();
		$(function () {
			$(window).scroll(function () {
				if ($(this).scrollTop() > ($("#bodyMainfull").offset().top)) {
					$('.gotop').fadeIn();
				} else {
					$('.gotop').fadeOut();
			}
		});
		
	$('body span.gotop').click(function () {
		$('body,html').animate({
			scrollTop: $("#bodyMainfull").offset().top
		}, 900);
		return false;
		});
	});
});
	
function mobile_Action(){			
	var getUrlForward	=	$('link[rel="canonical"]').attr('href');
	document.location	= 	"https://baoquocte.vn/apiservice@/device_switcher&mobile=yes&back_state_url="+getUrlForward;	
}	
$(document).ready(function() {
	$('.__ads_click').mousedown(function(){
		var checkHref		=	$(this).attr('href');
		if(checkHref!=undefined){
			window.open(checkHref);
		}
	});

	//Default Action
	$('.nav-main ul.nav-main-menu li.nav-main-menu-list:first-child a.nav-main-menu-link').html('<img src="/modules/frontend/themes/thegioivietnam/images/icon-home-page.png" alt="Thế giới & Việt Nam" />').attr('href','https://baoquocte.vn/');
	$('.nav-main-menu-append li:first-child a').attr('href','https://baoquocte.vn/');
	$('.fterBtomMain .boxShareColfull ul li:first-child a').html('<img style="position:relative; top:-2px;" src="/modules/frontend/themes/thegioivietnam/images/iconHomefter.png" alt="Thế giới & Việt Nam" />').attr('href','https://baoquocte.vn/');
	//<li><a href="#"><img style="position:relative; top:-2px;" src="/modules/frontend/themes/thegioivietnam/images/iconHomefter.png" alt=""></a></li>
	
	$("ul.tabs li:first").addClass("selected").show(); //Activate first tab
	$(".tab_content:first").show(); //Show first tab content
	
	$(".content,.content_1,#content_2").mCustomScrollbar({
		autoHideScrollbar:true,
		theme:"light-thin"
	});
	
	$(".tab_content").eq(1).hide(); //Hide all content
	
	//On Click Event
	$("ul.tabs li").click(function() {
		$("ul.tabs li").removeClass("selected"); //Remove any "active" class
		$(this).addClass("selected"); //Add "active" class to selected tab
		$(".tab_content").hide(); //Hide all tab content
		var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content		
		$(activeTab).fadeIn(); //Fade in the active content
		return false;
	});
	
	$('#newsHot').bxSlider({
	  mode: 'fade',
	  auto: true
	});
		
	$('#geller').bxSlider({
	  mode: 'fade',
	});
	$(window).bind('scroll', function() {
                 if ($(window).scrollTop() > 135) {
                     $('#mnu').addClass('f-nav');
                 }
                 else {
                     $('#mnu').removeClass('f-nav');
  	               }
    		        });	
	var myString = "" ;
	if (myString!=''){
		myArray = myString.split(" ");
		for(i=0;i<myArray.length;i++)
		{
			$('#listingGrNews li').highlight(myArray[i]);
		} 					   
	}
	$('.__mb_article_in_image a,.__MB_ARTICLE_A a').each(function(){
		$(this).removeAttr('target');
	});
});
</script>
