<style type='text/css'>img#wpstats{display:none}</style><script type="text/javascript">/* global mtmads */
var googletag  = googletag || {},
	isLoggedIn = !!document.cookie.match(/MSPAN_Paywall_user/),
	doInitialRefresh = true,
	AdSection,
	viewportwidth,
	viewportheight;

googletag.cmd      = googletag.cmd || [];
mtmads.iFrameData  = mtmads.iFrameData || {};
mtmads.sizeMapping = mtmads.sizeMapping || {};

  


function pathCategories() {
    var value = window.location.pathname;
	var re = new RegExp('^\/[a-z]');
	if(re.test(value)) {
        var words = value.split('/');
        words = words.filter(Boolean);
        var catArray = words.join();
        return catArray
	} else {
        return 'none';
	}
}
// gpt.js moved to functions.php

/*
 * 1. The more standards compliant browsers (mozilla/netscape/opera/IE7) use window.innerWidth and window.innerHeight.
 * 2. IE6 in standards compliant mode (i.e. with a valid doctype as the first line in the document).
 * 3. Older versions of IE.
 */
if ( typeof window.innerWidth !== 'undefined' ) {
	viewportwidth  = window.innerWidth;
	viewportheight = window.innerHeight;
} else if ( typeof document.documentElement !== 'undefined' && typeof document.documentElement.clientWidth !== 'undefined' && document.documentElement.clientWidth !== 0 ) {
	viewportwidth  = document.documentElement.clientWidth;
	viewportheight = document.documentElement.clientHeight;
} else {
	viewportwidth  = document.getElementsByTagName( 'body' )[0].clientWidth;
	viewportheight = document.getElementsByTagName( 'body' )[0].clientHeight;
}

// Enable Single Request Architecture.
googletag.cmd.push( function() {

	if ( 'undefined' !== typeof mtmads.disableAds ) {
		return;
	}

	// If refresh() is called elsewhere on the page, prevent refresh() on page load.
	googletag.pubads().addEventListener( 'slotRenderEnded', function( event ) {
		doInitialRefresh = false;
	});

	googletag.pubads().enableLazyLoad({
		fetchMarginPercent: 0,  // Fetch slots within 0 viewports.
		renderMarginPercent: 0,  // Render slots within 0 viewports.
		mobileScaling: 0.5  //  values on mobile.
	});

	mtmads.sizeMapping.pos1 = googletag.sizeMapping().

		addSize([1000, 200], [[970,90],[728, 90],[970, 250]]). // SLB only for desktop and tablet wide
		addSize([750, 200], [728, 90]). // 728 only for Tablet Portrait and above
		addSize([0, 0], [[320, 50], [300, 50], [300,100]]). //all sizes
		build();

	mtmads.sizeMapping.pos2 = googletag.sizeMapping().
		addSize([800, 200], [[300,600],[300,250]]). // Half Page in desktop and tablet wide
		addSize([0, 0], [300, 250]). // MR all sizes
		build();

	mtmads.sizeMapping.pencil = googletag.sizeMapping().
		addSize([0, 0], []). // supress below desktop
		build();

	mtmads.sizeMapping.pos3 = googletag.sizeMapping().
		addSize([800, 200], [[300,600],[300,250]]). // add half page in tablet wide and desktop
		addSize([0, 0], []). // MR all sizes
		build();

	mtmads.sizeMapping.btfmr = googletag.sizeMapping().
		addSize([800, 200], [300,250]). // Desktop
		addSize([0, 0], []). //supress positions 8 and 9 below desktop.
		build();

	mtmads.sizeMapping.footer = googletag.sizeMapping().
		addSize([970, 500], [[1, 1],[970, 90]]). // 1x1 above mobile widths
		addSize([0, 0], [[300, 50],[320, 50],[1, 1]]). // some sizes only in mobile
		build();

	mtmads.sizeMapping.marqears = googletag.sizeMapping().
		addSize([800, 200], [185, 90]). // drop below 800px wide
		addSize([0, 0], []). // no ears below desktop
		build();

	if ( 'undefined' !== typeof mtmads.isVideo ) {
		mtmads.pos1videocompanion728x90 = googletag.defineSlot( '/' + mtmads.dfpId + '/video/article', [728, 90], 'pos1videocompanion728x90' ).addService(googletag.companionAds()).addService(googletag.pubads());
		googletag.companionAds().setRefreshUnfilledSlots( true );
		googletag.pubads().enableVideoAds();
	} else {
		if ( 'undefined' !== typeof mtmads.postTags ) {
			googletag.pubads().setTargeting( 'tag', mtmads.postTags );
		}
		if (mtmads.pageType == 'Section Front') {
            var SFcategoryArray = pathCategories();
            googletag.pubads().setTargeting( 'category', SFcategoryArray );
        } else {
            if ( 'undefined' !== typeof mtmads.postCategories ) {
                googletag.pubads().setTargeting( 'category', mtmads.postCategories );
            }
        }
	}

	if ( 'undefined' !== typeof mtmads.pageType ) {
		googletag.pubads().setTargeting( 'pageType', mtmads.pageType );
	}

   
    
	if ( isLoggedIn ) {
		googletag.pubads().setTargeting( 'sub', 'true' );
	} else {
		googletag.pubads().setTargeting( 'sub', 'false');
	}
    if(viewportwidth > 1230) {
        if(viewportwidth > 1510) {
            googletag.pubads().setTargeting( 'p1width', ['largebb','wallpaper'] );
        } else {
            googletag.pubads().setTargeting( 'p1width', ['largebb'] );   
        }
    }
	googletag.enableServices();
});

/**
 * iFrame resize helper.
 *
 * @param {int} id Element ID.
 */
function iFrameAutoResize( id ){
	var newheight;

	if ( document.getElementById ){
		newheight = document.getElementById( id ).contentWindow.document .body.scrollHeight;
	}

	if ( 'undefined' === typeof mtmads.iFrameData[ id ] || mtmads.iFrameData[ id ] !== newheight ) {
		document.getElementById( id ).height = ( newheight ) + 'px';
		mtmads.iFrameData[ id ] = newheight;
	}
}

// PH home ad injects position I in home page. 
// PH home ad injects position I in home page.
googletag.cmd.push (function() {
    var homePage = AdSection.indexOf("home");
    if (window.innerWidth < 600 && homePage > 0) {

        var adUnit = AdSection;
        var sunJournal = AdSection.indexOf("journal");
        if(sunJournal > 0){
            adUnit = "smg/sunjournal.com/home";
        }
            
            var insert = document.querySelector('div.mtm-featured');
        var adjElement = "P"; //total paragraphs

        if (typeof(insert) === "object") {

            var myNode = insert; //select the node for insertion
            var divString = '<div id="adPosition-1" style="border-style:solid; border-color: #CCCCCC; border-width: .5px;margin: 10px auto;overflow: visible;height:252px;width:302px;background-color:#FFFFFF;color:red;"><!--My Big Red Div  --></div>'; //define the Div
            var adLabelString = '<div class="adlabel" style="text-align:center;color:LightGray;font-size:14px;margin:18px auto -5px;">Advertisement</div>';
            myNode.insertAdjacentHTML('afterend', divString); //insert the div

            myNode.insertAdjacentHTML('beforeend', adLabelString);
            adSlotToAdd.push('adPosition-1'); //add slot name
            var newNode = myNode.nextSibling; //The node created in this loop
           

            var s = document.createElement('script');
            s.type = 'text/javascript';
            var code = 'googletag.cmd.push(function() { googletag.defineSlot("6119/'+adUnit+'", [300, 250], "adPosition-1").setTargeting("pos", "i").addService(googletag.pubads()); googletag.display("adPosition-1"); });';
           

            s.appendChild(document.createTextNode(code));
            newNode.appendChild(s);
        }
    }

window.googletag.pubads().addEventListener('slotRenderEnded', function(event) {
    var posfheight = jQuery("#pos_f_320x50").height();
    //if of a certain height, append button html
    if (  (posfheight > 5) && (jQuery(window).width() > 970) ){
       jQuery("#fCloseBtn").show();     
    }       

})  
    

});</script><script type="text/javascript">var adSlotToAdd = []; //array to hold values for as Slots.

/**
 * Every five paragraphs, insert ads into the article content.
 */
googletag.cmd.push( function() {
    
  if ( 'undefined' !== typeof mtmads.disableAds || 'undefined' !== typeof mtmads.disableAdInjection) {
		return;
	}

	mode = mtmads.adInjection;

	switch ( mode ) {
		case 'post':
			var objects = document.querySelectorAll( '.post-content > p' );
			var insertPoint = 6;
			break;
		case 'category':
			var objects = document.querySelectorAll( '.mtm-river .river li, .basic-category-list li' );
			var insertPoint = 5;
			break;
		default:
			return;
	}

	var objectCount = objects.length; // total number of P nodes.
	var count = 1; // initialize count.
	var position = 6;
	var fallbackDomain = window.location.host.replace( /^(www|stage)\./i, '').replace('.test', '.com' );

	// Sun Journal has a unique ad domain.
	if ( 'sunjournal.com' === fallbackDomain ) {
		fallbackDomain = 'smg/sunjournal.com';
	}
	
	while ( position < objectCount ) {

		adTargetName = '/6119/' + ( typeof AdSection !== 'undefined' ? AdSection : fallbackDomain );
        adTargetName = adTargetName.replace('/6119/sunjournal.com','/6119/smg/sunjournal.com');

		var myNode = objects[position]; //select the node for insertion.

		// Do not display the ad if it is too close to the end of the article.
		if ( objectCount - position < 4 ) {
			return;
		}

		if ( ( 'P' === myNode.previousElementSibling.tagName && 'post' === mode) || ( 'LI' === myNode.previousElementSibling.tagName && 'category' === mode) ) {

			const adnotice = document.createElement( 'span' );
				adnotice.setAttribute( 'class', 'before-injected-ad' );
				adnotice.textContent = 'Advertisement';

			const adslot = document.createElement( 'div' );
				adslot.setAttribute( 'id', 'adslot' + count );
				adslot.setAttribute( 'style', 'display: block; margin:0 auto; text-align:center; width:302px;' );
	
			myNode.insertAdjacentElement( 'beforebegin', adnotice );
			adnotice.insertAdjacentElement( 'afterend', adslot );

			adSlotToAdd.push( 'adslot' + count ); //add slot name.

			var slotName = 'adslot' + count;

			mtmads.slotName = googletag.defineSlot( adTargetName, [ ['fluid'], [300, 250] ], slotName )
				.setTargeting('pos', [ 'i','i' +  count ])
				.addService( googletag.pubads() );
			
			googletag.display( slotName );
		}

		count = count + 1;  //increment values.
		position = count * insertPoint;
	}
});
/* APS Script ====================== */

!function(a9,a,p,s,t,A,g){if(a[a9])return;function q(c,r){a[a9]._Q.push([c,r])}a[a9]={init:function(){q("i",arguments)},fetchBids:function(){q("f",arguments)},setDisplayBids:function(){},targetingKeys:function(){return[]},_Q:[]};A=p.createElement(s);A.async=!0;A.src=t;g=p.getElementsByTagName(s)[0];g.parentNode.insertBefore(A,g)}("apstag",window,document,"script","//c.amazon-adsystem.com/aax2/apstag.js");
     
apstag.init({
     pubID: '31257f2f-b5ff-49d9-9bca-92a523e8c19b', 
     adServer: 'googletag'
});
function pos1sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 1000) {
    widthOptions =  [[970, 90],[728, 90],[970, 250]];
    } else if(width > 750) {
    widthOptions =  [728, 90];   
    } else {widthOptions = [[320, 50], [300, 50], [300, 100]];}
    return widthOptions;
}
function pos2sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 785) {
    widthOptions =  [[300, 600],[300, 250]];  
    } else {widthOptions = [[300, 250]];}
    return widthOptions;
}
function pos3sizes() {
    var widthOptions= [];
    var width = window.innerWidth || document.body.clientWidth;
    if(width > 785) {
    widthOptions =  [[300, 600], [300, 250]];  
    } else {widthOptions = [[300, 250]];}
    return widthOptions;
}  
