/**
 * Add link to anyone with old PayPal account
 */
(function transitionOnetimeToRecurring($, undefined){
   'use strict';

   var rate_codes = [
      'PT_O7Day_D_Paypal',
      'MS_O7Day_D_Paypal',
      'SJ_O7Day_D_Paypal'
   ];

   //$(init);

   function init(){
      var user;
      var url = 'https://myaccount.pressherald.com/PayPal/paypal-user-check/?oid=';

      if( typeof Paywall === 'undefined' ){
         return;
      }

      user = Paywall.Cookie.get('MSPAN_Paywall_user');

      if( ! user || user === 'undefined'){
         // no user
         return;
      }

      user = JSON.parse(user);

      if( typeof user.rate_code === 'undefined' ){
         // no rate code attached
         // we might consider returning out here. 
         // return;
      }

      if( rate_codes.indexOf(user.rate_code) === -1 ){
         // rate code not one we're looking for
         return;
      }

      $.getJSON(url + user.account, function(res){
         if( res.update_link ){
            $('<div />')
               .addClass('paypal-update-link no-mobile')
               .html('<div id="pp-bar-wrap"><div class="pp-notification"><div class="pp-nav-text">Lock in your subscription at $65/year, no charge until your subscription renews.</div><div class="pp-nav-cta"><a href="' + res.update_link + '">Sign me up</a></div></div></div>')
               .insertAfter('.mtm-header')
         }
      });
   }

   return {}

}(jQuery));
</script>
