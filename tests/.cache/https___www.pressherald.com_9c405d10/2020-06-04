	// Retrieve my current url parameters
function getParameter(theParameter) {
  var params = window.location.search.substr(1).split('&');
  for (var i = 0; i < params.length; i++) {
    var p=params[i].split('=');
     if (p[0] == theParameter) {
      return decodeURIComponent(p[1]);
    }
  }
  return false;
}
var url_src = getParameter('utm_source');
var  url_mdm = getParameter('utm_medium');
var  url_cpn = getParameter('utm_campaign')
	
jQuery(function(){
	if(url_cpn === "Daily"){
		
		document.cookie = "pum-5184858=; expires=Sat, 01 Jan 2022 00:00:00 UTC; path=/;"
		
	}
	
	
})
		 
	

	
	
	var MTM_isGuest = (function($, undefined){
   'use strict';

   var cookies = document.cookie.split(';');
   var userCookie;

   for( var i = 0; i < cookies.length; i++ ){
      if( /mspan_paywall_user/i.test(cookies[i]) ){
         userCookie = JSON.parse(decodeURIComponent(cookies[i].split('=')[1]));
					document.cookie = "pum-5235850=true; expires=Sat, 01 Jan 2025 00:00:00 UTC; path=/;"
				
					document.cookie = "pum-5228714=; expires=Sat, 01 Jan 2025 00:00:00 UTC; path=/;"
			
				
         if( userCookie.name === 'Guest' && userCookie.type ){
            if( userCookie.type === 'trial' ){
             
							 jQuery('.login ').hide().after('<div class="memic"><a href="http://pubads.g.doubleclick.net/gampad/clk?id=5301403376&iu=/6119/tracking" target="_blank"><img src="https://dev.mainetodaymedia.com/paywall/sponsorship-2019/PG-Glass.png"  style="max-height:37px;width:auto;" /></a></div>'); 
							
							
            }
         }
      }
   }

   return false;
}(jQuery));
	
 
