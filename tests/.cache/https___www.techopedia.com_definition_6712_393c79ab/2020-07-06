    $(document).scroll(function () {
        checkScrollPosition();
    });

    function checkScrollPosition() {
        var y = $(document).scrollTop(), //get page y value
        leftAd = $("#LHS-ad-wrapper"),
        footerOffset = $("#footer").position().top;

        if (y >= 108 && (y + 700) < footerOffset) {
            leftAd.css({ "position": "fixed", "top": "60px" });
        } else if (y >= 108 && y + 700 >= footerOffset) {
            leftAd.css({ "position": "absolute", "top": (footerOffset - 649) });
        } else {
            leftAd.css({ "position": "absolute", "top": "147px" });
        }
    }
      
</script>

