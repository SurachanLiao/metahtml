                <script type="text/javascript">
                var show_random = Math.floor(Math.random()*(4)+1);

                switch(show_random) 
                {
                    case 1 :
                        show_random = "tab_te";
                    break;

                    case 2 :
                        show_random = "tab_car";
                    break;

                    case 3 :
                            show_random = "tab_travel";
                    break;

                    case 4 :
                            show_random = "tab_health";
                    break;
                }

                jQuery("#"+show_random+" > a").addClass("on");
                jQuery("#"+show_random+" > div").css("display","block");

                jQuery(".life_news > ul > li").mouseover(function() 
                {
                    jQuery("#tab_health > div,#tab_travel > div,#tab_car > div,#tab_te > div").css("display","none");
                    jQuery("#tab_health > a,#tab_travel > a,#tab_car > a,#tab_te >a").removeClass("on");

                    var over_id = jQuery(this).attr("id");
                    jQuery("#"+over_id+" > a").addClass("on");
                    jQuery("#"+over_id+" > div").css("display","block");
                });
                </script>
	
