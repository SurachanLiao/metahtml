            <input type="hidden" id="menu_box_state" value="0">

            <script>
            jQuery("#menu_box_btn").on("click",function()
            {
                menu_box_abtn();
            });
            </script>
            <!-- // 메뉴박스 -->

                        <script>
            jQuery(window).scroll(function()
            {
                if (jQuery(window).scrollTop() > "150") { jQuery(".fx_topbar_wrap").stop().animate({"top" : "0px"}, 350); }
                else { jQuery(".fx_topbar_wrap").stop().animate({"top" : "-76px"}, 150); }
            });
            </script>
            
            <script>
            function menu_box_abtn()
            {
                var box_state = jQuery("#menu_box_state").val();
                if (box_state == "0")
                {
                    jQuery("#menuBox").stop().animate({"left" : "0px"},{
                        "complete" : function()
                        {
                            jQuery(".open_menu > .bg_sub").css("position","fixed");
                        }
                    }, 300);

                    jQuery("#menu_box_state").val("1");
                }
                else
                {
                    jQuery("#menuBox").stop().animate({
                        "left" : "-430px"
                    }, 300);
                    jQuery("#menu_box_state").val("0");
                    jQuery(".open_menu > .bg_sub").css("position","absolute");
                }
            }

            jQuery(".open_menu > ul > li").on("mouseover",function()
            {
                var menu_index = jQuery(this).index();
                //if (menu_index > 22)
                if (menu_index > 24)
                {
                    jQuery(this).addClass("on");
                    jQuery(this).addClass("service");
                }
                else
                {
                    jQuery(this).addClass("on");
                }

                if (!jQuery(this).hasClass('only_link'))
                {
                    jQuery(".open_menu > ul > li:eq("+jQuery(this).index()+") > ul, #menu_box_sub ").show();
                    jQuery("#menuBox").css("width","410px");
                }
                else
                {
                    jQuery("#menuBox").css("width","200px");
                }
            });

            jQuery(".open_menu > ul > li").on("mouseout",function()
            {
                var menu_index = jQuery(this).index();
                //if (menu_index > 22)
                if (menu_index > 23)
                {
                    jQuery(this).removeClass("on");
                    jQuery(this).removeClass("service");
                }
                else
                {
                    jQuery(this).removeClass("on");
                }

                jQuery(".open_menu > ul > li > ul , #menu_box_sub").hide();
                jQuery("#menuBox").css("width","200px");
            });

            jQuery(".open_menu > ul > li > ul > li").on("mouseover",function()
            {
                jQuery(this).addClass("on");
                //jQuery(".open_menu > ul > li:eq("+jQuery(this).index()+") > ul ").show();
            });

            jQuery(".open_menu > ul > li> ul > li").on("mouseout",function()
            {
                jQuery(this).removeClass("on");
                //jQuery(".open_menu > ul > li > ul ").hide();
            });
            </script>

            
                                    
            
            
