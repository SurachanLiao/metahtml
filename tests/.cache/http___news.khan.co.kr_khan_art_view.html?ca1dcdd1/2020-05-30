                        <script type="text/javascript">
                        var main_alarm = Math.ceil(Math.random()*2);

                        jQuery("#alarm_tab > li:eq("+(main_alarm-1)+")").addClass("on");
                        jQuery("#main_alarm_tab"+main_alarm).show();

                        function main_alarm_tab(val) 
                        {
                            var val2 = parseInt(val)+parseInt(1);
                            jQuery("#main_alarm_tab1, #main_alarm_tab2").hide();

                            jQuery("#main_alarm_tab"+val2).show();
                            jQuery("#alarm_tab > li").removeClass("on");
                            jQuery("#alarm_tab > li:eq("+val+")").addClass("on");
                        }
                        </script>
	
