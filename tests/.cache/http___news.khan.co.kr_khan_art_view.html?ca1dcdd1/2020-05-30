                <input type='hidden' id='now_main_top_num' value=''/>

                <script type="text/javascript">
                var banner2_cnt = "0";

                if (banner2_cnt == '1' || banner2_cnt == '0') 
                {
                    jQuery('#banner1_img_arr_left').hide();
                    jQuery('#banner1_img_arr_right').hide();
                }

                var banner_random_main = Math.floor(Math.random()*(banner2_cnt)+1);
                jQuery('#now_main_top_num').val(banner_random_main);
                jQuery("#main_top_banner_"+banner_random_main).show();

                jQuery('#banner1_img_arr_left').click(function() 
                {
                    var now_num = jQuery('#now_main_top_num').val();
                    var now_num_be = jQuery('#now_main_top_num').val();

                    now_num--;

                    if (now_num == "0") 
                    {
                        jQuery("#main_top_banner_"+now_num_be).hide();
                        jQuery("#main_top_banner_"+banner2_cnt).show();
                        jQuery('#now_main_top_num').val(banner2_cnt);
                    } 
                    else 
                    {
                        jQuery("#main_top_banner_"+now_num).show();
                        jQuery("#main_top_banner_"+now_num_be).hide();
                        jQuery('#now_main_top_num').val(now_num);
                    }
                });

                jQuery('#banner1_img_arr_right').click(function() 
                {
                    var now_num = jQuery('#now_main_top_num').val();
                    var now_num_be = jQuery('#now_main_top_num').val();

                    now_num++;

                    if (now_num > banner2_cnt) 
                    {
                        jQuery("#main_top_banner_"+now_num_be).hide();
                        jQuery("#main_top_banner_1").show();
                        jQuery('#now_main_top_num').val("1");
                    } 
                    else 
                    {
                        jQuery("#main_top_banner_"+now_num).show();
                        jQuery("#main_top_banner_"+now_num_be).hide();
                        jQuery('#now_main_top_num').val(now_num);
                    }
                });
                </script>
