<script>
(function($){
    $.getJSON('http://www.khan.co.kr/mainArea/opinion_kmain.json', function(json){
        var len = Object.keys(json).length; ///< object length
        if (len != 4) return;
        var html = '';
        var obj = json[0];

        html += '<li>';
        html += '    <a href="http://news.khan.co.kr/kh_news/khan_art_view.html?artid='+ obj.art_id +'&code='+ obj.sec_id +'">';
        html += '        <span class="sb_title">여적</span>';
        html += '        <strong class="hd_title">'+ obj.art_title +'</strong>';
        html += '        <span class="photo">';
        html += '            <img src="'+ obj.image +'" alt="">';
        html += '            <span class="mask"></span>';
        html += '        </span>';
        html += '    </a>';
        html += '</li>';

        for (var i = 1; i < 4; i++) {
            obj = json[i];
            html += '<li class="noimg">';
            html += '    <a href="http://news.khan.co.kr/kh_news/khan_art_view.html?artid='+ obj.art_id +'&code='+ obj.sec_id +'">';
            html += '        <span class="sb_title">사설</span>';
            html += '        <strong class="hd_title">'+ obj.art_title +'</strong>';
            html += '        <em>'+ obj.summary +'</em>';
            html += '    </a>';
            html += '</li>';
        }

        $('#opinionArea ul').html(html);
    });
})(jQuery);
</script>
