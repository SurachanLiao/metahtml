                    var main_sports = Math.ceil(Math.random()*2);

                    jQuery("#sports_tab > li:eq("+(main_sports-1)+")").addClass("on");
                    jQuery("#main_sports_tab"+main_sports).show();

                    function main_sports_tab(val) 
                    {
                        var val2 = parseInt(val)+parseInt(1);

                        jQuery("#main_sports_tab1, #main_sports_tab2").hide();
                        jQuery("#main_sports_tab"+val2).show();
                        jQuery("#sports_tab > li").removeClass("on");
                        jQuery("#sports_tab > li:eq("+val+")").addClass("on");
                    }
                    </script>

            </div>
            <!-- //스포츠경향 -->

