/* Client-side device detection */
var WFClientTypeDef = {"xlarge":"(min-width: 1280px)","large":"(min-width: 1024px) and (max-width: 1279px)","small":"(max-width: 747px)","medium":"(min-width: 748px) and (max-width: 1023px)"};
