.entryCommonmidtDiv-m{ float: left;
width: 100%;}
}
@media(max-width:375px){
.articleImagea-m {
max-width: 80%;
margin:auto;
}
.entrySubs .boxResLeft {
margin: 0px 0px;
}
b.text-top-bold1{ font-size: 12px;
line-height: normal;
color:#fff;
}
.entrySubs .boxResRight div {
margin: 0px 5px;
}
}
@media(max-width:330px){
p.offer-timecounttext {
font-size: 13px;
line-height: 1.7;
padding-bottom: 0;
}
}
</style>
<script type="aa493fdcb70496d7033b33e3-text/javascript">
require(['flipclock'], function(flipclock) {
$(document).ready(function() {
var offerdate = 'Apr 26, 2020 23:59:00';
var countDownDate = new Date(offerdate).getTime();
var oneDay = 60 * 60 * 24 * 1000;
var daily = '';
var now = new Date().getTime();
var distance = countDownDate - now;
if(oneDay < distance){
daily = 'DailyCounter';
}
else{
daily = 'HourlyCounter';
$('div#cd100').addClass('HourlyCounter');
$('div#cd100-2').addClass('HourlyCounter');
$('div#cd100').addClass('HourlyCounter');
$('div#cd100-3').addClass('HourlyCounter');
}
var desktopclock = $('#cd100').FlipClock(distance/1000, {
clockFace: daily,
countdown: true,
callbacks: {
stop: function() {
$('.timecountwidget').hide();
}
}
});
var mobileouterclock = $('#cd100-2').FlipClock(distance/1000, {
clockFace: daily,
countdown: true,
callbacks: {
stop: function() {
$('.timecountwidget').hide();
}
}
});
var mobileinnerclock = $('#cd100-3').FlipClock(distance/1000, {
clockFace: daily,
countdown: true,
callbacks: {
stop: function() {
$('.timecountwidget').hide();
}
}
});
var mobileinnerclock1 = $('#cd100active').FlipClock(distance/1000, {
clockFace: daily,
countdown: true,
callbacks: {
stop: function() {
$('.timecountwidget').hide();
}
}
});
var mobileinnerclock1 = $('#cd100-2-free').FlipClock(distance/1000, {
clockFace: daily,
countdown: true,
callbacks: {
stop: function() {
$('.timecountwidget').hide();
}
}
});
if (distance < 0) {
$('.timecountwidget').hide();
}
});
}); </script>
