$(".mbsearch" ).click(function() {
var ele = $('footer.container').find(".submenubar");
if (ele.hasClass("active")) {
ele.removeClass("active");
} else {
$(".submenubar").removeClass("active");
ele.addClass("active");
}
});
/*sticky nav bar start*/
$(window).scroll(function(){
if ($(window).scrollTop() >= 100) {
$('.navbar-default.navbar').addClass('fixed-header');
$('.submenubar').addClass('fixedmenubar');
$('.mbdivse').addClass('fixedsearch');
$('.bl-sub-menu').addClass('fixed-header');
}
else {
$('.navbar-default.navbar').removeClass('fixed-header');
$('.submenubar').removeClass('fixedmenubar');
$('.mbdivse').removeClass('fixedsearch');
$('.bl-sub-menu').removeClass('fixed-header');
}
});
