<div class="bl-edit">
<img src="https://alphabl.thehindu.co.in/todays-paper/tp-marketing/crurbn/article10399228.ece/BINARY/bleditpng" class="img-responsive" alt="Download BL Daily Stock Pages FREE">
</div>
<h3>Download BL Daily Stock Pages FREE</h3>
<p>Get our print stock pages as a PDF download daily in your inbox. Sign up FREE!</p>
<div class="form-group">
<div class="input-group">
<input type="text" placeholder="Your Email" id="usr" class="form-control" name="email">
<span class="input-group-addon"><i class="fa fa-envelope"></i></span>
</div>
<br />
<button type="button" id="nwemail_button" class="btn btn-primary btn-block">Get Stock Pages FREE</button>
<div id="nwemail_error" class="nwemail-error" style="display:none">
<p class="error-email-msg">Please enter a valid email address. Thank You.</p>
</div>
<div id="nwemail_success" class="nwemail-success" style="display:none"></div>
<div id="newsletter_load" class="form-group-load"></div>
</div>
</div>
</div>
<script type="aa493fdcb70496d7033b33e3-text/javascript">
var emailAddressFilter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/;
function validate_email(fieldId) {
var email = $(fieldId).val();
email= email.trim();
if (email == "" || email == "undefined") {
return false;
}else if (!emailAddressFilter.test(email)) {
return false;
}
return true;
}
$(document).ready(function(){
$(".bl-stock-inbox").each(function() {
var id = "div."+$(this).attr("class");
$(id + " button#nwemail_button").on("click", function(event){
$(id + " button").attr("disabled","disabled");
$(id + " input").click(function(){$(id + " button").removeAttr("disabled");});
var email = $(id +" input").val();
email= email.trim();
$(id + " #nwemail_error").css("display", "none");
$(id + " #nwemail_success").css("display", "none");
$(id + " input").removeClass("error");
if (validate_email(id + " input") ){
$(id + " #newsletter_load").show();
var data = {	"email" : email, "category" : "SU"};
$.ajax({
type : "POST",
contentType : "application/json",
url : publist("url") + "s/mailsubscription/subscribe/",
data : JSON.stringify(data),
dataType : 'json',
timeout : 10000,
success : function(response) {
// console.log("SUCCESS: ", response);
$(id + " #nwemail_success").html("<p>Thank you for subscribing to our Daily Stock update newsletter. Click on the link sent to your email to complete the subscription process.</p>");
$(id + " #nwemail_success").css("display", "block");
$(id + " #newsletter_load").hide();
},
error : function(e) {
// console.log("ERROR: ", e);
},
done : function(e) {
// console.log("DONE");
}
});
} else {
$(id + " #newsletter_load").fadeOut( "slow");$(id + " #nwemail_error").css("display", "block");
$(id + " input").addClass("error");
}
});
});
});
</script>
