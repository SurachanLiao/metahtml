    if (_Y_.DJaccount.is_team === false && ZAIUS_ENABLED) {
      if (_Y_.DJuserevents.first_time_on_workflow || _Y_.DJuserevents.uw_tl || _Y_.DJuserevents.workflow_saved) {
        var eventProp = _Y_.DJuserevents.first_time_on_workflow ||
          _Y_.DJuserevents.uw_tl || _Y_.DJuserevents.workflow_saved ;

        var workflow_action = null;
        if (_Y_.DJuserevents.first_time_on_workflow) {
          workflow_action = "create";
        } else if (_Y_.DJuserevents.uw_tl) {
          workflow_action = "TL_phase";
        } else {
          workflow_action = "save_answers";
        }

        var zaiusProps = eventProp.zaius_data;
        /* Create Customer entity if exists */
        if (Object.keys(zaiusProps.customer).length > 0) {
          _Y_.zaius.entity("customer", zaiusProps.customer)
        }

        /* Send custom "workflow" event indicating that UW was created */
        _Y_.zaius.event(
          "workflow",
          {action: workflow_action,
           order_id: zaiusProps.order.order_id,
           customer_id: zaiusProps.customer.customer_id }
       );

        function product_handler(p) {
         /* Update Product information */
         _Y_.zaius.entity(
           "product", p
         );
         /* Add UW product to cart */
         _Y_.zaius.event(
           "product",
           {action: "add_to_cart",
            order_id: zaiusProps.order.order_id,
            customer_id: zaiusProps.customer.customer_id,
            product_id: p.product_id}
        );
        }
        zaiusProps.product_list.map(product_handler)
      } else if (_Y_.DJuserevents.payment_notif_to_send) {
        /* Update Customer data */
        _Y_.zaius.entity("customer", _Y_.DJuserevents.payment_notif_to_send.zaius_data.customer)

        /* Update Order data */
        _Y_.zaius.entity("order", _Y_.DJuserevents.payment_notif_to_send.zaius_data.order)

        /* Purchase product */
        /* Send a Special Event for Order */
        _Y_.zaius.event(
          "order",
          {action: "purchase",
           customer_id: _Y_.DJuserevents.payment_notif_to_send.zaius_data.customer.customer_id,
           order: _Y_.DJuserevents.payment_notif_to_send.zaius_data.order }
        );

        function product_handler(p) {
          /* Update Product data */
          _Y_.zaius.entity("product", p)
        }
        _Y_.DJuserevents.payment_notif_to_send.zaius_data.product_list.map(product_handler);
      } else if (_Y_.DJuserevents.uw_f) {

        /* Update Customer data*/
        _Y_.zaius.entity("customer", _Y_.DJuserevents.uw_f.zaius_data.customer)

        /* Update Order data */
        _Y_.zaius.entity("order", _Y_.DJuserevents.uw_f.zaius_data.order)
        /* Send custom "workflow" event indicating that UW was created */
        _Y_.zaius.event(
          "workflow",
          {action: "finalize",
           order_id: _Y_.DJuserevents.uw_f.zaius_data.order.order_id,
           customer_id: _Y_.DJuserevents.payment_notif_to_send.zaius_data.customer.customer_id}
         );
        function product_handler(p) {
         /* Update Product data */
         _Y_.zaius.entity("product", p)
        }
        _Y_.DJuserevents.uw_f.zaius_data.product_list.map(product_handler)
      }
    }
    return;
  });
</script>
<script type="ac835e8da493ebf5eda2953b-text/javascript">
  $(document).ready(function() {
    SiteNavigationBar.init();
    Searchbar.init();
    FormNewsletter.init();
    stickybits(".js-site-header");
  });
</script>

