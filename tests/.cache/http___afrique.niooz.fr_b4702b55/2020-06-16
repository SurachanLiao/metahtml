	<script>
		initMoreButton('.srcofthm .more_elem');
		$('.srcofthm a').click(function(e){
			e.preventDefault();
			var myurl = $(this).attr('href');
			var theme = myurl.split('niooz.fr')[0].split('.')[0].replace('http://','').replace('//','');
			
			$.address.value('/s/'+theme +myurl.split('niooz.fr')[1]);
		});
		$('.srcofthm .root').change(function(){
			if($(this).is(':checked'))
				$('.srcofthm input[type=checkbox]').not('.root').prop('checked',true);
			else
				$('.srcofthm input[type=checkbox]').not('.root').prop('checked',false);
		});
		$('.srcofthm select').change(function(){
			var action = $(this).val();
			var ids = new Array();
			$('.srcofthm input[type=checkbox]:checked').not('.root').each(function(){
				var id = $(this).attr('relid');
				ids.push(id);
				if(action == 'on')
					$(this).closest('li').removeClass('disabled');
				else if(action == 'off')
					$(this).closest('li').addClass('disabled');
					
			});
			$(this).val('0')
			$('.srcofthm input[type=checkbox]').prop('checked',false);
			$.post('/cmd/feed_site_off',{'type':(action == 'on' ? 'true' : 'false'),'ids':ids},function(){
				growl.notify("create", {text: "Opération effectuée..."},{ expires:4000 });
				refresh();
			})
			
		})
	</script>

		
	
			
