else{$(this).removeClass('disabled')}});$(window).on('resize',function(){if(window.innerWidth>992)$('body').removeClass('opened_burger_menu');$(this).next($('.sub-menu')).slideToggle()});$(document).ready(function(){$('#new-subnav-PM .dropdown:first .dropdown-menu').each(function(){var o=$(this).html();$('.dropdown-menu-placeholder').data('placeholder',o).html(o)});$('.dropdown').hover(function(){$(this).children().each(function(){$(this).toggleClass('show-dropdown');if(!$(this).hasClass('show-dropdown')){return};var o=$(this).html().trim();if(''!==o&&o.indexOf('<li')>-1){$('.dropdown-menu-placeholder').html(o).show()}})});var e=!1,o=!1;$('#new-subnav-PM .dropdown, #new-subnav-PM .dropdown *, .dropdown-menu-placeholder').mouseout(function(n){e=!0;if(o){clearTimeout(o)};o=setTimeout(function(){if(e){$('.dropdown-menu-placeholder').each(function(){$(this).html($(this).data('placeholder')).show()})}},100)}).mouseover(function(d){e=!1;if(o){clearTimeout(o)};var n=$('.dropdown-menu.show-dropdown').html();if('undefined'!==typeof n){n.trim();$('.dropdown-menu-placeholder').html(n).show()}});$('.hideDropDown').hover(function(){$(this).parent().toggleClass('HideFirst');$('.dropdown-menu-placeholder').hide().html('')})});</script><section id="main"><div class="container"><ul id="social_sidebar" class="social_sidebar"><li id="fb_socialPageLink"><a data-url="https://www.facebook.com/sharer/sharer.php?u=https://www.parismatch.com/Actu/International/Australie-des-aiguilles-dans-les-fraises-le-Premier-ministre-denonce-un-sabotage-terroriste-1575295" class="fb socialPageLink" title="Partager sur Facebook"><span class="icon-Facebook"></span></a></li><li id="tw_socialPageLink"><a data-url="https://twitter.com/intent/tweet?text=Australie%20%3A%20des%20aiguilles%20dans%20les%20fraises%2C%20le%20Premier%20ministre%20d%C3%A9nonce%20un%20%22sabotage%20terroriste%22&url=https://www.parismatch.com/Actu/International/Australie-des-aiguilles-dans-les-fraises-le-Premier-ministre-denonce-un-sab