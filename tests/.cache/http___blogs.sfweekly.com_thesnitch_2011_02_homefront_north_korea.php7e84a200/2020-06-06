    <div class="search-link dropdown-link">
      <form class="search-form" id="searchbox_017793458706336421686:dxw6fggi0yc" action="https://archives.sfweekly.com/sanfrancisco/Search">
        <input type="hidden" name="cx" value="017793458706336421686:dxw6fggi0yc" />
        <input type="hidden" name="cof" value="FORID:9" />
        <input id="search-input" autocomplete="off" type="text"  name="q" size="25" placeholder="Search" />
        

