        var myMaxScroll = 1316;

        if (myMaxScroll > 0) {
          $$('#TopStoriesSlider .sliderInner').each(function(slider) {
            var currentPos = parseInt(slider.getStyle("left"));
            if (currentPos > -6) {
              slider.setStyle({'left':'-6px'});
            }
            else if (currentPos < -1316) {
              slider.setStyle({'left':'-1316px'});
