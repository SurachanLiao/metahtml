        <div id="skd_panorama1" class="adContainer md-ads-flex align-items-center justify-content-center" aria-label="Annons" tabindex="-1"></div>

        <script>
           var section = "";  
           section = section || skd_category;
       
          // If on sport use custom lw tags
          if (section && section == "sport") {
            $("#skd_panorama1").attr("id", "skd_panorama1".replace("_", "_sport"));

            skd_ad_lw_ad("Panorama1", "skd_panorama1".replace("_", "_sport"), section);
          } else {
            skd_ad_lw_ad("Panorama1", "skd_panorama1", section);
          }
