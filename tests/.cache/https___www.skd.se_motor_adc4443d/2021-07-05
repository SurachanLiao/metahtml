          // If on sport use custom lw tags
          if (section && section == "sport") {
            $("#skd_sticky").attr("id", "skd_sticky".replace("_", "_sport"));

            skd_ad_lw_ad("Sticky", "skd_sticky".replace("_", "_sport"), section);
          } else {
            skd_ad_lw_ad("Sticky", "skd_sticky", section);
          }
        </script>
          </div>
      </div>
    </div>
  </main>
      </div>
  
  </div><!-- /container -->

