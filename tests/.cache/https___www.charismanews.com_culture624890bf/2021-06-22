					appName = data.mobile.appName;
					ga('set', 'appName', appName);
					sessionStorage.setItem('appName', appName);
					isApp = true;
				}
				if(!appId && data.mobile.appId){
					appId = data.mobile.appId;
					ga('set', 'appId', appId);
