                });

                window.tag.internalSearch.set({
                keyword: '',
                resultPageNumber: '0'
                });

                if (document.cookie.indexOf('atuserid=') == -1 && document.cookie.indexOf('atoptedout=') == -1) {
                    window.tag.clientSideUserId.set("Consent-NO");
                }

                window.tag.dispatch();
            }
        };

