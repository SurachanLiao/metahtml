	<script>
		jQuery(function($){
			
		var windowwidth = $(window).width();
		//givemob();
		
		//console.log("I'm working?");
		
		function giveamp(){
			if(windowwidth<480){
				$("a").each(function(i){
					var linklink = $(this).attr("href");
					var ifmatch = linklink.match(/https:\/\/(?!frame|esports)\w+\.inquirer\.net\/[0-9]+\/[a-z]+/g);
					//console.log(ifmatch);
					if(ifmatch){
						$(this).attr("href",linklink+"/amp");
					}
				});
			}
		}
		function givemob(){
			if(windowwidth<480){
				$("a").each(function(i){
					var linklink = $(this).attr("href");
					if(linklink!=null){
						var getslash = linklink.split("/");
						var getcha = (getslash[2] ? getslash[2].split(".") : null);
						var ifmatch = linklink.match(/http:\/\/(?!frame|esports)\w+\.inquirer\.net\/[0-9]+\/[a-z]+/g);
						console.log(ifmatch);
						if(ifmatch){
							$(this).attr("href","//m.inquirer.net/"+getcha[0]+"/"+getslash[3]);
						}
					}
				});
			}
		}
		
			//giveamp();
		/*JS RESPONSIVE*/
		$(window).resize(function(){
			windowwidth = $(this).width();
				//giveamp();
			//givemob();
			
		});
		/*END OF JS RESPONSIVE*/			
			
			$(window).scroll(function(){
				var distancetotop = $("#inq-channel-wrap:first-child").offset().top - $(this).scrollTop();
				if(distancetotop<=0){
					$(".cct-tempskinad").addClass("stickyskin");
				}
				if(distancetotop>0){
					$(".cct-tempskinad").removeClass("stickyskin");
				}
			});
			
			
			
		});
		
		
		
		
		$(function() {
			
			

var nextpaginateID = 6;
function generateNextSlotName() {
   var id = nextpaginateID++;
   return id;
 }
	

			
			function load_posts(cat,ch){				 
 var nextid = generateNextSlotName();
 console.log(nextid);
    var str = '&pageNumber=' + nextid + '&cat='+ cat + '&ch='+ ch ;
    $.ajax({
        type: "POST",
        dataType: "html",
        url: '/api/post/',
        data: str,
		cache: false,		
        success: function(data){			
			 
			
            var $data = $(data);
            if($data.length){
				//console.log(data);
                $("#load-more-latest-stories").append($data);
                $("#ch-more-news2").attr("disabled",false);
            } else{
                $("#ch-more-news2").attr("disabled",true);
            }
        },
        error : function(jqXHR, textStatus, errorThrown) {
            $loader.html(jqXHR + " :: " + textStatus + " :: " + errorThrown);
        }

    });
	
    return false;
	}
	
	
	$("#ch-more-news2").on("click",function(){  
		$("#ch-more-news2").attr("disabled",true);  
	var cat = $(this).attr("data-cat");
	var ch = $(this).attr("data-ch");
		load_posts(cat,ch);
		return false;
	});

		
		
		});
		
		
		
		
		
		
		
