              userreportEl.id = 'userreport-launcher-script';

              var firstScript = document.querySelector('script');
              if (firstScript && firstScript.parentNode) {
                firstScript.parentNode.insertBefore(userreportEl, firstScript);
              } else {
                document.head.appendChild(userreportEl);
              }

              window.audienceProjectLayer = window.audienceProjectLayer || [];

