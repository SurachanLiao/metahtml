<script>
    (function() {
        var submenuToggleBtn = document.getElementById('submenu-toggle');
        var submenuContainer = document.getElementById('submenu-container');

        submenuToggleBtn.addEventListener('click', function() {
            submenuContainer.scrollTop = 0;
            submenuToggleBtn.classList.toggle('menu-item--active');
            submenuContainer.classList.toggle('submenu--active');
        });
    }());
</script>
