</section>

<script>
  (function () {
    var horizontalScroll = function (scrollContainerId) {
    function throttle(callback, wait) {
        var inThrottle;
        return function () {
            var args = [];
            for (var _i = 0; _i < arguments.length; _i++) {
                args[_i] = arguments[_i];
            }
            if (!inThrottle) {
                callback.apply(this, args);
                inThrottle = true;
                setTimeout(function () { return (inThrottle = false); }, wait);
            }
        };
    }
    /* Horizontial Scroll elements */
    var scrollContainer = document.getElementById(scrollContainerId);
    var scrollItemContainer = scrollContainer.querySelector('[data-horizontallist="itemcontainer"]');
    var prevScrollBtn = scrollContainer.querySelector('[data-horizontallist="button-prev"]');
    var nextScrollBtn = scrollContainer.querySelector('[data-horizontallist="button-next"]');
    var children = scrollItemContainer.children;
    var listLength = children.length;
    var containerRight = scrollContainer.getBoundingClientRect().right;
    var listCurrent = 0;
    /**
     * Find how many visible elements we have
     */
    var visibleChildren = Array.from(children).filter(function (child) { return child.getBoundingClientRect().right <= containerRight; })
        .length;
    var maxLength = listLength - visibleChildren;
    function updateDataSet(pos, isScroll) {
        if (isScroll === void 0) { isScroll = false; }
        switch (pos) {
            case 'neutral':
                scrollContainer.dataset.atstart = 'false';
                scrollContainer.dataset.atend = 'false';
                if (isScroll)
                    listCurrent = 1;
                break;
            case 'end':
                scrollContainer.dataset.atstart = 'false';
                scrollContainer.dataset.atend = 'true';
                listCurrent = maxLength;
                break;
            case 'start':
                scrollContainer.dataset.atstart = 'true';
                scrollContainer.dataset.atend = 'false';
                listCurrent = 0;
                break;
            case 'noScroll':
                scrollContainer.dataset.atstart = 'true';
                scrollContainer.dataset.atend = 'true';
                break;
        }
    }
    function updateButtons() {
        if (visibleChildren === listLength) {
            updateDataSet('noScroll');
        }
        else if (listCurrent === 0) {
            updateDataSet('start');
        }
        else if (listCurrent === maxLength) {
            updateDataSet('end');
        }
        else {
            updateDataSet('neutral');
        }
    }
    function updateButtonsThroughScroll() {
        var childLeft = children[0].getBoundingClientRect().left;
        var wrapLeft = scrollItemContainer.getBoundingClientRect().left;
        var childRight = children[listLength - 1].getBoundingClientRect().right;
        var wrapRight = scrollItemContainer.getBoundingClientRect().right;
        if (visibleChildren === listLength) {
            updateDataSet('noScroll', true);
        }
        else if (childLeft - 5 === wrapLeft) {
            updateDataSet('start', true);
        }
        else if (childRight - 10 <= wrapRight) {
            updateDataSet('end', true);
        }
        else {
            updateDataSet('neutral', true);
        }
    }
    /**
     * Advance scroll to make next or previous element visible
     */
    function scroll() {
        var newPos = children[listCurrent];
        scrollItemContainer.scrollTo({
            behavior: 'smooth',
            left: newPos.offsetLeft,
            top: 0
        });
        updateButtons();
    }
    nextScrollBtn.addEventListener('click', function () {
        if (listCurrent !== maxLength) {
            listCurrent++;
            scroll();
        }
    });
    prevScrollBtn.addEventListener('click', function () {
        if (listCurrent !== 0) {
            listCurrent--;
            scroll();
        }
    });
    if (visibleChildren === listLength) {
        /**
         * If there is no invisible elements hide buttons
         */
        prevScrollBtn.style.display = 'none';
        nextScrollBtn.style.display = 'none';
    }
    scrollItemContainer.addEventListener('wheel', throttle(function () {
        updateButtonsThroughScroll();
    }, 150));
    updateButtons();
};

    if(false) {
      var followSection = function (listId, section) {
    var wrapper = document.getElementById("follow-wrapper-" + listId);
    var followStatus = wrapper.dataset.initFollowStatus === 'true' ? true : false;
    var followBtn = wrapper.querySelector('.follow-section-btn');
    var tooltip = wrapper.querySelector('.tooltip-input');
    followBtn.addEventListener('click', function () {
        if (followStatus) {
            var unfollowTagRequest = window.mitEb.unfollowTag(section);
            unfollowTagRequest
                .then(function (response) {
                if (response.success) {
                    followStatus = false;
                    tooltip.checked = false;
                    followBtn.innerText = "F\u00F8lg";
                    followBtn.classList.toggle('button--solid');
                    followBtn.blur();
                }
                else {
                    throw new Error("Cannot unfollow taguri: " + section);
                }
            })["catch"](function (err) {
                console.log(err);
            });
        }
        else {
            var followTagRequest = window.mitEb.followTag(section);
            followTagRequest
                .then(function (response) {
                if (response.success) {
                    followStatus = true;
                    tooltip.checked = true;
                    followBtn.innerText = "F\u00F8lger";
                    followBtn.classList.toggle('button--solid');
                    followBtn.blur();
                }
                else {
                    throw new Error("Cannot follow taguri:  " + section);
                }
            })["catch"](function (err) {
                console.log(err);
            });
        }
    });
};

      try {
        /*
         * JSP varibles from ContentService List:
         * - followTag: string | provides a section to follow ONLY if one tag has been selected otherwise it returns empty string
         * - showFollowButton: boolean | checks if followButton has been actived in Escenic
         * - listId: string | provides a listId
         * - listName: string | provides listName = title
         */
        followSection("8598541", "");
      } catch (error) {
        console.error('follow-section err: ', error)
      }
    }
    try {
      horizontalScroll('scroll-container-8598541');
    } catch (error) {
      console.error('horz er', error)
    }
  })();
</script>
 </p></div>
    <div class="fnSpaceManagement" id="spm7988863x1iyx8" data-spcmngm="adv-nat2">
