    <div id="ebbanner_wrapper_monster1" class="eb-position eb-position--monster1 bannercenter">
          <div id="ebbanner_monster1" class="eb-placement">
          </div>
        </div>
      </div>
    <div class="flex flex-align--start margin-m--b">
      <div class="flex-item flex-item--grow pagecolumn-element" style="width: 50%;">
          <div class="fnSpaceManagement" id="spm5822639xTAox1" data-spcmngm="ban-vs-adv" data-spcmngmtype="advertorial">
    </div>
    </div>
      <div class="flex-item flex-item--grow pagecolumn-element" style="width: 50%;">
          <div class="fnSpaceManagement" id="spm5822640xTAox1" data-spcmngm="ban-hj-adv" data-spcmngmtype="advertorial">
