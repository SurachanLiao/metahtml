                window.onload = function() {
                    (function lazyLoadImage() {
  var imageDiv = document.querySelectorAll('[data-islazy]');

  for (var i = 0; i < imageDiv.length; i++) {
    if (imageDiv[i].getAttribute('data-islazy') && imageDiv[i].getAttribute('data-islazy') !== "" && imageDiv[i].getAttribute('data-islazy') !== null) {
      if (imageDiv[i].tagName.toLowerCase() == 'img') {
        if (imageDiv[i].getAttribute('data-islazy') && imageDiv[i].getAttribute('data-islazy') !== "" && imageDiv[i].getAttribute('data-islazy') !== null) {
          imageDiv[i].src = imageDiv[i].getAttribute('data-islazy');
        }

        imageDiv[i].removeAttribute('data-islazy');
      } else {
        var img = new Image(); //img.onload = function(imageDivNew) {
        //}(imageDiv[i]);

        img.src = imageDiv[i].getAttribute('data-islazy');
        setTimeout(function (imageDivNew) {
          if (imageDivNew.getAttribute('data-islazy') && imageDivNew.getAttribute('data-islazy') !== "" && imageDivNew.getAttribute('data-islazy') !== null) {
            imageDivNew.style['background-image'] = "url(" + imageDivNew.getAttribute('data-islazy') + ")";
          }

          imageDivNew.removeAttribute('data-islazy');
        }, 1000, imageDiv[i]);
      }
    }
  }
})()
                }
            }
