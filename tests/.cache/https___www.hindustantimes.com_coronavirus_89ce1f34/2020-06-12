    <script>
    function getMapData(evt, rel) {
    	if (rel == 'world') {
    	document.getElementById('india-coses').style.display = "none";
    	document.getElementById('india-Map').style.display = "none";
    	document.getElementById('world-coses').style.display = "block";
    	document.getElementById('world-Map').style.display = "block";
    	} else {
    	document.getElementById('world-coses').style.display = "none";
    	document.getElementById('world-Map').style.display = "none";
    	document.getElementById('india-coses').style.display = "block";
    	document.getElementById('india-Map').style.display = "block";

    	}

    	
    	//alert(evt.innerHTML);
    	var tElem = document.getElementsByClassName('tabLink');
    	for (var i = 0; i < tElem.length; i++) {
    	tElem[i].className = tElem[i].className.replace(" active", "");
    	}
    	evt.className += " active";

    	}
    </script><!--#em:content-->
