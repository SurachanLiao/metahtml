		if(elem.height() <  Math.ceil(maxHeight7127677 / 2))	moreLineHeight = true;
		$(this).find('.tag_link').each(function() {
			font = parseInt($(this).css('font-size'));
			font = $(font).toUnit('em', {'scope': elem});
			if(font > maxFont7127677)	maxFont7127677 = font;
		});

