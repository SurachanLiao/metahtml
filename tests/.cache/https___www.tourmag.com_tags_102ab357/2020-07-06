		height = $(this).outerHeight();
		if (height <= maxHeight7127677)	$(this).css('height', maxHeight7127677 + 'px');
	});

	$('.mod_7127677 div.tag').each(function() {
		var font = 0;
		var maxFont7127677 = 0;
