        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {
                $('nav').addClass("fixed").fadeIn();
                $('#logomin').empty();
                $('#logomin').prepend('<img src="../img/logomin.png"  alt="#">');
            }
            else {
                $('#logomin').empty();
                $('nav').removeClass("fixed");
            }
        });
    </script>
