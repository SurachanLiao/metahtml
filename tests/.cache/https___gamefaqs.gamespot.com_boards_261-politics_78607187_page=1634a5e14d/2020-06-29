<script type="text/javascript" src="https://gamefaqs1.cbsistatic.com/js/features.20200624210116.js"></script>
<script type="text/javascript" src="https://gamefaqs1.cbsistatic.com/js/jquery.textcomplete.min.20200624210116.js"></script>
<script type="text/javascript" src="https://gamefaqs1.cbsistatic.com/js/jquery.unveil.20200624210116.js"></script>
<script type="text/javascript">
var autoplay = 2;
$(document).ready(function()
{
	$('.edit_time, .post_time, .report_dialog li').tooltip({ position:{ at:"right+5 center"},tooltipClass:'tooltip' });
	
		
	$(".msg_body img, .msg_body video").unveil();

	$('.msg_body > blockquote > blockquote > blockquote').addClass('h_bq').parent().parent().addClass('tl_bq');
	$('blockquote.tl_bq').each(function()
	{
		var msgid = $(this).parents('.msg_body').attr('name');
		$(this).find('.h_bq').before('<blockquote class="expand_quote" data-msgid="'+msgid+'"><a onclick="toggle_quotes('+msgid+')"><i class="fa fa-expand"></i> show hidden quote(s)</a></blockquote>');
	});
});

function toggle_quotes(mid)
{
	$('.msg_body[name="'+mid+'"] blockquote > blockquote > blockquote:not(.expand_quote)').css('display','block');
	$('blockquote.expand_quote[data-msgid="'+mid+'"]').remove();
}

$('s').click(function() { $(this).toggleClass('spoil_on') });
var menu_data = new Array();

$('a.menu_toggle').click(function(e){
	e.stopPropagation();
	var mid = $(this).attr('data-msgid');
	var uid = $(this).attr('data-userid');
	toggle_user_menu(mid,uid);
});

$(document).click(function(e){
	if($('.user_submenu').has(e.target).length==0 && !$('.user_submenu').is(e.target))
		$('.user_submenu').hide();
});

function toggle_user_menu(mid, uid)
{
	$('.user_submenu[data-msgid!='+mid+']').hide();
	$('.user_tag_input').hide();
	if($('.user_submenu[data-msgid='+mid+']').is(':hidden'))
	{
		if(!menu_data[uid])
		{
			$.ajax({
					type: 'POST',
					url: '/ajax/user_basic_info',
					data:  { b: 261, m: mid, u: uid, key: '2adafd2f'},
					success: function(response)
					{
						$('.user_submenu[data-msgid='+mid+']').show();
						menu_data[uid]=1;

						var d = $.parseJSON(response);
						var menu = '.user_submenu[data-userid='+uid+']';

						if(d.error)
						{
							$(menu+' .user_tag').remove();
							$(menu+' div.head h3.title').html('User Info: '+ $(menu).attr('data-username'));
							$(menu+' div.body').html(d.error);
							return;
						}

						$(menu+' img').attr('src',d.avatar);
						$(menu+' span.user_since').html('<b>User Since:</b> '+d.first_visit);
						$(menu+' span.user_karma').html('<b>Karma:</b> '+d.karma);
						$(menu+' span.user_posts').html('<b>Active Posts:</b> '+d.active_posts);
						if(d.badge_count)
						{
							$(menu+' span.user_badge').html('<b>Total Badges:</b> '+d.badge_count);
							for(var k in d.badge_rollup.recent)
							{
								$(menu+' span.user_badge').append('<br/>&nbsp;&nbsp;&nbsp;<b>Newest:</b> <i class="fa fa-trophy badge_'+d.badge_rollup.recent[k].level_name+'"></i> ' +d.badge_rollup.recent[k].badge_name);
							}
							for(var k in d.badge_rollup.rarity)
							{
								$(menu+' span.user_badge').append('<br/>&nbsp;&nbsp;&nbsp;<b>Rarest:</b>  <i class="fa fa-trophy badge_'+d.badge_rollup.rarity[k].level_name+'"></i> ' +d.badge_rollup.rarity[k].badge_name);
							}
						}
						else
							$(menu+' span.user_badge').html('<b>Total Badges:</b> N/A<br/>&nbsp;&nbsp;&nbsp;<b>Recent:</b> N/A<br/>&nbsp;&nbsp;&nbsp;<b>Rarest:</b> N/A');
						$(menu+' span.user_level').html('<b>'+d.level_name+'</b>&nbsp;&nbsp;<i class="fa fa-question-circle" title="'+d.level_desc+'"></i>');
												$(menu+' span.user_level i').tooltip({ position:{ at:"right+5 center" },tooltipClass:'tooltip' });
					}
			});
		}
		else
		{
			$('.user_submenu[data-msgid='+mid+']').show();
		}
	}
	else
	{
		$('.user_submenu[data-msgid='+mid+']').hide();
	}
}

</script>
