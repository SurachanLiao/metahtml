</noscript>

<script>

	// Load deferred scripts
	metro.deferredScripts = metro.deferredScripts || [];

	/**
	 * Only load twitter if we have GDPR/CCPA consent or CMP is not required
	 */
	(window.requireConsent || Promise.resolve(false)).then(function(hasConsent) {
		if (hasConsent) {
			metro.deferredScripts.push({
				url: '//platform.twitter.com/widgets.js',
				id: 'twitter-widgets',
				callback: function () {
					// Stop Twitter widgets.js from taking over the function of the top Twitter button
					var shareButtons = document.getElementsByClassName('share-button');
					for (var i=0; i<shareButtons.length; i++) {
						var shareButton = shareButtons[i];
						if (shareButton.classList.contains('twitter')) {
							shareButton.addEventListener('click', metro.share.sharePageToTwitter);
						}
					}
				}
			});
		}
	});

	
	
	(function (metro) {

		function loadScripts() {
			if (metro.deferredScripts.length > 0) {
				for (var i = 0; i < metro.deferredScripts.length; i++) {
					loadScript(metro.deferredScripts[i]);
					metro.deferredScripts.splice(i,1);
				}
			}
			window.removeEventListener('load', loadScripts);
		}

		function loadScript(obj) {
			if (document.getElementById(obj.id)) {
				return;
			}
			var script = document.createElement('script');
			script.src = obj.url;
			script.id = obj.id;
			document.head.appendChild(script);
			if ('function' === typeof obj.callback) {
				obj.callback();
			}
		}

		if ('article' === metro.pageData.type) {
			window.addEventListener('metro:scroll', loadScripts);
		} else {
			window.addEventListener('load', loadScripts);
		}

	})(metro);

</script>


