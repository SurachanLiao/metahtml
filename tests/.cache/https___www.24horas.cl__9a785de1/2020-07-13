<script>
$(document).ready(function () {
$('#multiCarouselGeneric_3742773').find('.item').first().addClass('active');
}); multiCarrousel("multiCarouselGeneric_3742773", 2);
function multiCarrousel(elementId, initValue) {
$('#' + elementId + '[data-type="multi"] .item').each(function() {
var next = $(this).next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
for (var i = 0; i < initValue; i++) {
next = next.next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
}
});
}
</script>
