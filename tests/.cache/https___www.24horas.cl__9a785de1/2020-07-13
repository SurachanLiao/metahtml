$(document).ready(function() {
var getHtmlLoUltimo= function(){
var today = new Date();
var rNumber = today.getFullYear() + '' + String(today.getMonth() + 1).padStart(2, '0') + '' + String(today.getDate()).padStart(2, '0') + '' + today.getHours() + '' + today.getMinutes();
$.ajax({url: 'https://www.24horas.cl/static/loUltimo-dynamic.html?'+rNumber, success: function(result){
$("#divLoUltimo").html(result);
}});
}
