<section class="html5itemGroup hidden">
<div class="html5itemGroup container white">
<h6><a href="https://digitalizados.entel.cl/">Digitalizados</a></h6>
<div class="html5itemGroup row row-eq-height wrap4 margin-digitalizados">
<div class="html5itemGroup carouselBox">
<script>
$(document).ready(function () {
$('#multiCarouselGeneric_3894267').find('.item').first().addClass('active');
}); multiCarrousel("multiCarouselGeneric_3894267", 2);
function multiCarrousel(elementId, initValue) {
$('#' + elementId + '[data-type="multi"] .item').each(function() {
var next = $(this).next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
for (var i = 0; i < initValue; i++) {
next = next.next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
}
});
}
</script>
<div class="widget nextCode html widget-editable viziwyg-section-101 inpage-widget-3894270">
