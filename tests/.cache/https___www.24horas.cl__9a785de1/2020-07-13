<h6>Cazadores de Fakes</h6>
<div class="html5itemGroup row row-eq-height wrap4 margin-cazadores">
<div class="html5itemGroup carouselBox">
<script>
$(document).ready(function () {
$('#multiCarouselGeneric_3888060').find('.item').first().addClass('active');
}); multiCarrousel("multiCarouselGeneric_3888060", 2);
function multiCarrousel(elementId, initValue) {
$('#' + elementId + '[data-type="multi"] .item').each(function() {
var next = $(this).next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
for (var i = 0; i < initValue; i++) {
next = next.next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
}
});
}
</script>
