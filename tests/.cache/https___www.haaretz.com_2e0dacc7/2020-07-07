  $(document).ready(function() {
    $('.footer__nav > li > a:contains("Cancellation of Digital Subscriptions")').click(function(e) {
      
      e.preventDefault();
      var options = {
        'labelledBy': 'cancelSubTitle',
          'onShowFocus': 'a',
          'type': 'modal'
      }
      
      $('#cancel-subscription-popup').showDialog(options);
    });
