<script>
$(function(){
	$('a[href=#pagetop]').click(function(){ $('html, body').animate({scrollTop:0}, 'fast');	return false; });
	$(".user-search-form").submit(function(){ user_search(); return false; });
	$(".uf").submit(function(){ $(".ub").attr("disabled","disabled").css("color","#aaa"); });
    $('section.slidemenu-box a[href^="#"]:not([href$="#"])').click(function() {
        $('body, html').scrollTop($($(this).attr('href')).offset().top); menu.slideClose(); return false;
	});

	add_twbutton();
	$(".ub").css("color","#000");

	c_id = "";
	c_str = document.cookie+";";
	start = c_str.indexOf("update_id=");
	if (start != -1){
		end = c_str.indexOf(";", start);
		c_id = c_str.substring(start+10, end);
		if(c_id=="haiku_capsule") return;
	}

	exp=new Date(); exp.setTime(exp.getTime()+1000*60*5);
	document.cookie = "update_id=haiku_capsule; expires=" + exp.toGMTString();

	url = "https://twilog.org/j-check-update.rb?c=156082998&l=1282280617799897093&s=700703";
	$.ajax({
		dataType : "jsonp",
		url: url,
		success: function(data) {
			if(data.tw<0) return;
			if(data.tw==0) { $(".ub").css("color","#aaa").val("情報は最新です"); return; }
			$("#info").slideDown();
		}
	});


});

function user_search(){
	var user_id = $("#user-search-form input[type=text]").val() || $("#user-search-form-pc input[type=text]").val();

	user_id = user_id.replace(/^[ ]+|[ ]+$/g, "");
	if(!user_id) {
		alert("Twitter IDを入力してください"); return;
	}else if(!user_id.match(/^@?[a-zA-Z0-9_]+$/)) {
		alert("不正なTwitter IDです"); return;
	}

	user_id = user_id.replace("@","");
	location.href = "https://twilog.org/"+user_id;
}
function word_search(dv){
	var param = {}
	param["word"] = encodeURIComponent($("#wsearch-"+dv+" input[name='word']").val());
	param["ao"] = ($("#wsearch-"+dv+" .ao-a").is(":checked")) ? "a" : "o";
	
	if(!param["word"]) { alert("検索ワードを入力してください"); return; }

	var ar = [];
	for(k in param){
		if(param[k]=="") continue;
		ar.push(k + "=" + param[k]);
	}
	location.href = "https://twilog.org/haiku_capsule/search?" + ar.join("&");
}
function show_hidden_tw(){ $(".button-more").hide(); $(".hidden-tw").fadeIn(); }
function change_calendar(param){ $("#side-calendar").load("https://twilog.org/j-calendar.rb?"+param); }


var srcbtn_date = false;

function show_srcbtn(date){

	if(!srcbtn_date){
		var str = "<p class=\"mb5\">ソースを取得</p>";
		str += "<p class=\"bt-src bt-common\"><a href=\"javascript:show_srcbox('')\">div・p</a></p>";
		str += "<p class=\"bt-src bt-common\"><a href=\"javascript:show_srcbox('ul')\">ul・li</a></p>";
		str += "<p class=\"bt-src bt-common\"><a href=\"javascript:show_srcbox('p')\">p</a></p>";
		$("#s"+date).html(str).fadeIn("fast"); srcbtn_date = date;;
	} else {
		if(srcbtn_date==date){
			$("#s"+date).fadeOut("fast"); srcbtn_date = false;
		} else {
			$("#s"+srcbtn_date).fadeOut("fast"); $("#s"+date).fadeIn("fast"); srcbtn_date = date;
		}
	}

}

function show_srcbox(type){

	var date = srcbtn_date;
	$("#s"+date).fadeOut("fast"); srcbtn_date = false;

	$("#source-box").fadeOut("fast",function(){
		$("#source-box").html("<img src='https://twilog.org/img-dir/r/ajax-loader.gif' class='mt50'>")
	    wx = $(window).scrollLeft() + Math.floor(($(window).width() - $('#source-box').outerWidth()) / 2);
	    wy = $(window).scrollTop() + Math.floor(($(window).height() - $('#source-box').outerHeight()) / 2);
	    $("#source-box").css({ "top": wy, "left": wx }).fadeIn("fast");

		$.ajax({
			url : "https://twilog.org/j-source.rb?word=" ,
			data : { "id":"haiku_capsule", "date":date, "cate":"", "param":"desc-", "type":type },
			success : get_source_success,
			error : function(){
				alert("Error!!"); close_srcbox();
			}

		});
	});
}

function get_source_success(src){ $("#source-box").html(src); }
function close_srcbox(){ $("#source-box").fadeOut("fast"); }

function add_twbutton(){
	$("article.tl-tweet").each(function(){

		var id = $(this).attr("id")
		if(id){
			id = id.substr(2);
			var p_elem = $("<p/>").attr("class","tw-buttons");
			var a_elem = $("<a/>").html("&nbsp;").attr({"href":"javascript:void(0);","class":"tb-rep","title":"返信"}).click({url:"https://twitter.com/intent/tweet?in_reply_to="+id},open_window);
			$(p_elem).append(a_elem);
			a_elem = $("<a/>").html("&nbsp;").attr({"href":"javascript:void(0);","class":"tb-rt","title":"リツイート"}).click({url:"https://twitter.com/intent/retweet?tweet_id="+id},open_window);
			$(p_elem).append(a_elem);
			a_elem = $("<a/>").html("&nbsp;").attr({"href":"javascript:void(0);","class":"tb-fav","title":"いいね"}).click({url:"https://twitter.com/intent/favorite?tweet_id="+id},open_window);
			$(p_elem).append(a_elem);
			$(this).append(p_elem);
		}
	});
}


function open_window(e){
	var ww=550; var hh=520;
    var x = (screen.width - ww) / 2; var y = (screen.height - hh) / 2;
    open(e.data.url, "childWindow", "width="+ww+",height="+hh+",left="+x+",top="+y);
}



</script>

