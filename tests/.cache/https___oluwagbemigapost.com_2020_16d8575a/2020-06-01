        <div id="content" class="site-content">
	              
	<div id="primary" class="content-area">
		<main id="main" class="site-main">

		
			<header class="page-header">
				<h1 class="page-title">Year: 2020</h1>			</header><!-- .page-header -->

			<div class="row"><div id="post-24306" class="post-24306 post type-post status-publish format-standard has-post-thumbnail hentry category-news" class="col-xs-12 col-sm-12 col-md-12">
	<div class="mp-full-widget-wrap clearfix">
	    <div class="thumb-post">
	   	        <div class="col-md-6 col-xs-12 col-sm-6 gtl-content-image">
	             <figure class="post-img ">
	            <img width="618" height="412" src="https://oluwagbemigapost.com/wp-content/uploads/2020/06/Okotie-e1590969050421.jpg" class="attachment-thumb size-thumb wp-post-image" alt="" srcset="https://oluwagbemigapost.com/wp-content/uploads/2020/06/Okotie-e1590969050421.jpg 750w, https://oluwagbemigapost.com/wp-content/uploads/2020/06/Okotie-e1590969050421-618x412.jpg 618w, https://oluwagbemigapost.com/wp-content/uploads/2020/06/Okotie-e1590969050421-640x427.jpg 640w" sizes="(max-width: 618px) 100vw, 618px" />	        </figure>   
                              
		</div>
		
		

		<div class="col-md-6 col-xs-12 col-sm-6">
