    $('#nmLeadStoryThumb')

    .css({
       'width':'175px',
       'height':'146px',
       'border-left':'6px solid #AA64AA'
     })

    .imgLiquid({
       fill: true,
       horizontalAlign: "center",
       verticalAlign: "top"
    }); 


