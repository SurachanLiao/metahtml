<script>
var corona=getCookie("corona");
if(corona){
	$("#corona-div").hide();
}
$(".crossbutton").click(function(){
	$("#corona-div").hide();
	if(typeof setCookie!=='undefined'){
	setCookie("corona","true",1);
	}else{
		WriteCookie("corona","true",1);	
	}
});
</script></body>	
