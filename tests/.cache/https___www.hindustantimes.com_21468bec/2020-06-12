	var preViewportDistance=$(window).height(),homeAdList=[{ad:"/3106570/HT_Desk_Home_TopNews_BS_Multisize",adsizes:["fluid"]},{ad:"/1055314/HT_HomePage_728x90_Top",adsizes:[[970,66],[980,90],[728,90],[970,90],[960,90],[950,90],[750,100],[980,120],[750,200],[930,180],[970,250]]},{ad:"/3106570/HT_Desk_Home_DontMiss_BS_Multisize",adsizes:["fluid"]},{ad:"/1055314/ht_home_middle_centre_300x250",adsizes:[300,250]},{ad:"/1055314/HT_Desk_Home_Middle_728x90",adsizes:[[970,66],[980,90],[728,90],[970,90],[960,90],[950,90],[750,100],[980,120],[750,200],[930,180],[970,250]]},{ad:"/1055314/ht_home_bottom_centre_728x90",adsizes:[[970,66],[980,90],[728,90],[970,90],[960,90],[950,90],[750,100],[980,120],[750,200],[930,180],[970,250]]},{ad:"/1055314/HT_Desk_Home_TabRep_Bottom_Multisize",adsizes:[[970,66],[980,90],[728,90],[970,90],[960,90],[950,90],[750,100],[980,120],[750,200],[930,180],[970,250]]}];function displayAd(e,i,t){var o=e.attr("id");googletag.cmd.push(function(){googletag.defineSlot(i,t,o).addService(googletag.pubads()).setCollapseEmptyDiv(!0,!0).addService(googletag.pubads()),googletag.display(o)})}$.fn.isInViewport=function(){if($(this).length>0){var e=$(this).offset().top-preViewportDistance,i=e+$(this).outerHeight()+preViewportDistance,t=$(window).scrollTop(),o=t+$(window).height();return i>t&&e<o}return!1};
