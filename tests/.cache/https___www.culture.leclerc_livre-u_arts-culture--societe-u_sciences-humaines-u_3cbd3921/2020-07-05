 checked_input(i,filtre){if($(i).children('input').is(':checked')){$(i).children('input').prop('checked',false);$(i).addClass('actived');}else{$(i).children('input').prop('checked',true);$(i).removeClass('actived');}send_modif(filtre);}function send_modif(filtre){var new_filtre="";$('#list_'+filtre+' input:checkbox:checked').each(function(){if(new_filtre=="")new_filtre=$(this).val();else{new_filtre+="%3B"+$(this).val();}});filtre=filtre.replace(/\#/g,'%23');modifgetreinit(filtre,new_filtre);}function open_filtre(e){if($(e).next().css('display')!='none'){$(e).next().hide();$(e).children().attr('class','glyphicon glyphicon-menu-down');}else if($(e).next().css('display')=='none'){$(e).next().show();$(e).children().attr('class','glyphicon glyphicon-menu-up');}}$(function(){p_min=parseInt("1");p_max=Math.ceil(parseFloat("3150"));if(p_max==""){p_max=10000;}if(p_min==""){p_min=0;}if(p_min==p_max){p_min="0";}var limite=1;p_min_flux=parseInt("1");p_max_flux=Math.ceil(parseFloat("3150"));if(p_min_flux==p_max_flux){p_max_flux+=limite;}write_value(p_min_flux,p_max_flux);j("#slider_prix").slider({range:true,min:p_min_flux,max:p_max_flux,values:[p_min,p_max],slide:function(event,ui){if(ui.values[0]+limite>ui.values[1]){return false;}else{write_value(ui.values[0],ui.values[1]);$('#span0').css('background','rgba(255, 255, 255, 0) url("https://www.culture.leclerc/themes/images/reglette/picto-reglette-prix-on.svg") 100% 100% no-repeat');$('#span1').css('background','rgba(255, 255, 255, 0) url("https://www.culture.leclerc/themes/images/reglette/picto-reglette-prix-on.svg") 100% 100% no-repeat');$('#min_slider').css('color','#1794ed');$('#max_slider').css('color','#1794ed');$('.ui-slider-range').css('background','#1794ed url("") 10% 10% no-repeat');}},change:function(event,ui){modifget("price",ui.values[0]+";"+(ui.values[1]));}});j("#slider_prix_mobile").slider({range:true,min:p_min_flux,max:p_max_flux,values:[p_min,p_max],slide:function(event,ui){if(ui.values[0]+limite>ui.values[1]){r