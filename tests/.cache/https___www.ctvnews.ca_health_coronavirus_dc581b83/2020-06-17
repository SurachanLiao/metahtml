                        <script type="text/javascript" src="https://static.ctvnews.ca/bellmedia/common/js/comscore/beacon.js" ></script>
                    <script>
  const covidDevice = {};
  covidDevice.desktop = false;
  covidDevice.mobile = false;
  var covidSlickSlider = "";
  const landingPage = (typeof covid19LandingPage === 'undefined') ? false : covid19LandingPage;
  const mobileHomePage = (typeof covid19mobileHomePage === 'undefined') ? false : covid19mobileHomePage;
  
  let slickSettingsMobile = {
      infinite: false,
      slidesToShow: 1,
      swipeToSlide: true,
      arrows: false,
      variableWidth: true,
      dots: false,
      swipe: true,
      responsive: [
      {
            breakpoint: 9999,
            settings: "unslick"
        },
        {
          breakpoint: 767
          
        }]
   };
  
   let slickSettingsMobileHomepage = {
      infinite: false,
      slidesToShow: 1,
      swipeToSlide: true,
      arrows: false,
      variableWidth: true,
      dots: false,
      swipe: true
    };
  
  (function($){
    const covidPromoElement = $(".COVID-19-Promo"); 
    
    checkDeviceWidth();
    
    if ( $('.electionArticlePrimary').length > 0) {
      covidPromoElement.addClass('COVID-19-Promo-Article');
      updateLiveIconColour(covidPromoElement);
    }
    
    if (landingPage) {
      covidPromoElement.addClass('COVID-19-Promo-Hub');
      updateLiveIconColour(covidPromoElement);
      hideCompleteCoverage(covidPromoElement); 
    }
    
    setupSlider();
      
  })(jQuery);
  
    
  function hideCompleteCoverage(promoElement) {
    promoElement.find(".complete-coverage").parent().hide();
  }
  
  function updateLiveIconColour(covidPromoElement) {
    var circleTag = covidPromoElement.find("svg circle");
    $.each(circleTag, function(i, e){
      $(e).attr("stroke","#EB0000")
        if(i === 1){  
          $(e).attr("fill","#EB0000");
        }  
    })
  }
  
  function setupSlider() {
    slickifyCovidPromo();
    $(window).resize(function() {
      checkDeviceWidth();
      slickifyCovidPromo();
    });
  }
  
  function checkDeviceWidth(){
    if($(window).width() <= 768 || $(".mobile-header + .COVID-19-Container").length === 1){
      covidDevice.mobile = true;
      covidDevice.desktop = false;
    }
    else{
      covidDevice.mobile = false;
      covidDevice.desktop = true;
      if ($(".COVID-19-Promo").hasClass("slick-initialized")) {
        covidSlickSlider.slick("unslick");
        return covidSlickSlider.slick(slickSettingsMobile);
      }
      
    }
  }
  
  function slickifyCovidPromo() {
    
    if(covidDevice.mobile && (!$(".COVID-19-Promo").hasClass("slick-initialized"))){
      
      let slickSettings = (mobileHomePage) ? slickSettingsMobileHomepage : slickSettingsMobile;
      covidSlickSlider = $(".COVID-19-Promo").slick(slickSettings);
    }
    
  }
  
  
  
</script>    
