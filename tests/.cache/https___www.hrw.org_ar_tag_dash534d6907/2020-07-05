<script>window.dataLayer = window.dataLayer || []; window.dataLayer.push({"drupalLanguage":"en","drupalCountry":"","siteName":"Human Rights Watch","entityType":"taxonomy_term","entityBundle":"tags","entityId":"8614","entityTitle":"\u062f\u0627\u0639\u0634","entityTaxonomy":{"tags":{"8614":"\u062f\u0627\u0639\u0634"}},"userUid":0});</script>
