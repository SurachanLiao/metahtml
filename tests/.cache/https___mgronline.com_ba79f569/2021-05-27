<script language="javascript" type="text/javascript">
    (function() {
        var detectInterval = null;
        detectInterval = setInterval(function() {
            var check_ = false;
            if ((typeof $ === 'function')) {
                if (typeof $.browser == 'object') {
                    if (typeof $.fn.slick === 'function') { check_ = true; }
                }
            }
            if (check_ && (typeof Factory === 'function') && (typeof PMesssage === 'function')
                && (typeof Factory === 'function') && (typeof IEWindowResize === 'function')
                && (typeof DeviceSizeView === 'function')) {
                clearInterval(detectInterval);
                detectInterval = null;
                
                $(document).ready(function() {
                    var properties_ = {
                        resize_: {
                            timer_: null,
                            delay_: 250,
                            size_: {
                                width_: 0,
                                height_: 0
                            }
                        }
                    };
                    var background_ = {"xs":{"height":"260","url":"https:\/\/pordee.com\/assets\/img-mgr\/cover\/320x650editline.png"},"xsm":{"height":"310","url":"https:\/\/pordee.com\/assets\/img-mgr\/cover\/576x650editline.png"},"sm":{"height":"300","url":"https:\/\/pordee.com\/assets\/img-mgr\/cover\/768x600editline.png"},"md":{"height":"260","url":"https:\/\/pordee.com\/assets\/img-mgr\/cover\/992x600editline.png"},"lg":{"height":"200","url":"https:\/\/pordee.com\/assets\/img-mgr\/cover\/1200x600editline.png"}};
                    background_.xs.width = 320;
                    background_.xsm.width = 576;
                    background_.sm.width = 768;
                    background_.md.width = 992;
                    background_.lg.width = 1200;
                    var factoryUtil = new Factory();
                    var globalProperties = factoryUtil.getGlobalProperties();
                    var language = globalProperties.language;
                    var pMgsLib = new PMesssage();
                    var deviceSizeViewLib = new DeviceSizeView({item_: $('div#p-device-size-view')});
                    
                    // console.log('Asian Games initial');
                    function initial() {
                        var browserLib = $.browser;
                        
                        var containerItem = $('article.pordee-shops');
                        var listSlideItem = $('ul.slider', containerItem);
                        if (listSlideItem.length == 1) {
                            listSlideItem.on('init', function(event, slick) {
                                // console.log('init');
                                $(this).parent().addClass('slick-slide-init');
                                /**** if (browserLib.name == 'msie') {
                                    $('button.slide-btn', $(this)).css('z-index', 1);
                                } */
                                var item_ = $('div.info-item', $(this));
                                var _height = -1;
                                item_.each(function() {
                                    var height_ = $(this).height();
                                    if (height_> _height) {
                                        _height = height_;
                                    }
                                });
                                item_.height(_height);
                            });
                            listSlideItem.slick({
                                // dots: true,
                                infinite: true,
                                // centerMode: true,
                                slidesToShow: 2,
                                slidesToScroll: 1,
                                autoplay: true,
                                autoplaySpeed: 5000,
                                variableWidth: true,
                                pauseOnHover: true,
                                // arrows: false,
                                prevArrow: '<button type="button" class="btn btn-primary btn-sm custom slide-btn prev-btn" title="' + pMgsLib.msg({language: language, msgKey: 'PREVIOUS'}) + '"><span class="glyphicon glyphicon-menu-left"></span></button>',
                                nextArrow: '<button type="button" class="btn btn-primary btn-sm custom slide-btn next-btn" title="' + pMgsLib.msg({language: language, msgKey: 'NEXT'}) + '"><span class="glyphicon glyphicon-menu-right"></span></button>',
                                responsive: [
                                    /* {
                                        breakpoint: 1200,
                                        settings: {
                                            slidesToShow: 6,
                                            slidesToScroll: 3
                                        }
                                    }, */
                                    {
                                        breakpoint: 99999,
                                        settings: {
                                            slidesToShow: 6,
                                            slidesToScroll: 3
                                        }
                                    },
                                    {
                                        breakpoint: 1201,
                                        settings: {
                                            slidesToShow: 6,
                                            slidesToScroll: 3
                                        }
                                    },
                                    {
                                        breakpoint: 1025,
                                        settings: {
                                            slidesToShow: 5,
                                            slidesToScroll: 2
                                        }
                                    },
                                    {
                                        breakpoint: 769,
                                        settings: {
                                            slidesToShow: 4,
                                            slidesToScroll: 2
                                        }
                                    },
                                    {
                                        breakpoint: 577,
                                        settings: {
                                            slidesToShow: 4,
                                            slidesToScroll: 1
                                        }
                                    },
                                    {
                                        breakpoint: 576,
                                        settings: {
                                            slidesToShow: 2,
                                            slidesToScroll: 1
                                        }
                                    }
                                ]
                            });
                        }
                        
                        var ieWindowResizeLib = null;
                        function resizeTimer(_argument) {
                            var resize_ = properties_.resize_;
                            if (resize_.timer_ != null) {
                                clearTimeout(resize_.timer_);
                                resize_.timer_ = null;
                            }
                            if (_argument != undefined) {
                                if (_argument.start_) {
                                    resize_.timer_ = setTimeout(function() {
                                        resize_.timer_ = null;
                                        if (_argument.beforeResize) { _resize(); }
                                        else { resize(); }
                                    }, resize_.delay_);
                                }
                            }
                        }

                        function _resize() {
                            resizeTimer();
                            var resize_ = properties_.resize_;
                            var window_ = $(window);
                            var windowSize = {width_: window_.width(), height_: window_.height()};
                            // console.log('Home resize ' + $(window).width());
                            if ((resize_.size_.width_ != windowSize.width_) || (resize_.size_.height_ != windowSize.height_)) {
                                if (resize_.size_.width_ != windowSize.width_) {
                                    deviceSizeViewLib.execute();
                                    var deviceView = deviceSizeViewLib.view2();
                                    var bg_ = background_[deviceView];
                                    // console.log(bg_);
                                    var item_ = $('div.pordee-shops-container', containerItem);
                                    var _height = item_.children().eq(0).outerHeight() - 10;
                                    $('a.pordee-link', item_).height(_height);
                                    var height_ = (Math.round((item_.outerWidth() * bg_.height) / bg_.width)) + _height;
                                    // console.log(height_ + ' ' + windowSize.width_);
                                    item_.height(height_).css('background-image', "url(" + bg_.url + ")");
                                }
                                resizeTimer({start_: true});
                            }
                        }
                        _resize();

                        $(window).on('resize orientationchange', function() {
                            if (ieWindowResizeLib != null) {
                                ieWindowResizeLib.stop();
                                ieWindowResizeLib = null;
                            }
                            resizeTimer({start_: true, beforeResize: true});
                        });

                        if (browserLib.name == 'msie') {
                            ieWindowResizeLib = new IEWindowResize({callBack: _resize});
                            ieWindowResizeLib.start();
                        }

                        function resize() {
                            resizeTimer();
                            var resize_ = properties_.resize_;
                            var window_ = $(window);
                            var windowSize = {width_: window_.width(), height_: window_.height()};
                            if ((resize_.size_.width_ != windowSize.width_) || (resize_.size_.height_ != windowSize.height_)) {
                                if (resize_.size_.width_ != windowSize.width_) {
                                    // deviceSizeViewLib.execute();
                                    resize_.size_.width_ = windowSize.width_;
                                }
                                if (resize_.size_.height_ != windowSize.height_) {
                                    resize_.size_.height_ = windowSize.height_;
                                    // console.log(windowSize.height_);
                                }
                            }
                        }
                        // resize();
                    } // end function initial
                    
                    factoryUtil.readyInitial({
                        item_: $('div.image-guide-ready'),
                        callBack: function() { initial(); }
                    });
                });
            }
        }, 250);
    })();
