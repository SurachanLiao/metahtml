       border-top-left-radius: 5px;
    }
    
    @media (min-width: 360px) {
        .pordee-shops .slick-slide-container .slider .slide-btn.prev-btn {
            left: -16px;
        }
        
        .pordee-shops .slick-slide-container .slider .slide-btn.next-btn {
            right: -15px;
        }
