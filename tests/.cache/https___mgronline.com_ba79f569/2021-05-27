        margin-top: -28px; /* à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font 14-02-2563 12:09 */
    }

    /* à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font 14-02-2563 12:09 */
    html.m-gfs-c-1 .pordee-shops .slick-slide-container .slider .slide-btn,
    body.m-gfs-c-1 .pordee-shops .slick-slide-container .slider .slide-btn {
        margin-top: -29px;
    }

    .pordee-shops .slick-slide-container .slider .slide-btn.btn-primary.custom.slide-btn {
        /* background-color: transparent; */
        background-color: rgba(0, 0, 0, 0.2);
        border: 0px;
        padding-left: 5px;
        padding-right: 5px;
    }
    
    .pordee-shops .slick-slide-container .slider .slide-btn.btn-primary.custom.slide-btn:hover,
    .pordee-shops .slick-slide-container .slider .slide-btn.btn-primary.custom.slide-btn:active {
        background-color: rgba(0, 0, 0, 0.4);
    }

    .pordee-shops .slick-slide-container .slider .slide-btn.btn-primary.custom.slide-btn:active {
        -moz-box-shadow: none;
        -webkit-box-shadow: none;
        box-shadow: none;
    }

    .pordee-shops .slick-slide-container .slider .slide-btn.prev-btn {
       left: -12px;
       z-index: 1;
       -webkit-border-top-right-radius: 5px;
       -moz-border-top-right-radius: 5px;
       border-top-right-radius: 5px;
       -webkit-border-bottom-right-radius: 5px;
       -moz-border-bottom-right-radius: 5px;
       border-top-right-radius: 5px;
    }

    .pordee-shops .slick-slide-container .slider .slide-btn.next-btn {
        right: -10px;
        -webkit-border-top-left-radius: 5px;
       -moz-border-top-left-radius: 5px;
       border-top-left-radius: 5px;
       -webkit-border-bottom-left-radius: 5px;
