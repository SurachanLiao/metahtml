\n\n\u0645\u0631\u0632\u0647\u0627\u06cc \u0645\u062c\u0627\u0631\u0633\u062a\u0627\u0646 \u0628\u0627 \u0631\u0648\u0645\u0627\u0646\u06cc \u0628\u0627\u0632 \u0627\u0633\u062a \u0648 \u0627\u0632 \u0631\u0648\u0632 \u0686\u0647\u0627\u0631\u0634\u0646\u0628\u0647 \u06f2\u06f7 \u0645\u0647 \u0645\u0631\u0632\u0647\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0628\u0627 \u0627\u0633\u0644\u0648\u0648\u0627\u06a9\u06cc \u0648 \u062c\u0645\u0647\u0648\u0631\u06cc \u0686\u06a9 \u0646\u06cc\u0632 \u0628\u0627 \u062f\u0631 \u0646\u0638\u0631\u06af\u0631\u0641\u062a\u0646 \u0645\u062d\u062f\u0648\u062f\u06cc\u062a\u200c\u0647\u0627\u06cc\u06cc \u0628\u0627\u0632 \u06af\u0634\u0627\u06cc\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a. \n\n\u0627\u06cc\u0633\u0644\u0646\u062f \n\n\u0627\u06cc\u0633\u0644\u0646\u062f \u0642\u0631\u0627\u0631 \u0627\u0633\u062a \u0645\u0631\u0632\u0647\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u0627\u0632 \u0631\u0648\u0632 \u067e\u0627\u0646\u0632\u062f\u0647\u0645 \u0645\u0627\u0647 \u0698\u0648\u0626\u0646 \u0628\u0627\u0632\u06af\u0634\u0627\u06cc\u06cc \u06a9\u0646\u062f. \n\n\u062c\u0647\u0627\u0646\u06af\u0631\u062f\u0627\u0646 \u067e\u0633 \u0627\u0632 \u0648\u0631\u0648\u062f \u0628\u0647 \u0627\u06cc\u0633\u0644\u0646\u062f \u0645\u0648\u0631\u062f \u0622\u0632\u0645\u0627\u06cc\u0634 \u0642\u0631\u0627\u0631 \u0645\u06cc\u200c\u06af\u06cc\u0631\u0646\u062f. \u0646\u062a\u06cc\u062c\u0647 \u0622\u0632\u0645\u0627\u06cc\u0634 \u0627\u0646\u062c\u0627\u0645 \u0634\u062f\u0647 \u0633\u0627\u0639\u0627\u062a\u06cc \u0628\u0639\u062f \u0627\u0632 \u0637\u0631\u06cc\u0642 \u067e\u06cc\u0627\u0645 \u062a\u0644\u0641\u0646\u06cc \u0648 \u0627\u067e\u0644\u06cc\u06a9\u06cc\u0634\u0646\u200c\u0647\u0627\u06cc \u0645\u062e\u0635\u0648\u0635 \u0628\u0647 \u0627\u0637\u0644\u0627\u0639 \u062a\u0648\u0631\u06cc\u0633\u062a\u200c\u0647\u0627 \u062e\u0648\u0627\u0647\u062f \u0631\u0633\u06cc\u062f. \n\n\u0627\u06cc\u0631\u0644\u0646\u0