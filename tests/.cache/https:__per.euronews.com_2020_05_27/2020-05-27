\u0633\u06cc\u0627\u0631\u06cc \u0627\u0632 \u06a9\u0633\u0628 \u0648 \u06a9\u0627\u0631\u0647\u0627 \u0648 \u0645\u0634\u0627\u063a\u0644 \u0631\u0627 \u062f\u0631 \u0645\u0639\u0631\u0636 \u062a\u0647\u062f\u06cc\u062f \u062c\u062f\u06cc \u0642\u0631\u0627\u0631 \u062f\u0627\u062f\u0647 \u0648 \u0628\u0627 \u0648\u062c\u0648\u062f \u0628\u0627\u0632\u06af\u0634\u0627\u06cc\u06cc \u062a\u062f\u0631\u06cc\u062c\u06cc \u0641\u0639\u0627\u0644\u06cc\u062a\u200c\u0647\u0627\u06cc \u0627\u0642\u062a\u0635\u0627\u062f\u06cc\u060c \u0631\u0648\u0646\u0642 \u062f\u0648\u0628\u0627\u0631\u0647 \u0622\u0646\u0647\u0627 \u0646\u06cc\u0627\u0632\u0645\u0646\u062f \u0627\u0639\u0637\u0627\u06cc \u06a9\u0645\u06a9\u200c\u0647\u0627\u06cc \u0645\u0627\u0644\u06cc \u062f\u0631 \u0628\u0633\u062a\u0631 \u0632\u0645\u0627\u0646\u06cc \u0645\u06cc\u0627\u0646\u200c\u0645\u062f\u062a \u0627\u0633\u062a.&lt;\/p&gt;\n&lt;p&gt;\u06a9\u0645\u06cc\u0633\u06cc\u0648\u0646 \u0627\u0631\u0648\u067e\u0627 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0628\u0627\u0632\u0648\u06cc \u0627\u062c\u0631\u0627\u06cc\u06cc \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u067e\u06cc\u0634 \u062a\u0631 \u0645\u0648\u0641\u0642 \u0634\u062f\u0647 \u0628\u0648\u062f \u0628\u0627 \u062d\u0645\u0627\u06cc\u062a \u0622\u0644\u0645\u0627\u0646 \u0648 \u0641\u0631\u0627\u0646\u0633\u0647 \u0637\u0631\u062d \u06f5\u06f0\u06f0 \u0645\u06cc\u0644\u06cc\u0627\u0631\u062f \u06cc\u0648\u0631\u0648\u06cc\u06cc \u0627\u0646\u062a\u0634\u0627\u0631 \u0627\u0648\u0631\u0627\u0642 \u0642\u0631\u0636\u0647 \u0645\u0634\u062a\u0631\u06a9 \u0627\u0631\u0648\u067e\u0627\u06cc\u06cc \u0631\u0627 \u0628\u0631\u0627\u06cc \u062d\u0644 \u0645\u0634\u06a9\u0644\u0627\u062a \u0627\u0642\u062a\u0635\u0627\u062f\u06cc \u0627\u0639\u0636\u0627\u06cc \u0628\u062d\u0631\u0627\u0646\u200c\u0632\u062f\u0647\u060c \u0628\u0647 \u067e\u06cc\u0634 \u0628\u0628\u0631\u062f \u0648\u0644\u06cc \u067e\u06cc\u0634\u0646\u0647\u0627\u062f \u0646\u0647\u0627\u06cc\u06cc \u062e\u0648\u062f \u0631\u0627 \u0628\u0627 \u062a\u0631\u06a9\u06cc\u0628\u06cc \u0627\u0632 \u06f2\u06f5\u06f0 \u0645\u06cc\u0644\u06cc\u0627\u0631\u062f \u06cc\u0648\u0631\u0648 \u0648\u0627\u0645 \u0648 \u06f5\u06f0\u06f0 \u0645\u06cc\u0644\u06cc\u0627\u0631\u062f \u06cc\u0648\u0631\u0648 \u06a9\u0645\u06a9 \u0645\u0627\u0644\u06cc \u0628\u0647 \u06f7\u06f5\u06f0 \u0645\u06cc\u0644\u06cc\u0627\u0631\u062f \u06cc\u0648\u0631\u0648 \u0627\u0641\u0632\u0627\u06cc\u0634 \u062f\u0647\u062f.&lt;\/p&gt;\n&lt;div\n  data-stories-id=\&quot;4692986,4706822\&quot;\n  data-event=\&quot;widget_related\&quot;\n  class=\&quot;widget widget--type-related widget--size-fullwidth widget--align-center\&quot;&gt;\n  &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n    &lt;div class=\&quot;widget__ratio widget__ratio--auto\&quot;&gt;\n      &lt;div class=\&quot;widget__contents\&quot;&gt;\n          &lt;ul class=\&quot;widget__related_list\&quot;&gt;&lt;li class=\&quot;widget__related_listItem\&quot;&gt;      &lt;a href=\&quot;https:\/\/per.euronews.com\/2020\/05\/20\/coronavirus-recession-european-commission-plan-without-convergence\&quot;&gt;\u0633\u0647 \u062f\u0633\u062a\u06af\u06cc \u062f\u0631 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0628\u0631\u0627\u06cc \u06af\u0630\u0631 \u0627\u0632 \u0631\u06a9\u0648\u062f \u06a9\u0631\u0648\u0646\u0627&lt;\/a&gt;&lt;\/li&gt;&lt;li class=\&quot;widget__related_listItem\&quot;&gt;      &lt;a href=\&quot;https:\/\/per.euronews.com\/2020\/05\/27\/eu-commission-recovery-plan-coronavirus-economic-impacts\&quot;&gt;\u0637\u0631\u062d \u0645\u0627\u0644\u06cc \u00ab\u0646\u0633\u0644 \u0622\u06cc\u0646\u062f\u0647 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627\u00bb \u0631\u0648\u0646\u0645\u0627\u06cc\u06cc \u0634\u062f&lt;\/a&gt;&lt;\/li&gt;&lt;\/ul&gt;\n      &lt;\/div&gt;\n    &lt;\/div&gt;\n  &lt;\/div&gt;\n&lt;\/div&gt;\n\n\n&l