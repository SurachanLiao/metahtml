u06cc\u0627 \u0633\u0648\u0645\u06cc\u0646 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0628\u0632\u0631\u06af \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u0628\u0648\u062f\u0647\u200c\u0627\u0646\u062f. \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0627\u06cc\u0646 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u062f\u0631 \u0645\u062c\u0645\u0648\u0639 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f9 \u0645\u06cc\u0644\u0627\u062f\u06cc \u06f5\u06f7\u06f1 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0628\u0627\u0631 \u062f\u0631 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u0628\u0647 \u062c\u0633\u062a\u062c\u0648\u06cc \u0627\u0641\u0631\u0627\u062f \u06cc\u0627 \u06a9\u0627\u0644\u0627\u0647\u0627\u06cc \u0633\u0631\u0642\u062a \u0634\u062f\u0647 \u067e\u0631\u062f\u0627\u062e\u062a\u0647\u200c\u0627\u0646\u062f. \n\n\u0622\u0644\u0645\u0627\u0646\u060c \u0641\u0631\u0627\u0646\u0633\u0647 \u0648 \u0647\u0644\u0646\u062f \u0627\u0632 \u062c\u0645\u0644\u0647 \u0645\u062e\u0627\u0644\u0641\u0627\u0646 \u062a\u062f\u0627\u0648\u0645 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u067e\u0633 \u0627\u0632 \u0628\u0631\u06a9\u0633\u06cc\u062a \u0647\u0633\u062a\u0646\u062f. \n\n&quot;,&quot;htmlText&quot;:&quot;&lt;p&gt;\u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0628\u0647 \u0632\u0648\u062f\u06cc \u0628\u0647 \u0627\u0645\u06a9\u0627\u0646 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0628\u0647 \u0633\u06cc\u0633\u062a\u0645 \u0627\u0637\u0644\u0627\u0639\u0627\u062a\u06cc \u0634\u06cc\u0646\u06af\u0646 (SIS) \u067e\u0627\u06cc\u0627\u0646 \u0645\u06cc\u200c\u062f\u0647\u062f. \u0627\u06cc\u0646 \u062e\u0628\u0631 \u0627\u0632 \u0633\u0648\u06cc \u0628\u0631\u062e\u06cc \u0645\u0646\u0627\u0628\u0639 \u0622\u06af\u0627\u0647 \u062f\u0631 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0628\u0647 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u06af\u0627\u0631\u062f\u06cc\u0646 \u0627\u0639\u0644\u0627\u0645 \u0634\u062f\u0647 \u0627\u0633\u062a.&lt;\/p&gt;\n&lt;p&gt;\u0633\u06cc\u0633\u062a\u0645 \u0627\u0637\u0644\u0627\u0639\u0627\u062a\u06cc \u0634\u06cc\u0646\u06af\u0646\u060c \u06cc\u06a9 \u067e\u0627\u06cc\u06af\u0627\u0647 \u062f\u0627\u062f\u0647 \u062f\u0631 \u0633\u0637\u062d \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0627\u0633\u062a \u06a9\u0647 \u0627\u0632 \u0645\u0633\u06cc\u0631 \u0622\u0646 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0634\u0646\u06af\u0646 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0645\u06cc\u0644\u06cc\u0648\u0646\u200c\u0647\u0627 \u062f\u0627\u062f\u06c0 \u0645\u062e\u062a\u0644\u0641 \u062f\u0631\u0628\u0627\u0631\u0647 \u0645\u0638\u0646\u0648\u0646\u06cc\u0646 \u062c\u0646\u0627\u06cc\u06cc \u0631\u0627 \u0628\u0627 \u0647\u062f\u0641 \u06a9\u0645\u06a9 \u0628\u0647 \u067e\u0644\u06cc\u0633 \u062f\u0631 \u0633\u0631\u0627\u0633\u0631 \u0627\u06cc\u0646 \u0642\u0627\u0631\u0647\u060c \u0628\u0647 \u0627\u0634\u062a\u0631\u0627\u06a9 \u0628\u06af\u0630\u0627\u0631\u0646\u062f.&lt;\/p&gt;\n&lt;p&gt;\u0645\u0648\u0636\u0648\u0639 \u0627\u06cc\u0646 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u0639\u0645\u062f\u062a\u0627 \u062f\u0631\u0628\u0627\u0631\u06c0 \u062c\u0631\u0627\u06cc\u0645 \u0645\u0631\u062a\u0628\u0637 \u0628\u0627 \u0645\u0647\u0627\u062c\u0631\u062a\u060c \u0627\u0641\u0631\u0627\u062f \u0645\u0645\u0646\u0648\u0639 \u0627\u0644\u062e\u0631\u0648\u062c \u06cc\u0627 \u0645\u0645\u0646\u0648\u0639 \u0627\u0644\u0648\u0631\u0648\u062f \u0628\u0647 \u0645\u0646\u0637\u0642\u0647 \u0634\u0646\u06af\u0646\u060c \u0647\u0645\u0686\u0646\u06cc\u0646 \u0627\u0637\u0644\u0627\u0639\u0627\u062a\u06cc \u062f\u0631\u0628\u0627\u0631\u06c0 \u0646\u0627\u067e\u062f\u06cc\u062f \u0634\u062f\u06af\u0627\u0646\u060c \u067e\u0631\u0648\u0646\u062f\u0647\u200c\u0647\u0627\u06cc \u0642\u0636\u0627\u06cc\u06cc \u0648 \u0633\u0627\u0632\u0645\u0627\u0646\u200c\u0647\u0627\u06cc \u062c\u0646\u0627\u06cc\u06cc \u0627\u0633\u062a.&lt;\/p&gt;\n&lt;p&gt;\u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u062f\u0631 \u0633\u0627\u0644 \u06f1\u06f9\u06f9\u06f5 \u0645\u06cc\u0644\u0627\u062f\u06cc \u062a\u0648\u0633\u0637 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc\u06cc \u0627\u06cc\u062c\u0627\u062f \u0634\u062f \u06a9\u0647 \u06a9\u0646\u062a\u0631\u0644 \u0645\u0631\u0632\u0647\u0627\u06cc \u062e\u0648\u062f \u062f\u0631 \u062f\u0627\u062e\u0644 \u0645\u0646\u0637\u0642\u0647 \u0634\u0646\u06af\u0646 \u0631\u0627 \u0644\u063a\u0648 \u06a9\u0631\u062f\u0647 \u0628\u0648\u062f\u0646\u062f. \u0628\u0647 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u06a9\u0647 \u062a\u0627 \u067e\u0627\u06cc\u0627\u0646 \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u0647\u0645\u0686\u0646\u0627\u0646 \u0639\u0636\u0648 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0627\u0633\u062a \u0646\u06cc\u0632 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f5 \u0645\u06cc\u0644\u0627\u062f\u06cc \u0627\u062c\u0627\u0632\u0647 \u062f\u0627\u062f\u0647 \u0634\u062f \u062a\u0627 \u0628\u0647 \u0628\u062e\u0634\u06cc \u0627\u0632 \u0627\u06cc\u0646 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u062f\u0633\u062a\u0631\u0633\u06cc \u067e\u06cc\u062f\u0627 \u06a9\u0646\u062f.&lt;\/p&gt;\n&lt;div\n  data-stories-id=\&quot;4483960\&quot;\n  data-event=\&quot;widget_related\&quot;\n  class=\&quot;widget widget--type-related widget--size-fullwidth widget--align-center\&quot;&gt;\n  &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n    &lt;div class=\&quot;widget__ratio widget__ratio--auto\&quot;&gt;\n      &lt;div class=\&quot;widget__contents\&quot;&gt;\n          &lt;ul class=\&quot;widget__related_list\&quot;&gt;&lt;li class=\&quot;widget__related_listItem\&quot;&gt;      &lt;a href=\&quot;https:\/\/fa.euronews.com\/2020\/02\/06\/post-brexit-uk-australia-commit-free-trade-negotiations\&quot;&gt;\u0627\u0648\u0644\u06cc\u0646 \u06af\u0627\u0645 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u067e\u0633 \u0627\u0632 \u0628\u0631\u06a9\u0633\u06cc\u062a\u061b \u062a\u0641\u0627\u0647\u0645 \u0627\u0648\u0644\u06cc\u0647 \u062a\u062c\u0627\u0631\u062a \u0622\u0632\u0627\u062f \u0628\u0627 \u0627\u0633\u062a\u0631\u0627\u0644\u06cc\u0627&lt;\/a&gt;&lt;\/li&gt;&lt;\/ul&gt;\n      &lt;\/div&gt;\n    &lt;\/div&gt;\n  &lt;\/div&gt;\n&lt;\/div&gt;\n\n\n&lt;p&gt;\u062f\u0648\u0644\u062a \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u067e\u06cc\u0634 \u0627\u0632 \u0627\u06cc\u0646 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0631\u0627 \u062a\u0647\u062f\u06cc\u062f \u06a9\u0631\u062f\u0647 \u0628\u0648\u062f \u06a9\u0647 \u062f\u0631 \u0635\u0648\u0631\u062a\u06cc \u06a9\u0647 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647\u060c \u0628\u0648\u06cc\u0698\u0647 \u062f\u0631 \u0645\u0648\u0636\u0648\u0639 \u0645\u0628\u0627\u0631\u0632\u0647 \u0628\u0627 \u062c\u0631\u0627\u06cc\u0645 \u062a\u0636\u0645\u06cc\u0646 \u0646\u0634\u0648\u062f\u060c \u0627\u0632 \u0633\u0637\u062d \u062a\u0639\u0647\u062f\u0627\u062a \u0627\u0645\u0646\u06cc\u062a\u06cc \u062e\u0648\u062f \u062f\u0631 \u0642\u0628\u0627\u0644 \u0627\u06cc\u0646 \u0628\u0644\u0648\u06a9 \u062e\u0648\u0627\u0647\u062f \u06a9\u0627\u0633\u062a. \u062a\u0647\u062f\u06cc\u062f \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u062f\u0631 \u062d\u0627\u0644\u06cc \u0645\u0637\u0631\u062d \u0634\u062f\u0647 \u06a9\u0647 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0647 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u0628\u0631\u0627\u06cc \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u062e\u0627\u0631\u062c \u0627\u0632 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0645\u0645\u0646\u0648\u0639 \u0627\u0633\u062a.&lt;\/p&gt;\n&lt;p&gt;\u0627\u0632 \u0622\u0646\u062c\u0627 \u06a9\u0647 \u067e\u0644\u06cc\u0633 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0628\u0637\u0648\u0631 \u0631\u0648\u0632\u0627\u0646\u0647 \u0627\u0632 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc\u200c\u06a9\u0646\u062f\u060c \u062f\u0631 \u0622\u062e\u0631\u06cc\u0646 \u062f\u0648\u0631 \u06af\u0641\u062a\u06af\u0648\u0647\u0627\u06cc \u0628\u0631\u06a9\u0633\u06cc\u062a \u0646\u06cc\u0632 \u0645\u0630\u0627\u06a9\u0631\u0647 \u06a9\u0646\u0646\u062f\u06af\u0627\u0646 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627\u06cc\u06cc \u0627\u0632 \u0628\u0631\u0648\u06a9\u0633\u0644 \u062e\u0648\u0627\u0633\u062a\u0647 \u0628\u0648\u062f\u0646\u062f \u062a\u0627 \u0627\u0645\u06a9\u0627\u0646 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u06cc\u0634\u062a\u0631 \u0622\u0646\u0647\u0627 \u0631\u0627 \u0628\u0647 SIS \u0641\u0631\u0627\u0647\u0645 \u06a9\u0646\u0646\u062f.&lt;\/p&gt;\n&lt;p&gt;\u06af\u0632\u0627\u0631\u0634\u200c\u0647\u0627\u06cc \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0646\u0634\u0627\u0646 \u0645\u06cc\u200c\u062f\u0647\u062f \u06a9\u0647 \u062a\u0627 \u06a9\u0646\u0648\u0646 \u067e\u0644\u06cc\u0633 \u0648 \u0645\u0631\u0632\u0628\u0627\u0646\u0627\u0646 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0633\u0648\u0645\u06cc\u0646 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0628\u0632\u0631\u06af \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u062f\u0627\u062f\u0647\u200c\u0647\u0627 \u0628\u0648\u062f\u0647\u200c\u0627\u0646\u062f. \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0627\u06cc\u0646 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u062f\u0631 \u0645\u062c\u0645\u0648\u0639 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f9 \u0645\u06cc\u0644\u0627\u062f\u06cc \u06f5\u06f7\u06f1 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0628\u0627\u0631 \u062f\u0631 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u0628\u0647 \u062c\u0633\u062a\u062c\u0648\u06cc \u0627\u0641\u0631\u0627\u062f \u06cc\u0627 \u06a9\u0627\u0644\u0627\u0647\u0627\u06cc \u0633\u0631\u0642\u062a \u0634\u062f\u0647 \u067e\u0631\u062f\u0627\u062e\u062a\u0647\u200c\u0627\u0646\u062f.&lt;\/p&gt;\n&lt;p&gt;\u0622\u0644\u0645\u0627\u0646\u060c \u0641\u0631\u0627\u0646\u0633\u0647 \u0648 \u0647\u0644\u0646\u062f \u0627\u0632 \u062c\u0645\u0644\u0647 \u0645\u062e\u0627\u0644\u0641\u0627\u0646 \u062a\u062f\u0627\u0648\u0645 \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0631\u06cc\u062a\u0627\u0646\u06cc\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647 \u067e\u0633 \u0627\u0632 \u0628\u0631\u06a9\u0633\u06cc\u062a \u0647\u0633\u062a\u0646\u062f.&lt;\/p&gt;\n&quot;,&quot;hashtag&quot;:null,&quot;createdAt&quot;:1590592553,&quot;publishedAt&quot;:1590607443,&quot;updatedAt&quot;:1590607462,&quot;expiresAt&quot;:0,&quot;canonical&quot;:&quot;https:\/\/per.euronews.com\/2020\/05\/27\/european-union-end-uk-access-schengen-information-system-sis-criminal-suspects&quot;,&quot;images&quot;:[{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/70\/73\/08\/{{w}}x{{h}}_cmsv2_251a4270-0388-52b4-80b7-5fe4737774ec-4707308.jpg&quot;,&quot;altText&quot;:&quot;Emilio Morenatti&quot;,&quot;caption&quot;:&quot;Emilio Morenatti&quot;,&quot;captionCredit&quot;:&quot;AP Photo&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;width&quot;:1200,&quot;height&quot;:675