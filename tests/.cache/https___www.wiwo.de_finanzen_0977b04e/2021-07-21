(function () {
    var appBannerConfig = {
        isIOS: /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream,
        isAndroid: navigator.userAgent.toLowerCase().indexOf("android") > -1,
        iosLink: 'https://apps.apple.com/de/app/wirtschaftswoche-nachrichten/id1339187646',
        androidLink: 'https://play.google.com/store/apps/details?id=de.wiwo.one',
        appLink: '',
        appBannerClass: 'c-app-install--active'
    };
    if((appBannerConfig.isIOS || appBannerConfig.isAndroid) && "MutationObserver" in window){
        function changeAppBannerLink(){
            if(appBannerConfig.isIOS){
                appBannerConfig.appLink = appBannerConfig.iosLink;
            }else if(appBannerConfig.isAndroid){
                appBannerConfig.appLink = appBannerConfig.androidLink;
            }
            const appLink = document.querySelector("a.js-mobile-btn");
            if(appLink){
                document.querySelector("a.js-mobile-btn").href = appBannerConfig.appLink;
            }
        }
        var elemToObserve = document.querySelector("body");
        var prevClassState = elemToObserve.classList.contains(appBannerConfig.appBannerClass);
        var observer = new MutationObserver(function(mutations) {
            mutations.forEach(function(mutation) {
                if(mutation.attributeName == "class"){
                    var currentClassState = mutation.target.classList.contains(appBannerConfig.appBannerClass);
                    if(prevClassState !== currentClassState) {
                        prevClassState = currentClassState;
                        if(currentClassState){
                            console.log("class added!");
                            changeAppBannerLink();
                            observer.disconnect();
                        }
                    }
                }
            });
        });
        observer.observe(elemToObserve, {attributes: true});
        if (document.body.classList.contains(appBannerConfig.appBannerClass)) {
            changeAppBannerLink();
        }
    }else if((appBannerConfig.isIOS || appBannerConfig.isAndroid) && !"MutationObserver" in window){
        document.body.className = document.body.className.replace(appBannerConfig.appBannerClass,'');
    }
    if(_wiwo_paywall){
        const _paywallButton = document.querySelector("div.o-paywall a.c-button.c-button--red");
        if(_paywallButton){
            const _aboShopURL = _paywallButton.getAttribute('href');
            var _serviceUri = _aboShopURL.split('?');

            var _serviceId = _serviceUri[1];
            var _serviceId = _serviceId.substr( _serviceId.lastIndexOf('/') + 1);
            _serviceId = _serviceId.replace('.html', '');

            var servicePath = 'https://www.wiwo.de';
            if(window.location.origin){
                servicePath = window.location.origin;
            }

            var new_aboShopURL = _serviceUri[0] + '?service=' + servicePath + '/' + _serviceId + '.html';
            _paywallButton.setAttribute('href', new_aboShopURL);
        }
    }
})();

