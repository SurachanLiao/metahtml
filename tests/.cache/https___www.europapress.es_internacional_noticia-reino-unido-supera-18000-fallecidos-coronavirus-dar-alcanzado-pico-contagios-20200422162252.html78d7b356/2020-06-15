    <script>
        var fondo = document.getElementById('background');

        /* Open */
        function openNav() {
            var altura = window.innerHeight;
            document.getElementById("ctl00_NavegacionFixed_div_ultnoticias").style.right = "0px";
            fondo.style.height = altura + "px";
            fondo.classList.add('fondo-menu-ultimahora');
            document.getElementById("overlay-header").style.position = "fixed";
            document.getElementById("tabs-ultimasnoticias").style.position = "fixed";
            jQuery("#badge_notification").empty();
            jQuery("#badge_notification").addClass("badge_padding_0");
            jQuery("#badge_notification").removeClass("badge_padding_notification");
            jQuery(".articulo-nuevo").addClass("fadeIn");


        }

        /* Close */
        function closeNav() {
            document.getElementById("ctl00_NavegacionFixed_div_ultnoticias").style.right = "-400px";
            fondo.style.height = "0px";
            fondo.classList.remove('fondo-menu-ultimahora');
            document.getElementById("overlay-header").style.position = "relative";
            document.getElementById("tabs-ultimasnoticias").style.position = "relative";
            document.getElementById('background').classList.remove('fondo-menu-ultimahora');
            jQuery("#badge_notification").empty();
            jQuery(".articulo-nuevo").removeClass("fadeIn");
            jQuery(".articulo.articulo-nuevo").removeClass("articulo-nuevo");

        }
    </script>

