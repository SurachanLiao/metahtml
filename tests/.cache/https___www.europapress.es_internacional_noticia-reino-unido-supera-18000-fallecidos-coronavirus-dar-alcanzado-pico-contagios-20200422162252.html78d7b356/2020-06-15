<script>

    function ready(fn) {
        if (document.readyState != 'loading') {
            fn();
        } else {
            document.addEventListener('DOMContentLoaded', fn);
        }
    }

    ready(function () {
        // Obtener streamings
        jQuery.getJSON("/ws/streaming_json.ashx", function (data) {
            jQuery.each(data.streamings, function (i, item) {

                if (item.enlazadoEnMenu) {
                    // mostrar enlace al directo
                    jQuery("#panelStreamingMenu")
                        .html('<a class="btn btn-sm btn-danger" href="' + item.url + '" title="' + item.titular +'"><span class="icon-video"></span>' + item.titular + '</div></a>')
                        .show();
                    // encoger botón abonados
                    jQuery("#AbonadosEP").hide();
                    // encoger botón últimas noticias
                    jQuery("#UltimasEP").hide();
                    // ocultar módulo rrss
                    jQuery(".social-menu").hide();
                    return;
                }

            });
        });

    });

</script>



