				function chg_big_img(num){
					var current_img = $('li#img'+num+' img').attr('src');
					var current_href = $('li#img'+num+' a').attr('href');
					if (num<3){
						$('li#big_img0 img').attr('src',current_img);
						$('li#big_img0 img').attr('alt',$('li#img'+num+' img').attr('alt'));
						$('ul#photo_layer0 li:first strong').html($('li#img'+num+' img').attr('alt'));
						$('ul#photo_layer0 li a').removeClass('on');

					}else{
						$('li#big_img1 img').attr('src',current_img);
						$('li#big_img1 img').attr('alt',$('li#img'+num+' img').attr('alt'));
						$('ul#photo_layer1 li:first strong').html($('li#img'+num+' img').attr('alt'));
						$('ul#photo_layer1 li a').removeClass('on');
					}
					$('li#img'+num+' a').addClass('on');
				}
