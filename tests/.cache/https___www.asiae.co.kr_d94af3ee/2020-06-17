						<script type="text/javascript">
						$(document).ready(function(){
							var BAN_BOX = '';
							var c_name = 'FOODBAN_BOX=';
							var arr = document.cookie.split(';');
							for(var i=0; i<arr.length; i++) {
								var c = arr[i];
								while (c.charAt(0)==' ') c = c.substring(1);
								if (c.indexOf(c_name) == 0) BAN_BOX = c.substring(c_name.length, c.length);
							}

							if(BAN_BOX!='N')
							{
								$(".foodban_box").show();
							}

							$(".foodban_close").click(function(){
								$(".foodban_box").hide();
								var today=new Date();
								today.setDate(today.getDate()+1);
								document.cookie='FOODBAN_BOX=N;path=/;expires='+today.toGMTString()+';';
							});
						});
