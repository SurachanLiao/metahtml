var href = $(location).attr('href');
var mCheck = href.indexOf('?mobile=Y');
if(mCheck == -1)
{
	var UserAgent = navigator.userAgent;
	if( !UserAgent.match(/SHW-M380K/i) && (UserAgent.match(/iPhone|iPod|iPad|Android|Windows CE|BlackBerry|Symbian|Windows Phone|webOS|Opera Mini|Opera Mobi|POLARIS|IEMobile|lgtelecom|nokia|SonyEricsson/i) != null || UserAgent.match(/LG|SAMSUNG|Samsung/) != null ))
	{
		location.href = 'https://cm.asiae.co.kr';
	}
	else
	{
		var protocol = $(location).attr('protocol');
		var host = $(location).attr('host');
		if(!protocol.match('https') && !host.match('amgcms.asiae.co.kr'))
		{
			location.href = 'https://www.asiae.co.kr';
		}
	}
}
</script>

