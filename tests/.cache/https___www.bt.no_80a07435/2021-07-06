ksjon av kjemiske produkter","tag":"2b5afdf8-6329-46fd-b2eb-2612abaa5adc"},{"item":"Produksjon av metaller","tag":"9ec2e72e-3777-4d11-a4c6-9e8c870487e3"},{"item":"Produksjon av metallvarer","tag":"b8f6d298-75c9-4ea1-bf1e-10b1d62c595a"},{"item":"Produksjon av vannkraft","tag":"13601f10-1d6e-4988-8ee1-1949f4ca8bc7"},{"item":"Produksjon av vindkraft","tag":"09a73386-ff05-43a4-8b4c-3d4f57f0b329"},{"item":"Produksjoner av maskiner og utstyr","tag":"5a874ef1-73bb-4b69-9ab0-9f159d04cb35"},{"item":"Programmering","tag":"3129819a-2ecc-4d58-b81c-b0448470a6b4"},{"item":"Programvarebransjen","tag":"5df14dfe-c69f-47cc-a834-8f4760a8b9bc"},{"item":"Psykologer","tag":"60b882c5-b457-4185-90ce-f7532037001f"},{"item":"Puber og barer","tag":"af10cf4b-5906-42c7-83ed-ff06025f835e"},{"item":"Radiokanaler","tag":"cced4798-4a3f-452d-b08f-cb61241593b4"},{"item":"Regnskap og bokføring","tag":"609f793e-a062-4460-8148-c9b914748a12"},{"item":"Reiearrangør","tag":"bcc5422b-a8d6-4695-b62e-8834fba8c03c"},{"item":"Reisebyrå","tag":"680e98ee-252d-482a-ac0c-01612a6731a3"},{"item":"Rengjøring","tag":"c9fefdb6-7404-44ae-988d-df798817010b"},{"item":"Restauranter og kafeer","tag":"b8f03183-5220-4ed5-a01e-3e9782b29fc1"},{"item":"Revisjon","tag":"9b0613cf-9f53-46f4-b43b-71d542966355"},{"item":"Rutebiltransport","tag":"190a3a0c-dc07-45e4-abf0-9f4146efcee4"},{"item":"Rutebiltransport","tag":"190a3a0c-dc07-45e4-abf0-9f4146efcee4"},{"item":"Sauehold","tag":"a52d15b6-cdb4-47c1-a5d2-5b5e687741c4"},{"item":"Scenekunst","tag":"c7c25a41-a0a8-485f-b64e-799a251629d6"},{"item":"Sikkerhetsbransjen","tag":"e5405e69-685b-4e74-b548-f25b9d800983"},{"item":"Sjokoladeproduksjon","tag":"160f5415-4c21-4baf-852f-36c48b2a3b83"},{"item":"Skipsbyging","tag":"8fa660e3-c762-40b5-856c-04fff1ae3c08"},{"item":"Skobutikker","tag":"edce822c-c092-44ae-bb99-24573ce7844c"},{"item":"Slakterbutikker","tag":"2007195a-e16e-465c-af60-a4fb2d150e58"},{"item":"Snekkerbransjen","tag":"48f6d648-6059-4542-ab49-ac293a65898c"},{"item":"Spesialiserte legetjenester","tag":"6804f24c-8071-46f6-b102-a58712b564b4"},{"item":"Sportsbutikker","tag":"71aa3efd-851f-4d38-b72c-3519e57bb321"},{"item":"Supplybåter","tag":"f28b025e-16bc-46bd-837b-a97cd8b5c683"},{"item":"Taksering","tag":"4f787c0c-3ba9-491b-b536-61b2ebd52a43"},{"item":"Tannleger","tag":"76150d2a-3736-4221-b250-7cb94e49adb7"},{"item":"Teknisk design","tag":"8e2dd62c-4936-4ecd-b39c-0a92a29204fe"},{"item":"Tekniske konsulenter","tag":"ac573099-840a-49a0-a33b-2cce66a155b1"},{"item":"Telekommunikasjon","tag":"aad54ad8-06e0-49ca-ad54-8fce6481bbde"},{"item":"Tidsskrift","tag":"88dd2934-a295-4707-8e06-22496c6ffa2e"},{"item":"Tjenesteyting","tag":"effb9aa3-0aac-4d0c-8f25-786e4eb0b771"},{"item":"Torghandel","tag":"f7e4bb2c-0115-4212-b839-19cf12e68e73"},{"item":"Treningssentre","tag":"80f7f832-3282-4b8c-9f38-60f4c362216c"},{"item":"Trykkerivirksomhet","tag":"aa5ff9ec-00f0-4b34-91be-495a2ae9de0d"},{"item":"Turistbransjen","tag":"d2d09063-db3a-4fbc-8d4a-52459031bca6"},{"item":"TV-kanaler","tag":"355a50dc-2456-4fb8-99d5-8a14aee5fd0e"},{"item":"Underholdning","tag":"766180d1-7b1d-42bc-bf31-ec43d31adf15"},{"item":"Undervisning","tag":"cbaed600-6cc5-4be0-8852-81ac085d2d87"},{"item":"Urmakere","tag":"c7c203f6-5dcb-4eb7-b170-85baf2e722b4"},{"item":"Vaktmestere","tag":"2c46daf8-c3a7-4562-83f0-912561f28d56"},{"item":"Varehandel","tag":"c361b4da-b407-4e7c-9fa1-09f0b4e9940d"},{"item":"Veibygging","tag":"c0f1ed46-2a73-499f-923b-4eefd457d1b3"},{"item":"Veterinærer","tag":"b6a89dac-d576-4459-ab97-5c79376cf0c0"},{"item":"Vin og brennevin","tag":"6317f2d7-b721-400e-a4cf-28130bbdb6ad"},{"item":"VVS-bransjen","tag":"0af4249a-3a92-4797-9541-abb9264f3911"},{"item":"Øvrig handel utenfor butikk","tag":"1f32a391-e40a-462e-bf61-b83560b2f1bd"}]
  let publicationUrl = "bt.no"

  industries = industries.sort((a, b) => {
    const industryA = a.item.toUpperCase(), industryB = b.item.toUpperCase();

    if (industryA < industryB) {
      return -1;
    }
    if (industryA > industryB) {
      return 1;
    }
    return 0;
  });

  function resultatAutocomplete(inputElement, resultatDataArray) {
      let currentItem;
      var resultatListOpen = false;

      function doResultatAutocomplete(e) {
        const eventType = e.type;
        const val = inputElement.value;
        closeList();
        if (eventType !== 'click' && !val) {
          return false;
        }
        if (eventType === 'click' && resultatListOpen) {
          closeList();
          resultatListOpen = false;
          inputElement.classList.remove("open");
          inputElement.value = '';
          return false;
        }
        currentItem = -1;

        document.getElementById('resultat-al').classList.add("flipLeft");
        document.getElementById('resultat-ar').classList.add("flipRight");
        inputElement.classList.add("open");

        container = document.createElement("div");
        container.setAttribute("id", this.id + "-autocomplete-list");
        container.className = "resultat-autocomplete-items";

        this.parentNode.appendChild(container);

        for (let i = 0; i < resultatDataArray.length; i++) {
          if ((val.length > 2 && resultatDataArray[i].item.toUpperCase().includes(val.toUpperCase())) || eventType === 'click') {
            const topCategoryRegex = /^[A-Z][.][\s]/;
            const isTopCategory = resultatDataArray[i].item.match(topCategoryRegex);
            resultatDataArray[i].item = resultatDataArray[i].item.replace(topCategoryRegex, '');
            item = document.createElement("div");
            item.innerText = `${resultatDataArray[i].item}`;
            item.className = `autocomplete-link ${isTopCategory ? 'option-header' : ''}`;
            item.dataTag = resultatDataArray[i].tag;

            item.onclick = e => {
              inputElement.value =  e.target.textContent;
              document.location.href = `https://${publicationUrl}/tag/${e.target.dataTag}`;
            }
            if (resultatDataArray[i].tag) {
              container.appendChild(item);
            }
            resultatListOpen = true;
          }
        }
      };

      inputElement.addEventListener("input", doResultatAutocomplete);
      document.getElementById('resultat-chevron').addEventListener("click", doResultatAutocomplete);

      inputElement.addEventListener("keydown", function(e) {
        var autocompleteList = document.getElementById(this.id + "-autocomplete-list");
        if (autocompleteList) autocompleteList = autocompleteList.getElementsByTagName("div");
        if (e.keyCode == 40) {
          currentItem++;
          setActive(autocompleteList);
        } else if (e.keyCode == 38) {
          currentItem--;
          setActive(autocompleteList);
        } else if (e.keyCode == 13) {
          e.preventDefault();
          if (currentItem > -1) {
            if (autocompleteList) {
              autocompleteList[currentItem].click();
            }
          }
        }
      });

      function setActive(element) {
        if (!element) return false;
        setInactive(element);
        if (currentItem >= element.length) currentItem = 0;
        if (currentItem < 0) currentItem = (element.length - 1);
        element[currentItem].classList.add("autocomplete-active");
        document.getElementById('resultat-autocomplete').value =  element[currentItem].textContent;

      }

      function setInactive(element) {
        for (let i = 0; i < element.length; i++) {
          element[i].classList.remove("autocomplete-active");
        }
      }

      function closeList(element) {
        const items = document.getElementsByClassName("resultat-autocomplete-items");
        for (let i = 0; i < items.length; i++) {
          if (element != items[i] && element != inputElement) {
            items[i].parentNode.removeChild(items[i]);
          }
        }
        document.getElementById('resultat-al').classList.remove("flipLeft");
        document.getElementById('resultat-ar').classList.remove("flipRight");
      }

    }
    resultatAutocomplete(document.getElementById("resultat-autocomplete"), industries);

    trackResultatReadAllKlick = function () {
      window.pulse('track', 'engagementEvent', {
        action: 'Click',
        object: {
          id: 'resultat-frontpage-widget:see-all-link',
          type: 'UIElement',
          elementType: 'Link',
          name: 'se alle',
          page: {
            id: 'widget',
            type: 'Frontpage',
          },
          parent: {
            id: 'resultat-frontpage-widget',
            type: 'UIElement',
            elementType: 'Widget',
          },
        },
      });
    };
    trackResultatArticleKlick = function (articleId, title) {
      window.pulse('track', 'engagementEvent', {
        action: 'Click',
        object: {
          id: 'resultat-frontpage-widget-article:'+articleId,
          type: 'UIElement',
          elementType: 'Link',
          name: title,
          page: {
            id: 'widget',
            type: 'Frontpage',
          },
          parent: {
            id: 'resultat-frontpage-widget',
            type: 'UIElement',
            elementType: 'Widget',
          },
        },
      });
    };
  </script>
</div>



