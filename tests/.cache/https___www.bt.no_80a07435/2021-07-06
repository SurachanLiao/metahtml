:"Nøstet","tag":"0d9431cd-e943-4a98-973f-f64e6a0d3a65"},{"item":"Verftet","tag":"952ff060-e706-4323-a4a4-9ce929a07fec"},{"item":"Nordnes","tag":"b8f40995-ade1-4b16-9b65-1a1861e4b8d0"},{"item":"Kronstad","tag":"26a4aff8-7a6e-4b70-845e-9d54d0ed0395"},{"item":"Fridalen","tag":"c2f6e26c-9739-4a58-9b8e-ab11fc7bcf9b"},{"item":"Slettebakken","tag":"980db789-9ed9-4a77-a8bf-bfdbac173c8f"},{"item":"Landås","tag":"cea01fb2-c89c-47b3-827a-890624fcfbdf"},{"item":"Nattland","tag":"f5628621-5229-43ac-93d8-66b96026b88d"},{"item":"Fantoft","tag":"d3990959-b8e6-427f-936a-0735780d1876"},{"item":"Minde","tag":"92bd0d24-bfaf-467a-b86a-a5bf5040f817"},{"item":"Solheim","tag":"d61c72f3-865c-4e66-b77a-c5e2f6c9c283"},{"item":"Gyldenpris","tag":"4bfcb4f0-4f87-4156-9dc0-541196fca9f6"},{"item":"Askøy kommune","tag":"2e456a9e-2176-4f28-9ca2-770bab698be6"},{"item":"Kleppestø","tag":"a5b0b313-81c4-4624-8f6e-409ea0aa51a2"},{"item":"Strusshamn","tag":"ccf0b005-d51e-47de-acd7-b8cba5b4d708"},{"item":"Follese","tag":"97fc4004-a5fb-4b73-ae5a-997e96308083"},{"item":"Hetlevik","tag":"8b36c2f8-349a-4d0a-9f1e-80338a695479"},{"item":"Florvåg","tag":"e9857976-c74f-4ad8-8d7b-ee23a39806f8"},{"item":"Erdal","tag":"8840d321-47fd-4e23-9aaa-c2ce24a3cece"},{"item":"Ask","tag":"f4fa01d5-a200-4b61-8201-adbe1ff5abfb"},{"item":"Hauglandshella","tag":"20b34f8b-d402-4c60-90be-6a29fb8277c8"},{"item":"Kjerrgarden","tag":"cee82232-fb1a-4815-9dd6-43b8c1e63319"},{"item":"Herdla","tag":"62b3c599-b533-4ffc-8f1f-6d6a6d95f04f"},{"item":"Kinn kommune","tag":"15b71e13-512d-4a4c-8000-2aa32b92e3f5"},{"item":"Måløy","tag":"9b051e31-833d-443c-baa1-c1cc2d866d72"},{"item":"Deknepollen","tag":"3e5b09e2-5fd1-4194-b6bf-0fc23d3f6708"},{"item":"Raudeberg","tag":"3c75a234-cae4-453f-bd45-4b1471ccce53"},{"item":"Bryggja","tag":"a1318011-44f3-498c-9833-52d541264a26"},{"item":"Raudeberg","tag":"3c75a234-cae4-453f-bd45-4b1471ccce53"},{"item":"Bryggja","tag":"a1318011-44f3-498c-9833-52d541264a26"},{"item":"Allmenningen","tag":"42638428-1418-48e2-9f73-838038625111"},{"item":"Silda","tag":"a6d6fabd-c87b-4c61-9cb8-7fc6759f6737"},{"item":"Barmen","tag":"bf0a3a13-f08d-41d0-bb81-f92746eb65e7"},{"item":"Husevåg","tag":"1b0ea954-2b72-464c-a477-5bd20bba1703"},{"item":"Flatraket","tag":"e1322fbe-2721-475f-9d72-28fc122caf07"},{"item":"Florø","tag":"0576e11b-691e-4359-bea2-f554d46b97fc"},{"item":"Kinn","tag":"0576e11b-691e-4359-bea2-f554d46b97fc"},{"item":"Svanøybukt","tag":"c09317b9-6a4b-4504-a18b-e121cc2fb460"},{"item":"Rognaldsvåg","tag":"6010ec6d-05ba-4e36-afaf-7282509e59d4"},{"item":"Barekstad","tag":"6cb14fc5-f310-421b-8e32-792bb21cd2dc"},{"item":"Batalden","tag":"5bdee672-319e-4bde-9fd2-ef4953b9f084"},{"item":"Sør-Skorpa","tag":"e2c642d0-0292-4273-b0db-08d893da9ad7"},{"item":"Tansøy","tag":"9d9ad013-ca8c-4a21-b278-e3ae4e57d759"},{"item":"Eikefjord","tag":"7ca45c62-8446-4f51-8176-06aac2d08b0a"},{"item":"Svortevik","tag":"9dd11357-ed0b-427f-a81e-d0b84c49e237"},{"item":"Stavang","tag":"013fa303-5817-4b6f-8f31-021f7125343b"}];
  let boligPublicationUrl = "bt.no"

  regions = regions.sort((a, b) => {
    const placeA = a.item.toUpperCase(), placeB = b.item.toUpperCase();

    if (placeA < placeB) {
      return -1;
    }
    if (placeA > placeB) {
      return 1;
    }
    return 0;
  });

  function autocomplete(inputElement, dataArray) {
      let currentItem;
      var listOpen = false;

      function doAutocomplete(e) {
        const eventType = e.type;
        const val = inputElement.value;
        closeList();
        if (eventType !== 'click' && !val) {
          return false;
        }
        if (eventType === 'click' && listOpen) {
          closeList();
          listOpen = false;
          inputElement.classList.remove("open");
          inputElement.value = '';
          return false;
        }
        currentItem = -1;

        document.getElementById('al').classList.add("flipLeft");
        document.getElementById('ar').classList.add("flipRight");
        inputElement.classList.add("open");

        container = document.createElement("div");
        container.setAttribute("id", this.id + "-autocomplete-list");
        container.className = "autocomplete-items";

        this.parentNode.appendChild(container);

        for (let i = 0; i < dataArray.length; i++) {
          if ((val.length > 2 && dataArray[i].item.toUpperCase().includes(val.toUpperCase())) || eventType === 'click') {
            item = document.createElement("div");
            item.innerText = `${dataArray[i].item}`;
            item.className = 'autocomplete-link';
            item.dataTag = dataArray[i].tag;
            item.onclick = e => {
              inputElement.value =  e.target.textContent;
              document.location.href = `https://${boligPublicationUrl}/tag/${e.target.dataTag}`;
            }
            container.appendChild(item);
            listOpen = true;
          }
        }
      };

      inputElement.addEventListener("input", doAutocomplete);
      document.getElementById('chevron').addEventListener("click", doAutocomplete);

      inputElement.addEventListener("keydown", function(e) {
        var autocompleteList = document.getElementById(this.id + "-autocomplete-list");
        if (autocompleteList) autocompleteList = autocompleteList.getElementsByTagName("div");
        if (e.keyCode == 40) {
          currentItem++;
          setActive(autocompleteList);
        } else if (e.keyCode == 38) {
          currentItem--;
          setActive(autocompleteList);
        } else if (e.keyCode == 13) {
          e.preventDefault();
          if (currentItem > -1) {
            if (autocompleteList) {
              autocompleteList[currentItem].click();
            }
          }
        }
      });

      function setActive(element) {
        if (!element) return false;
        setInactive(element);
        if (currentItem >= element.length) currentItem = 0;
        if (currentItem < 0) currentItem = (element.length - 1);
        element[currentItem].classList.add("autocomplete-active");
        document.getElementById('autocomplete').value =  element[currentItem].textContent;

      }

      function setInactive(element) {
        for (let i = 0; i < element.length; i++) {
          element[i].classList.remove("autocomplete-active");
        }
      }

      function closeList(element) {
        const items = document.getElementsByClassName("autocomplete-items");
        for (let i = 0; i < items.length; i++) {
          if (element != items[i] && element != inputElement) {
            items[i].parentNode.removeChild(items[i]);
          }
        }
        document.getElementById('al').classList.remove("flipLeft");
        document.getElementById('ar').classList.remove("flipRight");
      }

    }
    autocomplete(document.getElementById("autocomplete"), regions);
    trackBoligReadAllKlick = function () {
      window.pulse('track', 'engagementEvent', {
        action: 'Click',
        object: {
          id: 'bolig-frontpage-widget:see-all-link',
          type: 'UIElement',
          elementType: 'Link',
          name: 'se alle',
          page: {
            id: 'widget',
            type: 'Frontpage',
          },
          parent: {
            id: 'bolig-frontpage-widget',
            type: 'UIElement',
            elementType: 'Widget',
          },
        },
      });
    };
    trackBoligArticleKlick = function (articleId, title) {
      window.pulse('track', 'engagementEvent', {
        action: 'Click',
        object: {
          id: 'bolig-frontpage-widget-article:'+articleId,
          type: 'UIElement',
          elementType: 'Link',
          name: title,
          page: {
            id: 'widget',
            type: 'Frontpage',
          },
          parent: {
            id: 'bolig-frontpage-widget',
            type: 'UIElement',
            elementType: 'Widget',
          },
        },
      });
    };
  </script>
</div>



