<form name="main_search_form" id="main_weather_search_form" class="main_search_form" action="https://www.urdupoint.com/en/set_search.php" method="get" onsubmit="return check_search_box();">
<input value="" name="kwd" id="kwd" class="big_search" type="text" placeholder="Search Here.....">
<input type="submit" name="kwd_submit" id="kwd_submit" value="Search">
</form>
