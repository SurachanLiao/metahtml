<script type="text/javascript">

function check_search_box()
{
    var kwd_el  = document.forms["main_search_form"]["kwd"];
    var kwd 	= kwd_el.value;
    var kwd_len = kwd.length;
    
    if(kwd == '' || kwd_len < 3)
    {
        kwd_el.className += " search_err";
        kwd_el.focus();
        return false;
    }
    else
    {
        kwd_el.classList.remove("search_err");
        return true;
    }
}
</script>
