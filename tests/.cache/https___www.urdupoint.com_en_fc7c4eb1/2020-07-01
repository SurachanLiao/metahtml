	var max_tfbl_pos = parseInt(tfbl_pos) + parseInt(tfbl_pos_next);
	
	var tfbl_scroll = function() 
	{
		if(window.pageYOffset > max_tfbl_pos)
		{
			if(el_tfbl.classList.contains('header_fixed_top'))
			{
				el_tfbl.classList.remove('header_fixed_top');
			}	
		}
		else
		{
			if(el_tfbl.classList.contains('header_fixed_top'))
			{
				
			}
			else
			{
				el_tfbl.classList.add('header_fixed_top');
			}
		}
	}
	window.addEventListener('scroll', tfbl_scroll);
	 
  var refreshed_HeadTopSticky = false;

	var pos_HeadTopSticky = document.getElementById("HeadTopSticky").getBoundingClientRect().top;

  var adAlmostVisibleScrollValueTop_HeadTopSticky = pos_HeadTopSticky - 10;
  
  //console.log('HeadTopSticky must be viewd after : '+adAlmostVisibleScrollValueTop_HeadTopSticky);
  
  var listener_HeadTopSticky = function() {
	if (window.scrollY >= adAlmostVisibleScrollValueTop_HeadTopSticky && !refreshed_HeadTopSticky) {
	 /* googletag.cmd.push(function() {
		googletag.pubads().refresh([HeadTopSticky]);
	  });*/
	  
	   googletag.cmd.push(function() { googletag.display(HeadTopSticky); });
	  refreshed_HeadTopSticky = true;

	  window.removeEventListener('scroll', listener_HeadTopSticky);
	}
  }
  window.addEventListener('scroll', listener_HeadTopSticky);
