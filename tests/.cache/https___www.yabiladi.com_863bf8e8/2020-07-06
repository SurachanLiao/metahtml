	function reloadNews(langueID) {
	WaitingEffect();
		rub	= '';
		page =1;
	$j.get('/home/get_focus', {langueID: langueID, rub:rub}, function (data) {
		$j("#focus").html(data);
	});	
	$j.get('/home/get_more_news', {langueID: langueID, rub:rub, page: page}, function (data) {
		$j("#all_news_page").html(data);
		$j("#reloadDivNews").html('');
		closeEffect();
	});
  }

</script>
