	// POP BUSCADOR
	$(function () {
	    $('a[href="#search"]').on('click', function(event) {
	        event.preventDefault();
	        $('#search').addClass('open');
	        $('#search > form > input[type="search"]').focus();
	    });
	    
	    $('#search, #search button.close').on('click keyup', function(event) {
	        if (event.target == this || event.target.className == 'close' || event.keyCode == 27) {
	            $(this).removeClass('open');
	        }
	    });
	    
	});
	
	function boletinsubs() {
		if(document.getElementById('termsbol').checked) {
			if((document.getElementById('nombre').value != '')&&(document.getElementById('email').value != '')) {
				return true;
			} else {
				alert('Debe rellenar los campos');
				return false;
			}
		} else {
			alert('Debe aceptar los terminos y condiciones');
			return false;
		}
	}
	
	function sendContact() {
		var name = document.getElementById('contact_name').value;
		var email = document.getElementById('contact_email').value;
		var ciudad = document.getElementById('contact_city').value;
		var message = document.getElementById('contact_message').value;
		
		if((name != '')&&(email != '')&&(message != '')) {
			var sendvars = 'action=contact&name='+name+'&email='+email+'&message='+message+'&ciudad='+ciudad;
			
			$.post('/includes/process.php', sendvars, function(theResponse){
				theResponse = $.trim(theResponse);
				if(theResponse == 'ok') {
					$('#response').html('<div class="alert alert-success">Formulario enviado correctamente</div>');
				} else {
					$('#response').html('<div class="alert alert-danger">No se ha podido enviar el formulario</div>');
				}
			}); 
		} else {
			alert('Debe rellenar los campos');
		}
	}

/* ]]> */
</script>
