  <script>
  function cierreBalcon(e){
    document.getElementsByTagName("body")[0].classList.remove("openDir");
    document.getElementsByClassName("emisBox")[0].classList.add("beoff");
    document.getElementsByClassName("emisBox")[0].classList.remove("be_on");
    document.getElementsByClassName("emisBox")[0].classList.remove("openBox");
    e.stopPropagation(); e.preventDefault();
    return false;
  };
</script>
