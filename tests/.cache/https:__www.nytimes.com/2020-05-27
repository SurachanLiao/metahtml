<script>
  if (!window.Promise || !window.fetch || !window.URL) {
    document.write('<script src="https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/bd8a628df0c1b97a142e4640808b39e23502f385/polyfills.js"><\/script>');
