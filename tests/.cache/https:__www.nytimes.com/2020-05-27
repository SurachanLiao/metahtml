  (function() {
    window.NYTG = window.NYTG || {};
    window.NYTG.ASSETS = "https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/33a5f5b3ca88148944f3209662f5a7e8f3f6c779/".slice(0, -1);
