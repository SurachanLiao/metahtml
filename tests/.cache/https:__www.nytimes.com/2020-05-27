3C\u002Ful\u003E\u003C\u002Fli\u003E\u003C\u002Ful\u003E\u003C\u002Fsection\u003E\u003Cscript\u003E\n  \u002F** #storylines *\u002F\n  function getDistance(aprime,bprime){var a=aprime.toLowerCase().trim();var b=bprime.toLowerCase().trim();if(a.length===0)return b.length;if(b.length===0)return a.length;var matrix=[];for(var i=0;i\u003C=b.length;i+=1){matrix[i]=[i]}\n  for(var j=0;j\u003C=a.length;j+=1){matrix[0][j]=j}\n  for(var _i=1;_i\u003C=b.length;_i+=1){for(var _j=1;_j\u003C=a.length;_j+=1){if(b.charAt(_i-1)===a.charAt(_j-1)){matrix[_i][_j]=matrix[_i-1][_j-1]}else{matrix[_i][_j]=Math.min(matrix[_i-1][_j-1]+1,matrix[_i][_j-1]+1,matrix[_i-1][_j]+1)}}}\n  return matrix[b.length][a.length]}\n\n  function getNormalizedDistance(a,b){var normalizationFactor=Math.max(a.length,b.length);return getDistance(a,b)\u002FnormalizationFactor}\n\n  (function () {\n    try {\n      var key = 'nyt-styln-headings';\n      var pastItemsRaw = localStorage.getItem(key);\n      var pastItems = JSON.parse(pastItemsRaw);\n      var headings = document.getElementsByClassName('styln-hp-briefing-subhead');\n      for (i = 0; i \u003C headings.length; i += 1) {\n        try {\n          var headingEl = headings[i];\n          var headingId = headingEl.dataset.id;\n          var headingText = headingEl.dataset.text;\n          var previousHeadingData = pastItems[headingId];\n          var previousHeadings;\n          if (Array.isArray(previousHeadingData)) {\n            previousHeadings = previousHeadingData.flat();\n          } else {\n            previousHeadings = previousHeadingData.headings.flat();\n          }\n          var minDistance = previousHeadings.reduce(function (acc, curr) {\n            var dist = getNormalizedDistance(headingText, curr);\n            return Math.min(dist, acc);\n          }, 1)\n          if (minDistance \u003E 0.5) {\n            headingEl.parentNode.setAttribute(\"data-isNew\", true);\n          }\n        } catch(e) {\n          \u002F\u002F do nothing\n         }\n      }\n    } catch(e) {\n      \u002F\u002F do nothing\n     }\n  })();\n\n  (function() {\n    function getExistingImpression() {\n      function getEvent(resolve) {\n        var existing = dataLayer.find(function(d) {\n          return d.event === 'impression' && d.block && d.block.label === 'Spotlight';\n        });\n        if (!existing) {\n          setTimeout(function() { getEvent(resolve) }, 100);\n          return;\n        }\n        resolve(existing);\n      }\n\n      return new Promise(getEvent);\n    }\n\n    function createCardData(index, label, url) {\n      return {\n        card: {\n         index: index,\n         id: '100000007085371',\n         type: 'styln-live-updates',\n         url: url,\n         label: label\n        }\n      };\n    }\n\n    function fireImpressionEvents() {\n      var headings = document.getElementsByClassName('styln-hp-briefing-section');\n      getExistingImpression().then(function(base) {\n        for (var i = 0; i \u003C headings.length; i++) {\n          var heading = headings[i].innerText.toLowerCase();\n          var url = headings[i].parentNode.href;\n          var trackingData = Object.assign({}, base, createCardData(i, heading, url));\n          dataLayer.push(trackingData);\n        }\n      })\n    }\n\n    function handleIntersection(entries, observer) {\n      var entry = entries[0];\n      if (entry.isIntersecting && entry.intersectionRatio \u003E 0.1) {\n        observer.unobserve(el);\n        fireImpressionEvents();\n      }\n    }\n\n    if (window.dataLayer) {\n      var el = document.getElementById('styln-hp-lbs');\n      var observer = new IntersectionObserver(handleIntersection, {\n        threshold: 0.1\n      });\n      observer.observe(el);\n    }\n  })();\n\n\n  function sendResizeToApps() {\n    try {\n      var el = document.getElementById('styln-hp-lbs');\n      var height = Math.ceil(el.offsetHeight);\n      var message = JSON.stringify({\n        id: 0,\n        type: \"resize\",\n        options: { height }\n      });\n      window.NYTG.enqueue(message);\n    } catch(e) {\n      \u002F\u002F do nothing\n    }\n  };\n  [0, 100, 500, 1000, 5000, 10000].forEach(ms =\u003E window.setTimeout(sendResizeToApps, ms));\n\u003C\u002Fscript\u003E\u003C!-- Pipeline: 2020-04-10-live-briefings-in-spotlight | May 27, 2020, 10:18PM | 15cb554979a789bead3c2af19a502ccd1c49468a --\u003E\n","compatibility":"INLINE","headline":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.headline","typename":"CreativeWorkHeadline"},"advertisingProperties":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.advertisingProperties","typename":"CreativeWorkAdvertisingProperties"},"displayProperties":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.displayProperties","typename":"CreativeWorkDisplayProperties"},"bylines":[],"sourceId":"100000007085371","credit":"","leadin":"","note":"","dataSource":"","uri":"nyt:\u002F\u002Fembeddedinteractive\u002F99fde678-dc78-5b93-8b61-f32796d33687","promotionalMedia":null},"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.headline":{"default":"coronavirus-hp-live-briefings","__typename":"CreativeWorkHeadline"},"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.advertisingProperties":{"sensitivity":"SHOW_ADS","__typename":"CreativeWorkAdvertisingProperties"},"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzk5ZmRlNjc4LWRjNzgtNWI5My04YjYxLWYzMjc5NmQzMzY4Nw==.displayProperties":{"displayFo