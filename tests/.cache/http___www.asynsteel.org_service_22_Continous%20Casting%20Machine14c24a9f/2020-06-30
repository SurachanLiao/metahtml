<script>
$(function(){
  // Animate loader off screen
  $(".se-pre-con").fadeOut("slow");
    $(document).on('submit', '.pagelogin', function(e) {
        e.preventDefault();

        $('input+small').text('');
        $('input').parent().removeClass('has-error');

        $.ajax({
            method: $(this).attr('method'),
            url: $(this).attr('action'),
            data: $(this).serialize(),
            dataType: "json"
        })

        .done(function(data) {
            $('.alert-success').removeClass('hidden');
            console.log("no");
            $('#myModal').modal('hide');
        })
        .fail(function(data) {
           if(data.responseJSON!== undefined && typeof data.responseJSON.errors === "object")
           {
            $.each(data.responseJSON.errors, function (key, value) {
                var input = '.pagelogin input[name=' + key + ']';
                $("#span"+key).text(value[0]);

              })
            }
              else {
                $('#modallogin').modal('hide');
                window.location.replace("/home");
              }

             //$(input).parent().addClass('has-error');

        });
    });
    $(document).on('submit', '.pageregister', function(e) {
        e.preventDefault();
console.log("hossein");
        $('input+small').text('');
        $('input').parent().removeClass('has-error');

        $.ajax({
            method: $(this).attr('method'),
            url: $(this).attr('action'),
            data: $(this).serialize(),
            dataType: "json"
        })

        .done(function(data) {
            $('.alert-success').removeClass('hidden');
            console.log("no");
            $('#myModal').modal('hide');
        })
        .fail(function(data) {
           if(data.responseJSON!== undefined && typeof data.responseJSON.errors === "object")
           {
            $.each(data.responseJSON.errors, function (key, value) {
                $("#small"+key).text(value[0]);

              })
            }
              else {
                $('#modallogin').modal('hide');
                Swal.fire({
                type: 'success',
                title: 'ثبت نام با موفقیت انجام شد.',
                text: 'لطفا لینک ایمیا را تایید کنید',
              });
              }

             //$(input).parent().addClass('has-error');

        });
    });
    $(document).on('submit', '.pageforgot', function(e) {
        e.preventDefault();
console.log("hossein");
        $('input+small').text('');
        $('input').parent().removeClass('has-error');

        $.ajax({
            method: $(this).attr('method'),
            url: $(this).attr('action'),
            data: $(this).serialize(),
            dataType: "json"
        })

        .done(function(data) {
            $('.alert-success').removeClass('hidden');
            console.log("no");
            $('#myModal').modal('hide');
        })
        .fail(function(data) {
           if(data.responseJSON!== undefined && typeof data.responseJSON.errors === "object")
           {
            $.each(data.responseJSON.errors, function (key, value) {
                $("#small"+key).text(value[0]);

              })
            }
              else {
                $('#modallogin').modal('hide');
                Swal.fire({
                type: 'success',
                title: 'ثبت نام با موفقیت انجام شد.',
                text: 'لطفا لینک ایمیا را تایید کنید',
              });
              }

             //$(input).parent().addClass('has-error');

        });
    });
    $("img").addClass("img-fluid");

})
</script>
    </html>
