      } else {
        (function checkDOMReadyState(c) {
          try {
            if (AdController.getRenderController().isReady()) {
              AdController.reinitialize(cmsObject);
            }
          } catch (e) {
            if (c < 50) {
              c++;
              setTimeout(function () {
                checkDOMReadyState(c);
              }, 100);
            }
          }
        })(0);
      }
    </script>
