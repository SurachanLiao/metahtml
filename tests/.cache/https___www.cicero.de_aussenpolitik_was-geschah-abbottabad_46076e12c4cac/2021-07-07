        function plenigoPostProductForm(productId) {
            var form = document.createElement('form');
            form.setAttribute('method', 'post');
            form.setAttribute('action', '/paywall/api/startCheckout');
            
            var selectedField = document.createElement('input');
            selectedField.setAttribute('type', 'hidden');
            selectedField.setAttribute('name', 'paywall-selection');
            selectedField.setAttribute('value', productId);
            form.appendChild(selectedField);
            
            var referrerField = document.createElement('input');
            referrerField.setAttribute('type', 'hidden');
            referrerField.setAttribute('name', 'paywall-referrer-url');
            referrerField.setAttribute('value', '/');
            form.appendChild(referrerField);
        
            var loginField = document.createElement('input');
            loginField.setAttribute('type', 'hidden');
            loginField.setAttribute('name', 'paywall-login-url');
            loginField.setAttribute('value', 'contains login url');
            form.appendChild(loginField);

            var actionField = document.createElement('input');
            actionField.setAttribute('type', 'hidden');
            actionField.setAttribute('name', 'paywall-action');
            actionField.setAttribute('value', 'login');
            form.appendChild(actionField);
            
            document.body.appendChild(form);
            form.submit();
        }
        
       
       
       
