jQuery(function () {

    var total = 0;
    var totalDrag = 0;
    jQuery("#slider-page-simple, #slider-photos")
    .mousedown(function () {
        totalDrag = 0;   //on remet totalDrag à 0 lors du clic
        total += 10;
    })
    .mousemove(function () {
        totalDrag += 10;   //on incrémente totalDrag qui repart à zéro lors du clic
    })
    .mouseup(function () {
        var wasTotal = total;
        isDragging = false;

        //si le déplacement est un minimum important on lance le script
        if (totalDrag > 160) {
            dataLayer.push({ 'event': 'dragphotos' });
			console.log('totalDra>120 envoie: '+totalDrag);
            //alert('totalDra>120 envoie: '+totalDrag);
        }
        //on remet à zéro les variables pour suivant   
        total = 0;
        totalDrag = 0;
    });
});



jQuery( document ).ready(function() {

    // Register touchstart and touchend listeners for element 'source'
    if(document.getElementById("slider-page-simple")) {
        var src = document.getElementById("slider-page-simple");
        var clientX, clientY, deltaX, deltaX2,deltaY;
		var zoom4;
		var zoom41;
		var nbPhotosTotal;

        src.addEventListener('touchstart', function(e) {
            // Cache the client X/Y coordinates
            clientX = e.touches[0].clientX;
            clientY = e.touches[0].clientY;
			zoom4 = e.touches.length;
        }, {passive: true});

        src.addEventListener('touchend', function(e) {
            var deltaX, deltaY;
            deltaX = e.changedTouches[0].clientX - clientX;
            deltaY = e.changedTouches[0].clientY - clientY;
			zoom41 = e.touches.length;

            deltaX2 = Math.abs(deltaX);
			var nbPhotos = document.getElementsByClassName("rs-count");
            for (var i = 0; i < nbPhotos .length; i++) {
              var txtNbphoto= nbPhotos [i].innerText;
			  nbPhotosTotal = txtNbphoto.split('/')[1];
              console.log('Nb photos : '+txtNbphoto);
              console.log('Nb photos : '+txtNbphoto.split('/')[1]);
              console.log('Nb photos : '+txtNbphoto.split('/')[0]);
            }

            if(deltaX2>160) {			
				if (zoom4 < 2 && nbPhotosTotal>2) { // On teste si zoom pour pas envoyer
					dataLayer.push({'event': 'dragphotos'});       // CE QUI ENVOIE CHEZ NETMETRIX
					console.log('IF Zoom4<2 envoie: '+zoom4+', nbPhotos:'+nbPhotosTotal+' deltaX2:'+deltaX2);
					//alert('IF Zoom4<2 envoie: '+zoom4+', nbPhotos:'+nbPhotosTotal+' deltaX2:'+deltaX2);
				} else {
					//alert('ELSE Zoom4<2 envoie PAS: '+zoom4+', nbPhotos:'+nbPhotosTotal+' deltaX2:'+deltaX2); //ELSE envoie pas
					console.log('ELSE Zoom4<2 envoie PAS: '+zoom4+', nbPhotos:'+nbPhotosTotal+' deltaX2:'+deltaX2); //ELSE envoie pas
				}

            } else {  }

        } );

    }

    if (document.getElementById("#slider-photos")) {
        var src = document.getElementById("#slider-photos");
        var clientX, clientY, deltaX, deltaX2,deltaY;
		var zoom4;

        src.addEventListener('touchstart', function (e) {
            // Cache the client X/Y coordinates
            clientX = e.touches[0].clientX;
            clientY = e.touches[0].clientY;
			zoom4 = e.touches.length;
        }, {passive: true});

        src.addEventListener('touchend', function (e) {
            var deltaX, deltaY;
            deltaX = e.changedTouches[0].clientX - clientX;
            deltaY = e.changedTouches[0].clientY - clientY;

            deltaX2 = Math.abs(deltaX);

            if (deltaX2 > 160 && zoom4 < 2) {
                dataLayer.push({ 'event': 'dragphotos' });
				console.log('Zoom4<2 envoie2: '+zoom4);
				//alert('Zoom4<2 envoie2: '+zoom4);
            } else { }

        } );

    }

});

