    function timeDiff (date1, date2) {
        var a = new Date(date1).getTime(),
            b = new Date(date2).getTime(),
            diff = {};

        diff.milliseconds = a > b ? a % b : b % a;
        diff.seconds = diff.milliseconds / 1000;
        diff.minutes = diff.seconds / 60;
        diff.hours = diff.minutes / 60;
        diff.days = diff.hours / 24;
        diff.weeks = diff.days / 7;

        return diff;
    }

    var refresh = false;
    var popup_already_displayed = false;
    if(localStorage.getItem("nb_reloads")===null){
        localStorage.setItem("nb_reloads",1);
        var nb_reloads = 1;
    }
    else{

        if(localStorage.getItem("last_update")!=null){
            var cl_case = true;
            var new_date = new Date();
            var day = new_date.getDate();
            var monthIndex = new_date.getMonth();
            if(monthIndex.toString().length==1){monthIndex="0"+monthIndex;}
            if(day.toString().length==1){day="0"+day;}
            var year = new_date.getFullYear();
            var full_date = year+""+monthIndex+""+day;

            if(full_date>=20200316 && full_date<20200321){
                var cl_case = false;
            }
            
            var date_to_compare = new Date(localStorage.getItem("last_update"));
            var diff = timeDiff(new Date(), date_to_compare);

            if(cl_case == true && diff.days>=1){
                //popup_already_displayed = true;
                //localStorage.setItem("last_update",new Date());
                popup_already_displayed = false;
                localStorage.removeItem('last_update');
                localStorage.setItem("nb_reloads",0);
                refresh = true;
            }

            if(cl_case == false && diff.hours>=4){
                popup_already_displayed = false;
                localStorage.removeItem('last_update');
                localStorage.setItem("nb_reloads",0);
                refresh = true;
                //popup_already_displayed = true;
                //localStorage.setItem("last_update",new Date());
            }
        }
        else{
            localStorage.setItem("last_update",new Date());
        }

        if(refresh === false) {
            var nb_reloads = parseInt(localStorage.getItem("nb_reloads")) + 1;
            localStorage.setItem("nb_reloads", nb_reloads);
        }
    }

    
    
