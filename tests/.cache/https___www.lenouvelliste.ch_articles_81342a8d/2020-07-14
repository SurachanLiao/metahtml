        jQuery(document).ready(function($){
            var arrDatalyerList = [];
            var indexAdd = 1;

            var htmlDatalayer = $(".datalayer");
            var length = htmlDatalayer.length;

            htmlDatalayer.each(function(index){
                var id = $(this).attr("dl-id");
                var name = $(this).attr("dl-name");
                var brand = $(this).attr("dl-brand");
                var category = $(this).attr("dl-category");
                var price =  $(this).attr("dl-price");
                var list = null;

                // Check dans quel colonne il se trouve
                                    if($('div.col-1-1').find($(this)).length == 1) {
                        list = 'Article_Col1';
                    }
                    else if($('div.col-1-2').find($(this)).length == 1) {
                        list = 'Article_Col2';
                    }
                    else if($('div.tab-content').find($(this)).length == 1) {
                        list = 'Article_Les+Lus';
                    }
                
                var info = {
                    id: id,
                    name: name,
                    brand: brand,
                    price: price,
                    category: category,
                    list: list,
                    position: indexAdd
                };
                arrDatalyerList.push(info);

                // Le talbeau ne doit pas être trop gros pour google, donc on envoie un tous les 16 articles.
                if(indexAdd % 16 === 0 || index === (length - 1)) {
                    var dataLayerImpressionDone = {
                        'event': 'impressionDone',
                        'ecommerce': {
                            'impressions': []
                        }
                    };

                    $.each(arrDatalyerList, function( index, value ) {
                        dataLayerImpressionDone.ecommerce.impressions.push(value);
                    });

                    dataLayer.push(dataLayerImpressionDone);
                    arrDatalyerList = [];
                }

                indexAdd++

            });
        });

