<script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script src="https://code.highcharts.com/highcharts.js" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script src="https://code.highcharts.com/maps/modules/map.js" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script src="https://code.highcharts.com/modules/data.js" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script src="https://code.highcharts.com/mapdata/custom/world.js" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script src="https://virusncov.com/js/jquery.datetimepicker.full.min.js?v=1.1" type="65d80ce7f213f917168acb24-text/javascript"></script>
<script type="65d80ce7f213f917168acb24-text/javascript">
    var base_url = window.location.origin;
    $.ajaxSetup({
        headers : {
            'api-key' : 'c865edaf-10ce-4848-8c28-f674a2edef29',
            'Content-Type':'application/json'
        }
    });
    jQuery('#datetimepicker').datetimepicker({
        timepicker:false,
        format:'Y-m-d'
    });
    $("#datetimepicker").bind("change", function() {
        $('.loading').show();
        $.getJSON(base_url+'/api/getbydate/'+$(this).val(), function (data) {
            $('#now').removeAttr('disabled');
            $('#now').removeClass('active');
            $('#yesterday').removeAttr('disabled');
            $('#yesterday').removeClass('active');
            $('#table_stas').hide();
            $('#table_stas_yesterday').hide();
            $('#table_stas_by_date').remove();
            var first = '<table id="table_stas_by_date" class="table-fix-header"> <thead> <tr> <th>#</th> <th>Country, Other</th> <th>Total Cases</th> <th>New</br>Cases</th> <th>Total Deaths</th> <th>New</br>Deaths</th> <th>Active Cases</th> <th>Total Recovered</th> <th>Serious, Critical</th> <th>Tot Cases<br>/1M pop</th> <th>Deaths<br>/1M pop</th> <th>Total <br>Tests</th> <th>Tests<br>/1M pop</th> </tr> </thead> <tbody>';
            var end = '</tbody></table>';
            var middle = '';
            $.each(data, function(index,el) {
                middle = middle + '<tr><td>'+parseInt(index+1)+'</td><td>' + (el.code ? '<img style="vertical-align: middle" src="https://www.countryflags.io/'+el.code+'/flat/16.png" alt="country flag '+el.name+'">' : '' )+' <a class="country-detail" href="/covid-statistics/'+el.url+'" title="COVID19 '+el.name+' Statistics"><strong>'+el.name+'</strong><img style="vertical-align: middle;margin-left: 3px" src="/images/external-link-ltr-icon.png" alt="external-link"></a></td><td>'+el.total_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_cases) > 0 ? '<td class="orage-bg"> +'+el.sub_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .4;z-index: 0">+'+(parseInt(el.total_cases)*100/(parseInt(el.total_cases) - parseInt(el.sub_cases)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.total_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_deaths) > 0 ? '<td class="red-bg"> +'+el.sub_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .5;z-index: 0">+'+(parseInt(el.total_deaths)*100/(parseInt(el.total_deaths) - parseInt(el.sub_deaths)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.active_case.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_recovered.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_critical.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.death_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_test.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.test_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td></tr>';
            });
            $('#table_wrap').append(first+middle+end);
            checkStatus();
            $('.loading').hide();
        });
    });
    var options_cases_deaths = {
        chart: {
            renderTo: 'cases_deaths',
            type: 'spline'
        },
        exporting: {
            enabled: false
        },
        title: { text: 'Cases and Deaths' },
        subtitle: { text: '(Linear Scale)' },
        credits: { enabled: false },
        yAxis: { title: { text: 'Total Cases and Deaths' } },
        responsive: { rules: [{ condition: { maxWidth: 800 }, chartOptions: { legend: { layout: 'horizontal', align: 'center', verticalAlign: 'bottom' } } }] },
        legend: { layout: 'vertical', align: 'right', verticalAlign: 'middle' },
        xAxis: {},
        series: []
    };

    $.getJSON(base_url+'/api/list', function (data) {
        var date_time = [],
            cases = [],
            deaths = [];
        $.each(data, function(i, el) {
            date_time.push(el.date_time);
            cases.push(parseFloat(el.total_cases));
            deaths.push(parseFloat(el.total_deaths));
        });
        var cases_deaths = [
            {
                "name":"Cases",
                "color": "#33CCFF",
                "data": cases
            },
            {
                "name":"Deaths",
                "color": "#FF9900",
                "data": deaths
            }];
        options_cases_deaths.xAxis.categories = date_time;
        options_cases_deaths.series = cases_deaths;
        var chart_cases_deaths = new Highcharts.Chart(options_cases_deaths);
    });
    var scroll_count = 0;
    $( window ).scroll(function() {
        scroll_count ++;
        if(scroll_count == 1){
            $('.loading').show();
            $.getJSON(base_url+'/api/allpost', function (data) {
                var first = '<thead> <tr> <th>#</th> <th>Country, Other</th> <th>Total Cases</th> <th>New</br>Cases</th> <th>Total Deaths</th> <th>New</br>Deaths</th> <th>Active Cases</th> <th>Total Recovered</th> <th>Serious, Critical</th> <th>Tot Cases<br>/1M pop</th> <th>Deaths<br>/1M pop</th> <th>Total <br>Tests</th> <th>Tests<br>/1M pop</th> </tr> </thead> <tbody>';
                var end = '</tbody>';
                var middle = '';
                $.each(data, function(index,el) {
                    middle = middle + '<tr><td>'+parseInt(index+1)+'</td><td>' + (el.code ? '<img style="vertical-align: middle" src="https://www.countryflags.io/'+el.code+'/flat/16.png" alt="country flag '+el.name+'">' : '' )+' <a class="country-detail" href="/covid-statistics/'+el.url+'" title="COVID19 '+el.name+' Statistics"><strong>'+el.name+'</strong><img style="vertical-align: middle;margin-left: 3px" src="/images/external-link-ltr-icon.png" alt="external-link"></a></td><td>'+el.total_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_cases) > 0 ? '<td class="orage-bg"> +'+el.sub_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .4;z-index: 0">+'+(parseInt(el.total_cases)*100/(parseInt(el.total_cases) - parseInt(el.sub_cases)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.total_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_deaths) > 0 ? '<td class="red-bg"> +'+el.sub_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .5;z-index: 0">+'+(parseInt(el.total_deaths)*100/(parseInt(el.total_deaths) - parseInt(el.sub_deaths)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.active_case.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_recovered.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_critical.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.death_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_test.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.test_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td></tr>';
                });
                $('#table_stas').html(first+middle+end);
                checkStatus();
                $('.loading').hide();
            });
        }

    });
    $('#now').attr('disabled','disabled');
    $('#yesterday').on('click',function () {
        $('.loading').show();
        $(this).attr('disabled','disabled');
        $(this).addClass('active');
        $('#now').removeAttr('disabled');
        $('#now').removeClass('active');
        $.getJSON(base_url+'/api/yesterday', function (data) {
            $('#table_stas').hide();
            $('#now').removeAttr('disabled');
            var first = '<table id="table_stas_yesterday" class="table-fix-header"> <thead> <tr> <th>#</th> <th>Country, Other</th> <th>Total Cases</th> <th>New</br>Cases</th> <th>Total Deaths</th> <th>New</br>Deaths</th> <th>Active Cases</th> <th>Total Recovered</th> <th>Serious, Critical</th> <th>Tot Cases<br>/1M pop</th> <th>Deaths<br>/1M pop</th> <th>Total <br>Tests</th> <th>Tests<br>/1M pop</th> </tr> </thead> <tbody>';
            var end = '</tbody></table>';
            var middle = '';
            $.each(data, function(index,el) {
                middle = middle + '<tr><td>'+parseInt(index+1)+'</td><td>' + (el.code ? '<img style="vertical-align: middle" src="https://www.countryflags.io/'+el.code+'/flat/16.png" alt="country flag '+el.name+'">' : '' )+' <a class="country-detail" href="/covid-statistics/'+el.url+'" title="COVID19 '+el.name+' Statistics"><strong>'+el.name+'</strong><img style="vertical-align: middle;margin-left: 3px" src="/images/external-link-ltr-icon.png" alt="external-link"></a></td><td>'+el.total_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_cases) > 0 ? '<td class="orage-bg"> +'+el.sub_cases.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .4;z-index: 0">+'+(parseInt(el.total_cases)*100/(parseInt(el.total_cases) - parseInt(el.sub_cases)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.total_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td>'+( parseInt(el.sub_deaths) > 0 ? '<td class="red-bg"> +'+el.sub_deaths.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'<small style="display: block;font-weight: normal;opacity: .5;z-index: 0">+'+(parseInt(el.total_deaths)*100/(parseInt(el.total_deaths) - parseInt(el.sub_deaths)) - 100).toFixed(1)+'%</small></td>' : '<td></td>')+'<td>'+el.active_case.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_recovered.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_critical.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.death_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.total_test.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td><td>'+el.test_pop.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")+'</td></tr>';
            });
            $('#table_wrap').append(first+middle+end);
            checkStatus();
            $('.loading').hide();
        });
    });

    $('#now').on('click',function () {
        $(this).attr('disabled','disabled');
        $(this).addClass('active');
        $('#yesterday').removeAttr('disabled');
        $('#yesterday').removeClass('active');
        $('#table_stas').show();
        $('#table_stas_yesterday').remove();
    });
    var options_cases_ex = {
        chart: {
            renderTo: 'cases_ex',
            type: 'column'
        },
        exporting: {
            enabled: false
        },
        title: { text: 'Daily New Cases' },
        subtitle: { text: 'Cases per Day' },
        credits: { enabled: false },
        yAxis: { title: { text: 'Novel Coronavirus Daily Cases' } },
        responsive: { rules: [{ condition: { maxWidth: 800 }, chartOptions: { legend: { layout: 'horizontal', align: 'center', verticalAlign: 'bottom' } } }] },
        legend: { layout: 'vertical', align: 'right', verticalAlign: 'middle' },
        xAxis: {},
        series: [{}]
    };

    $.getJSON(base_url+'/api/cases', function (data) {
        var date_time = [],
            cases_ex = [];
        $.each(data, function(i, el) {
            date_time.push(el.date_time);
            cases_ex.push(parseFloat(el.ex_cases));
        });
        options_cases_ex.xAxis.categories = date_time;
        options_cases_ex.series[0].data = cases_ex;
        options_cases_ex.series[0].color = '#999';
        options_cases_ex.series[0].name = 'Daily Cases';
        options_cases_ex.series[0].lineWidth = 5;

        var chart_cases_ex = new Highcharts.Chart(options_cases_ex);
    });

    var options_deaths_ex = {
        chart: {
            renderTo: 'deaths_ex',
            type: 'column'
        },
        exporting: {
            enabled: false
        },
        title: { text: 'Daily Deaths' },
        subtitle: { text: 'Deaths per Day' },
        credits: { enabled: false },
        yAxis: { title: { text: 'Novel Coronavirus Daily Deaths' } },
        responsive: { rules: [{ condition: { maxWidth: 800 }, chartOptions: { legend: { layout: 'horizontal', align: 'center', verticalAlign: 'bottom' } } }] },
        legend: { layout: 'vertical', align: 'right', verticalAlign: 'middle' },
        xAxis: {},
        series: [{}]
    };
    $.getJSON(base_url+'/api/deaths', function (data) {
        var date_time = [],
            deaths_ex = [];
        $.each(data, function(i, el) {
            date_time.push(el.date_time);
            deaths_ex.push(parseFloat(el.ex_deaths));
        });
        options_deaths_ex.xAxis.categories = date_time;
        options_deaths_ex.series[0].data = deaths_ex;
        options_deaths_ex.series[0].color = '#999';
        options_deaths_ex.series[0].name = 'Daily Deaths';
        options_deaths_ex.series[0].lineWidth = 5;
        var chart_deaths_ex = new Highcharts.Chart(options_deaths_ex);
    });

    var effCountry = $('#effCountry').text();
    $.getJSON(base_url+'/api/map', function (data) {
        Highcharts.mapChart('mapcases', {
            chart: {
                map: 'custom/world'
            },
            title: {
                text: 'Confirmed Cases by Country'
            },
            exporting: {
                enabled: false
            },
            subtitle: {
                text: 'Affecting '+ effCountry +' countries and territories'
            },
            credits: { enabled: false },
            legend: {
                enabled: false
            },
            mapNavigation: {
                enabled: false,
                buttonOptions: {
                    verticalAlign: 'bottom'
                }
            },
            series: [{
                name: 'Countries',
                color: '#E0E0E0',
                enableMouseTracking: false
            }, {
                type: 'mapbubble',
                name: 'Cases',
                joinBy: ['iso-a3', 'code3'],
                data: data,
                minSize: 5,
                maxSize: '12%',
                tooltip: {
                    pointFormat: '{point.properties.hc-a2}:{point.z}'
                }
            }]
        });
    });
    function checkStatus() {
        $('#table_wrap table.table-fix-header tr').each(function() {
            var total_cases = $(this).find('td:nth-child(3)').text();
            total_cases = total_cases.replace(",", "");
            total_cases = parseInt(total_cases);
            var total_deaths = $(this).find('td:nth-child(5)').text();
            if(total_deaths == ''){
                total_deaths = '0';
            }
            total_deaths = total_deaths.replace(",", "");
            total_deaths = parseInt(total_deaths);
            var total_recovered = $(this).find('td:nth-child(8)').text();
            if(total_recovered == ''){
                total_recovered = '0';
            }
            total_recovered = total_recovered.replace(",", "");
            total_recovered = parseInt(total_recovered);
            var total_critical = $(this).find('td:nth-child(9)').text();
            if(total_critical == ''){
                total_critical = '0';
            }
            total_critical = total_critical.replace(",", "");
            total_critical = parseInt(total_critical);
            var total_x = total_deaths + total_recovered;
            if(total_cases == total_x && total_deaths < 1){
                $(this).addClass("bg-green");
            }
            if(total_cases == total_x && total_deaths > 0){
                $(this).addClass("bg-gray");
            }
        });
    }
    var country_cases = $('#country_cases').text();
    var country_deaths = $('#country_deaths').text();
    var country_recovered = $('#country_recovered').text();
    if(parseInt(country_cases) === (parseInt(country_deaths) + parseInt(country_recovered))){
        $('#box-country').addClass("green_color");
    }else{
        $('#box-country').addClass("red_color");
    }
</script>
