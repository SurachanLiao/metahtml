    $(document).ready(function () {
        $("#dialog-form-Success").dialog({
            autoOpen: false,
            width: 420,
            bgiframe: true,
            title: "Feedback Submitted Successfully",
            modal: true,
            close: function () {
                $(this).hide();
            }
        });
        $("#dialog-form-msg").dialog({
            autoOpen: false,
            height: 420,
            width: 550,
            bgiframe: true,
            modal: true,
            resizable: false,
            title: "Share Your Comments or Suggestions",
            buttons: {
                "Submit": function () {
                    SubmitData();
                },
                Cancel: function () {
                    $(this).dialog("close");
                }
            },
            close: function () {
                $(this).hide();
            }
        });
        //å³é­ä¿¡æ¯æ¡
        $("#TellUsSuggestion").bind('click', function () {
            $.ajax({
                type: "POST",
                url: "/Ajax/Handler/loginEmail.ashx",
                success: function (mse) {
                    if (mse != "") {
                        var ss = mse.split("|");
                        $("#HeaderContainer_Control_BaseHeader1_txtEmail").val(ss[0]);
                        $("#HeaderContainer_Control_BaseHeader1_companyID").val(ss[1]);
                    }
                }
            });
            $("#dialog-form-msg").dialog("open");
        });
    });
    //æäº¤æ°æ®
    function SubmitData() {
        var obj = { check: null };
        obj.check = $("#showCheckCharacters");
        var userEmail = getCookie("___LastLoginEmailCookieName");
        if (userEmail != null && userEmail != "") {
            $("#HeaderContainer_Control_BaseHeader1_txtEmail").val(userEmail);
        }
        var newExpires = new Date();
        var get_newExpires = newExpires.getTime();
        var get_starttime = $("#start_time").val();
        var contents = $("#txtSuggestions").val();
        var email = $("#HeaderContainer_Control_BaseHeader1_txtEmail").val();
        var companyID = $("#HeaderContainer_Control_BaseHeader1_companyID").val();
        var matchArray = email.match(/\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/);
        if (contents.length < 10) {
            obj.check.show();
        } else if (email == "") {
            obj.check.text("Please enter the Email");
        } else if (matchArray == null) {
            obj.check.text("Please enter a valid email format");
        } else {
            if (get_starttime == "" || get_newExpires - get_starttime >= 120000) {
                $("#showCheckCharacters").hide();
                $.ajax({
                    type: "POST",
                    url: "/Ajax/Handler/InsertAskReply.ashx",
                    data: "contents=" + contents + "&email=" + email + "&companyID=" + companyID,
                    success: function (msg) {
                        if (msg == "ok") {
                            $("#dialog-form-msg").dialog("close");
                            $("#dialog-form-Success").dialog("open");
                            $("#txtSuggestions").val("");
                            $("#start_time").val(new Date().getTime());
                        } else {
                            alert("You can only submit one suggestion in 2 minutes!");
                        }
                    }
                });
            } else {
                obj.check.show();
                obj.check.text("You can only submit one suggestion in 2 minutes");
            }
        }
    }


    var searchBox = $("#HeaderContainer_Control_BaseHeader1_txtEmail");
    searchBox.focus(function () {
        if (searchBox.val() == "Write Email or Sign in") { searchBox.val(""); }
    });
    searchBox.blur(function () {
        if (searchBox.val() == "") { searchBox.val("Write Email or Sign in"); }
    });
    var useremail = getCookie("___LastLoginEmailCookieName");
    if (useremail != null) {
        $("#HeaderContainer_Control_BaseHeader1_txtEmail").val();
    }

</script>
<script type="text/javascript" src="/Scripts/V3/trace.js?v=181228"></script>
</div>
