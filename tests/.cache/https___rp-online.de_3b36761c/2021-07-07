    <script type="text/javascript" class="optanon-category-C0004">
    if(rpd_debug===undefined){
        var rpd_debug=[];
            rpd_debug.push("RPD Debug");
            rpd_debug.push("Seite: " + window.location.href);
            rpd_debug.push("Seitenbreite: " + window.innerWidth);
            rpd_debug.push("Useragent: " + window.navigator.userAgent);
    }
    if(typeof this['rpd_debugs'] != 'function') {
        function rpd_debugs(){
            for (var i=0; i<rpd_debug.length;i++){
                console.log(rpd_debug[i]);
            }
        }
    }
    (function() {
        function init() {
     
            // Variablen definieren
            var gpt_rpd_adslot=[];
            var rpd_adunit0 = "1017380";
            var rpd_adunit1 = "rp-online.de";
            var rpd_adunit2 = "specialads/svoe_mini"; //mobil adunit
            var rpd_adunit2_alt = "specialads/svoe_maxi"; //desktop adunit
            var rpd_div_id_1 = 'gpt_rpd_adslot_svoe_mini_1'; // DIV IDs
            var rpd_div_id_2 = 'gpt_rpd_adslot_svoe_mini_2';
            var rpd_div_id_3 = 'gpt_rpd_adslot_svoe_mini_3';
            var rpd_div_id_4 = 'gpt_rpd_adslot_svoe_mini_4';
            var rpd_div_id_5 = 'gpt_rpd_adslot_svoe_maxi';
            var adsize=['fluid']; // Ad Size
    
    
            if( !window.googletag ||
                !googletag.pubads ||
                !googletag.pubads().getTargetingKeys ||
                !googletag.pubads().getTargetingKeys().length 
                ){
                    console.log("warten auf GPT");
                    window.setTimeout(init, 100);
            return; 
            }
    
            //Eventlistener Debug  
            rpd_debug.push("Adunit: "+ rpd_adunit0 + "/" + rpd_adunit1 + "/" + rpd_adunit2);
            rpd_debug.push("Ad Sizes: "+adsize);
    
            googletag.cmd.push(function() {
                gpt_rpd_adslot[0] = googletag.defineSlot('/'+rpd_adunit0+'/'+rpd_adunit1+'/'+rpd_adunit2, adsize, rpd_div_id_1).setTargeting('pos', 'mini-1').addService(googletag.pubads());
                gpt_rpd_adslot[1] = googletag.defineSlot('/'+rpd_adunit0+'/'+rpd_adunit1+'/'+rpd_adunit2, adsize, rpd_div_id_2).setTargeting('pos', 'mini-2').addService(googletag.pubads());
                gpt_rpd_adslot[2] = googletag.defineSlot('/'+rpd_adunit0+'/'+rpd_adunit1+'/'+rpd_adunit2, adsize, rpd_div_id_3).setTargeting('pos', 'mini-3').addService(googletag.pubads());
                gpt_rpd_adslot[3] = googletag.defineSlot('/'+rpd_adunit0+'/'+rpd_adunit1+'/'+rpd_adunit2, adsize, rpd_div_id_4).setTargeting('pos', 'mini-4').addService(googletag.pubads());
                gpt_rpd_adslot[4] = googletag.defineSlot('/'+rpd_adunit0+'/'+rpd_adunit1+'/'+rpd_adunit2_alt, adsize, rpd_div_id_5).addService(googletag.pubads());
    
                //Eventlistener Debug  
                googletag.pubads().addEventListener('slotOnload', function(event) {
                    var times = performance.now();
                    var diftimes = Math.round(times)/1000;
                    rpd_debug.push('Dauer SlotOnload '+ event.slot.getSlotElementId() + ' : ' + diftimes + ' sek.');
                });
                googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                    var time = performance.now();
                    var diftime = Math.round(time)/1000;
                    rpd_debug.push('Dauer slotRenderEnded '+ event.slot.getSlotElementId() + ' : ' + diftime + ' sek.');
                });
                googletag.pubads().collapseEmptyDivs();
                googletag.enableServices();
                for(var i = 0; i<gpt_rpd_adslot.length;i++){
                    googletag.display(gpt_rpd_adslot[i]);
                }
            });
        }
     
    
                window.setTimeout(init, 3500);
    
            
    })();
    </script>
