function gaEventCall(category, action, label, value)
{
    if (typeof value == 'undefined') value = '';

    gtag('event', action, {
      'send_to': 'UA-79840639-2',
      'event_category': category,
      'event_label': label,
      'value': value
    });
}

</script>
            
