                },
                style: {
                    pointerEvents: 'auto'
                },
                useHTML: true
            },

            plotOptions: {
                treemap : {
                    allowDrillToNode: false,
                    borderWidth: 1,
                    dataLabels: {
                        enabled: false,
                        allowOverlap: false
                    },
