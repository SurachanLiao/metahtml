function showGraphMatprem(iLine)
{
    $.each(aCodeZbMatpremHome,function(i,val){
        $("#graphMatprem"+"_"+val).css("display","none");        
    });
    
    //check if charts need to set src
    if($("#graphMatprem"+"_"+aCodeZbMatpremHome[iLine]).prop("src").indexOf(aCodeZbImagesMatprem[aCodeZbMatpremHome[iLine]])==-1)
    {
        $("#graphMatprem"+"_"+aCodeZbMatpremHome[iLine]).prop("src",aCodeZbImagesMatprem[aCodeZbMatpremHome[iLine]]);
    } 
    
    $("#graphMatprem"+"_"+aCodeZbMatpremHome[iLine]).css("display","");
    
    var aTrToUpdate = $("#tab_Matprem").find("table").find("tr");
    $.each(aTrToUpdate,function(iLine,trToUpdate)
    {
        $(trToUpdate).removeClass('tr_hovered');
    });
    $(aTrToUpdate[iLine]).addClass('tr_hovered');
}

$(function()
{
    showGraphMatprem(0); 
    
    $.each(aCodeZbMatpremHome,function(index,aCodezb)
    {
        var aTrToUpdate = $("#tab_Matprem").find("table").find("tr");
        
        $.each(aTrToUpdate,function(iLine,trToUpdate)
        {
            $(trToUpdate).mouseover(function(){
                showGraphMatprem(iLine);   
            });
        });    
    });
});
</script>  
