  <script>
    var isShowChat = false;
    $(".chat-text").click(function (event) {
      event.stopPropagation();
      event.preventDefault();
      $(".chat-frame").toggle();
      if (!isShowChat) {
        $(".chat-content").text("Hide Chat");
      } else {
        $(".chat-content").text("Live Chat");
      }
      isShowChat = !isShowChat;
    });
    
  
  </script>
