        $(".bannerDiv").css("height","180px");
        $(".bannerDiv").mouseover(function() {
            if ($(".bannerDiv").height() > 60) {
                $(".bannerDiv").animate({ height: 580 });
            }
        });
        $(".bannerDiv").mouseleave(function() {
            if ($(".bannerDiv").height() > 180) 
