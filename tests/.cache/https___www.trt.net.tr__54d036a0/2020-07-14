    <script type="text/javascript">
    $("li.hbrLI").hover(
        function() {
            $("li.hbrLI").removeClass('aktifHbrLI');
            $(this).addClass('aktifHbrLI');
            var gosterilecek = "#hbr" + $(this).attr("id");
            $(".hbrIcerikClass").hide();
            $(gosterilecek).show();
        },
        function() {
            $("li.hbrLI").removeClass('aktifHbrLI');
            $(this).addClass('aktifHbrLI');
        }
    );
    
    $("#trtworldsiteleri").click(function() {
        if ($("#trtworldsiteleri").height() < 160) {
            $("#trtworldsiteleri").animate({ height: 590 });
        }
        else {
        $("#trtworldsiteleri").animate({ height: 145 });
        };
    });
    
    var GenelGecisSURESI = 8000;
    
    $("#televizyonKarosel").jCarouselLite({
        auto: GenelGecisSURESI,
        speed: 400,
        btnNext: ".televizyonNext",
        btnPrev: ".televizyonPrev",
        hoverPause: true,
        visible: 1,
        circular:true,
        scroll: 1
    });

    $("#televizyonKarosel").touchwipe({
        wipeLeft: function() {
            $(".televizyonNext").trigger("click");
        },
        wipeRight: function() {
            $(".televizyonPrev").trigger("click");
        }
    });
    
    if ($.trim($("#radyoKarosel2").html())!== "")
    {
        $("#radyoKarosel2").jCarouselLite({       
            hoverPause: true,
            btnPrev: ".radyoP",
            btnNext: ".radyoN",
            visible: 1,
            start: 0,
            scroll: 1,
            circular: true,
            auto: GenelGecisSURESI+2000,
            speed: 400,
            btnGo: [".rI1", ".rI2", ".rI3", ".rI4", ".rI5"],
            afterEnd: function(a, to, btnGo) {
                if (btnGo.length <= to) {
                    to = 0;
                }
                $(".aktifR").removeClass("aktifR");
                $(btnGo[to]).addClass("aktifR");
            }
        });

        $("#radyoKarosel2").touchwipe({
            wipeLeft: function() {
                $(".radyoN").trigger("click");
            },
            wipeRight: function() {
                $(".radyoP").trigger("click");
            }
        });
    };
    
    if ($.trim($("#sporHaberleriBolumu").html())!== "")
    {
        $("#sporHaberleriBolumu").jCarouselLite({
            hoverPause: true,
            btnPrev: ".sporhP",
            btnNext: ".sporhN",
            visible: 1,
            start: 0,
            scroll: 1,
            circular: true,
            auto: GenelGecisSURESI+3000,
            speed: 400,
            btnGo: [".sHI1", ".sHI2", ".sHI3", ".sHI4", ".sHI5"],
            afterEnd: function(a, to, btnGo) {
                if (btnGo.length <= to) {
                    to = 0;
                }
                $(".aktifSH").removeClass("aktifSH");
                $(btnGo[to]).addClass("aktifSH");
            }
        });
    };
    $("#sporHaberleriBolumu").touchwipe({
        wipeLeft: function() {
            $(".sporhN").trigger("click");
        },
        wipeRight: function() {
            $(".sporhP").trigger("click");
        }
    });
    </script>
    
