    <script type="text/javascript">
		$(document).ready(function(){
		
		
            //$('#myModal').modal('show');
		
		    var simdi = new Date();          
			var utcsimdi = new Date(simdi.getTime() + simdi.getTimezoneOffset() * 60000);
			
			var popupBas = new Date(2017, 1, 15, 17, 05, 00);   
			var popupBit = new Date(2017, 1, 15, 21, 00, 00);   
			
			var popupcanliBas = new Date(2015, 0, 25, 20, 15, 0);  
			var popupcanliBit = new Date(2015, 0, 25, 21, 20, 0);  
			
			var pencereW = window.innerWidth*0.90;
			var pencereH = window.innerHeight*0.90;
			
			if ((utcsimdi.getTime() >= popupBas.getTime()) && (utcsimdi.getTime() <= popupBit.getTime())) {
			    var overlayobject = $("#dizitanitim").colorbox({iframe:true, innerWidth:pencereW, innerHeight:pencereH, overlayClose:false, closeButton:false});
			    overlayobject.click();
			    setTimeout("$.colorbox.close()", 90000);
			};
			
			if((utcsimdi.getTime() >= popupcanliBas.getTime()) && (utcsimdi.getTime() <= popupcanliBit.getTime())) 
			{
			    $("#dizitanitim").attr("href", "/AnaSayfa/trt1canli.htm");
			    $("#dizitanitim").attr("title", "TRT 1 Canlı Yayın");
			    
			    var overlaycanliobject = $("#dizitanitim").colorbox({iframe:true, innerWidth:pencereW, innerHeight:pencereH, overlayClose:false, closeButton:true});
			    overlaycanliobject.click();
			};
		});
    </script>

