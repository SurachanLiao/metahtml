    <script type="text/javascript">
        function dosyaGetir(dosyaUrl, nereyeId) {
            $.ajax(
            {
                type: 'GET',
                url: dosyaUrl,
                cache: false,
                beforeSend: function() {
                    $(nereyeId).html('<p style=\"text-align:center;margin-top:50px;\"><img style="border:0" src="./images2012/yukleniyor.gif" /></p>');
                },
                success: function(ajaxYanit) {
                    $(nereyeId).html(ajaxYanit);
                },
                error: function() {
                    $(nereyeId).html('<p style=\"text-align:center; color: #ff0000;margin-top:20px;\">Sayfaya ulaşılamıyor!</p>');
                }
            });
        };
    </script>

