    <script type="text/javascript">
        if (readCookie(hdmCerezKodu) == null) {
            $('select#havaDurumuMerkezleri option:contains("ANKARA")').attr('selected', 'selected');
            createCookie(hdmCerezKodu, $("#havaDurumuMerkezleri option:selected").text(), 60);
        }
        else {
            var secilmisMerkez = readCookie(hdmCerezKodu);
            $('select#havaDurumuMerkezleri option:contains("' + secilmisMerkez + '")').attr('selected', 'selected');
        };

        if ($("#havaDurumuMerkezleri").is(":empty")) {
            document.getElementById("hvFOTO").innerHTML = '';
            document.getElementById("hvDegerleri").innerHTML = '';
        }
        else {

            $("select#havaDurumuMerkezleri").change(function() {
                var secilenMerkez = $("#havaDurumuMerkezleri option:selected").text();
                var secilenMerkezPlakaKodu = $("#havaDurumuMerkezleri option:selected").attr("ilKodu");
                var secilenMerkezdegerleri = $("select#havaDurumuMerkezleri option:selected").val().split(",");

                $("#dC2011").text(secilenMerkezdegerleri[0]);
                $("#yC2011").text(secilenMerkezdegerleri[1]);
                $("#mrkzADI").text(secilenMerkez);
                createCookie(hdmCerezKodu, secilenMerkez, 60);
                createCookie(hdmIlPlakaCerezKodu, secilenMerkezPlakaKodu, 60);
                hvDurumuFotoURL(secilenMerkezdegerleri[2], "#havaDurumuFotoImg");
            }).trigger('change');

        }
        
    </script>

