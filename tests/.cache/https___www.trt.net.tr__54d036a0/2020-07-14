                    <script type="text/javascript">          
                        function TRTSunucuTarihSaati(json) {
                            try{
                                var gdt = new Date(json.sdts);
                                var gtz = parseInt(json.stz);
                                var dt1 = new Date();
                                var dtf = dt1.getTime()-dt0.getTime();
                                SimdiDT.setTime(gdt.getTime() + (gdt.getTimezoneOffset() * 60 * 1000) + (gtz * 60 * 60 * 1000) - dtf);
                                robotoSaat(SimdiDT);
                            }
                            catch (e) {
                                var cdt = new Date();
                                SimdiDT = cdt;
                                robotoSaat(SimdiDT);
                            }
                        }
                    </script>

                </div>
