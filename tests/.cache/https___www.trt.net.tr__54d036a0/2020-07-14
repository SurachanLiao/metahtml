                <script type="text/javascript">
                        
                        var uniqueRandoms = [];
                        var numRandoms = datagnmdklr.length;
                        
                        function makeUniqueRandom() {
                            if (!uniqueRandoms.length) {
                                for (var i = 0; i < numRandoms; i++) {
                                    uniqueRandoms.push(i);
                                }
                            }
                            var index = Math.floor(Math.random() * uniqueRandoms.length);
                            var val = uniqueRandoms[index];                              
                            uniqueRandoms.splice(index, 1);
                            return val;
                        }
                        
                        function gundemdekileridoldur()
                        {
                            if (datagnmdklr != null && datagnmdklr.length>0)
                            {   
                                uniqueRandoms = [];
                                $("#gundemdekilerXHRicerik").empty();
                                var rwid = -1;
                                for(var i=0;i<4;i++)
                                {
                                    rwid = makeUniqueRandom();
                                    $("#gundemdekilerXHRicerik").append("<div class=\"container-fluid\">");
                                    $("#gundemdekilerXHRicerik").append("<div class=\"col-lg-6 col-md-6 col-sm-12 col-xs-12\" style='min-height: 200px;'>" + 
                                        "<a href=\"" + datagnmdklr[rwid].rwlinkurl + "\" target='_blank' style='color: Black;'>" +
                                        "<img src=\"" + datagnmdklr[rwid].rwmedyaurl + "\" class='img-responsive center-block' />" +
                                        "<div style='padding-top:2%; min-height: 60px;' class='text-center'><b>"+datagnmdklr[rwid].rwismi+"</b></div>" +
                                        datagnmdklr[rwid].rwaciklama +
                                        "</div></a>");
                                    rwid = makeUniqueRandom();
                                    $("#gundemdekilerXHRicerik").append("<div class=\"col-lg-6 col-md-6 col-sm-12 col-xs-12\" style='min-height: 200px;'>" + 
                                        "<a href=\"" + datagnmdklr[rwid].rwlinkurl + "\" target='_blank' style='color: Black;'>" +
                                        "<img src=\"" + datagnmdklr[rwid].rwmedyaurl + "\" class='img-responsive center-block' />" +
                                        "<div style='padding-top:2%; min-height: 60px;' class='text-center'><b>"+datagnmdklr[rwid].rwismi+"</b></div>" +
                                        datagnmdklr[rwid].rwaciklama +
                                        "</div></a>");
                                        $("#gundemdekilerXHRicerik").append("</div>");
                                }
                            }
                            else{
                                $("#gundemdekilerXHRicerik").html("");
                            }
                        }
                        
                        gundemdekileridoldur();
                        
                        setInterval("gundemdekileridoldur()", 59000);
