        document.addEventListener('domReady.OnDemand', function(event){
            triggerRenderAds(event);
        });
    }else{
        $(document).on('domReady.OnDemand', function(event){
            triggerRenderAds(event);
        });
    }

    sasClarinHelper.pushDataGTM();
