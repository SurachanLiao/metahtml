<script type="text/javascript">

        function linkMetrics(url,target,title){

            sendGAHit({hitType: "event", eventCategory: "header-services", eventAction: "Click", eventLabel: title});
            if ( !url.match(/tapas\.clarin/)){
                setTimeout(function(){ window.open(url,target); }, 500);
            }


        }

        function formatNumber(val){
            return ('0'+val).slice(-2);
        }

        function noHayDiario(hoy) {
            var diasSinDiario = ["01-01","01-05","07-11","25-12"];
            var hoyFormatted = formatNumber(hoy.getDate())+'-'+formatNumber(hoy.getMonth()+1);

            var founded = false;
            for(var i=0, len=diasSinDiario.length; i<len && !founded; i++)
                if (hoyFormatted == diasSinDiario[i])
                    founded = true;

            return founded;
        }

        function createCLANamespace(object) {
            if(typeof(window.CLA) == 'undefined')
                window.CLA = {};

            var objects = [window.CLA, object];
            window.CLA = objects.reduce(function(r, o){
                Object.keys(o).forEach(function (k) {
                    r[k] = o[k];
                });
                return r;
            }, {});
        }

        function setTapaLink() {
            var now = new Date();
            if(now.getHours()<6 || noHayDiario(now)) //no hay diario (obtener el del dia anterior)
                now.setDate(now.getDate() - 1);

            var tapa = now.getFullYear()+formatNumber(now.getMonth()+1)+formatNumber(now.getDate());
            var dia = formatNumber(now.getDate());
            var mes = formatNumber(now.getMonth()+1);
            var anio = now.getFullYear();
            createCLANamespace({tapa:{}});
            return CLA.tapa.link = 'https://images.clarin.com/tapas/'+anio+'/'+mes+'/'+dia+'/'+tapa+'_thumb.jpg';
        }

        setTapaLink();

        
    </script>
</div>
