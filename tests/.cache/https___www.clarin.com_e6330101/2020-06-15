    var status = sasClarinHelper.getKwPw();
    if (status !=2 ) {
        $(".suscribe").show();
        $(document).ready(function() {
            $(".footer_suscribe").css("display","block");
        });

    } else if (status == 2) {
        $(document).ready(function() {
            $("li#misuscripcion").show();
        });

