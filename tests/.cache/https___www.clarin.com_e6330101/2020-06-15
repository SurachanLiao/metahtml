<script type="text/javascript">
    bk_allow_multiple_calls=true;
    bk_use_multiple_iframes=true;
    bk_is_debug=true;

    function sendBlueKai(){
        try{

            var GDPRv;

            if(typeof $.cookie("cookieconsent_status") == "undefined"){
                GDPRv = "-2";
            }else{
                if($.cookie("cookieconsent_status") == "deny"){
                    GDPRv = "0";
                }
                if($.cookie("cookieconsent_status") == "allow"){
                    GDPRv = "1";
                }
            }

            if(typeof(window.WAuth) == "undefined")
                throw 'waiting WAuth';

            if(typeof(window.isPrivateMode) == "undefined")
                throw 'waiting bprivate';
            
            var _weizenbockUser = WAuth.getUserSync();
            if(_weizenbockUser){
                bk_addEmailHash(_weizenbockUser.email);
            }else{
                bk_addEmailHash("-2");
            }

            bk_addPageCtx('bajada', "-2");
            bk_addPageCtx('kw_ck', GDPRv);
            bk_addPageCtx('section', 'home');
            bk_addPageCtx('tipo_pagina', 'home');
            bk_addPageCtx('ns_site', 'clarin');
            bk_addPageCtx('name', 'home.inicio');
            bk_addPageCtx('ET_multimedia', '-2');
            bk_addPageCtx('kw_referrer', [document.referrer]);
            bk_addPageCtx('keywords', []);
            bk_addPageCtx('tags', []);
            bk_addPageCtx('bprivate', window.isPrivateMode);
            bk_addPageCtx('idPase', getValueFromCookie('idPase')=='' ? "-2" : getValueFromCookie('idPase'));
            bk_addPageCtx('pase_sexo', _weizenbockUser ? _weizenbockUser.genero : "-2");
            bk_addPageCtx('pase_edad', _weizenbockUser ? sasClarinHelper.getAge(_weizenbockUser.fechaNacimiento) : "-2");
            bk_addPageCtx('contador_gral', sasClarinHelper.getPaseCounter());
            bk_addPageCtx('block_LW', sasClarinHelper.getPaseStatusSW() ? "si" : "no");
            bk_addPageCtx('block_PW', 'no');
            bk_addPageCtx('tipo_suscriptor', sasClarinHelper.getTipoSuscripcion("-2"));
            bk_addPageCtx('tipo_lector', sasClarinHelper.getTipoLector());
            bk_addPageCtx('kw_pw_counter', sasClarinHelper.getPaseCounter());
            bk_addPageCtx('kw_choque', sasClarinHelper.getPaywallChoque());
            bk_doJSTag(66466, 1);
        } catch(e) {
            setTimeout(function(){
                sendBlueKai();
            }, 200);
        }
    }
    sendBlueKai();
</script>

