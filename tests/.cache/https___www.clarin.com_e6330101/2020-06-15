<script>
    const dock = document.querySelector('.dock');
    const triggers = dock.querySelectorAll('.dock__banner a');
    const target = dock.querySelector('#dockImage');
    const logoTrigger = document.querySelector('.dock__head h2 a');
    console.log(target);
    triggers.forEach(function (trigger) {
      trigger.addEventListener('mouseover', MouseOver);
      trigger.addEventListener('mouseout', MouseOut);
    });
    console.log(logoTrigger)
    logoTrigger.addEventListener('mouseover', MouseOver);
    logoTrigger.addEventListener('mouseout', MouseOut);
    function MouseOver() {
      target.classList.add('rotate');
    }
    function MouseOut() {
      target.classList.remove('rotate');
    } 
  </script>
