					function cambiar(esto)
						{
						vista=document.getElementById(esto).style.display;
						if (vista=='none'){
							document.getElementById(esto).style.display='block'
							setTimeout(function(){document.getElementById(esto).style.display='none'},5000);
						}
						else{
							document.getElementById(esto).style.display = 'none';}
						}
					function setCookie(cname,cvalue)
						{
						document.cookie = cname + "=" + cvalue + "; expires=0; path=/; domain=prevention-world.com";
						} 
					function setCountryCode(str)
						{
						setCookie('country_code',str);
						location.reload();
						}
				</script>
