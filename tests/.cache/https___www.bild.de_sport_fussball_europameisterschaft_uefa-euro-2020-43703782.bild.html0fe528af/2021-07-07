    var redirect = function (current, target, hash) {
        if (window.location.hostname !== target) {
            window.redirectInPlace = true;
            var t = window.location.href.replace(current, target);
            t = t.indexOf(".bild.html") === -1 ? t.replace(".bildMobile.html", ".bild.html") : t.replace(".bild.html", ".bildMobile.html");
            if (t.indexOf("###wt_ref") === -1) t += hash;
            window.location.href = t;
        }
    };
