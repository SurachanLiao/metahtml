<script>

    

    function logIn() {

        $("#loginDia").hide();


        var urlPath = "https://www.strategypage.com/accountapi.aspx/login";
        var user = new Object();
        user.email = $("#email").val();
        user.password = $("#password").val();

        $.ajax({
            url: urlPath,
            type: "POST",
            data: JSON.stringify(user),
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                if (data) {
                    var result = data.d;
                    if (result === "true") {
                        $("#loginMsg").hide();
                        $("#errLogin").hide();
                        $("#commentPanel").show();
                    } else {
                        $("#loggingInDia").hide();
                        $("#loginDia").show();
                        $("#errLogin").show();
                    }

                } else {
                    $("#loggingInDia").hide();
                    $("#loginDia").show();
                    $("#errLogin").show();
                }
                    

            }
            
        });

    }

    $(document).ready(function () {

        $("#commentLinkButton").click(function checkLogin(event) {
            event.preventDefault();
            if (document.cookie.indexOf("sp=") >= 0)
                $("#commentPanel").show();
            else
                $("#loginMsg").show();
        });

        $("#cancelButton").click(function () { $("#commentPanel").hide(); });

        $("#logIn").click(logIn);

        $("#loginCancel").click(function () { $("#loginMsg").hide(); });

    });
</script>



