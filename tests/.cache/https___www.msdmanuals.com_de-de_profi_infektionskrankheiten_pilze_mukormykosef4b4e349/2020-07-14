    $(".quiz__submit").click(function () {
        var value = $('input[type="radio"]:checked').val();
        var url = 'https://de.msdmanualquiz.com/gcn/startquiz';
        if (value != undefined) {
                var answerChoice = value.replace(value.substr(0, value.indexOf("&quiz_id")), "");
                var valueBaseUrl = getBaseUrl(value);
                value = MManual.quizzes.getCorrectQuizzesUrlWithLanguages(value, valueBaseUrl) + answerChoice;
