							let el = document.querySelector( '#HP5' );
							if( 'HP' != 'SA' && FFADS.slide_pixels ) {
								max_wh[1] += parseInt( FFADS.slide_pixels );
								el.classList.add( 'sliding' );
							}
							el.style.width = max_wh[0] + 'px';
							el.style.height = max_wh[1] + 'px';
							el.classList.add( 'fixed-size' );
						}
						googletag.cmd.push( function() {
							FFADS.debug( 'googletag.displaying HP5' );
							googletag.display( 'HP5' );
						} );
					}
					else document.querySelector( '#HP5-ad' ).style.display = 'none';
				</script>
</div>
</div></div><div class="tgp-post post-id-510748"><div class="img-overlay"><img class="overlay-logo" src="https://www.thegatewaypundit.com/wp-content/plugins/tgp-functionality/assets/images/site-logo.gif"><div class="entry-image "><a href="https://www.thegatewaypundit.com/2020/07/far-left-governor-jay-inslee-booed-off-stage-eastern-washington/" data-type='Internal link' data-type-location='not-set'><img width="360" height="188" class="attachment-index-small size-index-small wp-image-510754 wp-post-image" alt="" sizes="(max-width: 360px) 100vw, 360px" data-srcset="https://www.thegatewaypundit.com/wp-content/uploads/inslee-360x188.jpg 360w, https://www.thegatewaypundit.com/wp-content/uploads/inslee-345x181.jpg 345w" data-src="https://www.thegatewaypundit.com/wp-content/uploads/inslee-360x188.jpg" /></a></div></div><h3 class="entry-archive-title"><a class="post-link" href="https://www.thegatewaypundit.com/2020/07/far-left-governor-jay-inslee-booed-off-stage-eastern-washington/">Far Left Governor Jay Inslee Booed Off Stage in Eastern Washington</a></h3><span class="entry-date entry-info">July 1, 2020, 7:38am</span><span class="entry-author entry-info">by Jim Hoft</span><a class="post-comments entry-comment-count" href="https://www.thegatewaypundit.com/2020/07/far-left-governor-jay-inslee-booed-off-stage-eastern-washington/#post-comments">
