	<script>
		window.google_analytics_uacct = "UA-1101388-67";
		
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

		ga('create', 'UA-1101388-67', 'charismanews.com');
		ga('require', 'displayfeatures');
		ga('send', 'pageview');

		
		var appName = sessionStorage.getItem('appName'), appId = sessionStorage.getItem('appId'), appVersion = sessionStorage.getItem('appVersion'), uuid = sessionStorage.getItem('uuid'), checkedMobile = sessionStorage.getItem('checkedMobile'), isApp = false;

		function getMobile(ifrm) {
        		ifrm.contentWindow.postMessage({"getMobile": true}, "*");
    		}

		window.addEventListener('message', function (event) {
			if(event.origin !== "https://charismamedia.com"){
				return;
			}
			var data = JSON.parse(event.data);
		    	if (data.mobile) {
				console.log(JSON.stringify(data));
				sessionStorage.setItem('checkedMobile', true);
        			if(!appName && data.mobile.appName){
					appName = data.mobile.appName;
					ga('set', 'appName', appName);
					sessionStorage.setItem('appName', appName);
					isApp = true;
				}
				if(!appId && data.mobile.appId){
					appId = data.mobile.appId;
					ga('set', 'appId', appId);
					sessionStorage.setItem('appId', appId);
					isApp = true;
				}
				if(!appVersion && data.mobile.appVersion){
					appVersion = data.mobile.appVersion;
					ga('set', 'appVersion', appVersion);
					sessionStorage.setItem('appVersion', appVersion);
					isApp = true;
				}
				if(!uuid && data.mobile.uuid){
					uuid = data.mobile.uuid;
					ga('set', 'userId', uuid);
					sessionStorage.setItem('uuid', uuid);
					isApp = true;
				}
    			}

			if(isApp){
									ga('send', 'screenview', {screenName: '/opinion/standing-with-israel/85817-israel-s-new-government-seeks-broad-consensus-hangs-by-thread'});
							}
		});

		if(appName){
			ga('set', 'appName', appName);
			isApp = true;
		}
		if(appId){
			ga('set', 'appId', appId);
			isApp = true;
		}
		if(appVersion){
			ga('set', 'appVersion', appVersion);
			isApp = true;
		}
		if(uuid){
			ga('set', 'userId', uuid);
			isApp = true;
		}

		if(!checkedMobile){
			document.write('<iframe src="https://charismamedia.com/api/v1/Mobile.php" frameborder="0" scrolling="no" onload="getMobile(this)" width="0" height="0" style="display: none;"></iframe>');
		}else if(isApp){
							ga('send', 'screenview', {screenName: '/opinion/standing-with-israel/85817-israel-s-new-government-seeks-broad-consensus-hangs-by-thread'});
			
		}

	</script>

