<script>
    const inBodyDynamicUnits = function() {

        let slotNumber = 1;
        let nextInsertionPoint = 1;
        const distanceBetweenSlots = 4;

        document.querySelectorAll("article .article-content > p").forEach((el, i) => { 
            if (i && i === nextInsertionPoint) {
                
                // DYNAMIC UNITS SHOULD ONLY BE INJECTED IN BETWEEN 2 PARAGRAPH TAGS 
                if( el.tagName == "P" && el.nextElementSibling.nodeName == "P") {

                    var dynamicUnitPlaceholder = document.createElement("div");
                    dynamicUnitPlaceholder.className="ad-placeholder anyad slot-" + slotNumber;
                    el.parentNode.insertBefore(dynamicUnitPlaceholder, el.nextSibling);
                    slotNumber++;
                    nextInsertionPoint = nextInsertionPoint + distanceBetweenSlots;
                } else {
                    nextInsertionPoint = nextInsertionPoint + 1;
                }
            }
        })

    }   
    inBodyDynamicUnits();
</script>

