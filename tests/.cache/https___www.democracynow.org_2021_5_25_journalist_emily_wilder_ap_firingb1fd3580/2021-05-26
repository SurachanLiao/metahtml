  function getOptions(element) {
    var form = $(element).closest('form');
    return {
        callback: function() {
          console.log("SUBMITTING FORM VIA CALLBACK", form)
          DN.RecaptchaEmailInput.setRecaptchaTriggered(form);
          form.submit();
        },
        'error-callback': function() {
          console.log("ERROR CALLBACK");
          DN.RecaptchaEmailInput.setRecaptchaUntriggered(form);
        },
        'expired-callback': function() {
          console.log("EXPIRED CALLBACK");
          DN.RecaptchaEmailInput.setRecaptchaUntriggered(form);
          alert("Your reCAPTCHA expired - please try again!");
        }
      };
  }

  $(document).ready(function() {
    var recaptchaElements = document.getElementsByClassName('g-recaptcha');

    // .slice turns HTMLCollection into Array
    Array.prototype.slice.call(recaptchaElements).forEach(function (element) {
      var options = getOptions(element);
      var recaptchaId = grecaptcha.render(element, options);

      element.setAttribute('data-recaptcha-id', recaptchaId);
    });
  });
