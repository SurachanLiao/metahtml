	$(".keywords").focus(function() {
		if ($(".keywords").val() == "Rechercher par mots-clés") {
			$(".keywords").val("");
		}
	});
	$(".keywords").blur(function() {
		if ($(".keywords").val() == "") {
			$(".keywords").val("Rechercher par mots-clés");
		}
	});
	
});

/* La fonction choix_image permet de déterminer un nombre aléatoirement entre 1 et 4 */
function choix_image() {
	var aleatoire = Math.floor((Math.random()*4)+1);
	return aleatoire;
}
	
