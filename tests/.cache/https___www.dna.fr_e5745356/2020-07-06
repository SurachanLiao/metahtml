	var paginationStatus = false;
	var navigationStatus = true;
	var responsiveStatus = true;

	if (1 <= 3) {
		paginationStatus = true;
		navigationStatus = false;
	}

	if (1 >= 2) {
		$("#liseuse").hide();
        $("#liseuse .modContent").lightSlider({
            controls: navigationStatus,
            /*gallery: paginationStatus,*/
			gallery: "nextweb",
            thumbItem: 6,
            slideMargin: 0, 
            item: 1, 
            keyPress: true,
            slideMargin: 16
        });
	}
});
