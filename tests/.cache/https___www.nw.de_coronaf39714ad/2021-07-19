<script type="text/javascript">

    $(document).ready(function () {
        var frm = $('#nl-anmeldung');
        $('#referrer_url').val(window.location.href);

        frm.submit(function (event) {
            event.preventDefault();

            $.ajax({
                url: frm.attr('action'),
                type: frm.attr('method'),
                type: "POST",
                data: frm.serialize(),
                success: function(data) {
                    var resp = JSON.parse(data);
                    $('#nl-anmeldung-submit, #firstname, #lastname, #mail, #nl-info').hide();
                    $('#nl-check, #nl-success-message').show();
                    if (resp.message == "Sie sind bereits angemeldet.") {
                        $('#nl-success-message').text(resp.message);
                    }else {
                        $('#nl-message').show().css('color','rgb(19, 109, 30)').text(resp.message);
                    }
                    _paq.push(['FormAnalytics::trackFormConversion', 'nl_update_zum_mittag', 'nl-anmeldung']);
                },
                error: function (data) {
                    var resp = JSON.parse(data);
                    $('#nl-message').show().css('color','rgb(210, 10, 17)').text(resp.message);
                }
            });
        });
        _paq.push(["trackEvent", "display", "view", "nl_update_zum_mittag"]);
    });

</script>

