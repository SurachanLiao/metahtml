  <script>
    tp = window.tp || [];
    tp.push(["setContentAuthor", "Joseph Clark"]);         // The content author
    tp.push(["setContentSection", "News"]);       // The content section
    tp.push(["setTags", ["Health", "Inside Politics", "Politics", "Security", "World"]]);                 // The content tags
