<script type="text/javascript">
	function receiveMessage(event) {
		// Log console message when verbose mode is on.
		function logToConsole(msg) {
			if (document.cookie.indexOf('oil_verbose')) {
				console.log('OIL: ', msg);
			}
		}

		// Check the event data for specific string.
		function eventDataContains(str) {
			return JSON.stringify(event.data).indexOf(str) !== -1;
		}

		// Compare cookie and config version on oil_has_optedin event.
		if (event && event.data && (eventDataContains('oil_has_optedin'))) {
			// Change confirmation label to 'Apply'.
			AS_OIL.CONFIG.locale.texts.label_button_yes = AS_OIL.CONFIG.locale.texts.label_cpc_button_yes;

			// This is the configuration version set on the site.
			var configVersion = AS_OIL.CONFIG.config_version;
			// Configuration set in the cookie.
			var cookieVersion = AS_OIL.status().configVersion

			// When verbose mode is on log config version to console.
			logToConsole('Config Version ' + configVersion);
			logToConsole('Cookie Version ' + cookieVersion);

			// If versions match exit.
			if ( configVersion === cookieVersion ) {
				logToConsole('Cookie is up to date');
				return;
			}

			// Versions do not match, reset consent cookie and reload to display banner.
			logToConsole('Cookie is out of date, force optout and display banner');
			AS_OIL.triggerOptOut();
			AS_OIL.reload();
		}

		// When the user clicks customise settings.
		if (event && event.data && (eventDataContains('oil_click_advanced_settings'))) {
			// Toggle off all purposes.
			AS_OIL.CONFIG.advanced_settings_purposes_default = false;
		}

		// When the user clicks back from the advanced settings screen.
		if (event && event.data && (eventDataContains('oil_shown'))) {
			// Check if the advanced settings is being displaued.
			if (document.getElementById('as-oil-cpc')) {
				// Revert confirmation label to 'Agree'
				AS_OIL.CONFIG.locale.texts.label_button_yes = AS_OIL.CONFIG.locale.texts.label_button_yes_default;
			} else {
				// Change confirmation label to 'Apply'
				AS_OIL.CONFIG.locale.texts.label_button_yes = AS_OIL.CONFIG.locale.texts.label_cpc_button_yes;
			}
		}

		// When the user has opted in.
		if (event && event.data && (eventDataContains('oil_optin_done'))) {
			// Change confirmation label to 'Apply'
			AS_OIL.CONFIG.locale.texts.label_button_yes = AS_OIL.CONFIG.locale.texts.label_cpc_button_yes;
		}
	}

	window.addEventListener('message', receiveMessage, false);
</script>
