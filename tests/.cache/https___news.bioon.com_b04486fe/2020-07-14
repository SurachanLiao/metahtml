<script type="text/javascript">
    $(function() {
        function getData() {
            $.ajax({
                type: "get",
                url: 'https://api.center.medsci.cn/bioon-edu/bioon/meet',
                dataType: "json",
                success: function(res) {
                    let meetingList = res.result;
                    meetingList.forEach((item, index) => {
                        $('#meeting-box').append(`<a class='hover-content' href=${item.title_link} target="_blank"><span class='hover-index'>${index+1}</span><span class='hover-txt'>${item.title}</span></a>`)
                    })
                }
            });
            $.ajax({
                type: "get",
                url: 'https://api.center.medsci.cn/bioon-edu/bioon/live',
                dataType: "json",
                success: function(res) {
                    let courseList = res.result;
                    courseList.forEach((item, index) => {
                        $('#live-box').append(`<a class='hover-content' href=${item.url} target="_blank"><img class='hover-img' src=${item.img} /><span class='hover-txt2'>${item.title}</span></a>`)
                    })
                }
            });
        }
        getData();
    })

    function showBox(type) {
        if(type == 1) {
            $('#box2').hide();
            $('#content2').removeClass('seleted');
            if($('#box1').css('display') == 'none') {
                $('#box1').show();
                $('#content1').addClass('seleted');
            }else {
                $('#box1').hide();
                $('#content1').removeClass('seleted');
            }
        }else if (type ==2) {
            $('#box1').hide();
            $('#content1').removeClass('seleted');
            if($('#box2').css('display') == 'none') {
                $('#box2').show();
                $('#content2').addClass('seleted');
            }else {
                $('#box2').hide();
                $('#content2').removeClass('seleted');
            }
        }
    }
</script>


