				    grecaptcha.execute("6Le2jNEZAAAAABvOmcHh8i7HDCW57jdBn8_BnWrq", {action: "wpforms"}).then(function (token) {
				        Array.prototype.forEach.call(document.getElementsByName("wpforms[recaptcha]"), function (el) {
				            el.value = token;
				        });
				    });
