function openModal() {
$(".singleSlides-moadal").show();
WFImager.update($('.article-image'));
WFImager.update($('.singleSlides'));
document.getElementByClass("singleSlides-moadal").classList.remove("display-none");
}
function closeModal() {
// document.getElementByClass("singleSlides-moadal").classList.add("display-none");
$("div#infiniteartModal").hide();
}
function photoslideobject(){
if(isIE()) {
$(".photoPopupSlider-img").each(function () {
var $container = $(this),
imgUrl = $container.find("img").attr("src");
if (imgUrl) {
$container.css("backgroundImage", 'url(' + imgUrl + ')').addClass("custom-object-fit-IE");
}
});
}
}
