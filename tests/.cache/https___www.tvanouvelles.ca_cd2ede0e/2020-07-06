<div class="tvai-ab-barre-div"></div>

<script type="text/javascript"> 

  /* Execute the function if the object is not undefined */
  if (typeof AlertBarObject !== 'undefined'){

    /*
    * Instanciate the bar Object
    */
    var bar = new AlertBarObject();

    /*
    * Setting the Container in which the bar will be Displayed
    */
    bar.setContainer('.tvai-ab-barre-div');

    /*
    * Activate the Services. The Order determines the One on The AlertBox
    *
    * transport      => Twitter Feeds Service
    * ecoles_fermees => Ecole Fermees Service
    * gas            => Gars Service
    * meteo          => Weather Service
    * road           => Road Service.
    *
    */
    bar.setServices(new Array( 'meteo', 'road', 'gas', 'transport', 'ecoles_fermees'));

    //bar.setMobile(true);
    $(window).on('resize', function() {
      if ($(window).width() < 1020) {
        if ( !bar.isMobile ) {
         bar.setMobile(true);
         bar.init();
       }
      } else {
        if ( bar.isMobile ) {
         bar.setMobile(false);
         bar.init();
       }
      }
    });

    /*
    * Initiate the Bar
    */
    $(window).load(function(){
      if ($(window).width() < 1020) {
        bar.setMobile(true);
      }
      bar.init();
    });

  }/* End if */

</script>
