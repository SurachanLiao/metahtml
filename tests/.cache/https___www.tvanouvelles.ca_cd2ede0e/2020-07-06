<!-- Custom style -->
<style>
#wrapper .tvai-ab-barre-div .services-docking-wrap {
  z-index: 0 !important;
}
#wrapper .tvai-ab-barre-div .services-bar {
  z-index: 0 !important;
}
#wrapper .tvai-ab-barre-div .services-bar .bars-wrap{
  max-width: 1020px;
  padding-right: 20px;
  padding-left: 20px;
  width: 100%;
  @media (max-width: 1299px)
  {
    max-width: 1020px;
  }
}
#wrapper .tvai-ab-barre-page
{
  width: 100%;
}
