  <div id="covid_infolettre">
    <div class="info_covid">
      Un résumé pertinent de la journée, chaque soir, grâce aux diverses<br>sources du Groupe Québecor Média.
    </div>
    <form class="quick-signup-form">
        <div class="type-text field">
            <div class="field-wrapper">
                <label for="newsletter_email" class="sr-only"></label>
                <input type="email" class="textfield" id="newsletter_email" name="newsletter_email" placeholder="Adresse courriel" required data-test="newsletter-widget-email">
                <p><button type="submit" class="btn submit-btn" name="newsletter_submit" data-test="newsletter-widget-submit">Je m’abonne</button>
            </div>
        </div>
        <div class="type-checkbox field">
            <div class="field-wrapper">
                <input type="checkbox" id="signup-confirmation_tvasdmls" name="signup_confirmation" required checked>
            </div>
        </div>
    </form>
    <div class="message_handler"></div>
  </div>

  <script>
    /*INFOLETTRE*/
    $(function() {
      $(document).on('click', '.quick-signup-form .submit-btn', function(evt) {
          var btn = $(this);
          var form = btn.closest('form');
          var newsletterWrapper = form.closest('.newsletter-form-wrapper');
          var textfield = form.find('.type-text input');
          var email = textfield.val();
          var checkboxWrapper = $("input[name='signup_confirmation']").closest('.type-checkbox');
          var bdcOptIn = $("input[name='signup_confirmation_bdcoptin']").prop('checked');
          var hasErrors = false;
          evt.preventDefault();
          form.find('.error-message').remove();
          if (checkboxWrapper.find('input:checked').length === 0) {
              hasErrors = true;
              $('<p class="error-message">Veuillez confirmer votre inscription.</p>').insertAfter(checkboxWrapper.find('.field-wrapper'));
          }
          if (email === '') {
              hasErrors = true;
              $('<p class="error-message validation_txt"><span>Ce champ est obligatoire.</span></p>').appendTo(textfield.closest('.field'));
              $('.textfield').css('border', '1px solid #A60000');
              setTimeout(function(){ $('.validation_txt').fadeOut(2000); }, 2000);
          } else if (!validateEmail(email)) {
              hasErrors = true;
              $('<p class="error-message validation_txt"><span>Veuillez fournir une adresse courriel valide.</span></p>').appendTo(textfield.closest('.field'));
              $('.textfield').css('border', '1px solid #A60000');
              setTimeout(function(){ $('.validation_txt').fadeOut(2000); }, 2000);
          }
          if (!hasErrors) {
              var loadingGif = newsletterWrapper.find('.loading-gif');
              form.hide().remove();
              loadingGif.show();
                  // Send email to Zendata
                  var newsletterData = {};
                  newsletterData["email"] = email;
                  newsletterData["listId"] = "28376";
                  $.ajax({
                      type: "POST",
                      contentType: "application/json",
                      url: "/api/register/newsletter",
                      data: JSON.stringify(newsletterData),
                      dataType: 'json',
                      cache: false,
                      timeout: 600000,
                      success: function (data) {
                          if (data.success) {
                              feedbackHTML = '<div class="signup-success signup-feedback"><p>Abonnement réussi.<br>Merci.</p></div>';
                              $('.info_covid').hide();
                          } else if (data.alreadyExists) {
                              feedbackHTML = '<div class="signup-error signup-feedback"><p>Vous êtes déjà abonné(e) à l\'infolettre!</p></div>';
                              $('.info_covid').hide();
                          } else {
                              feedbackHTML = '<div class="signup-error signup-feedback"><p>Une erreur s\'est produite lors de votre inscription. Veuillez réessayer plus tard.</p></div>';
                              $('.info_covid').hide();
                          }
                          $('.message_handler').append(feedbackHTML);
                          $('.quick-signup-form').hide();
                          loadingGif.hide(0, function() {
                              $(this).remove();
                          });
                      },
                      error: function (e) {
                          feedbackHTML = '<div class="signup-error signup-feedback"><p>Une erreur s\'est produite lors de votre inscription. Veuillez réessayer plus tard.</p></div>';
                          $('.message_handler').append(feedbackHTML);
                          $('.quick-signup-form').hide();
                          loadingGif.hide(0, function() {
                              $(this).remove();
                          });
                      }
                });
            }
        });
    });

    function validateEmail(email) {
        var emailReg = new RegExp(/^([\w-\.]+)@((?:[\w]+\.)+)([a-zA-Z]{2,4})/i);
        var valid = emailReg.test(email);
        if (!valid) {
            return false;
        } else {
            return true;
        }
    }
  </script>
          </li>
          
          <li class="col-xs-12 col-sm-6 col-md-4 col-lg-3 dfpMultiflexBigBox" data-page-index=0 data-block-type="dfpMultiflexBigBox"> 
