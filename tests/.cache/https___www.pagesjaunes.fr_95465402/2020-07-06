<script type="text/javascript">
        var histo_lr = localStorage.getItem('pjhisto-lr');
        //si l'utilisateur a déjà un historique de lr
        var histo_lr_present = !!(histo_lr && JSON.parse(histo_lr).t && JSON.parse(histo_lr).t.length);

        var histo_fd = localStorage.getItem('pjhisto-fd');
        //si l'utilisateur a déjà un historique de fd
        var histo_fd_present = !!(histo_fd && JSON.parse(histo_fd).t && JSON.parse(histo_fd).t.length);

        //si l'utilisateur a désactivé son historique
        var histo_desactive = localStorage.getItem('pjhisto-lr_off') !== null || localStorage.getItem('pjhisto-bi_off') != null || localStorage.getItem('pjhisto-fd_off') != null;

        //on affiche le bloc histo si l'utilisateur a un historique de fd ou de lr, ou si il l'a desactivé de lui-même
        var affichageHisto =  histo_lr_present || histo_fd_present || histo_desactive;
    </script>
