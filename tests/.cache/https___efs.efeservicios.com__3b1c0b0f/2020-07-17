            {{else (t=='colf')}}<i class="fa fa-2x fa-photo align-self-center pr-2"></i>
            {{else (t=='iptc')}}<i class="fa fa-2x fa-tag align-self-center pr-2"></i>
            {{else (t=='grts')}}<i class="fa fa-2x fa-euro align-self-center pr-2"></i>
            {{else (t=='prds')}}<i class="fa fa-2x fa-product-hunt align-self-center pr-2"></i>
            {{/if}}
            <span class="list-group-item-heading align-self-center pr-3">{{:q}}</span>
            {{if (t!='befe')}}<span class="list-group-item-info align-self-center px-2">{{:s}}</span>{{/if}}
            <span class="badge align-self-center ml-auto"></span><span class="badge align-self-center ml-auto"></span>
        </div>       
    </li>
</script>

</div>


        </div>
