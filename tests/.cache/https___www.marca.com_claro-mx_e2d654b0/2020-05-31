                           <svg role="img" aria-hidden="true" focusable="false" class="is-arrow-icon" xmlns="http://www.w3.org/2000/svg" width="6" height="10" viewBox="0 0 6 10"><path fill-rule="evenodd" d="M0 8.946L3.91 5 0 1.054 1.045 0 6 5l-4.955 5z"></path></svg>
                         </button>
                         <script type="module">
                         export class Carousel{constructor(t="DOMelement",e=0,i=260,s=0,r=0){this.carouselSelector=t,this.currentSlide=e,this.intersectionWidth=i,this.scrollPosition=s,this.slideNumber=r,this.contentRef=this.carouselSelector.querySelector(".js-carousel-content"),this.contentRef&&(this.containerWidth=this.contentRef.scrollWidth,this.viewportWidth=this.contentRef.offsetWidth,"ontouchstart"in document&&this.contentRef.setAttribute("style","overflow: scroll"),this.contentRef.addEventListener("scroll",this.calculateCurrentSlide.bind(this)),window.addEventListener("resize",function(){this.containerWidth=this.contentRef.scrollWidth,this.viewportWidth=this.contentRef.offsetWidth}.bind(this)),this.arrowLeft=this.carouselSelector.querySelector(".js-carousel-prev"),this.arrowRight=this.carouselSelector.querySelector(".js-carousel-next"),this.arrowLeft.addEventListener("click",this.moveSlide.bind(this)),this.arrowRight.addEventListener("click",this.moveSlide.bind(this)),this.calculateCurrentSlide())}calculateCurrentSlide(){let t=0,e=0;this.contentRef&&(t=this.contentRef.scrollLeft,e=this.contentRef.scrollWidth),this.currentSlide=Math.ceil(t/this.viewportWidth),t>0?(this.arrowLeft.style.display="block",this.arrowLeft.setAttribute("tabindex","0")):(this.arrowLeft.style.display="none",this.arrowLeft.setAttribute("tabindex","-1")),t+this.viewportWidth<e?(this.arrowRight.style.display="block",this.arrowRight.setAttribute("tabindex","0")):(this.arrowRight.style.display="none",this.arrowRight.setAttribute("tabindex","-1"))}moveSlide(t){if(t&&t.currentTarget&&this.contentRef){let e=parseInt(t.currentTarget.getAttribute("data-offset"));isNaN(e)||(this.currentSlide+=e,this.contentRef.scrollLeft+=(this.viewportWidth-this.intersectionWidth)*e)}}};window.onload=function(){document.querySelectorAll(".js-carousel").forEach(t=>{new Carousel(t)})};
                         </script>
