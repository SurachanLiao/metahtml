        document.addEventListener('submit', function (event) {
            if (!event.target.classList.contains('mc-validate') ) {
              return;
            }
            event.preventDefault();
            submitMailChimpForm(event.target);
        }, false);

        function serialize (form) {
                var serialized = '';
                for (i = 0; i < form.elements.length; i++) {
                        var field = form.elements[i];
                        if (!field.name || field.disabled || field.type === 'file' || field.type === 'reset' || field.type === 'submit' || field.type === 'button') continue;
                        if ((field.type !== 'checkbox' && field.type !== 'radio') || field.checked) {
                            serialized += '&' + encodeURIComponent(field.name) + "=" + encodeURIComponent(field.value);
                        }
                }
                return serialized;
        };

        window.displayMailChimpStatus = function (data) {
            var mcResponse = document.querySelector('.mc-response');
            if (!mcResponse) {
              return;
            }
            mcResponse.setAttribute('tabindex', '-1');
            mcResponse.setAttribute('style', 'display:inline');
            mcResponse.focus();
            mcResponse.innerHTML = data.msg;
        };

        function submitMailChimpForm (form) {
            var url = form.getAttribute('action');
            url = url.replace('/post?u=', '/post-json?u=');
            url += serialize(form) + '&c=displayMailChimpStatus';
            var script = window.document.createElement( 'script' );
            script.src = url;
            var ref = window.document.getElementsByTagName( 'script' )[ 0 ];
            ref.parentNode.insertBefore( script, ref );
            script.onload = function () {
                    this.remove();
            };
        };


