<script>
    function getCookie(name) {
        var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
        return v ? v[2] : null;
    }
    var edition = getCookie("epoch_homepage_edition");
    var us_edition = getCookie("epoch_us_edition");
    var pathname = document.location.pathname;
    if (pathname == "/" && us_edition == 0) {
        if (edition == 'ca' || edition == 'au') {
            var elemDiv = document.createElement('div');
            elemDiv.innerHTML = '<img src="/assets/themes/eet/images/loading.gif">';
            elemDiv.style.cssText = 'display: block; margin: 20vh auto 0; text-align: center;';
            document.body.appendChild(elemDiv);
        }
        if (edition == 'ca') {
            document.location.href = '/canada';
        }
        else if (edition == 'au') {
            document.location.href = '/australia';
        }
    }
</script>
	<script>
    	// We want persisten UUID
    	function epochuser () {
			this.getUUID = function getUUID () {
				if (localStorage && "_epochUUID" in localStorage) {
					euid = localStorage._epochUUID;
				} else {
					euid = this.euid();
					localStorage.setItem("_epochUUID",this.euid());
				}
				return euid;
			}
			this.euid = function () {
				return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
					var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
					return v.toString(16);
				});
			}
		}
		var epoch = {}
		var user = new epochuser();
	</script>

