<script>
function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

if (document.location.pathname == '/' && getCookie("epoch_homepage_country") == 'ca') {
	location.replace(document.location.protocol + "//" + document.location.hostname + '/canada');
} else if (document.location.pathname == '/' && getCookie("epoch_homepage_country") == 'au') {
	location.replace(document.location.protocol + "//" + document.location.hostname + '/australia');
}
</script>
