                <li class="">
                    <a href="/en/european-council/euro-summit/" role="menuitem"><strong>Euro Summit</strong></a>
                        <ul class="dropdown-list-box">
                                <li><a href="/en/european-council/euro-summit/history/" role="menuitem">History of the Euro Summit</a></li>
                        </ul>
                </li>
        </ul>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
