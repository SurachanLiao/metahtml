var exampleAd = {};
var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];
  googletag.cmd.push(function() {
    "use strict";
    var mapping = googletag.sizeMapping().
    addSize([1000, 0], [1000, 540]).
    build();
    googletag.defineSlot('/21912588402/UnoSantaFe_Desk/Notas_RichMedia_LAYER', [], 'prestitial_adslot').
    setTargeting('kz', ['startup']).defineSizeMapping(mapping).
    addService(googletag.pubads());
    googletag.pubads().enableSingleRequest();
    googletag.enableServices();
    document.body.style.overflow="visible";
    exampleAd.hide = function() {
      document.getElementById("prestitial_layer").style.display="none";
      document.getElementById("prestitial_ad").style.display="none";
      document.body.style.overflow="visible";
      }
    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
      if(event.isEmpty && event.slot.getSlotElementId()=="prestitial_adslot") {
       exampleAd.hide();
      } else if(!event.isEmpty && event.slot.getSlotElementId()=="prestitial_adslot") {
        var adWidth = event.size[0];
        var adHeight = event.size[1];
