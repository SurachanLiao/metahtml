    // oculto el layer en blanco y cierro el aviso
    // esto se llama en 3 ocasiones:
    // - no hubo aviso
    // - apreté el boton cerrar
    // - timeout 8 segundos 
    zocalo.hide = function() {
      document.getElementById("prestitiall_layer").style.display="none";
      document.getElementById("prestitiall_ad").style.display="none";
      document.body.style.overflow="visible";
    }

    // este bloque puede convivir con otros slotRenderedEnded listeners
    // esta parte es la que hace la magia del aviso Startup
    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
      if(event.isEmpty && event.slot.getSlotElementId()=="prestitiall_adslot") {
        // no hubo aviso
        
        zocalo.hide();

      } else if(!event.isEmpty && event.slot.getSlotElementId()=="prestitiall_adslot") {
        // hay un prestitial para mostrar!

        // coloco el ad slot centrado
        var adWidth = event.size[0];
