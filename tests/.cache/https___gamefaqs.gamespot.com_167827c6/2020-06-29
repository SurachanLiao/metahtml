<script type="text/javascript">
var timer, lastsearch;

$('body :not(.game_selector)').bind('click touch', function(e)
{
	if($('.game_selector').css('display')=='block' || $('.home_game_search').hasClass('fixed'))
	{
		e.stopPropagation();
		if($(this).attr('class')!='gamespace_search' && $(this).attr('id')!='game_search' && $(this).attr('id')!='game_search_input' && $(this).attr('class')!='head')
		{
			dismiss_search_results(0);
		}
	}
});

$('#game_search_input').val('');

$('#game_search_input').click(function()
{
	$('.search_head_mobile').remove();
	$('#game_search_module').addClass('fixed');
		$('#game_search_input').focus();
});

$('#game_search_input').autocomplete(
{
	minLength: 1,
	delay: 250,
	classes: { 'ui-autocomplete':'game_selector home' },
	source: '/ajax/home_game_search?term=' + $('#game_search_input').val(),
	focus: function(){ return false; },
	close: function(event, ui)
	{
		if($('#game_search_input').val())
		{
			$('.game_selector').show();
			if($('.game_selector').has('li').length===0)
				dismiss_search_results(0);
		}
	},
	select: function(event, ui)
	{
		log_result(ui.item.row_num);
	}
})
.data('ui-autocomplete')._renderItem = function(ul, item)
{
	var list;
	if(item.product)
	{
		list += '<li><a data-row="'+item.row_num+'" data-pid="'+item.pid+'" data-url="'+item.url+'"><span class="result_img"><img class="search_img" src="'+item.image+'"/></span><span class="result_title">'+item.product_name+'<br><span style="font-size:12px;">('+item.platform_abbreviation+', '+item.release_date+')</span></span></a></li>';
	}
	else if(item.footer)
	{
		list += '<li><a data-row="'+item.row_num+'" data-url="'+item.url+'" class="dismiss_results">'+item.text+'</a></li>';
	}
	else
	{
		list += '<li><a class="dismiss_results" onclick="dismiss_search_results(0);">No Results Found - Close</a></li>';
	}
	return $(list).appendTo(ul);
};

function dismiss_search_results(fixed)
{
	$('.game_selector').hide();
	if(!fixed)
	{
				$('#game_search_module').removeClass('fixed');
		$('body').removeClass('modal_active');
	}
	$('.game_selector').empty();
}

function log_result(row)
{
	if($('.game_selector a[data-row="'+row+'"]').length>0)
	{
		var pid = $('.game_selector a[data-row="'+row+'"]').attr('data-pid');
		var query = $('#game_search_input').val();
		var label = $('.game_selector a[data-row="'+row+'"] span.result_title').text();
		$.ajax(
		{
			type: 'POST',
			url: '/ajax/log_search_click',
			data: { t:2000, s:query, c:label, r:row, x:1, p:pid, key:'f6c76915' },
			success: function(response)
			{
				window.location = $('.game_selector a[data-row="'+row+'"]').attr('data-url');
			}
		});
	}
}

</script>
	</div>
