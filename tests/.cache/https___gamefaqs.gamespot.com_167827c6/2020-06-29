<script>
$().ready(function(){

$('.po_block').click(function(){
	var new_id = $(this).attr('id').substr(3);
	var old_id = $('#poll_opt').val();
	$('#po_box_' + old_id).html('&#x2610;');
	if(new_id==old_id)
	{
		$('#poll_opt').val(0);
		$('#poll_vote').attr("disabled", "disabled");
	}
	else
	{
		$('#po_box_' + new_id).html('&#x2611;');
		$('#poll_opt').val(new_id);
		$('#poll_vote').removeAttr("disabled");
	}
});

});
</script>

