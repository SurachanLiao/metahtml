<script type="text/javascript">
function toggle_popqna(type)
{
	if(type==1)
	{
		$('ol.hot_uqna').hide();
		$('ol.hot_aqna').show();
		$('a.hot_qna_ans').css('text-decoration','underline');
		$('a.hot_qna_unans').css('text-decoration','none');
	}
	else if(type==2)
	{
		$('ol.hot_aqna').hide();
		$('ol.hot_uqna').show();
		$('a.hot_qna_unans').css('text-decoration','underline');
		$('a.hot_qna_ans').css('text-decoration','none');
	}
}
</script>
