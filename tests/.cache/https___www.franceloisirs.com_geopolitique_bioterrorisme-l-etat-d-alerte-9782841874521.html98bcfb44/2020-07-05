$(document).on('click', '#search_init', function ()
{
var button = $('#search_button');
$('#search_query_top').val("");
button.addClass("disabled");
button.attr("disabled", "disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').removeClass("content-body-blur");
$('#page').removeClass("overlay");
$(this).hide();
$("#search_autocomplete > ul").empty();
return false;
});
$('#search_block_top').on('click', '.other_button', function ()
{
var tab = { };
tab.query = { };
tab.query.keyword = $('#search_query_top').val();
var search_query = JSON.stringify(tab);
window.location = "https://www.franceloisirs.com/other-search.php?search_query=" + search_query;
return false;
});
$(document).on('keyup', '#search_query_top', function()
{
var button = $('#search_button');
if($(this).val().length >=1)
{
button.removeClass("disabled");
button.removeAttr("disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').addClass("content-body-blur");
$('#page').addClass("overlay");
$('#search_init').show();
}
else
{
button.addClass("disabled");
button.attr("disabled", "disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').removeClass("content-body-blur");
$('#page').removeClass("overlay");
$('#search_init').hide();
}
});
