$(function(){
// test formulaire envoi par mail
$('#mail_sharer').submit(function(){
var errorMsg="";
var errors = new Array();
var emptyinput = true;
var validinput = true;
var tempemptycible = new Array();
var tempvalidcible = new Array();
// check name
if(!checkInput($('body input[name="shr_firstname"]')))
{
errors.push( { 'error':"Vous n'avez pas entré de nom",'cible':"body input[name='shr_firstname']" } );
kGaSend('pageview', '/geopolitique/bioterrorisme-l-etat-d-alerte-9782841874521/Partager-fiche-produit-email/Erreur/Firstname','Partage de la fiche produit par email- Erreur firstname');
}
if(!checkInput($('body input[name="shr_lastname"]')))
{
errors.push( { 'error':"Vous n'avez pas entré de prénom",'cible':"body input[name='shr_lastname']" } );
kGaSend('pageview', '/geopolitique/bioterrorisme-l-etat-d-alerte-9782841874521/Partager-fiche-produit-email/Erreur/Lastname','Partage de la fiche produit par email- Erreur lastname');
}
$('#mail_sharer').find('input[type="email"]').each(function(){
var inputname = $(this).attr('name');
if(!checkInput($(this)))
{
tempemptycible.push("body input[name='"+inputname+"']");
}
else
{
emptyinput = false;
if(checkAjaxMail($(this), "https://www.franceloisirs.com/contactez-nous"))
{
validinput = false;
tempvalidcible.push("body input[name='"+inputname+"']");
}
else{
validinput = true;
}
}
});
//gestion des erreurs sur tous input mail
if(emptyinput == true){
var arraycible = tempemptycible.join();
errors.push( { 'error':"Vous n'avez pas entré d\'e-mail",'cible': arraycible } );
}
else{
if(validinput == false){
var arraycible = tempvalidcible.join();
errors.push( { 'error':"l\'e-mail entré n'est pas valide",'cible': arraycible } );
}
}
// check message
if(!checkInput($('body textarea[name="shr_msg"]')))
{
errors.push( { 'error':"Vous n'avez pas entré de message",'cible':"body textarea[name='shr_msg']" } );
kGaSend('pageview', '/geopolitique/bioterrorisme-l-etat-d-alerte-9782841874521/Partager-fiche-produit-email/Erreur/NoMsg','Partage de la fiche produit par email- Erreur NoMsg');
}
// afichage des erreurs
var errorContent = '';
$('body input, body textarea').removeClass('error_border');
$('.partage_email_confirm').removeClass('none');
for(var i=0;i<errors.length;i++)
{
errorContent += '<li><span class="picto">!</span><span class="centered-height">'+errors[i].error+'</span></li>';
$(errors[i].cible).addClass('error_border');
}
if(errors.length)
{
$(this).find('.log_error ul').html(errorContent);
$(this).find('.log_error').removeClass('masque');
return false;
}
});
var idx_lig = 1;
var cnt_lig = 1;
$('body').delegate("a.close_button", "click", function() {
var tr_src = $(this).closest('.main_line');
tr_src.remove();
cnt_lig--;
return false;
});
$('body').delegate("a#clone_lig", "click", function() {
if(cnt_lig < 10){
idx_lig++;
cnt_lig++;
var tr_src = $(this).closest('.main_line').find('.to_duplicate');
var tr_cible = $(this).closest('.main_line');
var tr_dst = tr_src.clone();
tr_dst.append('<a class="button bp6 close_button"><span class="icon-cross"></span></a>')
var cpl_tr_dst = $("<div class='main_line'></div>");
cpl_tr_dst.append(tr_dst);
tr_dst.find('input').attr('id', 'shr_mail_' + idx_lig);
tr_dst.find('input').attr('name', 'shr_mail_' + idx_lig);
tr_dst.find('input').attr('value', '');
tr_dst.attr('data-id', idx_lig);
tr_cible.after(cpl_tr_dst);
tr_dst.find('input').focus();
}
return false;
});
}); </script> <!--/ Partage mail --> </div> <div class="clear"></div> <figure id="mobile_zoom_product" class="only-mobile"> <img id="mpic2014488" itemprop="image"
