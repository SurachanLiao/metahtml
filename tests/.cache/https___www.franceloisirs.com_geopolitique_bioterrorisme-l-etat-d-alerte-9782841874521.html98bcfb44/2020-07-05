$("#shopping_cart").focusin(function () {
var mnu_selec = $(this);
to_mnu = setTimeout(function () {
var q = mnu_selec.queue();
if (q && (q.length > 0))
return true;
$("#header_user_info_connect_content").fadeOut(100);
/* masque panier */
$("#header_user_info").find(".header_user_info_link").removeClass("current");
$("#shopping_cart_info_content").fadeIn(200);
mnu_selec.find(".header_shopping_cart_link").addClass("current");
}, 200);
});
$("#shopping_cart").focusout(function () {
clearTimeout(to_mnu);
to_mnu_close = setTimeout(function ()
{
$("#shopping_cart_info_content").fadeOut(100);
$("#shopping_cart").find(".header_shopping_cart_link").removeClass("current");
}, 100);
});
$("#shopping_cart").hover(function () {
var mnu_selec = $(this);
to_mnu = setTimeout(function ()
{
var q = mnu_selec.queue();
if (q && (q.length > 0))
return true;
$("#header_user_info_connect_content").fadeOut(100);
/* masque panier */
$("#header_user_info").find(".header_user_info_link").removeClass("current");
$("#shopping_cart_info_content").fadeIn(200);
mnu_selec.find(".header_shopping_cart_link").addClass("current");
}, 200);
}, function () {
clearTimeout(to_mnu);
to_mnu_close = setTimeout(function ()
{
$("#shopping_cart_info_content").fadeOut(100);
$("#shopping_cart").find(".header_shopping_cart_link").removeClass("current");
}, 100);
});
