        document.getElementById(id).height= (newheight) + "px";
        document.getElementById(id).width= (newwidth) + "px";
        document.getElementById(id).style.maxHeight = (newheight) + "px";
        document.getElementById(id).style.maxWidth = (newwidth) + "px";
        document.getElementById(id_d).style.maxWidth = (newwidth) + "px";
