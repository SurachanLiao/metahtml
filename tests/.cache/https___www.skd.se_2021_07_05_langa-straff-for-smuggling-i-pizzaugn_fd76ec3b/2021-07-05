        <div id="skd_outsider_39" class="adContainer " aria-label="Annons" tabindex="-1"></div>

        <script>
           var section = "Malmö";  
           section = section || skd_category;
       
          // If on sport use custom lw tags
          if (section && section == "sport") {
            $("#skd_outsider_39").attr("id", "skd_outsider_39".replace("_", "_sport"));

            skd_ad_lw_ad("Outside", "skd_outsider_39".replace("_", "_sport"), section);
          } else {
            skd_ad_lw_ad("Outside", "skd_outsider_39", section);
          }
        </script>	
