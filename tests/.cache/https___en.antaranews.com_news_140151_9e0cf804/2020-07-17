<script src="https://en.antaranews.com/js/plugins/validate/jquery.validate.min.js" type="text/javascript"></script>
<script src="https://en.antaranews.com/js/plugins/validate/id.js" type="text/javascript"></script>
<script src="https://en.antaranews.com/js/plugins/ladda/spin.min.js" type="text/javascript"></script>
<script src="https://en.antaranews.com/js/plugins/ladda/ladda.min.js" type="text/javascript"></script>
<script type="text/javascript">
$(function() {
	$.validator.setDefaults({
		highlight: function (element, errorClass, validClass) {
			var elem = $(element);
			elem.closest(".form-group").addClass("has-error");
		},
		unhighlight: function (element, errorClass, validClass) {
			var elem = $(element);
			elem.closest(".form-group").removeClass("has-error");
		},
		errorElement: "span",
		errorClass: "help-block",
		errorPlacement: function (error, element) {
			var elem = $(element);
			error.insertAfter(element);
		}
	});
	var validatelogin = $("#loginForm").validate({
		framework: "bootstrap",
		rules: {
			username: {
				required: true
			},
			pass: {
				required: true
			}
		},
		submitHandler: function(form) {
			var formData = $("#loginForm").serialize();
			formData += "&ajax=true";
			var l = Ladda.create(document.querySelector(".ladda-button"));
			l.start();
			$.ajax({
				type: "POST",
				cache: false,
				url: "/ajax/login_ajax.php",
				data: formData,
				dataType: "json",
				success: function(data) {
					var status = parseInt(data.status);
					if(status==1) {
						$(".alert").removeClass("alert-danger").addClass("alert-success").removeClass("hidden");
						$(".alert").empty().html(data.message);
						setTimeout(function () {
							window.location.replace(data.url);
						}, 500);
					}
					else
					{
						$(".alert").removeClass("alert-success").addClass("alert-danger").removeClass("hidden");
						$(".alert").empty().html(data.message);
					}
				}
			}).always(function() {
				l.stop();
			});
		}
	});
});
</script><script>
	window._izq = window._izq || []; window._izq.push(["init"]);
</script>
