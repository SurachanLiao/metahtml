    <script>
    var dtLL = true;
    var dtWW = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth; var dtHH = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
    var dtMaxW = 100, dtShowVLarge = true;
    var getIEVersion = function(){var myNav = navigator.userAgent.toLowerCase(); return (myNav.indexOf('msie') != -1) ? parseInt(myNav.split('msie')[1]) : false;}    ;

    function resizeThumbsHeight() {
        var dtWW = window.dtWW;
        var dtMaxW = window.dtMaxW;
        var dtShowVLarge = window.dtShowVLarge;
        var dtGallerySettings = window.dtGallerySettings;

        dtWW = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth; var dtHH = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;

        if(dtWW <= 414){
        dtMaxW = isVideoPage()?(dtWW - 40):(dtWW - 80);
        dtShowVLarge = false;
        }
        else if(dtWW <= 576){
            dtMaxW = isVideoPage()?125:150;
            dtShowVLarge = false;
        }
        else if(dtWW <= 767){
            dtMaxW = isVideoPage()?150:175;
            dtShowVLarge = false;
        }
        else if(dtWW <= 991)
            dtMaxW = isVideoPage()?200:225;
        else if(dtWW <= 1280)
            dtMaxW = isVideoPage()?225:275;
        else
            dtMaxW = isVideoPage()?275:325;

        dtGallerySettings.maxh = dtMaxW;
    }

            var dtGallerySettings = false;
    
    var dtTH = dtHH * 0.25;

    function isVideoPage(){
        var pn = document.location.pathname || '';
        var s = document.location.search || '';
        if(s != '' && s.indexOf('s_video=y') != -1)
            return true;
        if(pn != '' && (pn.indexOf('stock-video-footage') != -1 || pn.indexOf('stock-footage') != -1) || pn.indexOf('-popular-videos') != -1 || pn.indexOf('-more-latest-videos') != -1)
            return true;
    }
    function enableHoverOnVideo(count){
        var $container = getThumbsContainer(window['dtAllImagesOptions' + count]);
        var $videos = $container.find('.item-video');
        $videos.unbind()
            //.hover(videoMouseOver, videoMouseOut);
            .bind('mouseenter touchstart', videoMouseOver)
            .bind('mouseleave touchend', videoMouseOut);
    
        function videoMouseOver(evt){
            var isTouch = ['touchstart', 'touchend'].indexOf(evt.type) != -1;
            if(this.over) return;
            this.over = true;
            playInlineVideo(this);
        }

        function videoMouseOut(evt){
            var isTouch = ['touchstart', 'touchend'].indexOf(evt.type) != -1;
            this.over = false;
            var el = this;
            /*
            if(isTouch){
                clearTimeout(this.tmrPlay);
                this.tmrPlay = setTimeout(function(){stopInlineVideo(el);}, 5000);
            }
            */
            stopInlineVideo(el);
        }
        function playInlineVideo(el){
            var $el = $(el);
            
            stopInlineVideo(el);

            var $player = $('<div></div>').addClass('inline-video');//.css('background-color', '#F00');
            //var thumbimgprev = $el.find('.bigthumb').data('src') || '';
            $el.attr('id').match(/((thb_cell)|(thcell))(\d+)/);
            var imgid = parseInt(RegExp.$4) || 0;
            if (!imgid) {
                return false;
            }
            var playerHtml = [];
            playerHtml.push('<div class="video-loading"></div>')
            playerHtml.push('<video height="100%" preload="metadata" autoplay muted>');
            playerHtml.push('<source src="//thumbs.dreamstime.com/videothumb_' + (dtShowVLarge?'large':'') + Math.floor(imgid / 10000) + '/' + imgid + '.mp4" type=\'video/mp4; codecs="avc1.42E01E, mp4a.40.2"\'  />');
            playerHtml.push('<source src="//thumbs.dreamstime.com/videothumb_' + (dtShowVLarge?'large':'') + Math.floor(imgid / 10000) + '/' + imgid + '.webm" type=\'video/webm; codecs="vp8, vorbis"\'  />');
            playerHtml.push('</video>');

            $player.html(playerHtml.join(''));
            $el.find('>a').append($player);
            $el.find('video').bind('play', function(){
                $(this).parent().find('.video-loading').addClass('loaded');
            });
        }

        function stopInlineVideo(el){
            $(el).find('.inline-video').remove();
        }
    }
    function enableLazyLoadNew(){
        if(this.enabled) return;
        this.enabled = true;
        if(getIEVersion() && getIEVersion() <=10){
            $('.lazy').each(function(){
                $(this).attr('src', $(this).data('src'));
            });
        }
        else {
            yall({
                observeChanges: true,
                threshold: Math.max(150, dtTH || 200),
                events: {
                    load: function (event) {
                        if (!event.target.classList.contains("lazy") && event.target.nodeName == "IMG") {
                            //force clean-up!
                            event.target.classList.remove("showonload");
                        }
                    }
                }
            });
        }
    }
    
</script>
