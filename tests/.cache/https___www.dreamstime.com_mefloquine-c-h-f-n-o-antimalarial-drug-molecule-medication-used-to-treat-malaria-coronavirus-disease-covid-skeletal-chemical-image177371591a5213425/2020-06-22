    function dtProcessAllImages0(){
        arrangeImages.options = arrangeImages.options || [];
        arrangeImages.options.push(dtAllImagesOptions0);
        try {
            expandUnifiedMarkupThumbnail({"count":0,"container":"#ilc1","nohover":true,"showtitle":false,"nolb":1,"notitle":0,"assignments_vote":0,"assignments_archive":0,"gimali":"DTL_BOTTOMFRAME_036","gimalv":"DTL_BOTTOMFRAME_038"});
        }
        catch (e) {}
        
