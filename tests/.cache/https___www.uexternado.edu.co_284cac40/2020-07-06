  <div class="centrar">
    <div class="slider-eventos">
      <section class="centrar">
        <div class="carrusel-interno-1columna">
          <!--Eventos y calendario -->
			
	  	
	  	<article class="modulo-centrado modulo-relacionado modulo-evento-image administracion-de-empresas-turisticas-y-hoteleras defecto">	
	    		    <a href="https://www.uexternado.edu.co/evento/administracion-de-empresas-turisticas-y-hoteleras/orientacion-para-implementar-los-protocolos-especificos-de-bioseguridad-en-restaurantes/">
		      <!--Esta imagen debe ser de 500 x 305px-->
		      <img src="https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-457x305.jpg" srcset="https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-457x305.jpg 457w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-300x200.jpg 300w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-1024x683.jpg 1024w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-768x512.jpg 768w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-1200x800.jpg 1200w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-450x300.jpg 450w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-380x253.jpg 380w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes-140x93.jpg 140w, https://www.uexternado.edu.co/wp-content/uploads/2020/07/restaurantes.jpg 1300w" sizes="(max-width: 457px) 100vw, 457px" alt="" />		    </a>
		  			    <div class="fecha-evento">
				    <div class="fondo">
				       <!--dia-evento-->
				      <span>06</span>
				      <!--mes-evento-->
