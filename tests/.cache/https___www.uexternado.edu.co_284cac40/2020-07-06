        <input type="radio" id="next-week" onclick="addDisableRange()" name="date" value="2020-07-13|2020-07-19"><label for="next-week" >Próxima semana</label>
        <input type="radio" id="next-month"  onclick="addDisableRange()" name="date" value="2020-07-01|2020-07-31"><label for="next-month"  style="text-transform: capitalize;">julio</label>
        <input type="radio" id="next-next-month"  onclick="addDisableRange()" name="date" value="2020-08-01|2020-08-31"><label for="next-next-month" style="text-transform: capitalize;">agosto</label>
        <input type="radio" id="all" onclick="addDisableRange()" name="date" value=""><label for="all">Todos</label>
        <input type="radio" id="range" onclick="removeDisableRange()" name="date" value=""><label for="range">Rango</label>
        <div class="search type-date">
          <input type="text" id="form" name ="from" class="range-filter-year" placeholder="Desde" disabled>
          <input type="text" id="to" name ="to" class="range-filter-year" placeholder="Hasta" disabled>
        </div>



        <p class="aligncenter">
          <button id="apply" class="close boton" data-dismiss="modal" aria-label="Close" >Aplicar</button>
        </p>
      </form>
