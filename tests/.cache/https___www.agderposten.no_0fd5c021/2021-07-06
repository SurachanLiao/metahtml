            function loadGDPRTransparencyScript() {
                var d = document,
                e = d.createElement('script'),
                p = d.getElementsByTagName('head')[0];
                e.type = 'text/javascript';
                e.async = true;
                e.src = '//cogwheel.inventory.schibsted.io/prod/latest/gdpr-bundle.js';
                p.insertBefore(e, p.firstChild);
            };

            (function() {
                var d = document, e = d.createElement('script'),
                p = d.getElementsByTagName('head')[0];
                e.type = 'text/javascript'; e.async = true;
                e.src = '//acdn.adnxs.com/ast/ast.js';
                e.onload = loadGDPRTransparencyScript;
                p.insertBefore(e, p.firstChild);
            })();

