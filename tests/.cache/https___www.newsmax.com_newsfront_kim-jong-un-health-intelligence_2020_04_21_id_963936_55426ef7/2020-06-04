}
else if(window.location.pathname.toLowerCase().indexOf("/awards")>=0){ document.getElementById("nmHomeLink").className="nmMainNavLink"; document.getElementById("nmBestListsLink").className="nmMainNavLinkSel"; document.getElementById("nmBotHomeLink").className="nmMainNavLink"; document.getElementById("nmBotBestListsLink").className="nmMainNavLinkSel";
}
else if(window.location.pathname.toLowerCase().indexOf("/jokes")>=0){ document.getElementById("nmHomeLink").className="nmMainNavLink"; document.getElementById("nmJokesLink").className="nmMainNavLinkSel"; document.getElementById("nmBotHomeLink").className="nmMainNavLink"; document.getElementById("nmBotJokesLink").className="nmMainNavLinkSel";
}
</script>

