	<script >
		var bidder_turn;

		if (switcher_id != 0) {
			bidder_turn = 'is yl';
			window.yieldlove_stop_adserver_requests = true;
			window.setTimeout(function() {
				if (typeof YLHH == "undefined") {
					googletag.cmd.push(function() {
