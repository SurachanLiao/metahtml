//});

//$(document).ready(function() {
//	$('.class', '#class-container').css ('color', '#123456');
//	$('.zitate', '#article').wrapInner('<div class="article__text__zitat article__text__zitat--left"></div>')
//	$(".article iframe[src^='https://www.youtu']").wrap('<div class="ngVideo"></div>');
