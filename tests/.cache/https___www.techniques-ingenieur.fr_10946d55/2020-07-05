	function submitFormWithParams(formName, fields){
		var objFrm = document.forms[formName];
		for (j=fields.length-1;j>=0;j--){var f= objFrm.elements[fields[j].f];if (f){f.value=fields[j].v};}
		objFrm.submit();
	}
</script>

