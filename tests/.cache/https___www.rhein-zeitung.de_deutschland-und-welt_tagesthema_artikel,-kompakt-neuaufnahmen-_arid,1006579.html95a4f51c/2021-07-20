		document.querySelector('.sharebuttonsul').classList.add('hidden');
		document.querySelector('#webshare').classList.remove('hidden');
	}
	async function mrvWebShare() {
		if (navigator.share === undefined) {
			return;
		}
		const title = 'Rhein-Zeitung.de - Artikel';
		const text = ' \nKompakt:  Neuaufnahmen\n\n
Der Wortschatz entwickelt sich stetig weiter. Unter den 5000 Neuaufnahmen in den Duden sind deshalb viele W&amp;ouml;rter, die Entwicklungen aus den Bereichen Umwelt, Politik, Internet und unterhaltende Informationstechnik widerspiegeln. Hier eine Auswahl:
\n\n';
		const url = 'https://www.rhein-zeitung.de/ar.1006579';
		try {
			await navigator.share({title, text, url});
		} catch (error) {
			console.log('Error sharing: ' + error);
			return;
		}
		console.log('Successfully sent share');
	}
	document.querySelector('#webshare').addEventListener('click', mrvWebShare);
