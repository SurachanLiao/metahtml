       function tpCheckIfEmpty(array) {
         if (array !==""){
           return array + ", "
         }
       } 
       function tpFormatTags (keywords, persons, places, organizations){
         let keywordString = tpCheckIfEmpty(keywords) + tpCheckIfEmpty(persons) + tpCheckIfEmpty(places) + tpCheckIfEmpty(organizations);
         //keywordString = keywordString.slice(0, -2); Diese funktion ist verantwortlich, das der Block nicht mehr weiter ausgeführt wird.
         return keywordString;
       }
