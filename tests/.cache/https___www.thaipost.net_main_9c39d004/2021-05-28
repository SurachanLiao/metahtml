	}
	var baseUrl = 'https://www.thaipost.net/main/';
	var footerMenuFirst = '6';
	var footerNewsAllHtml = {};
	var footerAllCategoryId = [6,7,11];
	function getFooterMenuBody(category_id) {
		if (typeof footerNewsAllHtml['category_id_' + category_id] === "undefined") {
    		$.getJSON( "https://www.thaipost.net/main/home/ajax_posts_via_category?category_id=" + category_id, function( jsonData ) {
    			// console.log(jsonData);
    			// jsonData = $.parseJSON(jsonData);
    			var html = '';
    			$.each(jsonData, function(i, val) {
                    html += '<div class="col-md-6 col-xs-6">';
                    html += '	<div class="row">';
    				html += '		<div class="col-md-4"><a href="'+baseUrl+'detail/'+val.id+'"><img src="'+val.image_mid+'" class="img-responsive" /></a></div>';
                    html += '		<div class="col-md-8"><a class="post-title" href="'+baseUrl+'detail/'+val.id+'">'+val.title+'</a></div>';
                    html += '	</div>';
                    html += '</div>';
                });
                html += '<div class="col-md-12 col-xs-12"><a href="'+baseUrl+'category/'+category_id+'" class="pull-right btn"><strong>ดูทั้งหมด</strong></a></div>';
                footerNewsAllHtml['category_id_' + category_id] = html;
                $(".footerMenu__body").html(html);
    		});
		}
		else {
			$(".footerMenu__body").html(footerNewsAllHtml['category_id_' + category_id]);
		}
	}
	function getAllFooterMenuBodyHtml() {
		for (var i = 1; i < footerAllCategoryId.length; i++) {
			$.getJSON( "https://www.thaipost.net/main/home/ajax_posts_via_category?category_id=" + footerAllCategoryId[i], function( jsonData ) {
    			var html = '';
    			$.each(jsonData, function(i, val) {
                    html += '<div class="col-md-6 col-xs-6">';
                    html += '	<div class="row">';
    				html += '		<div class="col-md-4"><a href="'+baseUrl+'detail/'+val.id+'"><img src="'+val.image_mid+'" class="img-responsive" /></a></div>';
                    html += '		<div class="col-md-8"><a class="post-title" href="'+baseUrl+'detail/'+val.id+'">'+val.title+'</a></div>';
                    html += '	</div>';
                    html += '</div>';
                });
                html += '<div class="col-md-12 col-xs-12"><a href="'+baseUrl+'category/'+footerAllCategoryId[i]+'" class="pull-right btn"><strong>ดูทั้งหมด</strong></a></div>';
                footerNewsAllHtml['category_id_' + footerAllCategoryId[i]] = html;
    		});
		}
	}
	$(document).ready(function() {
// 		getFooterMenuBody(footerMenuFirst);
// 		getAllFooterMenuBodyHtml();
	});
</script>

