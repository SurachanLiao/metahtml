              let siteId = hbBiddersParams[bidder]['siteId'];

              // cannot run JSON.parse() on undefined, so check that size is configured first
              if (sizeAsString) {
                let sizeAsArray = JSON.parse(sizeAsString);
                  // according to docs we have to send one bid per size for IX
                  sizeAsArray.forEach(function(unitSize){
                    params.push({
                        bidder: bidder,
                        params: {
                          'size': unitSize,
                          'siteId': siteId
                        }
                    });
                  });
              }
              else {
                params.push({
                        bidder: bidder,
                        params: {
                          'siteId': siteId
                        }
                    })
              }
            }
            // if bidder not IX
            else {
              params.push({
                bidder: bidder,
                params: hbBiddersParams[bidder]
              })
            }
        }
        adUnits = window.adUnits || [];
        adUnits.push({
            code: 'div-sidebar2_desk-99887',
            mediaTypes: {
                banner: {
                    sizes: [[300, 250], [300, 600]]
                }
            },
        bids: params
        });
    }
    

    googletag.cmd.push(function() {
        slots['div-sidebar2_desk-99887'] = googletag.defineSlot('/3910/investing/sidebar2_desk', [[300, 250], [300, 600]], 'div-sidebar2_desk-99887')
    .addService(googletag.pubads())
        
    .setTargeting('pos', adCount)
        
    .setTargeting('placement', 'sidebar2_desk')
        
    .setCollapseEmptyDiv(true);
    });

    
</script>


  



  
  

</head>

