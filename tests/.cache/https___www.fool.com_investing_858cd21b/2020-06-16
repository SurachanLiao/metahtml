    if (typeof window.Infotrack !== "undefined")  {
        var infotrackUrl = '//g.foolcdn.com/mms/resources/js/infotrack_min.js';
        Infotrack.load(infotrackUrl);
        var infotrackInitializeWait = infotrackInitializeWait || 0;
