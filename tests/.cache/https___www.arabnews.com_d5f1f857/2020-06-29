          if(_img_public.length == 2){
            var _img_most = 'https://www.arabnews.com/sites/default/files/'+_img_public[1];
            
          } else if (_img_public.length == 1){
            var _img_most = item.type_article;
          }
          html +='<div class="object-item" data-nid="'+_nid_article[2]+'"><div class="media-object"><div class="media-object-section"><div class="media-object-img"><a href="'+item.url+'"><img width="100" src="'+_img_most+'"></a></div><div class="meta-area"><span class="views-count  views-count-placeholder nid-'+_nid_article[2]+'"><i class="icomoon-icon icon-views"></i> <span>'+_number_view+'</span></span></div></div><div class="media-object-section"><h6 class="font-bold"><a href="'+item.url+'">'+item.page+'</a></h6></div></div>';
              if (i == 5){
              return false;
              }    
          });
