  jQuery(document).ready(function() {
    var input = jQuery('input.search-input.form-text');
    defaultValue = input.val();
    input.click(function() {
      if( this.value == defaultValue ) {
        jQuery(this).val("");
      }
    });

    $('div.dlz_ssmenu div.pane-content').hide();
    $('div.dlz_ssmenu').each(function() {
      if ($(this).hasClass('open')) {
        $(this).children('div.dlz_ssmenu div.pane-content').show();
      }
    });

    $('div.dlz_ssmenu div.pane-title').click( function () {
      if ( $(this).parents('div.dlz_ssmenu').hasClass('open') ) {
        $(this).parents('div.dlz_ssmenu').removeClass('open');
        $(this).parents('div.dlz_ssmenu').children('div.dlz_ssmenu div.pane-content').slideUp('fast', function () {
          $(this).parents('div.dlz_ssmenu').children('div.dlz_ssmenu div.pane-content').hide();
        });
      }else {
        $(this).parents('div.dlz_ssmenu').addClass('open');
        $(this).parents('div.dlz_ssmenu').children('div.dlz_ssmenu div.pane-content').slideDown('fast', function () {
          $(this).parents('div.dlz_ssmenu').children('div.dlz_ssmenu div.pane-content').show();
        });
      }
      return false;
    });

    $('a[rel*=external]').attr('target','_blank');
    $('a.type_link_external_link').attr('target','_blank');
    $("a.dz_hype").click(function() {
      var targetWndName = 'PopupInterRevue';
      var popupWidth = 800;
      var popupHeight = 700;
      var posX = screen.availWidth/2 - popupWidth/2;
      var posY = screen.availHeight/2 - popupHeight/2;
      var newwindow = window.open(this.href, targetWndName, 'scrollbars=yes,resizable=yes,status=yes,width=' + popupWidth + ',height=' + popupHeight + ',top=' + posY + ',left=' + posX);
      this.target = targetWndName;
      if (window.focus) {
        newwindow.focus()
      }
      return true;
    });
  });
