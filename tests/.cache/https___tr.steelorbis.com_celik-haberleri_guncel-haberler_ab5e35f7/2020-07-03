<script>
$(document).ready(function(){
	
		$('#fProductGroup').multiselect({
			 nonSelectedText: 'Ürün Grubu',
			 enableFiltering: true,
			 enableCaseInsensitiveFiltering: true,
			 buttonWidth:'100%',
			 includeSelectAllOption: true,
			 selectAllText: 'Hepsi',
			 selectAllValue: 10000,
			 selectAllNumber: false,
			 allSelectedText: 'Ürün Grubu: Hepsi',
			 buttonText: function(options, select) {
			 	if (options.length == 0) {
			 		return 'Ürün Grubu';
			 	}else if (options.length == 1) {
	                   var labels = [];
	                   options.each(function() {
	                       if ($(this).attr('label') !== undefined) {
	                           labels.push($(this).attr('label'));
	                       }
	                       else {
	                           labels.push($(this).html());
	                       }
	                   });
	                return 'Ürün Grubu: ' + labels;
	           } else if ($('option', $(select)).length == options.length) {
	        	   return 'Ürün Grubu: Hepsi';
	           } else {
	        	   return 'Ürün Grubu: ' + options.length + ' seçili';
	           }
			 }
		});
	
