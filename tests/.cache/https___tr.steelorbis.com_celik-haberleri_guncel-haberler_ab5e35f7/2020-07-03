	});
	$('#fCountry').multiselect({
		 nonSelectedText: 'Ülke',
		 enableFiltering: true,
		 enableCaseInsensitiveFiltering: true,
		 buttonWidth:'100%',
		 includeSelectAllOption: true,
		 selectAllText: 'Hepsi',
		 selectAllValue: 10003,
		 selectAllNumber: false,
		 allSelectedText: 'Ülke: Hepsi',
		 buttonText: function(options, select) {
		 	if (options.length == 0) {
		 		return 'Ülke';
		 	}else if (options.length == 1) {
                   var labels = [];
                   options.each(function() {
                       if ($(this).attr('label') !== undefined) {
                           labels.push($(this).attr('label'));
                       }
                       else {
                           labels.push($(this).html());
                       }
                   });
                return 'Ülke: ' + labels;
           } else if ($('option', $(select)).length == options.length) {
        	   return 'Ülke: Hepsi';
           } else {
        	   return 'Ülke: ' + options.length + ' seçili';
           }
		 }
	});
	$('#fSector').multiselect({
		 nonSelectedText: 'Sektör',
		 enableFiltering: true,
		 enableCaseInsensitiveFiltering: true,
		 buttonWidth:'100%',
		 includeSelectAllOption: true,
		 selectAllText: 'Hepsi',
		 selectAllValue: 10004,
		 selectAllNumber: false,
		 allSelectedText: 'Sektör: Hepsi',
