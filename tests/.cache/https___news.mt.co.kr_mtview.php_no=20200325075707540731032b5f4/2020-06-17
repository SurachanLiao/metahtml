    function goShare(sns, type) {
        switch (sns) {
            case  'fb':
                shareManager.facebook(sns, type);
                break;
            case  'kl':
                shareManager.kakaolink(sns, type);
                break;
            case  'klm':
                shareManager.kakaolinkMe(sns, type);
                break;
            default:
                shareManager.countUpdate(sns);
                share(sns, type);
                break;
        }
    }

