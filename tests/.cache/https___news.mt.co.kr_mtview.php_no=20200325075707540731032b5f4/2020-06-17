<script type="text/javascript">
	//베스트클릭 슬라이드
	$('.bestclick_list').each(function(){
		var btnPrev = $(this).find('.btn_wrap2 .btn_prev'); //커스텀 이전 버튼
		var btnNext = $(this).find('.btn_wrap2 .btn_next'); //커스텀 다음 버튼
		var btnDots = $(this).find('.btn_dots'); //커스텀 버튼 페이지
		var slider = $(this).children('.group'); //슬라이더 선택자

		slider.slick({
			dots: true,
			prevArrow: btnPrev,
			nextArrow: btnNext,
			appendDots: btnDots
		});
	});
	//베스트클릭, 최신 뉴스, 많이 본 뉴스 탭메뉴 이벤트
	$('.vtabbox1 li a').mouseenter(function(){
		vtabEvent(this);
	});

	//탭메뉴 이벤트 함수
	function vtabEvent(_this){
		var vtabli = $(_this).parent('li');
		var idx = vtabli.index();
		var cont_list = $(_this).parents('.vtabbox1').next('.vtabcont1').children('div');

		vtabli.siblings().children('a').removeClass('on');
		$(_this).addClass('on');
		cont_list.hide();
		cont_list.eq(idx).show();
		if (cont_list.hasClass('bestclick_list'))
		{
			cont_list.eq(idx).children('.group').get(0).slick.setPosition();
		}
	}
</script>
    </div>
    <!-- 좌측 배너영역 일괄적용 -->
