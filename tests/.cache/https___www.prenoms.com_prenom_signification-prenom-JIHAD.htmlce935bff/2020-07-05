<script type="text/javascript">

    if(jQuery.cookie("Voted_JIHAD"))
        jQuery('#form_vote').html("Vous avez déjà voté.");

    function SetVote(value) {
        if(jQuery.cookie("Voted_JIHAD"))
        {
            return;
        }

        $.ajax({
            type: "POST",
            url: '/Rating/SetRatingPrenomPages',
            data: JSON.stringify({
                'IdObj': 'JIHAD',
                'Value' : value
                }),
             contentType: "application/json; charset=utf-8",
             success: function () {
                 jQuery.cookie("Voted_JIHAD",true ,{ expire: new Date(2021 , 7 , 6 ,00,00,00) } );
                 jQuery('#form_vote').html("Votre vote a été pris en compte");

             },
            error: function (data) {
                jQuery('#ConfirmVote').html('Une erreur est survenue, votre vote n\' pas été pris en compte.');
                jQuery('#ConfirmVote').prop('style', 'color:red;');
                jQuery('#ConfirmVote').show();
                jQuery('#ConfirmVote').fadeOut(1500);
             }
        });

    }

    function changeStarlet(index,etat) {
        imgStarSrc = "";

        if(etat == 1)
        {
            imgStarSrc = "fa fa-star";

        }
        else
        {
            imgStarSrc = "fa fa-star-o";
        }
        for(i=1 ; i <= index;i++)
        {
            jQuery('#star_' + i).prop('class',imgStarSrc);
        }
    }
</script>

