<form action="/Research/RedirectToResultPage" class="form-inline" id="FormResearch" method="post">    <div class="collapse" id="searchbox">
        <input data-val="true" data-val-number="The field SectionId must be a number." data-val-required="The SectionId field is required." id="SectionId" name="SectionId" type="hidden" value="0" />
        <input data-val="true" data-val-required="The Type_Container field is required." id="type_Container" name="type_Container" type="hidden" value="0" />
        <input id="PageRech" name="PageRech" type="hidden" value="1" />

        <input PlaceHolder="Recherche ..." class="form-control" id="Recherche_box" name="tb_research" type="text" value="" />
        <button class="btn my-2 my-sm-0" type="submit">OK</button>

<input id="type_Search" name="type_Search" type="hidden" value="3" /><input id="type_Ordre_Hidden" name="type_Ordre" type="hidden" value="1" />    </div>
    <style type="text/css">
        .ui-autocomplete-term {
            font-weight: bold;
            color: blue;
        }

        .ui-autocomplete-objecttype {
            font-weight: bold;
            color: red;
        }
    </style>
    <script type="text/javascript">

    $(function () {
        var termTemplate = "<span class='ui-autocomplete-term'>%s</span>";
        var objectTemplate = "<span class='ui-autocomplete-objecttype'>%t</span>";
        var strItemTypes = [];

        $('#Recherche_box').autocomplete({
            source: function (request, response) {
                var data_research = $("#Recherche_box").val();
                var data_temp = data_research;
                var data_final = "";
                var nb_Ing = 0;

                while (data_temp.indexOf(',') > 0) {
                    data_final += data_temp.substr(0, data_temp.indexOf(',') + 1);
                    data_temp = data_temp.substr(data_temp.indexOf(',') + 1);
                    nb_Ing++;
                }
                if (data_temp.trim().length < 2 || nb_Ing > 3)
                    return;

                $.ajax({
                    url: '/AutoCompleteResearch/AutoComplete',
                    dataType: "json",
                    contentType: 'application/json, charset=utf-8',
                    data: {
                        search: data_temp,
                        SectionId: '0'
                    },
                    success: function (data) {
                        var result = $.map(data, function (item) {
                            var strItemType = " - " + item.type;
                            strItemTypes.push(strItemType);
                            return {
                                label: item.mot + strItemType,
                                url: item.url
                            };
                        })
                        response(result);
                    },
                    error: function (xhr, status, error) {
                        alert(error);
                    }
                });
            },
            open: function(e,ui) {
                var
                    acData = $(this).data('uiAutocomplete'),
                    styledTerm = termTemplate.replace('%s', acData.term);
                    term = new RegExp(acData.term, "gi");

                acData
                    .menu
                    .element
                    .find('li')
                    .each(function() {
                        var me = $(this);

                        var styledMe = me.text().replace(term, styledTerm).replace(strItemType, htmlItemType);

                        for (i = 0; i < strItemTypes.length; i++){
                            var strItemType = strItemTypes[i];
                            var htmlItemType = objectTemplate.replace('%t', strItemType);
                            styledMe = styledMe.replace(strItemType, htmlItemType);
                        }

                        me.html( styledMe );
                    });
            },
            minLength: 2,
            select: function (event, data)
            {
                window.location = data.item.url;
            }
        });
    });
    </script>
</form>

