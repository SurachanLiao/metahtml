urée. Le camp où ils habitent depuis une petite année à Aulnay-sous-Bois en Seine-Saint-Denis, va être expulsé dans quelques jours.","hook":null,"label":"90' Enquêtes - Les roms : immersion dans une communauté mal aimée","programLabel":"90' Enquêtes","subLabel":"du dimanche 12 juillet 2020 à 00h33","images":[{"type":"id","generated":false,"id":"Image:a31580ce","typename":"Image"},{"type":"id","generated":false,"id":"Image:462040e2","typename":"Image"},{"type":"id","generated":false,"id":"Image:2c4223cd","typename":"Image"}]},"Image:a31580ce":{"__typename":"Image","id":"preview_5132c313-449e-49a6-a233-3b2c40ab2aa2","type":"PREVIEW","sources":[{"type":"id","generated":true,"id":"Image:a31580ce.sources.0","typename":"SrcSet"}]},"Image:a31580ce.sources.0":{"__typename":"SrcSet","url":"https://vidpreviews.tf1.fr/video/d5f0faca09c6b1642435f737bc8aa65f2423270bc523d585a1b3ce3a15d9cd67.mp4","width":"0","media":null,"type":"mp4"},"Image:462040e2":{"__typename":"Image","id":"vignette-90-enque-roms","type":"THUMBNAIL","sources":[{"type":"id","generated":true,"id":"Image:462040e2.sources.0","typename":"SrcSet"},{"type":"id","generated":true,"id":"Image:462040e2.sources.1","typename":"SrcSet"},{"type":"id","generated":true,"id":"Image:462040e2.sources.2","typename":"SrcSet"},{"type":"id","generated":true,"id":"Image:462040e2.sources.3","typename":"SrcSet"},{"type":"id","generated":true,"id":"Image:462040e2.sources.4","typename":"SrcSet"},{"type":"id","generated":true,"id":"Image:462040e2.sources.5","typename":"SrcSet"}]},"Image:462040e2.sources.0":{"__typename":"SrcSet","url":"https://photos.tf1.fr/396/222/vignette-90-enque-roms-85f300-0@1x.jpg","width":"396","media":null,"type":"jpg"},"Image:462040e2.sources.1":{"__typename":"SrcSet","url":"https://photos.tf1.fr/198/111/vignette-90-enque-roms-85c3d4-0@1x.jpg","width":"198","media":null,"type":"jpg"},"Image:462040e2.sources.2":{"__typename":"SrcSet","url":"https://photos.tf1.fr/132/74/vignette-90-enque-roms-34950a-0@1x.jpg","widt