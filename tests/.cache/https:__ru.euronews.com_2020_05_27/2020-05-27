043e\u0440\u044b\u0435 \u043d\u0435 \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u0430\u0433\u0430\u044e\u0442 \u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0430 \u0441 \u043a\u043b\u0438\u0435\u043d\u0442\u0430\u043c\u0438 (\u0445\u0438\u043c\u0447\u0438\u0441\u0442\u043a\u0438, \u043f\u0440\u0430\u0447\u0435\u0447\u043d\u044b\u0435, \u043c\u0430\u0441\u0442\u0435\u0440\u0441\u043a\u0438\u0435 \u043f\u043e \u0440\u0435\u043c\u043e\u043d\u0442\u0443 \u043e\u0431\u0443\u0432\u0438 \u0438 \u043e\u0434\u0435\u0436\u0434\u044b, \u0430\u0432\u0442\u043e\u043c\u0430\u0441\u0442\u0435\u0440\u0441\u043a\u0438\u0435).  \n\n\u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u0441\u0442\u043e\u043b\u0438\u0447\u043d\u0430\u044f \u043c\u044d\u0440\u0438\u044f \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u043e\u0442\u043c\u0435\u043d\u044b \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0439 \u043d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0438 \u0432 \u043f\u0430\u0440\u043a\u0430\u0445. \u0412\u043f\u0440\u043e\u0447\u0435\u043c, \u0440\u0435\u0447\u044c \u0438\u0434\u0451\u0442 \u043f\u043e\u043a\u0430 \u043e \u043f\u043e\u0441\u0442\u0435\u043f\u0435\u043d\u043d\u043e\u043c \u0441\u043c\u044f\u0433\u0447\u0435\u043d\u0438\u0438 \u0440\u0435\u0436\u0438\u043c\u0430, \u2014 \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u043b \u0421\u043e\u0431\u044f\u043d\u0438\u043d \u0432 \u0445\u043e\u0434\u0435 \u0432\u0438\u0434\u0435\u043e-\u043a\u043e\u043d\u0444\u0435\u0440\u0435\u043d\u0446\u0438\u0438 \u0441 \u043f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u043e\u043c. \u041f\u0440\u043e\u0433\u0443\u043b\u043a\u0438 \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u044b \u0431\u0443\u0434\u0443\u0442 \u043f\u043e \u0433\u0440\u0430\u0444\u0438\u043a\u0443: \u0434\u0432\u0430 \u0440\u0430\u0437\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438 \u0438 \u043e\u0434\u0438\u043d \u0440\u0430\u0437 \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u043e\u0439:   \n\n\u00ab\u0415\u0441\u043b\u0438 \u0441\u043a\u0430\u0437\u0430\u0442\u044c: \u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0442\u0435\u043f\u0435\u0440\u044c \u0433\u0443\u043b\u044f\u0442\u044c, \u043a\u0430\u043a \u0445\u043e\u0442\u0438\u043c \u0438 \u0433\u0434\u0435 \u0445\u043e\u0442\u0438\u043c, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0445\u043e\u0442\u0438\u043c \u2014 \u044f \u043f\u0440\u043e\u0441\u0442\u043e \u0431\u043e\u044e\u0441\u044c, \u0447\u0442\u043e \u043d\u0430 \u0443\u043b\u0438\u0446\u0430\u0445 \u041c\u043e\u0441\u043a\u0432\u044b \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u0438\u043c\u0435\u0440\u043d\u043e \u043a\u0430\u043a \u043d\u0430 \u043f\u0435\u0440\u0432\u043e\u043c\u0430\u0439\u0441\u043a\u043e\u0439 \u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0430\u0446\u0438\u0438. \u042d\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u043d\u0435\u043f\u0440\u043e\u0441\u0442\u0430\u044f \u043c\u0435\u0440\u0430, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u044b \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u043c \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u044d\u0442\u043e \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435 \u0438 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0433\u0440\u0430\u0444\u0438\u043a, \u043a\u043e\u0433\u0434\u0430 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0435, \u043f\u0440\u043e\u0436\u0438\u0432\u0430\u044e\u0449\u0438\u0435 \u0432 \u0442\u0435\u0445 \u0438\u043b\u0438 \u0438\u043d\u044b\u0445 \u0434\u043e\u043c\u0430\u0445, \u043c\u043e\u0433\u0443\u0442 \u0432\u044b\u0445\u043e\u0434\u0438\u0442\u044c \u043d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0438. \u0421\u0434\u0435\u043b\u0430\u0442\u044c \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u0443\u044e \u0441\u0445\u0435\u043c\u0443 \u2014 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u0443\u044e \u0438 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0443\u044e, \u043d\u043e \u0432\u0441\u0451-\u0442\u0430\u043a\u0438 \u0447\u0442\u043e\u0431\u044b \u043d\u0435 \u0432\u0441\u0435 \u0441\u0440\u0430\u0437\u0443, \u2014 \u0438 \u0442\u0430\u043a \u043d\u0435\u0434\u0435\u043b\u044e-\u0434\u0440\u0443\u0433\u0443\u044e \u043f\u043e\u0436\u0438\u0442\u044c, \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c, \u043a\u0430\u043a \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f \u0431\u0443\u0434\u0435\u0442 \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044c\u0441\u044f\u00bb. \n\n\u041f\u043e \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u043c \u0434\u0430\u043d\u043d\u044b\u043c, \u0432 \u041c\u043e\u0441\u043a\u0432\u0435 \u0432\u044b\u044f\u0432\u043b\u0435\u043d\u043e \u0431\u043e\u043b\u0435\u0435 170 \u0442\u044b\u0441. \u0441\u043b\u0443\u0447\u0430\u0435\u0432 \u0437\u0430\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u043a\u043e\u0440\u043e\u043d\u0430\u0432\u0438\u0440\u0443\u0441\u043e\u043c, 2183 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0443\u043c\u0435\u0440\u043b\u0438. \u0412 \u0420\u043e\u0441\u0441\u0438\u0438, \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u044d\u0442\u0438\u043c \u0436\u0435 \u043e\u0442\u0447\u0451\u0442\u0430\u043c, \u0437\u0430\u0431\u043e\u043b\u0435\u043b\u0438 \u0441\u0432\u044b\u0448\u0435 370 \u0442\u044b\u0441. \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u0436\u0435\u0440\u0442\u0432\u0430\u043c\u0438 \u043f\u0430\u043d\u0434\u0435\u043c\u0438\u0438 \u043f\u0440\u0438\u0437\u043d\u0430\u043d\u044b \u043f\u043e\u0447\u0442\u0438 4 \u0442\u044b\u0441. \u0443\u043c\u0435\u0440\u0448\u0438\u0445. \n\n&quot;,&quot;htmlText&quot;:&quot;&lt;p&gt;\u0420\u0435\u0436\u0438\u043c \u0441\u0430\u043c\u043e\u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438 \u0432 \u041c\u043e\u0441\u043a\u0432\u0435 \u043f\u0440\u043e\u0434\u043b\u0435\u043d \u0434\u043e 14 \u0438\u044e\u043d\u044f. \u041e\u0431 \u044d\u0442\u043e\u043c \u0437\u0430\u044f\u0432\u0438\u043b \u043c\u044d\u0440 \u041c\u043e\u0441\u043a\u0432\u044b \u0421\u0435\u0440\u0433\u0435\u0439 \u0421\u043e\u0431\u044f\u043d\u0438\u043d.&lt;\/p&gt;\n&lt;p&gt;\u0412 \u0433\u043e\u0440\u043e\u0434\u0435 \u0431\u0443\u0434\u0443\u0442 \u0434\u0435\u0439\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u043d\u043e\u0439 \u0440\u0435\u0436\u0438\u043c \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u043c\u0435\u0440\u044b: \u043f\u043e-\u043f\u0440\u0435\u0436\u043d\u0435\u043c\u0443 \u043f\u043e\u0434 \u0437\u0430\u043f\u0440\u0435\u0442\u043e\u043c \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u0435 \u043c\u0430\u0441\u0441\u043e\u0432\u044b\u0445 \u043c\u0435\u0440\u043e\u043f\u0440\u0438\u044f\u0442\u0438\u0439, \u043d\u0435 \u0431\u0443\u0434\u0443\u0442 \u0440\u0430\u0431\u043e\u0442\u0430\u0442\u044c \u043a\u0430\u0444\u0435, \u0440\u0435\u0441\u0442\u043e\u0440\u0430\u043d\u044b, \u0441\u0430\u043b\u043e\u043d\u044b \u043a\u0440\u0430\u0441\u043e\u0442\u044b \u0438 \u0440\u044f\u0434 \u0434\u0440\u0443\u0433\u0438\u0445 \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u044f\u0442\u0438\u0439 \u0441\u0444\u0435\u0440\u044b \u0443\u0441\u043b\u0443\u0433.&lt;\/p&gt;\n&lt;p&gt;\u041f\u0440\u0438 \u044d\u0442\u043e\u043c, \u043a\u0430\u043a \u043e\u0442\u043c\u0435\u0447\u0430\u0435\u0442\u0441\u044f \u0432 \u0443\u043a\u0430\u0437\u0435, \u0437\u0430\u043d\u043e\u0432\u043e \u043e\u0444\u043e\u0440\u043c\u043b\u044f\u0442\u044c \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u0430 \u043d\u0435 \u043f\u0440\u0438\u0434\u0435\u0442\u0441\u044f: \u043e\u043d\u0438 \u0431\u0443\u0434\u0443\u0442 \u043f\u0440\u043e\u0434\u043b\u0435\u043d\u044b \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438.&lt;\/p&gt;\n&lt;p&gt;\u0420\u0430\u043d\u0435\u0435 \u043c\u044d\u0440 \u0430\u043d\u043e\u043d\u0441\u0438\u0440\u043e\u0432\u0430\u043b \u043f\u043b\u0430\u043d \u0441\u043c\u044f\u0433\u0447\u0435\u043d\u0438\u044f \u043a\u0430\u0440\u0430\u043d\u0442\u0438\u043d\u043d\u044b\u0445 \u043c\u0435\u0440 \u0441 1 \u0438\u044e\u043d\u044f. \u0412 \u0433\u043e\u0440\u043e\u0434\u0435 \u0441\u043c\u043e\u0433\u0443\u0442 \u0432\u043e\u0437\u043e\u0431\u043d\u043e\u0432\u0438\u0442\u044c \u0440\u0430\u0431\u043e\u0442\u0443 \u043d\u0435\u043f\u0440\u043e\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u043c\u0430\u0433\u0430\u0437\u0438\u043d\u044b, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u044f\u0442\u0438\u044f \u0431\u044b\u0442\u043e\u0432\u044b\u0445 \u0443\u0441\u043b\u0443\u0433, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043d\u0435 \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u0430\u0433\u0430\u044e\u0442 \u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0430 \u0441 \u043a\u043b\u0438\u0435\u043d\u0442\u0430\u043c\u0438 (\u0445\u0438\u043c\u0447\u0438\u0441\u0442\u043a\u0438, \u043f\u0440\u0430\u0447\u0435\u0447\u043d\u044b\u0435, \u043c\u0430\u0441\u0442\u0435\u0440\u0441\u043a\u0438\u0435 \u043f\u043e \u0440\u0435\u043c\u043e\u043d\u0442\u0443 \u043e\u0431\u0443\u0432\u0438 \u0438 \u043e\u0434\u0435\u0436\u0434\u044b, \u0430\u0432\u0442\u043e\u043c\u0430\u0441\u0442\u0435\u0440\u0441\u043a\u0438\u0435).&lt;\/p&gt;\n&lt;p&gt;\u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u0441\u0442\u043e\u043b\u0438\u0447\u043d\u0430\u044f \u043c\u044d\u0440\u0438\u044f \u0440\u0430\u0441\u0441\u043c\u0430\u0442\u0440\u0438\u0432\u0430\u0435\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0441\u0442\u044c \u043e\u0442\u043c\u0435\u043d\u044b \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0435\u043d\u0438\u0439 \u043d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0438 \u0432 \u043f\u0430\u0440\u043a\u0430\u0445. \u0412\u043f\u0440\u043e\u0447\u0435\u043c, \u0440\u0435\u0447\u044c \u0438\u0434\u0451\u0442 \u043f\u043e\u043a\u0430 \u043e \u043f\u043e\u0441\u0442\u0435\u043f\u0435\u043d\u043d\u043e\u043c \u0441\u043c\u044f\u0433\u0447\u0435\u043d\u0438\u0438 \u0440\u0435\u0436\u0438\u043c\u0430, \u2014 \u043e\u0431\u044a\u044f\u0441\u043d\u044f\u043b \u0421\u043e\u0431\u044f\u043d\u0438\u043d \u0432 \u0445\u043e\u0434\u0435 \u0432\u0438\u0434\u0435\u043e-\u043a\u043e\u043d\u0444\u0435\u0440\u0435\u043d\u0446\u0438\u0438 \u0441 \u043f\u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0442\u043e\u043c. \u041f\u0440\u043e\u0433\u0443\u043b\u043a\u0438 \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u044b \u0431\u0443\u0434\u0443\u0442 \u043f\u043e \u0433\u0440\u0430\u0444\u0438\u043a\u0443: \u0434\u0432\u0430 \u0440\u0430\u0437\u0430 \u0432 \u0431\u0443\u0434\u043d\u0438 \u0438 \u043e\u0434\u0438\u043d \u0440\u0430\u0437 \u0432 \u0432\u044b\u0445\u043e\u0434\u043d\u043e\u0439:&lt;\/p&gt;\n&lt;p&gt;&lt;em&gt;\u00ab\u0415\u0441\u043b\u0438 \u0441\u043a\u0430\u0437\u0430\u0442\u044c: \u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0442\u0435\u043f\u0435\u0440\u044c \u0433\u0443\u043b\u044f\u0442\u044c, \u043a\u0430\u043a \u0445\u043e\u0442\u0438\u043c \u0438 \u0433\u0434\u0435 \u0445\u043e\u0442\u0438\u043c, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0445\u043e\u0442\u0438\u043c \u2014 \u044f \u043f\u0440\u043e\u0441\u0442\u043e \u0431\u043e\u044e\u0441\u044c, \u0447\u0442\u043e \u043d\u0430 \u0443\u043b\u0438\u0446\u0430\u0445 \u041c\u043e\u0441\u043a\u0432\u044b \u0431\u0443\u0434\u0435\u0442 \u043f\u0440\u0438\u043c\u0435\u0440\u043d\u043e \u043a\u0430\u043a \u043d\u0430 \u043f\u0435\u0440\u0432\u043e\u043c\u0430\u0439\u0441\u043a\u043e\u0439 \u0434\u0435\u043c\u043e\u043d\u0441\u0442\u0440\u0430\u0446\u0438\u0438. \u042d\u0442\u043e \u043e\u0447\u0435\u043d\u044c \u043d\u0435\u043f\u0440\u043e\u0441\u0442\u0430\u044f \u043c\u0435\u0440\u0430, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u043c\u044b \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u043c \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u044d\u0442\u043e \u0432 \u0442\u0435\u0441\u0442\u043e\u0432\u043e\u043c \u0440\u0435\u0436\u0438\u043c\u0435 \u0438 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0433\u0440\u0430\u0444\u0438\u043a, \u043a\u043e\u0433\u0434\u0430 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0435, \u043f\u0440\u043e\u0436\u0438\u0432\u0430\u044e\u0449\u0438\u0435 \u0432 \u0442\u0435\u0445 \u0438\u043b\u0438 \u0438\u043d\u044b\u0445 \u0434\u043e\u043c\u0430\u0445, \u043c\u043e\u0433\u0443\u0442 \u0432\u044b\u0445\u043e\u0434\u0438\u0442\u044c \u043d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0438. \u0421\u0434\u0435\u043b\u0430\u0442\u044c \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u0443\u044e \u0441\u0445\u0435\u043c\u0443 \u2014 \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0441\u0442\u0443\u044e \u0438 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0443\u044e, \u043d\u043e \u0432\u0441\u0451-\u0442\u0430\u043a\u0438 \u0447\u0442\u043e\u0431\u044b \u043d\u0435 \u0432\u0441\u0435 \u0441\u0440\u0430\u0437\u0443, \u2014 \u0438 \u0442\u0430\u043a \u043d\u0435\u0434\u0435\u043b\u044e-\u0434\u0440\u0443\u0433\u0443\u044e \u043f\u043e\u0436\u0438\u0442\u044c, \u043f\u043e\u0441\u043c\u043e\u0442\u0440\u0435\u0442\u044c, \u043a\u0430\u043a \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u044f \u0431\u0443\u0434\u0435\u0442 \u0440\u0430\u0437\u0432\u0438\u0432\u0430\u0442\u044c\u0441\u044f\u00bb.&lt;\/em&gt;&lt;\/p&gt;\n&lt;p&gt;\u041f\u043e \u043e\u0444\u0438\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u043c \u0434\u0430\u043d\u043d\u044b\u043c, \u0432 \u041c\u043e\u0441\u043a\u0432\u0435 \u0432\u044b\u044f\u0432\u043b\u0435\u043d\u043e \u0431\u043e\u043b\u0435\u0435 170 \u0442\u044b\u0441. \u0441\u043b\u0443\u0447\u0430\u0435\u0432 \u0437\u0430\u0440\u0430\u0436\u0435\u043d\u0438\u044f \u043a\u043e\u0440\u043e\u043d\u0430\u0432\u0438\u0440\u0443\u0441\u043e\u043c, 2183 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0430 \u0443\u043c\u0435\u0440\u043b\u0438. \u0412 \u0420\u043e\u0441\u0441\u0438\u0438, \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u044d\u0442\u0438\u043c \u0436\u0435 \u043e\u0442\u0447\u0451\u0442\u0430\u043c, \u0437\u0430\u0431\u043e\u043b\u0435\u043b\u0438 \u0441\u0432\u044b\u0448\u0435 370 \u0442\u044b\u0441. \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u0436\u0435\u0440\u0442\u0432\u0430\u043c\u0438 \u043f\u0430\u043d\u0434\u0435\u043c\u0438\u0438 \u043f\u0440\u0438\u0437\u043d\u0430\u043d\u044b \u043f\u043e\u0447\u0442\u0438 4 \u0442\u044b\u0441. \u0443\u043c\u0435\u0440\u0448\u0438\u0445.&lt;\/p&gt;\n&lt;div class=\&quot;widget widget--type-freeform\nwidget--size-fullwidth\nwidget--align-center\&quot;&gt;\n  &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n    &lt;div class=\&quot;widget__ratio widget__ratio--auto\&quot;&gt;\n      &lt;div class=\&quot;widget__contents\&quot;&gt;\n         &lt;center&gt;&lt;blockquote style=\&quot;max-width: 400px; color:#565656; margin:5px; padding:5px; text-align:center; border-radius: 15px;\&quot;&gt;&lt;p&gt;\u041f\u043e\u0434\u043f\u0438\u0441\u044b\u0432\u0430\u0439\u0442\u0435\u0441\u044c \u043d\u0430 Euronews \u0432 \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0445 \u0441\u0435\u0442\u044f\u0445&lt;br \/&gt;&lt;a href=\&quot;https:\/\/t.me\/russian_euronews\&quot;&gt;Telegram&lt;\/a&gt;,&lt;a href=\&quot;https:\/\/ok.ru\/euronews\&quot;&gt; \u041e\u0434\u043d\u043e\u043a\u043b\u0430\u0441\u0441\u043d\u0438\u043a\u0438&lt;\/a&gt;, &lt;a href=\&quot;https:\/\/vk.com\/euronews\&quot;&gt;\u0412\u041a\u043e\u043d\u0442\u0430\u043a\u04