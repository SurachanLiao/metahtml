u044c\u043d\u0435\u0435 \u0432\u0441\u0435\u0433\u043e \u0443\u0434\u0430\u0440\u0438\u043b\u0438 \u043f\u043e \u043c\u043e\u043b\u043e\u0434\u0451\u0436\u0438, \u0441\u0440\u0435\u0434\u0438 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u0443\u0440\u043e\u0432\u0435\u043d\u044c \u0431\u0435\u0437\u0440\u0430\u0431\u043e\u0442\u0438\u0446\u044b \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0432\u044b\u0440\u043e\u0441. \u0417\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0435 \u0442\u0440\u0438 \u043c\u0435\u0441\u044f\u0446\u0430 \u0432 \u044d\u0442\u043e\u0439 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u043a\u0430\u0436\u0434\u044b\u0439 \u0448\u0435\u0441\u0442\u043e\u0439 \u043e\u0441\u0442\u0430\u043b\u0441\u044f \u0431\u0435\u0437 \u0440\u0430\u0431\u043e\u0442\u044b, \u0430 \u0443 \u0442\u0435\u0445, \u043a\u043e\u043c\u0443 \u0443\u0434\u0430\u043b\u043e\u0441\u044c \u044d\u0442\u043e\u0433\u043e \u0438\u0437\u0431\u0435\u0436\u0430\u0442\u044c, \u0440\u0430\u0431\u043e\u0447\u0435\u0435 \u0432\u0440\u0435\u043c\u044f \u0441\u043e\u043a\u0440\u0430\u0442\u0438\u043b\u043e\u0441\u044c \u043d\u0430 23%. \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u043f\u043e \u0434\u0430\u043d\u043d\u044b\u043c \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u0430\u0446\u0438\u0438, \u043e\u043f\u0443\u0431\u043b\u0438\u043a\u043e\u0432\u0430\u043d\u043d\u044b\u043c \u0432 \u0447\u0435\u0442\u0432\u0451\u0440\u0442\u043e\u043c \u0438\u0437\u0434\u0430\u043d\u0438\u0438 \u00ab\u041a\u043e\u0432\u0438\u0434-19 \u0438 \u0442\u0440\u0443\u0434\u043e\u0432\u0430\u044f \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c\u00bb, \u0441\u0440\u0435\u0434\u0438 \u043c\u043e\u043b\u043e\u0434\u0451\u0436\u0438, \u043f\u043e\u0442\u0435\u0440\u044f\u0432\u0448\u0435\u0439 \u0440\u0430\u0431\u043e\u0442\u0443, \u0437\u0430\u043c\u0435\u0442\u043d\u043e \u0432\u044b\u0448\u0435 \u0434\u043e\u043b\u044f \u0436\u0435\u043d\u0449\u0438\u043d. \u0421\u0435\u0439\u0447\u0430\u0441 \u0438\u0437\u0443\u0447\u0430\u044e\u0442\u0441\u044f \u043c\u0435\u0440\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u043e\u0442\u0440\u0435\u0431\u0443\u044e\u0442\u0441\u044f \u0434\u043b\u044f \u0438\u0441\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438.&lt;\/p&gt;\n&quot;,&quot;hashtag&quot;:null,&quot;createdAt&quot;:1590570718,&quot;publishedAt&quot;:1590596312,&quot;updatedAt&quot;:1590596644,&quot;expiresAt&quot;:0,&quot;canonical&quot;:&quot;https:\/\/ru.euronews.com\/2020\/05\/27\/lagarde-ilo-warnings&quot;,&quot;images&quot;:[{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/70\/69\/10\/{{w}}x{{h}}_cmsv2_d0b9e237-397c-55ee-a7f1-16cf2c148c42-4706910.jpg&quot;,&quot;altText&quot;:null,&quot;caption&quot;:null,&quot;captionCredit&quot;:&quot;AP Photo&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;width&quot;:1200,&quot;height&quot;:800}],&quot;authors&quot;:{&quot;journalists&quot;:[{&quot;urlSafeValue&quot;:&quot;mitnik&quot;,&quot;title&quot;:&quot;\u041b\u0435\u0432 \u0423\u0434\u043e\u043b\u044c\u0441\u043a\u0438\u0439&quot;,&quot;twitter&quot;:null}],&quot;producers&quot;:[],&quot;videoEditor&quot;:[]},&quot;keywords&quot;:[{&quot;id&quot;:78,&quot;slug&quot;:&quot;ecb&quot;,&quot;urlSafeValue&quot;:&quot;ecb&quot;,&quot;title&quot;:&quot;\u0415\u0426\u0411&quot;,&quot;titleRaw&quot;:&quot;\u0415\u0426\u0411&quot;},{&quot;id&quot;:6426,&quot;slug&quot;:&quot;christine-lagarde&quot;,&quot;urlSafeValue&quot;:&quot;christine-lagarde&quot;,&quot;title&quot;:&quot;\u041a\u0440\u0438\u0441\u0442\u0438\u043d \u041b\u0430\u0433\u0430\u0440\u0434&quot;,&quot;titleRaw&quot;:&quot;\u041a\u0440\u0438\u0441\u0442\u0438\u043d \u041b\u0430\u0433\u0430\u0440\u0434&quot;},{&quot;id&quot;:18258,&quot;slug&quot;:&quot;labour-code&quot;,&quot;urlSafeValue&quot;:&quot;labour-code&quot;,&qu