                            document.addEventListener("DOMContentLoaded", function(e) {
                                var articleBody = document.getElementsByClassName('article__body')[0];
                                var sideColumn = document.getElementsByClassName('sidebar article__aside')[0];
                                var sideBarColumn;
                                var sideBarColumnChildren = 0;
                                try {
                                    sideBarColumn = sideColumn.getElementsByClassName('sidebar__column')[0];
                                    sideBarColumnChildren = sideBarColumn.childElementCount;
                                } catch (e) {
                                    sideBarColumnChildren = 0;
                                }
                                // remove sidebar column, if there are no children (ads)
                                if (sideBarColumnChildren < 1 && sideColumn !== undefined) {
                                    sideColumn.remove();
                                }

                                // hide side column and set article width 100% to avoid gap on right
                                // todo: only do this, if all ads on right column are hidden, else show it
                                if ((sideBarColumn === false) && articleBody !== undefined) {
                                    articleBody.style.width = '100%';
                                }

                            });

                                 // get the checkbox value from widget (true/false)
                            

