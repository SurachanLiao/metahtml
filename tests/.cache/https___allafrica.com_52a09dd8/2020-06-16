    function loadConsented () {
      try { setCMPCookieStatus(); } catch (e) {}
      try { loadAds({ requestNonPersonalizedAds: 0 } ); } catch (e) {}
      try { loadTracking(); } catch (e) {}
      try { loadSocial(); } catch (e) {}
    }
    function loadNonConsented () {
      try { setCMPCookieStatus(); } catch (e) {}
      try { loadAds({ requestNonPersonalizedAds: 1 } ); } catch (e) {}
      try { loadTracking(); } catch (e) {}
    }
