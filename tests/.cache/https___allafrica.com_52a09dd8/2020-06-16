    async: true,
  });
}

    loadjs.ready( 'newsletter-signup-assets', function() {
      $('.newsletter-signup.modal.fade.footer-button-target').newsletterModal({
        maxShowCount: 10,
        maxCloseCount: 3,
        maxSuccessCount: 1,
      });
    });
  });
</script>



