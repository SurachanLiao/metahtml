              /** Calculate and update sizes. Can be called over again on eg. window resize. */
              function updateSizes(newIframeHeight) {
                if (typeof newIframeHeight === 'number') {
                  currentIframeHeight = newIframeHeight;
                }
          
                var $dnParentBlock = $('#dn-ncp-popup').parent('.article-main-content');
          
