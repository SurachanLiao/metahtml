<script type="text/javascript">

			
	$("#scKeyword").keypress(function (event) {
		if (event.which == 13) {
			TopSearchClick();
			return false;
		}		
	});

	$("#scKeyword").keydown(function (event) {
		if ($('#scKeyword').val().indexOf("ê²ìì´ë¥¼ ìë ¥íì¸ì") >= 0) {
		    $("#scKeyword").val($("#scKeyword").val().replace("ê²ìì´ë¥¼ ìë ¥íì¸ì",""));
		}
	});

	
	function TopSearchClick() {
		if ($('#scKeyword').val() == "" || $('#scKeyword').val() == "ê²ìì´ë¥¼ ìë ¥íì¸ì") {
			alert("ê²ìì´ë¥¼ ìë ¥íì¸ì");
			$("#scKeyword").focus();
			return false;
		}
		if (pubByteCheckTextarea("scKeyword") <= 1) {
			alert("ê²ìì´ë íê¸1ì, ìë¬¸2ì ì´ì ìë ¥í´ ì£¼ì­ìì.");
			$("#scKeyword").focus();
			return false;
		}

		$('#topSearchFrm').attr('action', '/Search/?scText=' + encodeURIComponent($.trim($("#scKeyword").val())));
		$("#topSearchFrm").submit();
	}

	$('#scKeyword').click(function (e) {
		if ($('#scKeyword').val() == "ê²ìì´ë¥¼ ìë ¥íì¸ì") {
			$('#scKeyword').val('');
		}
	});

</script>			
            
