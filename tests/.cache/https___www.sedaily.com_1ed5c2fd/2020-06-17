<script type="text/javascript">
	var StarArray = new Array();
	var mStarSwiper;
	var mStarPageCount = 0;
	$(function () {
		InitMStarSwiperSwiper();
		var resizeTime;
		$(window).resize(function () {
			clearTimeout(resizeTime);
			resizeTime = setTimeout(function () {
				if ($(window).width() < 900) {

					InitMStarSwiperSwiper();
				}
			}, 250);
		});
	})
	
	function InitMStarSwiperSwiper() {

		if ($(window).width() < 640) {
			mStarPageCount = 2;
		}
		else {
			mStarPageCount = 4;
		}

		if (mStarSwiper != undefined) {
			mStarSwiper.destroy(true, true);
		}

		if (mStarPageCount == 2) {
			$("#swiper_m_star").find("ul").find("li").each(function (index, Li) {
				if (index > 9) {
					StarArray.push(Li);
					$(this).remove();
				}
			})
		}
		else {
			StarArray.forEach(function (Li) {
				$("#swiper_m_star").find("ul").append(Li);
			});
			StarArray = new Array();
		}

		mStarSwiper = new Swiper('#swiper_m_star', {
			slidesPerView: mStarPageCount,
			speed: 1000,
			preventClicksPropagation: false,
			autoplayDisableOnInteraction: false,
			slidesPerGroup: mStarPageCount,
			onSlideChangeStart: function (swiper) {

				var PageNo = 0;

				if (swiper.activeIndex > 0) {
					PageNo = Math.ceil(swiper.activeIndex / mStarPageCount);
				}

				$("#swiper_star_paging").find("li").removeClass();
				$("#swiper_star_paging").find("li").eq(PageNo).addClass("on");
			}
		});

		$("#swiper_star_paging").find("li").removeClass("on");
		$("#swiper_star_paging").find("li").eq(0).addClass("on");
	}

	function MStarPaging(index) {
		mStarSwiper.slideTo((index * mStarPageCount) - 1, 1000, true);
	}
</script>	

	

<div class="mm_sec9 mm9_2">
	<div class="mm_hd">
		<div>
			<h3>ì¸ê¸°í¬í </h3>
			<ul class="c_2And7_nav" id="swiper_bestphoto_paging">
				<li><a href='javascript:MBestPhotoPaging(1)'>1</a></li>
				<li><a href='javascript:MBestPhotoPaging(2)'>2</a></li>
				<li><a href='javascript:MBestPhotoPaging(3)'>3</a></li>
				<li><a href='javascript:MBestPhotoPaging(4)'>4</a></li>
				<li><a href='javascript:MBestPhotoPaging(5)'>5</a></li>
			</ul>
