    <script type="text/javascript">
        //2019.01.07   dblee   ëª¨ë°ì¼ ê¸°ê¸°ë¡ ì ê·¼ì ëª¨ë°ì¼ íì´ì§ë¡ ì´ë ì¶ê°
        var ChkMobile = $.Cookie('mobile');
        if (ChkMobile == '' && getParameterKey('mobile') == 'mobile') {
            $.Cookie('mobile', 'm', { domain: '.sedaily.com' });    // ëª¨ë°ì¼ ì ì¿ í¤ ì½ì
        }

        // ì¿ í¤ ì²´í¬ 
        if (ChkMobile == '') {
            var AbsolutePath = "https://m.sedaily.com" + '/Main/Content/_Index';
            if ("/Main/Content/_Index".toLowerCase().indexOf("/main/content/_index") > -1) {
                AbsolutePath = "https://m.sedaily.com";
            }
           
            // 1. ëª¨ë°ì¼ ê¸°ê¸° && 2. PC ë²ì  ë³´ê¸° 
            if ((DeviceCheck() == 'mobile_app' || DeviceCheck() == 'mobile_web') && getParameterKey("mobile") == '') {
	            location.href = AbsolutePath;
            }
        }

        // íë¼ë¯¸í° ì²´í¬ 
        function getParameterKey(name) {
	        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
	        var regex = new RegExp("[\\?&]" + name + "([^&#]*)"),
                results = regex.exec(location.search);
	        return results === null ? "" : decodeURIComponent(results[0].replace(/\?/g, "").replace(/\&/g, ""));
        }

        // ëª¨ë°ì¼ ë°ë¡ê°ê¸° ë²í¼ í´ë¦­ ì ì¿ í¤ ì­ì  í íì´ì§ ì´ë
        function delMobileCookie(url) {
            $.Cookie('mobile', '', { expires: -1, domain: '.sedaily.com' });   
            location.href = url;
        }

        // ëª¨ë°ì¼ ë³´ê¸° ë²í¼ ë¸ì¶ 
        $(document).ready(function () {
            if (ChkMobile == 'm' || getParameterKey("mobile") != '') {
                $("#BtnMobile").css("display", "");
            }
        });

	
        var NClass = "";
