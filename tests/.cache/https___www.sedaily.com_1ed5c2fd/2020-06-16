	$(function () {
		SetChartOptionChart();
        PCScript();

		//if ($(window).width() >= 900) {
		//	PCScript();
		//}
		//else {
		//	MobileScript();
		//}

		var resizeTime;
		$(window).resize(function () {
			clearTimeout(resizeTime);
			resizeTime = setTimeout(function () {
				if ($(window).width() >= 900) {
					PCScript();
					if (CategorySwiper != undefined) {
						CategorySwiper.slideTo(0, 0, false);
					}
				}
				else {
					MobileScript();
				}
			}, 250);
		});


		//ìë³´ í¤ëíì¸ ìì
		ClickHeadLineStartStop();

		$("#headline_first a").mouseover(function () {
			clearInterval(HeadLineInterval);
		});
		$("#headline_first a").mouseleave(function () {

			if ($(".c_flash_down").css("display") != "block") {
				HeadLineInterval = setInterval("HeadLineCycle('Next')", 3000);
			}


		});

		//í¤ëë¼ì¸ ë°ì¸ë©
		$("[id^=HeadLineNum]").focusout(function () {
			if ($("[id^=HeadLineNum]").length - 1 == $(this).index()) {
				OpenHeadLine();
			}
		});

		//ë² ì¤í¸ í´ë¦­ ëª¨ë°ì¼
		$("#div_m_bestclick").find(".no_img").hide();

		//ì¹´íê³ ë¦¬ ë´ì¤
		$("#ul_categorySwiper").find("a").focusin(function () {
			CategorySwiper.slideTo($(this).closest(".swiper-slide").index());
		});

		//ì¦ê¶
		$("#searchStockCode").keypress(function (event) {
			if (event.which == 13) {
				Search($(this).val());
				$('#searchStockCode_M').blur();
				return false;
			}
		});

		$(".now_time_refresh").click(function () {
			document.location.reload();
		});

		//í¼ë¸ ì¤í¬ë¦½í¸
		//ì¶ì²ê¸°ì í¸ë²í¨ê³¼
		$(".top_people").mouseover(function () {
			$(this).find(".image_scale").stop().animate({ scale: 1.3 }, 200);
		});
		$(".top_people").mouseout(function () {
			$(this).find(".image_scale").eq(0).stop().animate({ scale: 1 }, 200);
		});


		$(".lev1_rel_h").removeClass("open");
		$(".openRel").click(function () {
			$(".lev1_rel").slideDown();
			$(this).css({ "display": "none" });
			$(".closeRel").css({ "display": "inline-block" });
			$(".lev1_rel_h").addClass("open");
			return false;
		});
		$(".closeRel").click(function () {
			$(".lev1_rel").slideUp();
			$(this).css({ "display": "none" });
			$(".openRel").css({ "display": "block" });
			$(".lev1_rel_h").removeClass("open");
			return false;
		});

		$(window).resize(function () {
			if ($(window).width() <= 640) {
				if ($(".lev1_rel_h").attr("class") == "lev1_rel_h open") {
					$(".closeRel").show();
				}
				else {
					$(".lev1_rel").hide();
					$(".openRel").show();
				}
			}
			else {
				$(".lev1_rel").show();
				$(".openRel").hide();
				$(".closeRel").hide();
			}


		});

		$(".mm_2_1 .mm2_slideTop").removeClass('on');
		$(".mm_2_1 .mm2_slide").css({ "display": "none" });
		$(".mm_2_1 .mm2_slideBtn").click(function () {
			$(".mm_2_1 .mm2_slide").slideToggle();
			$(".mm_2_1 .mm2_slideTop").toggleClass('on');
			return false;
		});

		$(".mm_2_2 .mm2_slideTop").removeClass('on');
		$(".mm_2_2 .mm2_slide").css({ "display": "none" });
		$(".mm_2_2 .mm2_slideBtn").click(function () {
			$(".mm_2_2 .mm2_slide").slideToggle();
			$(".mm_2_2 .mm2_slideTop").toggleClass('on');
			return false;
		});
	});
</script>


    <script type="text/javascript">
        $(document).ready(function () {
            var Box = $('.c_sec1_1');
            var CheckHH = $('.c_sec1_1 .news_zone h3 a').outerHeight();
            var classSize = Box.attr('class');
            var arr = classSize.split(" ");
            var ClassNameNum = Number(arr[2].replace("leTy", ""));
            var ClassNameNum2 = (ClassNameNum * 10) + ClassNameNum;

            SizeCheck();
            $(window).resize(function () { SizeCheck(); });

            function SizeCheck() {
                CheckHH = $('.c_sec1_1 .news_zone h3 a').outerHeight();
                if ((CheckHH > 40) && ClassNameNum != "44") { //2ì¤ // 2019.04.03 dblee ì´ë¯¸ì§ í´ëì¤ ìì    // 2019.06.05 chbaek ieììë 40ì¸¡ì , 40ì¼ë¡ ë³ê²½
                    //55í´ëì¤
                    Box.removeClass('leTy' + ClassNameNum);
                    Box.addClass('leTy' + ClassNameNum2);
                } else if (CheckHH <= 40) { //1ì¤
                    //5í´ëì¤
                    Box.removeClass('leTy' + ClassNameNum2);
                    Box.addClass('leTy' + ClassNameNum);

                    if ("33" == ClassNameNum2) {    //2020.01.02 dblee (ìì) cssëì  ì¤ë¥ ìì 
                        Box.removeClass('leTy' + ClassNameNum);
                        Box.addClass('leTy' + ClassNameNum2);
                    }
                }
                
                //leTy11 -> leTy66 í´ëì¤ ë³ê²½
                if (ClassNameNum2 == "121") {
                    Box.removeClass("leTy121").addClass("leTy11");
                }
            }
        });
    </script>



	



