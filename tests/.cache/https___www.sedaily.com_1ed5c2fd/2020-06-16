<script type="text/javascript">

	var PhotoArray = new Array();
	var mBestPhotoSwiper;
	var mPhotoPageCount = 0;

	$(function () {
		InitMBestPhotoSwiperSwiper();
		var resizeTime;
		$(window).resize(function () {
			clearTimeout(resizeTime);
			resizeTime = setTimeout(function () {
				if ($(window).width() < 900) {

					InitMBestPhotoSwiperSwiper();
				}
			}, 250);
		});
	})

	function InitMBestPhotoSwiperSwiper() {

		if ($(window).width() < 640) {
			mPhotoPageCount = 2;
		}
		else {
			mPhotoPageCount = 4;
		}

		if (mBestPhotoSwiper != undefined) {
			mBestPhotoSwiper.destroy(true, true);
		}

		if (mStarPageCount == 2) {
			$("#swiper_m_bestphoto").find("ul").find("li").each(function (index, Li) {
				if (index > 9) {
					PhotoArray.push(Li);
					$(this).remove();
				}
			})
		}
		else {
			PhotoArray.forEach(function (Li) {
				$("#swiper_m_bestphoto").find("ul").append(Li);
			});
			PhotoArray = new Array();
		}

		mBestPhotoSwiper = new Swiper('#swiper_m_bestphoto', {
			slidesPerView: mPhotoPageCount,
			speed: 1000,
			preventClicksPropagation: false,
			autoplayDisableOnInteraction: false,
			slidesPerGroup: mPhotoPageCount,
			onSlideChangeStart: function (swiper) {

				var PageNo = 0;

				if (swiper.activeIndex > 0) {
					PageNo = Math.ceil(swiper.activeIndex / mPhotoPageCount);
				}

				$("#swiper_bestphoto_paging").find("li").removeClass();
				$("#swiper_bestphoto_paging").find("li").eq(PageNo).addClass("on");
			}
		});

		$("#swiper_bestphoto_paging").find("li").removeClass("on");
		$("#swiper_bestphoto_paging").find("li").eq(0).addClass("on");
	}

	function MBestPhotoPaging(index) {
		mBestPhotoSwiper.slideTo((index * mStarPageCount) - 1, 1000, true);
	}
</script>	

	<div class="mm_sec9 mm9_3">
		<div class="mm_hd">
			<div>
