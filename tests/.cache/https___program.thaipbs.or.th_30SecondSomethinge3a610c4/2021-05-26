            function isLivePage () {
                var pathArray = window.location.pathname.split( '/' );
                if (pathArray[1] == 'live') {
                    return true;
                }
                return false;
            };

            function getTrackingCodeGoogle () {
                if (isLivePage()) {
                    return 'UA-72745579-1';
                } else {
                    return 'UA-72408298-1';
                }
            }

