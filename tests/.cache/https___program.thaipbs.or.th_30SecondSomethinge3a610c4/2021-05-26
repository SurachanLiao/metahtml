    <script src="https://account.thaipbs.or.th/sdk/thaipbs-account-client/v0.2.2/ThaiPBSAccountClient.js"></script>
    <script>
      var client = new ThaiPBSAccountClient({
        iframeUrl: 'https://account.thaipbs.or.th?minimal=1',
        onLogin: function(user) {
          var nodes = document.getElementsByClassName('userMenuButton');
          for (var i = 0; i < nodes.length; i++) {
            var node = nodes[i];
            node.innerHTML = '<img width="24" height="24" class="img-circle" style="margin-top: -6px" src="' + user.image_url + '">';
          };
        },
        onLogout: function() {
          var nodes = document.getElementsByClassName('userMenuButton');
          for (var i = 0; i < nodes.length; i++) {
            var node = nodes[i];
            node.innerHTML = '<i class="fa fa-user"></i>';
          };
        }
      });
      var nodes = document.getElementsByClassName('userMenuButton');
      for (var i = 0; i < nodes.length; i++) {
        var node = nodes[i];
        node.onclick = function(event) {
          event.preventDefault();
          client.show();
        }
      };
    </script>
