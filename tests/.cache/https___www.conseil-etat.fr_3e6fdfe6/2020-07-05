        <div id="loginpage" style="display:none" data-loginpage="/login"></div>
        <script type="text/javascript">
        $(document).ready(function() {

            addFavorite();
            // Favorite Link //
            function addFavorite() {
                $("a.arfavorite").click(function() {
                    if($("#loginpage").length){
                        window.location.href = '/login';
                    }else{
                        var locationId = $(this).data('location-id');
                        $.post("/mon-compte/addFavorite", {locationId: locationId}, function(data, status) {
                            if(status==="success") {
                                $('a.arfavorite[data-location-id="'+data+'"]').addClass('is-faved');
                            }else{
                            }
                        }); 
                    }
                    
                    return false;
                });
            } });
    </script>

