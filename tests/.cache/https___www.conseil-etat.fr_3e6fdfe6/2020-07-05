                <style type="text/css">
            .starter-template {
              padding: 3rem 1.5rem;
              text-align: center;
            }
            .query .card-block {
                padding: 1.25rem;
            }
            .ui-autocomplete {
                z-index: 1050 !important;
                cursor: pointer;
                background-color: white;
            }
            .ui-menu-item:hover {
                color:#005092;
            }
            </style>
        <script type="text/javascript">
            $( function() {
                $('#search_term').autocomplete({
                    source : function( request, response ) {
                        $.ajax({
                            url: '/autocomplete',
                            dataType: 'json',
                            data: {
                                name_startsWith: $('#search_term').val() 
                            },
                            success: function(data){
                                response( data );
                            }
                        });
                    },
                    select: function( event, ui ) {
                        $('#search_term').val(ui.item.value);
                        $('#search_term').closest('form').submit();
                    }
                }).focus(function() {
                    $(this).autocomplete("search");
                });
            });
            $( function() {
                $('#aside_search_term').autocomplete({
                    source : function( request, response ) {
                        $.ajax({
                            url: '/autocomplete',
                            dataType: 'json',
                            data: {
                                name_startsWith: $('#aside_search_term').val() 
                            },
                            success: function(data){
                                response( data );
                            }
                        });
                    },
                    select: function( event, ui ) {
                        $('#aside_search_term').val(ui.item.value);
                        $('#aside_search_term').closest('form').submit();
                    }
                }).focus(function() {
                    $(this).autocomplete("search");
                });

            });
        </script>
    
