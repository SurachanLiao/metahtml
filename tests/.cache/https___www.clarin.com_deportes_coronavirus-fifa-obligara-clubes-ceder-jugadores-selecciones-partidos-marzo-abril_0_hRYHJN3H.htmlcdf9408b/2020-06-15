            bk_addPageCtx('tags', ["coronavirus","fifa","gianni infantino"]);
            bk_addPageCtx('bprivate', window.isPrivateMode);
            bk_addPageCtx('idPase', getValueFromCookie('idPase') == '' ? "-2" : getValueFromCookie('idPase'));
            bk_addPageCtx('pase_sexo', _weizenbockUser ? _weizenbockUser.genero : "-2");
            bk_addPageCtx('pase_edad', _weizenbockUser ? getAge(_weizenbockUser.fechaNacimiento) : "-2");
            bk_addPageCtx('contador_gral', WeizenBockCompatible.getPaywallCount());
            bk_addPageCtx('block_LW', 'no');
            bk_addPageCtx('block_PW', 'no');
