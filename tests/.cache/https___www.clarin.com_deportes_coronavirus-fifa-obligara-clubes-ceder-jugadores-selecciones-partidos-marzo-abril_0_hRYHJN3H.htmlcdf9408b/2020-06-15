    function sendBlueKai(){
        try{

            var GDPRv;

            if(typeof $.cookie("cookieconsent_status") == "undefined"){
                GDPRv = "-2";
            }else{
                if($.cookie("cookieconsent_status") == "deny"){
                    GDPRv = "0";
                }
                if($.cookie("cookieconsent_status") == "allow"){
                    GDPRv = "1";
                }
            }

            if(typeof(window.WAuth) == "undefined")
                throw 'waiting WAuth';

            if(typeof(window.isPrivateMode) == "undefined")
                throw 'waiting bprivate';

            var _weizenbockUser = WAuth.getUserSync();

            if(_weizenbockUser){
                bk_addEmailHash(_weizenbockUser.email);
            }else{
                bk_addEmailHash("-2");
