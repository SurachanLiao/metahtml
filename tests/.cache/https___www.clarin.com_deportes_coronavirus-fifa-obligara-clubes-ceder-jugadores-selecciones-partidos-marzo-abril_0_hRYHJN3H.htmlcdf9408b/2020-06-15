        });
        function RemoveAccents(s){
            var i = 'ÁÉÍÓÚÑáéíóúñ'.split('');
            var o = 'AEIOUNaeioun'.split('');
            var map = {};
            i.forEach(function(el, idx) {map[el] = o[idx]});
            return s.replace(/[^A-Za-z0-9]/g, function(ch) { return map[ch] || ch; }).toLowerCase();
        }

    </script>
</div>
</div>
