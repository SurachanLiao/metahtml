<script id="CLATimerWorker" type="javascript/worker" data-autorefresh="true">
    var CLATimerWorker = function () {

        var refreshTime; //seg
        var tiempoRestante; //seg
        var initDate = {};
        var params = {
            timeoutValue: false,
            active: false
        };

        var autoRefresh = true;

        var init = function (options) {
            if (autoRefresh) {
                console.log('Timer Init.');
                refreshTime = options.refreshTime || refreshTime;
                tiempoRestante = refreshTime;
                start(refreshTime);
            } else {
                console.log('Timer Disabled');
            }
        };

        var start = function (tiempo) {
            params.active = true;
            initDate = new Date();
            params.timeoutValue = setTimeout(finish, tiempo * 1000);
            console.log('Timer Start: ', tiempo);
        };

        var pause = function () {
            if (!params.active) 
                return false;
            
            clearTimeout(params.timeoutValue);
            params.active = false;

            var now = new Date();
            var segDiff = Math.round((now.getTime() - initDate.getTime()) / 1000);
            tiempoRestante = tiempoRestante - segDiff;
            console.log('Timer Pause: ', tiempoRestante);
        };

        var resume = function () {
            console.log('Timer Resume.');
            if (params.active) 
                return false;
            
            start(tiempoRestante);
        };

        var finish = function () {
            console.log('Timer Finish.');
            active = false;
            postMessage({'status': 'finish'});
        };

        self.onmessage = function (e) {
            try {
                switch (e.data.cmd) {
                    case 'start':
                        init(e.data.options || {});
                        break;
                    case 'pause':
                        pause();
                        break;
                    case 'resume':
                        resume();
                        break;
                }
            } catch (e) {}
        };

    }();
</script>
