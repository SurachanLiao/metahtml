                        streamID: "hRYHJN3H",
                        streamTags: "3",
                        userAction: act1,
                        activeForComment: 1
                    });
                }
            }



            function GigyaLoadFunctions() {
                myComments();
            }
            var onGigyaServiceReady = function() {
                GigyaLoadFunctions();
            }

        </script>
