	<script>
		function convertToSlug(Text){
		   return Text
				.replace(" (W)","")
			   .replace(" (C)","")
			   .toLowerCase()
			   .replace(/[^\w ]+/g,'')
			   .replace(/ +/g,'-')
			   ;
		}
		
	function get_ipl_teams(teamFullName){
		var teamArr = new Array();
		
		teamArr['Kolkata Knight Riders']	= 'Kolkata';
		teamArr['KKR'] 						= 'Kolkata';
		teamArr['Mumbai Indian']			= 'Mumbai';
		teamArr['MI']						= 'Mumbai';
		teamArr['Sunrisers Hyderabad']		= 'Hyderabad';
		teamArr['SRH']						= 'Hyderabad';
		teamArr['Kings XI Punjab']			= 'Punjab';
		teamArr['KXIP']						= 'Punjab';
		teamArr['Royal Challengers Bangalore'] = 'Bangalore';
		teamArr['RCB']						= 'Bangalore';
		teamArr['Delhi Daredevils']			= 'Delhi';
		teamArr['DD']						= 'Delhi';
		teamArr['Rising Pune Supergiant']	= 'Pune';
		teamArr['RPS']						= 'Pune';
		teamArr['Gujarat Lions']			= 'Gujarat';
		teamArr['GL']						= 'Gujarat';
		teamArr['Rajasthan Royals']			= 'Rajasthan';
		teamArr['Chennai Super Kings']		= 'Chennai';
		teamArr['TBC']						= 'TBC';
		
		/* if(teamFullName in teamArr){
			return teamArr[teamFullName]; 
		}else{
			return teamFullName;
		} */
		
		return teamFullName;
		
	}
	
	
	function get_ipl_teams_full(teamFullName){
		var teamArr = new Array();
		
		teamArr['Kolkata']			= 'Kolkata Knight Riders';
		teamArr['Mumbai']			= 'Mumbai Indian';
		teamArr['Hyderabad']		= 'Sunrisers Hyderabad';
		teamArr['Punjab']			= 'Kings XI Punjab';
		teamArr['Bangalore']		= 'Royal Challengers Bangalore';
		teamArr['Delhi']			= 'Delhi  Daredevils';
		teamArr['Pune']				= 'Rising Pune Supergiant';
		teamArr['Gujarat']			= 'Gujarat Lions';
		teamArr['Royals']			= 'Rajasthan Royals';
		teamArr['Chennai']			= 'Chennai Super Kings';
		teamArr['TBC']				= 'TBC';
		
		if(teamFullName in teamArr){
			return teamArr[teamFullName];
		}else{
			return teamFullName;
		}
		
	//	return teamFullName;
		
	}
	
	function get_ipl_teams_short(teamFullName){
		var teamArr = new Array();
		
		teamArr['Kolkata']		= 'KKR';
		teamArr['KOL']			= 'KKR';
		teamArr['MUM']			= 'MI';
		teamArr['Mumbai']		= 'MI';
		teamArr['HYD']			= 'SRH';
		teamArr['Hyderabad']	= 'SRH';
		teamArr['PUN']			= 'RPS';
		teamArr['Punjab']		= 'KXIP';
		teamArr['PNJ']			= 'KXIP';
		teamArr['Bangalore']	= 'RCB';
		teamArr['BLR']			= 'RCB';
		teamArr['Delhi']		= 'DD';
		teamArr['DEL']			= 'DD';
		teamArr['Pune']			= 'RPS';
		teamArr['Gujarat']		= 'GL';
		teamArr['GUJ']			= 'GL';
		teamArr['TBC']			= 'TBC';
		
		if(teamFullName in teamArr){
			return teamArr[teamFullName]; 
		}else{
			return teamFullName;
		}
		
	//	return teamFullName;
	//	return teamFullName;
		
	}
	
	
		/* Search WEB */
		var site_url	= 'https://www.news18.com/cricketnext/';
		$(document).on("click",".search_box_button",function(){
			var topic_slug=$("#search_for_text").val();
			if(topic_slug==''){
				$("#search_for_text").focus();
				return false;
			}
			topic_slug	=convertToSlug(topic_slug);
			//window.location.href=site_url+"search/"+topic_slug+"/news/";
			window.location.href="https://secure.news18.com/cricketnext_search.php?topic="+topic_slug;
		});
		$("input#search_for_text").on('keyup', function (e) {
			if (e.keyCode == 13) {
				var topic_slug=$("#search_for_text").val();
				if(topic_slug==''){
					$("#search_for_text").focus();
					return false;
				}
				topic_slug	= convertToSlug(topic_slug);
				//window.location.href=site_url+"search/"+topic_slug+"/news/";
				window.location.href="https://secure.news18.com/cricketnext_search.php?topic="+topic_slug;
			}
		});
		/* End search WEB */

		$(document).ready(function(){
	function getMobileOperatingSystem() {
		var userAgent = navigator.userAgent || navigator.vendor || window.opera;

		if (/android/i.test(userAgent)) {
			return "Android";
		}
		if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
			return "iOS";
		}
		return "unknown";
	}
	var operatingSystemType=getMobileOperatingSystem();
	// console.log("OS type:"+operatingSystemType);
	var appStoreUrl="";
	if(operatingSystemType=='Android'){
		appStoreUrl="https://play.google.com/store/apps/details?id=com.divum.CricketNext&hl=en";
		$("#cn_app_link").attr("href",appStoreUrl);
	}else if(operatingSystemType=='iOS'){
		appStoreUrl="https://itunes.apple.com/in/app/cricketnext-live-for-iphone/id421171900?mt=8";
		$("#cn_app_link").attr("href",appStoreUrl);
	}else{
		$("#cn_app_link").hide();
	}
	
	$('.col-click').click(function(e){
		e.preventDefault();
		$('.col-dd').slideToggle();
	});
	
	});
	</script>
