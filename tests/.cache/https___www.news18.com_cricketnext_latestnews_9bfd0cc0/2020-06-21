<script>
$(document).ready(function(){
    $(".search-icon").click(function(){
        $(".exp-search").toggle();
		$(".s-icon").toggleClass("close-thik")
    });
});

// mobile score widget script
$('.mobile-tab li').on('click', function(e){
	e.preventDefault();
	var liSlug	= $(this).data('slug');
	$(this).addClass('act').siblings('li').removeClass('act');
	console.log(liSlug);
	$('#scoresContainerWAP ul').hide();
	$('#'+liSlug).show();
	if(liSlug=="liveMatchMob"){
		var matchlength = $(this).attr('matchlength');
		if(matchlength>0){
			$('.mscrol').show();
		}else{
			$('.mscrol').hide();
		}
	}else{
		$('.mscrol').show();
	}
	$('.mscrol').attr('tab_ul_id', liSlug);
	$('#scoreWedgetBox ul').animate({marginLeft:'0'}, 500);
});

var mcontener=$(".mobile-scard ul")
var mcount=0;
$("a.mscrol").click(function(e){
	var dataSlugArrow		= $(this).attr('tab_ul_id');
	var slideContener		= $('#'+dataSlugArrow);
	var mtotal_li			= slideContener.find("li").length;
	console.log(mtotal_li);
	var mli_width			= slideContener.find("li").width();
	e.preventDefault();
	var lip=$(this).data("type")
	if(lip=="mright"){
		mcount=mcount+1;
	}else{
		mcount=mcount-1;
	}
	if(mcount <0 || mcount>=mtotal_li){
		mcount=0;
	}
	slideContener.animate({marginLeft:'-'+mcount*mli_width+'px'},500);
});
	$('ul.mobile-tab li').on('click', function(){
		$(this).addClass('act').siblings('li'). removeClass('act');
	});

	
// web score widget script
var contener=$(".top-score-card .tsc-inner ul")
var count=0;
var li_width=contener.find("li").outerWidth();  
var totel_li=contener.find("li").length; /* total li contener width*/
$("a.dtscrol").click(function(){
	var dir=$(this).data("dir")
	if(dir=="right"){
		count=count+1;
	}else{
		count=count-1;
	}
	if(count <0 || count>=totel_li){
		count=0;
	}
	contener.animate({marginLeft:'-'+count*li_width+'px'},500);
});

$(document).ready(function(){	
	top_widget_live_scores();
	var siteUrl		= 'https://www.news18.com/cricketnext/';
	function top_widget_live_scores(){
		//var jsonUrl		= 'http://ed-nm.ibnlive.in.com/livescore/json/crosslivescores.json';
		var jsonUrl		= 'https://cricketnext.nw18.com/json/crosslivescores.json';
		$.ajax({
			url			: jsonUrl,
			type		: 'GET',
			dataType	: 'jsonp',
			jsonp		: 'jsonp',
			jsonpCallback: 'scores',
			crossDomain	: true,
			cache		: true,
			success:function(data){
				var liveMatchCount		= data.matches.length;
				if(liveMatchCount>0){
					var scoreBoxHtmlWEB 	= '<div class="match-type"><span>Live</span></div>';
					var scoreBoxHtml		= '';
					var scoreBoxHtmlMobile	= '';
					for(var a=0; a < liveMatchCount; a++){
						if(a==3){
							break;
						}
						var matchData		= data.matches[a];
						var matchId			= matchData.matchid;
						//console.log(matchData.series_short_name);
						var getSeriesName	= matchData.series_short_name.split(',');
						//console.log(getSeriesName);
						var	seriesName		= getSeriesName[0];
						//console.log(seriesName);
						var matchNum		= matchData.matchno;
						var matchDate		= matchData.date;
						var matchType		= matchData.matchtype;
						var matchStatus		= matchData.status;
						var needToWin		= matchData.needtowin;
						if(matchData.hasOwnProperty('hometeam') && matchData.hometeam!='undefined'){
						var homeTeam		= get_ipl_teams_full(matchData.hometeam);
							var team1Url		= convertToSlugNew(homeTeam);
						}else{
							var homeTeam = get_ipl_teams_full(matchData.teama.full_name);;
							var team1Url = convertToSlugNew(homeTeam);	
						}
						if(matchData.hasOwnProperty('awayteam') && matchData.awayteam!='undefined'){							
						var awayTeam		= get_ipl_teams_full(matchData.awayteam);
							var team2Url		= convertToSlugNew(awayTeam);
						}else{
							var awayTeam = get_ipl_teams_full(matchData.teamb.full_name);
							var team2Url = convertToSlugNew(awayTeam);	
						}
						
						if(window.location.pathname.indexOf(matchId) != -1){
							$('#needtowin').html(needToWin);
						}
						
						var scorelive= get_live_team_scores(matchData);
						var quickScoresUrl	= siteUrl+'cricket-live-scorecard/'+team1Url+'-vs-'+team2Url+'-live-score-'+matchId+'.html';
						
						scoreBoxHtml		= scoreBoxHtml+'<div class="scbox"><a href="'+quickScoresUrl+'"><div class="team-date"><span>'+seriesName+'</span>  |  '+matchNum+'</div>'+scorelive+'</a></div>';
						
						scoreBoxHtmlMobile		= scoreBoxHtmlMobile+'<li><div class="scbox"><a href="'+quickScoresUrl+'"><div class="team-date"><span>'+seriesName+'</span>  |  '+matchNum+'  |  '+matchDate+'</div>'+scorelive+'</a></div></li>';
					}
					//console.log(scoreBoxHtml);
					webscorecardhtml	= scoreBoxHtmlWEB+scoreBoxHtml;
					mobilescorecardhtml = scoreBoxHtmlMobile;
					
					$('.dtscrol').show();
					$(".mobileScoreWidgetTabs li[data-slug=liveMatchMob").addClass('act');
					$(".mobileScoreWidgetTabs li[data-slug=liveMatchMob").attr('matchLength', data.matches.length);
					
					$('#liveMatchBoxWEB').html(webscorecardhtml);
					$('.liveMatchBoxMOBILE').html(mobilescorecardhtml);
						
				}else{
					webscorecardhtml	= '';
					mobilescorecardhtml = '<li><div class="scbox">No Live Match</div></li>';
					
					if(typeof updateTopScoreWidget!='undefined'){
						$(".mobileScoreWidgetTabs li[data-slug=liveMatchMob").attr('matchLength', '0');
						clearInterval(updateTopScoreWidget);
					}
					
					$(".mobileScoreWidgetTabs li").removeClass('act');
					$(".mobileScoreWidgetTabs li[data-slug=recentMatchMob").addClass('act');
					$('.dtscrol').hide();
					$('.mscrol').show();
					$(".mscrol").attr('tab_ul_id', "recentMatchMob");
					$("#liveMatchBoxMOBILE").hide();
					$("#recentMatchMob").show();

					$(".tsc-inner li:first-child").css("display", "none");
					$('.liveMatchBoxMOBILE').css("display", "none");
					$('.liveMatchBoxMOBILE').html(mobilescorecardhtml);
				}
			},
			error: function(error){
				console.log('error in live score widget');
				$(".tsc-inner li:first-child").css("display", "none");
				$('.liveMatchBoxMOBILE').html('NO LIVE MATCH');
			}
			
		})
	}
	
	// inningwise live scores function
	function get_live_team_scores(scoresData){
		
		//var teamAname			= get_ipl_teams_short(scoresData.teama.name);
		var teamAname			= scoresData.teama.name;
		var teamAscores			= scoresData.teama.scores;
		//var teamBname			= get_ipl_teams_short(scoresData.teamb.name);
		var teamBname			= scoresData.teamb.name;
		var teamBscores			= scoresData.teamb.scores;
		var matchType			= scoresData.matchtype;
		var teamAScoreHtml = '';
		var teamBScoreHtml ='';
		// CALCULATE TEAM A SCORES
		var teamATotalInnings	= scoresData.teama.scores.length;
		if(teamATotalInnings>0){
			var x 	= 1;
			
			var innerBoxHtml = '';
			var actclass='team';
			for(var b = 0; b < teamATotalInnings; b++){
				
				var teamARuns			= teamAscores[b].scores;
				var teamAWickets		= teamAscores[b].wickets;
				var teamAOvers			= teamAscores[b].overs;
				var activeAStatus		= teamAscores[b].active;				
				if(activeAStatus==1){
					actclass 	= 'ytplay';
				}
				if(x==2){
					var seperator='<span> & </span>';
				} else{ 
					var seperator=''; 
				}

				if(matchType=='test' || matchType=='first-class'){
					innerBoxHtml=innerBoxHtml+' '+seperator+teamARuns+'/'+teamAWickets;
					x=x+1;
				}else{
					innerBoxHtml=innerBoxHtml+' '+teamARuns+'/'+teamAWickets+'('+teamAOvers+')'+seperator;				
					x=x+1;
				}
	
			}
			var teamAScoreHtml ='<div class="'+actclass+'">'+teamAname;
			teamAScoreHtml=teamAScoreHtml+innerBoxHtml+'</div>';
		}
		// CALCULATE TEAM B SCORES
		var teamBTotalInnings	= scoresData.teamb.scores.length;
		
		if(teamBTotalInnings>0){
			var y 	= 1;
			var innerBoxHtmlB = '';
			var actclassB='team';
			for(var c = 0; c < teamBTotalInnings; c++){
				
				var teamBRuns			= teamBscores[c].scores;
				var teamBWickets		= teamBscores[c].wickets;
				var teamBOvers			= teamBscores[c].overs;
				var activeBStatus		= teamBscores[c].active;				
				if(activeBStatus==1){
					var actclassB 	= 'ytplay';
				}
				if(y==2){
					var seperatorB='<span> & </span>';
				} else{ 
					var seperatorB=''; 
				}
				if(matchType=='test' || matchType=='first-class'){
					innerBoxHtmlB=innerBoxHtmlB+' '+seperatorB+teamBRuns+'/'+teamBWickets;
					y=y+1;
				}else{
					innerBoxHtmlB=innerBoxHtmlB+' '+teamBRuns+'/'+teamBWickets+'('+teamBOvers+')'+seperatorB;				
					y=y+1;
				}				
				
			}
				var teamBScoreHtml ='<div class="'+actclassB+'">'+teamBname;
				teamBScoreHtml=teamBScoreHtml+innerBoxHtmlB+'</div>';

		}else{
			teamBScoreHtml		= '<div class="'+actclassB+'" style="color:#909090">'+teamBname+' (YET TO BAT)</div>';
		}
		
		liveMatchScoreHtml	= teamAScoreHtml+teamBScoreHtml;
		
		return liveMatchScoreHtml;		
	}
	
	var updateTopScoreWidget=setInterval(function(){
		var inningName=$("#active_inning").val();
		var teamName=$("#active_country").val();
		top_widget_live_scores(inningName,teamName)
		}, 15000);
	
		
	function convertToSlugNew(Text){
		if(Text!='' && Text!='undefined'){
			return Text.replace(" (W)","").replace(" (C)","").toLowerCase().replace(/[^\w ]+/g,'').replace(/ +/g,'-');
		}else{
			var a = '';
			return a;
		}
	}
	
});

		/* Search WEB */
		var site_url	= 'https://www.news18.com/cricketnext/';
		$(document).on("click",".search_box_button",function(){
			var topic_slug=$("#search_for_text2").val();
			if(topic_slug==''){
				$("#search_for_text2").focus();
				return false;
			}
			topic_slug	=convertToSlug(topic_slug);
			//window.location.href=site_url+"search/"+topic_slug+"/news/";
			window.location.href="https://secure.news18.com/cricketnext_search.php?topic="+topic_slug;
		});
		$("input#search_for_text2").on('keyup', function (e) {
			if (e.keyCode == 13) {
				var topic_slug=$("#search_for_text2").val();
				if(topic_slug==''){
					$("#search_for_text2").focus();
					return false;
				}
				topic_slug	= convertToSlug(topic_slug);
				window.location.href=site_url+"search/"+topic_slug+"/news/";
				window.location.href="https://secure.news18.com/cricketnext_search.php?topic="+topic_slug;
			}
		});
		/* End search WEB */
	</script> <style>.widnone{display:none}.budget_divider{margin-top:0}#main{padding-top:56px}.mobheader{border-bottom:1px solid #b6b6b6;padding:0 10px}.mobheader ul{overflow-x:scroll;-webkit-overflow-scrolling:touch;display:flex;line-height:1.3;font-size:13px;height:30px}.mobheader li{margin:auto;flex:none;border-right:1px solid #b6b6b6;padding-right:8px;padding-left:8px}.mobheader li:last-child{border-right:0;padding-right:0}.mobheader li.active a{background:#ed3a4f;font-weight:400;color:#fff;font-weight:400}.mobheader a{color:#000;display:block}.mobheader{display:flex}.mobheader>ul:first-of-type{flex:none}.mobheader>ul:first-of-type li:first-of-type{border-right:1px solid #b6b6b6;padding-right:5px;padding-left:0}@media screen and (max-width:720px){.widnone{display:block}}</style>
