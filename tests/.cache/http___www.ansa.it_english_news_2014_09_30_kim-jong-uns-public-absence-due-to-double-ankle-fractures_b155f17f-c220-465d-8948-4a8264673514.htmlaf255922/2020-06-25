	function embedCode() {
		var embeddingCode = '<div><p><strong id="vidTit">TITLE</strong></p></div><iframe class="resp-iframe" src="https://www.ansa.it/sito/video/incorporaVideo.html?video=VIDEO&amp;photo=PHOTO&amp;title=TITLE" width="100%" height="150" seamless allowfullscreen scrolling="no"></iframe>';
		var responsive = "<script>var w = $('.resp-iframe').width();var h = parseInt(((w / 16) * 9));$('.resp-iframe').height(h);<\/script>";
		var video = $('#id_content_video source').attr('src');
		var photo = $('#id_content_video').attr('poster');
		var title = $('.news-title').html();
		var embeddingCode = embeddingCode.replace('PHOTO', photo).replace('VIDEO', video).replace('TITLE', title);
		$('#ID_CODICE_EMBEDHTML5').val(embeddingCode + responsive).select();
	}
</script>
