<script type="text/javascript">
var embedding = ["facebook", "youtube"];

embedding.forEach(fitIframe);

function fitIframe(embed) {
	elem = $('iframe[src*="'+embed+'"]');
	elem.width(elem.parent().width());
	var w = elem.width();
	var h = parseInt(((w / 16) * 9));
	elem.height(h);
	//$('iframe')[].parent().fitVids();
}
</script>


