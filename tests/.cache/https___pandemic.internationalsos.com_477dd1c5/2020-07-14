<script type="text/javascript">
    var PandemicMap = new Map({ username: 'internationalsos.i5j01mg3', zoom: 2});
    PandemicMap.init();
    if (typeof Reportlist !== 'undefined') {
        Reportlist.viewModel.reports.subscribe(function (items) {
            PandemicMap.clearPoints();
            for (var i = 0, len = items.length; i < len; i++) {
                var report = items[i];
                if (report && report.Locations && report.Locations.length > 0) {
                    for (var j = 0, lenj = report.Locations.length; j < lenj; j++) {
                        var location = report.Locations[j];
                        if (location.Longitude != null && location.Latitude != null) {
                            var markercolor = report.Diseases && report.Diseases.length > 0 ? report.Diseases[0].ColorCode : "#ffffff";
                            var repDate;
                            if (typeof report.ReportDateStr != 'undefined') repDate = report.ReportDateStr;
                            else repDate = report.ReportDate;
                            PandemicMap.addPoint(location.Latitude, location.Longitude, report.Headline, repDate, location.Name, report.ReportUrl, markercolor);
                        }
                     }
                }
           }
            PandemicMap.showPoints();
        });
    }
    
</script>

