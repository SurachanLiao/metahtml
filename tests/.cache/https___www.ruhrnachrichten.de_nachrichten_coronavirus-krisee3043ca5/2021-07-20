var portalNav = document.querySelector('.HeaderUserNavigation')

if(portalNav) {
portalNav.addEventListener(
  'click',
  function(e) {
    if (
      !e.target.classList.contains('green') &&
      !e.target.parentNode.classList.contains('green')
    ) {
      window.dispatchEvent(new Event('close_tee'));
    }
  },
  false
);
