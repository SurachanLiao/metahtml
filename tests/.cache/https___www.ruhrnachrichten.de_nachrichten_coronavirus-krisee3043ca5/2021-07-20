    <script type="text/javascript">
        $(document).ready(function () {
            $('nav .dropdown').click(function () {
                if ($(this).hasClass('open') && !$(this).hasClass('fi-node-only')) {
                    var strUrl = $(this).find('a').attr('href');
                    if (!!strUrl) {
                        window.location.href = strUrl;
                    }
                }
            });

            $('nav .dropdown-submenu > a').click(function () {
                var jLi = $(this).closest('li');

                if (jLi.hasClass('open')) {
                    if (!jLi.hasClass('fi-node-only')) {
                        var strUrl = $(this).attr('href');
                        if (!!strUrl) {
                            window.location.href = strUrl;
                        }
                    }
                    else {
                        return false;
                    }
                }
                else {
                    $('nav .dropdown-submenu').removeClass('open');
                    jLi.toggleClass('open');
                    return false;
                }
            });

            $(window).resize(function() {
            	if ($(".LoginLinkModule .fi-logout-content").css("display") == "none") {
            		$("button[data-target*='m_wdivMobileUserNavbar'] svg.fi-login-content").show();
            		$("button[data-target*='m_wdivMobileUserNavbar'] svg.fi-logout-content").hide();
            	}
            	else
            	{
            		$("button[data-target*='m_wdivMobileUserNavbar'] svg.fi-login-content").hide();
            		$("button[data-target*='m_wdivMobileUserNavbar'] svg.fi-logout-content").show();
            	}
            });

        	// PMODIF TS 2018-06-18 changes due to sticky nav

				

				//$(".WcmsHeader").sticky({ topSpacing: 0 }).on(
