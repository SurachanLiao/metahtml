                        catch(err){
                            var pwu_id = null;
                        }

                        if(pwu_id !== null) {
                            pulse('update', { actor:{ id:pwu_id, realm:'spid.no' }});
                        }
                    
                    pulse('trackPageView', { object:{
                    type:'Article'}
                    });
</script> 
<script>
function getAdId() {
    var cookie = decodeURIComponent(
        document.cookie.replace(
            new RegExp(
                '(?:(?:^|.*;)\\s*' + encodeURIComponent('_pulse2data').replace(/[\-\.\+\*]/g, '\\$&'
            ) + '\\s*\\=\\s*([^;]*).*$)|^.*$'), '$1'
        )
    ) || false;
    if (cookie) {
        return cookie.split(',')[9];
    }
    return;
}
