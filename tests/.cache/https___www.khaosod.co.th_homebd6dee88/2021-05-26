<script>document.onreadystatechange=function(){if("complete"===document.readyState){document.querySelectorAll(".glide-home-desktop-red .glide__slides .glide__slide").length>0&&new Glide(".glide-home-desktop-red",{type:"carousel",startAt:0,perView:1,focusAt:"center",autoplay:5e3,peek:500,gap:16,breakpoints:{1700:{peek:450},1600:{peek:400},1500:{peek:350},1400:{peek:300},1300:{peek:250},1140:{peek:170},960:{peek:50},720:{peek:20},540:{peek:20}}}).mount();var e=document.querySelector(".glide-home-desktop-red");if(e&&(e.style.visibility="visible"),"IntersectionObserver"in window){var t=!1,i=document.querySelector(".glide-2");if(i){var r={root:null,rootMargin:"250px 0px",threshold:0};new IntersectionObserver((function(e,i){e[0].isIntersecting&&(t||(initGlideDesktop(".glide-2"),t=!0))}),r).observe(i)}var o=!1,d=document.querySelector(".glide-3");if(d){r={root:null,rootMargin:"250px 0px",threshold:0};new IntersectionObserver((function(e,t){e[0].isIntersecting&&(o||(initGlideDesktop(".glide-3"),o=!0))}),r).observe(d)}initVideoClipsHot()}new Sticky(".ads-sidebar__inner--left"),new Sticky(".ads-sidebar__inner--right");var l=document.querySelector(".glide-home-desktop-red .glide__arrows .glide__arrow--left"),n=document.querySelector(".glide-home-desktop-red .glide__arrows .glide__arrow--right");function s(){var e=document.querySelector(".glide-home-desktop-red .glide__slides .udblock");if(e){var t=(window.innerWidth-e.offsetWidth)/2;t-=14,l.style.height=e.clientHeight+"px",n.style.height=e.clientHeight+"px",l.style.width=t+"px",n.style.width=t+"px",document.querySelectorAll(".glide-home-desktop-red .glide__slides .ads_position_wrapper__slide-container").forEach((function(t){t.style.height=e.clientHeight+"px"}))}}function c(){var e=document.querySelector(".container");return e?e.offsetLeft:0}function a(){var e=document.querySelector(".ads-sidebar-left");if(e){var t=e.offsetWidth,i=c();o=0-(o=Math.abs(t-i)),i>=t&&(o=i/2-t/2),e.style.left=o+"px",e.style.visibility="visible"}var r=document.querySelector(".ads-sidebar-right");if(r){var o;t=r.offsetWidth,i=c();o=0-(o=Math.abs(t-i)),i>=t&&(o=i/2-t/2),r.style.right=o+"px",r.style.visibility="visible"}}function u(){var e=document.querySelector(".glide-home-desktop-red .glide__slides .udblock"),t=document.querySelector(".glide-home-desktop-red .glide__slides .udblock__imgwrap"),i=document.querySelectorAll(".glide-home-desktop-red .glide__slides .udblock__textwrap");if(e){var r=75*e.offsetWidth/100-t.offsetHeight;i.forEach((function(e){e.style.height=r+"px"}))}}u(),s(),a(),window.addEventListener("resize",(function(){u(),s(),a()}))}};</script>

	<!-- Start Flush Ads -->
	
