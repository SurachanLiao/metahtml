    height: 60%;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 100%;
}

.ei-title {
    bottom: 125px;
    color: #fff;
    margin-left: 2%;
    margin-right: 0;
    opacity: 0.9;
    position: absolute;
    width: 100%;
}


