	var input = document.querySelector('#stock_serach');
	// events
	input.onkeyup = function(e) {
		input_val = this.value; // updates the variable on each ocurrence
		if (input_val.length > 0) {
			var stockItem = [];
			autocomplete_results = document.getElementById("autocomplete-results");
			autocomplete_results.innerHTML = '';
		if (window.XMLHttpRequest) {
			// code for IE7+, Firefox, Chrome, Opera, Safari
			xmlhttp=new XMLHttpRequest();
		} else {  // code for IE6, IE5
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
		xmlhttp.onreadystatechange=function() {
			if (this.readyState==4 && this.status==200) {
				stockItem = JSON.parse(this.responseText);
				for (i = 0; i < stockItem.length; i++) {
					autocomplete_results.innerHTML += "<li>"+"<a onclick=\"ga('send', 'event', 'Search', 'click', 'Stock_Search', 1, {'nonInteraction': 1});\" href='"+ stockItem[i].URL +"' target='_blank'>" + stockItem[i].FULL_NAME + "</a>"+"</li>";
				}
				autocomplete_results.style.display = 'block';
			}
		}
		xmlhttp.open("GET",stock_autosuggest_url+"?keyword="+input_val,true);
		xmlhttp.send();
		} else {
			stockItem = [];
			autocomplete_results.innerHTML = '';
		}
	}
	
