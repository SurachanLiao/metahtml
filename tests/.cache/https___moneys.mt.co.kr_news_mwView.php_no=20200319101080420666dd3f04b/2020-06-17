    function loginWithKakao(urls) {
        var kakaoUrl = "https://story.kakao.com/share?url=" + urls;
        var popupOptions = "width=600, height=370, resizable=no, scrollbars=yes, status=no";
        window.open(kakaoUrl, "kakaoPop", popupOptions);
    }

    function goTweet() {
