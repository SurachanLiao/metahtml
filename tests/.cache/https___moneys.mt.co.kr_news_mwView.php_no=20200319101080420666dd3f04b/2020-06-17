    function article_print_new(gid) {
        window.open('https://moneys.mt.co.kr/news/mwPrint.html?no=' + gid + '&type=1', 'printpop', 'toolbar=0,scrollbars=0,width=620,height=500,scrollbars=yes,resizable=yes,left=0,top=0');
    }

