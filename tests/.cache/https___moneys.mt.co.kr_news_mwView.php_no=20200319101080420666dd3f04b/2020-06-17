        var arg = 'https://moneys.mt.co.kr/news/mwView.php?no=2020031910108042066&code=w0408';
        window.open(encodeURI("https://twitter.com/share?url=" + arg + "&text=" + txt) + encodeURIComponent("#MoneyWK"), "tweetPop", "width=486, height=286,scrollbars=yes");
        /*fb_form.mode.value = 'tweet';
        fb_form.target = 'hiddenframe';
        fb_form.submit();*/
    }

