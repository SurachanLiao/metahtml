<script>
    var adstring = /^div-gpt-/;
    var addivs=[],els=document.getElementsByTagName('*');
    var adSlots=[];
    for (var i=els.length;i--;) {
        if (adstring.test(els[i].id)) {

            var elem = els[i];
            var theSlotID = els[i].id;
            var theSlotName = '/21740664820/kiro/MyNW_' + els[i].id.replace("div-gpt-ad-", "");

            //handle the different sizes
            var theSizes=[];
            if( (theSlotID.indexOf('right') >= 0) || (theSlotID.indexOf('left') >= 0)) {
                if( theSlotID.indexOf('home') >= 0) {
                    theSizes.push([300,250]);
                }else{
                    theSizes.push([300,250],[300,600]);
                }
            }
            if( theSlotID.indexOf('article') >= 0) {
                theSizes.push([300,250]);
            }
            if( theSlotID.indexOf('leaderboard') >= 0) {
                continue;
            }

            slot = {
                slotID: theSlotID,
                slotName: theSlotName,
                sizes: theSizes
            };
            adSlots.push(slot);
        }
    }
