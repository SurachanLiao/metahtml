    <script>

      $(document).ready(function(){

          if(window.location.hash == '#newsletter') {
              window.location.replace("https://" + window.location.hostname + "/fr/#newsletter");
          }
        $( ".cd-words-wrapper" ).click(function() {


          $('.wheel-info').removeClass('fadeIn').addClass('animated fadeOut faster');

          setTimeout(function(){
            $id = $( ".cd-words-wrapper .is-visible" ).attr('data-id');


            $('.card-program').addClass('d-none animated fadeOut');

            $('#card-program-' + $id).addClass('animated fadeIn').removeClass('d-none fadeOut');

            $('.cd-words-wrapper').addClass('stop');
            $('.wheel-info').removeClass('fadeOut').addClass('animated fadeIn');

            ga('send', 'event', { eventCategory: 'Landing page', eventAction: 'Clicked on a button', eventLabel: 'Roulette', eventValue: $id});

           }, 1500);

          setTimeout(function(){
            $('.wheel-info span').removeClass('d-none');
           }, 500);
        });

    
        var $offsetWhatscreation;

        var waypoints = $('#whats-creation').waypoint(function(direction) {
          $id = '#'+this.element.id;
          $offset = getOffset($id);
          $offsetWhatCreation = $offset + '%';
          $($id).css('opacity', (100 - $offset - 5) / 50);
        }, {
          offset: function() {
            return $offsetWhatscreation;
          }
        })

    
        var $offsetWhatscamera;

        var waypoints = $('#whats-camera').waypoint(function(direction) {
          $id = '#'+this.element.id;
          $offset = getOffset($id);
          $offsetWhatCreation = $offset + '%';
          $($id).css('opacity', (100 - $offset - 5) / 50);
        }, {
          offset: function() {
            return $offsetWhatscamera;
          }
        })

    
        var $offsetWhatsmap;

        var waypoints = $('#whats-map').waypoint(function(direction) {
          $id = '#'+this.element.id;
          $offset = getOffset($id);
          $offsetWhatCreation = $offset + '%';
          $($id).css('opacity', (100 - $offset - 5) / 50);
        }, {
          offset: function() {
            return $offsetWhatsmap;
          }
        })

    
        var $offsetWhatsunlimited;

        var waypoints = $('#whats-unlimited').waypoint(function(direction) {
          $id = '#'+this.element.id;
          $offset = getOffset($id);
          $offsetWhatCreation = $offset + '%';
          $($id).css('opacity', (100 - $offset - 5) / 50);
        }, {
          offset: function() {
            return $offsetWhatsunlimited;
          }
        })

    
        setAnimation();

      });


    </script>

