    <!-- Modal -->
<div class="modal fade" id="player-video-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-center" role="document">
    <div class="modal-content">
      <div class="modal-body">

        <div class="player-video">
            <div id="player-placeholder"class="widescreen">
                <div id='pmodal' width="100%" height="100%" style="background-repeat:no-repeat;background-position:center center;background-size:cover" class="no video"></div>
                <div class="title"></div>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
        </div>

      </div>
    </div>
  </div>
</div>


  <script type="text/javascript">

    var player;
    var mPlayerData;
    var mUserId = 0;
    var modal_played = false;
    // On click, destroy existing player, setup new player in target div
    function handleActivePlayer(e, video) {
      if (player) {
        player.stop();
      }

      player = jwplayer('pmodal').setup({

        playlist: [{
          image:   mPlayerData.picture,
          sources: mPlayerData.stream,
          tracks:  mPlayerData.subtitle,
          mediaid: mPlayerData.id,
        }],
        captions: {
          backgroundOpacity: 0,
          edgeStyle: 'uniform'
        },
        width: '100%',
        androidhls: true,
        sharing: {
          heading: mPlayerData.share.heading,
          link: mPlayerData.share.url
        },
        related: mPlayerData.related,
        skin: mPlayerData.color,
        wmode: 'transparent',
        stagevideo: false,
        aspectratio: '16:9',
        repeat: false,
        autostart: 'viewable',
        provider: 'http',
        ga: {
          idstring: "title",
          trackingobject: "pageTracker",
          label: "mediaid"
        },
      });

      jwplayer('pmodal').onPlay(function(e){
	if(!modal_played){
		var title = '';
				fbq('track', 'ViewContent', {'content_ids':[mPlayerData.id],'content_type':'product','content_name':title,'contents':[{'id':mPlayerData.id,'quantity':1 }]});
		modal_played = true;
	}
        if (0 == this.getPosition()) {
          updateViewCount();
        }
      });

    
      jwplayer('pmodal').onPause(function(e) {
        setPosition(this.getPosition());
      });

      jwplayer('pmodal').onComplete(function(e) {
        setPosition(0);
      });

      jwplayer('pmodal').on('firstFrame', function() {
        jwplayer('pmodal').seek(mPlayerData.view)
      });

    }

    function setPosition(position) {
      if (mUserId) {
        $.ajax({
            method: "POST",
            url: "/pviews/" + mUserId + "/" + mPlayerData.id,
            data: { program: mPlayerData.id, user: mUserId, etime: position }
        });
      }
    }

    function updateViewCount(position) {
      if (mUserId) {
        $.ajax({
            method: "POST",
            url: "/pviews/" + mUserId + "/" + mPlayerData.id,
            data: { program: mPlayerData.id, user: mUserId, eview: 1 }
        });
      }
    }

    $('#player-video-modal').on('hide.bs.modal', function (event) {
      if (player) {
        player.pause();
      }
    });

    $('#player-video-modal').on('hidden.bs.modal', function (event) {
      if (player) {
        player.stop();
        player.remove();
      }
    });

    $('#player-video-modal').on('show.bs.modal', function (event) {
      var button = $(event.relatedTarget) // Button that triggered the modal
      var programId = button.data('whatever') // Extract info from data-* attributes

      if(programId == undefined) {
        programId = mAutoPlayId;
      }

      if(!jQuery.isEmptyObject(jwplayer('playeriWkGnUVrDFys')) && typeof jwplayer('playeriWkGnUVrDFys').pause === "function") {
        jwplayer('playeriWkGnUVrDFys').stop();
      }

      $( "#player-video-modal .title" ).html('Chargement...');


      $.getJSON( "/fr/program-request/" + programId, function( data ) {
        console.log(data);
        mPlayerData = data;

      $( "#player-video-modal .title" ).html(data.title + '<small>' + data.module + '</small>');
        handleActivePlayer('e', 'video');
      });

    });

    // Autoplay only on program page and for the same episode
    var mAutoPlayId = 0
    if (mAutoPlayId) {
      $('#player-video-modal').modal('show')
    }

  </script>


