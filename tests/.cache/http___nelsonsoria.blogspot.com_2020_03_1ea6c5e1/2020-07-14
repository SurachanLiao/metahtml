        });
      });
      jQuery(document).ready(function() {
        var blloc = window.location.href;
        jQuery("#pagelistmenusblogul li a").each(function() {
          var blloc2 = jQuery(this).attr('href');
          if(blloc2 == blloc) {
            jQuery(this).parent('li').addClass('current-cat');
          }
        });
      });
