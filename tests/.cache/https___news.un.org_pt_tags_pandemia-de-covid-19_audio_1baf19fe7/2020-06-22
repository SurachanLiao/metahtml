  <li class="row row-__index__ audio-playlist-item">
    <div class="col-lg-12">
      <div class="row col-lg-12 close-icon-wrapper">
        <a href="javascript:removeItemFromPlaylist('__index__')" class="fa fa-times pull-right close-icon"></a>
      </div>
      <div class="row col-lg-12">
        <span class="item-tag">__item.product__</span>
        <span class="item-duration pull-right">__item.duration__<a href="__item.file__" download="__item.file__" class="fa fa-cloud-download"></a>
        </span>
      </div>
      <div class="row col-lg-12">
        <span><a class="item-title" href="javascript:playThis('__index__')">__shortTitle__</a></span>
      </div>
    </div>
  </li>
</ul>
