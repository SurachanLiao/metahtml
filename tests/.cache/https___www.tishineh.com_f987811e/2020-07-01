        window.addEventListener('DOMContentLoaded', function () {

            var owl = $("#owl-publiccity"),
                status = $("#owlStatus");

            owl.owlCarousel({
                autoPlay: 5000,
                items: 4,
                navigation: true,
                afterAction: afterAction
            });

            function updateResult(pos, value) {
                status.find(pos).find(".result").text(value);
            }

            function afterAction() {
                updateResult(".owlItems", this.owl.owlItems.length);
                updateResult(".currentItem", this.owl.currentItem);
                updateResult(".prevItem", this.prevItem);
                updateResult(".visibleItems", this.owl.visibleItems);
                updateResult(".dragDirection", this.owl.dragDirection);
            }

            var owlcity = $('.owl-cities').carousel({
            });

            owlcity.owlCarousel({
                items: 3,
                navigation: true,
                afterAction: afterAction
            });

            var owlgallery = $('.owl-gallery').carousel({
            });

            owlgallery.owlCarousel({
                items: 1,
                //navigation: true,
                afterAction: afterAction
            });

            //startCollapse
            $('.panel-collapse').on('show.bs.collapse', function () {
                $(this).siblings('.panel-heading').addClass('active');
            });

            $('.panel-collapse').on('hide.bs.collapse', function () {
                $(this).siblings('.panel-heading').removeClass('active');
            });
            //EndCollapse


            $("#foreignTourBox.TourBox .owl-carousel").css("display", "none").css("opacity", 0);
            $('.TourTab').live('click', function () {
                var affectElement = $(this).data("affectid");
                $(".TourBox .owl-carousel").css("display", "none").css("opacity", 0);
                $("#" + affectElement + ".TourBox .owl-carousel").css("display", "block").css("opacity", 1);
            });

        });
