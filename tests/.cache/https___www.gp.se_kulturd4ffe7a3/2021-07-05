            <script src="https://x.klarnacdn.net/kp/lib/v1/api.js" async></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.js"></script>

        <script>
            window.headerScripts = window.headerScripts || {}
            window.headerScripts.cacheBuster = function() {
                let cacheBuster = "";
                if (window.location.href.indexOf("1.") !== -1) {
                    cacheBuster = "?login=true";
                    if (window.location.href.indexOf("?") !== -1) {
                        cacheBuster = "&login=true";
                    }
                }

                return cacheBuster;
            }
        </script>

                <script>
            window.slm = window.slm || {};
            window.slm.premiumLinks = window.slm.premiumLinks  || {};
            window.slm.premiumConfig = window.slm.premiumConfig || {};
            /*Går mot prod, men behövs inte för att inloggningen ska funka*/
            window.slm.premiumLinks.subscribe = "https://info.gp.se/prenumerera/";
            window.slm.premiumLinks.ePaper = "http://info.gp.se/prenumerera/etidning/";
            window.slm.premiumLinks.service = "http://info.gp.se/kundservice/";
            window.slm.premiumLinks.offers = "https://medlem.gp.se/";
            /*Behövs, men är samma för stage och prod*/
            window.slm.premiumConfig.tuloSite = "stampen";
            window.slm.premiumConfig.accountOrigin = "GP";
            window.slm.premiumConfig.klarnaSite = "Göteborgs-Posten";
            window.slm.premiumConfig.m365SharedAccessSignature = "SharedAccessSignature sr=https%3a%2f%2fmedia365siteevent.servicebus.windows.net%2fsiteevents%2fmessage&sig=1ZicgVd6akP9d1sZWeh8TzNz6Y5FLKmyK%2bJA0RHdVOA%3d&se=1620981209&skn=SiteEventSendAccessKey";
