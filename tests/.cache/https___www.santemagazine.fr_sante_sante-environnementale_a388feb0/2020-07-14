    var btnSearch = document.getElementById('js-search');
    var btnNewsletter = document.getElementById('newsletterbtn');
    var searchbloc = document.getElementById('searchbloc');

    btnNewsletter.addEventListener("click", function () {
        hideSearchBarWithoutAjax();
    });


    btnSearch.addEventListener("click", function () {
        hideFormNL();
    });

    function hideSearchBarWithoutAjax() {
        var searchbloc = document.getElementById('searchbloc');
        var searchBtn = document.getElementById('js-search');
        if (btnNewsletter && searchbloc.classList.contains('active')) {
            searchbloc.classList.remove('active');
            searchBtn.classList.remove('active');
        }
    }

    function hideFormNL() {
        var nlbloc = document.getElementById('nlbloc');
        var newsletterbtn = document.getElementById('newsletterbtn');
        if (nlbloc.style.display === "block") {
            nlbloc.style.display = "none";
            newsletterbtn.classList.remove('active');
        }
    }
</script></header><div id="header-alt"><div class="bgc-lighter d-n@lg"><ol class="breadcrumb ta-c p-1 viewport m-auto"><li class="fs-xs c-main:h c-p d-ib c-dark"><a href="https://www.santemagazine.fr/">Accueil</a></li><li class="fs-xs c-main:h c-p d-ib c-dark"><a href="https://www.santemagazine.fr/sante">Santé</a></li><li class="fs-xs c-main:h c-d d-ib c-dark"><span>Santé environnementale</span></li></ol></div><div class="bgc-lighter p-1 hot-topics-list p-r viewport m-auto d-f ai-bs p-r oy-h@sm ox-a@sm"><span class='tt-u ws-nw pr-0c5'>En ce moment: </span><ul class="d-f fw-w ai-bs  fw-nw@sm "><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/sante/maladies/maladies-infectieuses/maladies-virales/coronavirus-depister-et-traiter-cette-infection-carte-de-france-de-lepidemie-431783">Coronavirus</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/sante/maladies/maladies-infectieuses/maladies-virales/tout-savoir-sur-les-masques-de-protection-contre-le-coronavirus-433485">Masques anti-coronavirus</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/sante/examens-medicaux/prelevements/test-de-depistage-au-coronavirus-covid-19-qui-ou-et-comment-432724">Test dépistage coronavirus</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/alimentation/acheter-et-cuisiner/cuisiner/que-manger-en-ete-nos-meilleures-recettes-sante-424508">Recettes d&#039;été</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/minceur/coaching-minceur/comment-lutter-contre-la-retention-deau-200432">Rétention d’eau</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/alimentation/regime-alimentaire/regime-detox/detox-tous-les-conseils-201906">Détox</a></li><li class="fs-0 fg-0  tt-u c-main bgc-main:h c-lighter:h br-xl b-main-s m-0c5"><a class="pl-1 pr-1 d-b" href="https://www.santemagazine.fr/m/prix-sante-mag-2020/home.html">Prix Santé Magazine 2020</a></li></ul></div></div><main id="main" class="bgc-lighter viewport m-auto  pl-2 pr-2   pl-1@xs pr-1@xs pl-1@sm pr-1@sm"><div class="p-f zi-3"></div><div class="pt-2 pb-2 p-r zi-4 d-f"><div id='contenu_haut-centre' class="ad-container m-auto "><script>
                    googletag.cmd.push(function() {
