<script type='text/javascript' language='javascript'>
    // define value;
    var codePic_id = "validatePic";
    var validatePageUrl = "/UIControls/Common/ValidateCode.aspx";
    var txtContent_id = "MainContainer_Control_RequestSubmitRapid1_txtContent";
    var vcustomContent_id = "";
    var inputValidate_id = "MainContainer_Control_RequestSubmitRapid1_txtValidateCode";
    var vcustomValidatorCode_id = "";
    var contentMinLength = 10;
    var txtName_id = "MainContainer_Control_RequestSubmitRapid1_txtName";
    var txtEmail_id = "MainContainer_Control_RequestSubmitRapid1_txtEmain";
    var txtTel_id = "MainContainer_Control_RequestSubmitRapid1_txtTel";
    var dpCountry_id = "MainContainer_Control_RequestSubmitRapid1_ddlCountryID";
    var content = $("#"+txtContent_id);
    var nameR = $("#"+txtName_id);
    var email = $("#"+txtEmail_id);
    var tel = $("#"+txtTel_id);
    var country = $("#"+dpCountry_id);
    var vCode = $("#"+inputValidate_id);
    function ClineValid(){
        var isValid = true;
        $("#vContent").text("");
        $("#vName").text("");
        $("#vEmail").text("");
        $("#vVCode").text("");
        if(content.val().trim().length==0){
            $("#vContent").text("content is required");
            isValid = false;
        }else if(content.val().trim().length<contentMinLength){
            $("#vContent").text("Required more than "+ contentMinLength +" Words");
            isValid = false;
        }
        if(nameR.val().trim().length==0){
            $("#vName").text("name is required");
            isValid = false;
        }
        if(email.val().trim().length==0){
            $("#vEmail").text("email is required");
            isValid = false;
        }else{
            var regex=/^\w{3,}@\w+(\.\w+)+$/;
            if(!regex.test(email.val())){
                $("#vEmail").text("email is error!");
                isValid = false;
            }
        }
        if(vCode.val().trim().length==0){
            $("#vVCode").text("verification code is required");
            isValid = false;
        }
        return isValid;
    }

    function PostSubmit(handler){
        var data = {
            Action : "RequestSubmit",
            content : content.val(),
            name : nameR.val(),
            email : email.val(),
            tel : tel.val(),
            country : country.val(),
            vCode : vCode.val(),
            materialID : '1756'
        };
        var ajaxUrl = "/Ajax/Request.aspx"
        jQuery.get(ajaxUrl, data,
                function (xml) {
                    var isSuccess = $("isSuccess", xml).eq(0).text().trim();
                    var returnText = $("returnText", xml).eq(0).text();
                
                    $("<div id='showBox'/>").html(returnText)
                    .css("text-aligh","center")
                    .dialog({                        
                        autoOpen: true,
                        title:"Infomation",
                        height: 150,
                        width: 200,
                        modal: true,
                        buttons: {
                            OK: function() {
                                $( this ).dialog( "close" );
                                if(isSuccess=="1") handler();
                            }
                        },
                        close: function(){
                            $("#showBox").remove();
                        }
                    });
                }
            );
    }

    // Validate Code temp
    var iTemp = 1;
</script>
