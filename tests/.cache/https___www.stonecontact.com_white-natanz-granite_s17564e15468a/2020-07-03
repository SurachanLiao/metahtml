<script type="text/javascript">
    var HistoryproIndex = 0;
    var FavariteproIndex = 0;
    var HistorystoIndex = 0;
    var FavaritestoIndex = 0;
    var HistorysupIndex = 0;
    var FavaritesupIndex = 0;
    var FavariteType = 1;
    var HistoryType = 1;
    var pichost = "https://pic.stonecontact.com/";
    var accountEmail = "";
    $(document).ready(function () {
        $("#Baskethover").hover(function () {
            $("#myBasket").css("display", "block");
        },function () {
            $("#myBasket").css("display", "none");
        });

        $("#Historyhover").hover(function () {
            $("#myHistory").css("display", "block");
        }, function () {
            $("#myHistory").css("display", "none");
        });
           
        $("#Products").click(function () {
            $("#Products").removeClass("selectn");
            $("#Products").addClass("selectz");
            $("#Suppliers").removeClass("selectz");
            $("#Suppliers").addClass("selectn");
            $("#Stone").removeClass("selectz");
            $("#Stone").addClass("selectn");
            getFavariteList(1, 10, 0);
            FavariteType = 1;
        });
        $("#Stone").click(function () {
            $("#Stone").removeClass("selectn");
            $("#Stone").addClass("selectz");
            $("#Suppliers").removeClass("selectz");
            $("#Suppliers").addClass("selectn");
            $("#Products").removeClass("selectz");
            $("#Products").addClass("selectn");
            getFavariteList(7, 10, 0);
            FavariteType = 7;
        });
        $("#Suppliers").click(function () {
            $("#Suppliers").removeClass("selectn");
            $("#Suppliers").addClass("selectz");
            $("#Stone").removeClass("selectz");
            $("#Stone").addClass("selectn");
            $("#Products").removeClass("selectz");
            $("#Products").addClass("selectn");
            getFavariteList(2, 10, 0);
            FavariteType = 2;
        });

        $("#FavoriteShowMore").click(function () {
            getFavariteList(FavariteType, 11, FavaritesupIndex);
        });
        $("#HistoryShowMore").click(function () {
            getHistoryList(HistoryType, 11, HistoryproIndex);
        });
        $("#Products1").click(function () {
            $("#Products1").removeClass("selectn");
            $("#Products1").addClass("selectz");
            $("#Suppliers1").removeClass("selectz");
            $("#Suppliers1").addClass("selectn");
            $("#Stone1").removeClass("selectz");
            $("#Stone1").addClass("selectn");
            getHistoryList(1,10,0);
            HistoryType = 1;
        });
        $("#Stone1").click(function () {
            $("#Stone1").removeClass("selectn");
            $("#Stone1").addClass("selectz");
            $("#Suppliers1").removeClass("selectz");
            $("#Suppliers1").addClass("selectn");
            $("#Products1").removeClass("selectz");
            $("#Products1").addClass("selectn");
            getHistoryList(7,10,0);
            HistoryType = 7;
        });
        $("#Suppliers1").click(function () {
            $("#Suppliers1").removeClass("selectn");
            $("#Suppliers1").addClass("selectz");
            $("#Stone1").removeClass("selectz");
            $("#Stone1").addClass("selectn");
            $("#Products1").removeClass("selectz");
            $("#Products1").addClass("selectn");
            getHistoryList(2,10,0);
            HistoryType = 2;
        });

        if (accountEmail == "") {

            accountEmail = getCookie('___LastLoginEmailCookieName');
        }
        if (accountEmail != "") {
            getHistoryList(1,10,HistoryproIndex);
            getFavariteList(1,10,FavariteproIndex);
        }
    });

 
    function getFavariteList(cDataType, isAdd, index) {
        postJson("https://w.stonecontact.com/api/v1/myfavoritesprovide/postmyfavoritesprovid", JSON.stringify({ "inRequestId": "1232", "PageIndex": index, "DataType": cDataType, "PageSize": "20", "From": "2", "Email": accountEmail }), function (data) {
            if (data.success) {
                var li = "";
                for (var i = 0; i < data.data.length; i++) {
                    if ( cDataType==1) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                              "<a href=\"" + data.data[i].ProductUrl + "\">" +
                              "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost + data.data[i].Picture + "\"/></div></div>" +
                              "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:130px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].ProductName + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                              "</a>" +
                              "<div style=\"clear:both;\"></div>" +
                              "</li>";
                    }else if (cDataType==2) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                             "<a href=\"" + data.data[i].CompanyUrl + "\" onclick=\"UpdateCompanyHistory(" + data.data[i].CompanyID + ")\">" +
                             "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost + data.data[i].LogoUrl + "\"/></div></div>" +
                             "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:200px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].Name + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                             "</a>" +
                             "<div style=\"clear:both;\"></div>" +
                             "</li>";
                    }else if ( cDataType ==7) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                              "<a href=\"" + data.data[i].MaterialUrl + "\">" +
                              "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost + data.data[i].Pic + "\"/></div></div>" +
                              "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:200px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].MaterialName + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                              "</a>" +
                              "<div style=\"clear:both;\"></div>" +
                              "</li>";
                    }
                }
                if (data.data.length <= 0) {
                    $("#FavoriteShowMore").hide();
                }
                if (isAdd==11) {
                    $("#FavoriteUl").append(li);
                } else {
                    $("#FavoriteUl").html(li);
                }
                if (data.data.length < 20) {
                    $("#FavoriteShowMore").hide();
                } else {
                    if (isAdd == 11) {
                        if (cDataType == 1) {
                            FavariteproIndex++;
                        }
                        if (cDataType == 2) {
                            FavaritesupIndex++;
                        }
                        if (cDataType == 7) {
                            FavaritestoIndex++;
                        }
                    }
                    
                }
            }
        });
    }
    function getHistoryList(cDataType,isAdd,index) {
        postJson("https://w.stonecontact.com/api/v1/history/gethistorylist", JSON.stringify({ "inRequestId": "1232", "PageIndex": index, "PageSize": "20", "From": "2", "Email": accountEmail,"Type":cDataType }), function (data) {
            if (data.success) {
                var li=""
                for (var i = 0; i < data.data.length; i++) {
                    if (data.data[i].DataType == cDataType && data.data[i].DataType==1) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                              "<a href=\"" + data.data[i].ProductUrl + "\">" +
                              "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost+data.data[i].Picture + "\"/></div></div>" +
                              "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:200px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].ProductName + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                              "</a>" +
                              "<div style=\"clear:both;\"></div>" +
                              "</li>";
                    } else if (data.data[i].DataType == cDataType && data.data[i].DataType == 2) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                             "<a href=\"" + data.data[i].CompanyUrl + "\" onclick=\"UpdateCompanyHistory(" + data.data[i].CompanyID + ")\">" +
                             "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost+data.data[i].LogoUrl + "\"/></div></div>" +
                             "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:200px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].Name + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                             "</a>" +
                             "<div style=\"clear:both;\"></div>" +
                             "</li>";
                    } else if (data.data[i].DataType == cDataType && data.data[i].DataType == 7) {
                        li += "  <li style=\"padding:5px 0px;border-bottom: 1px dashed #dedede;\">" +
                              "<a href=\"" + data.data[i].MaterialUrl + "\">" +
                              "<div style=\"float:left;\"><div class=\"phonecenter\" style=\"width:40px;height:40px;\"><img style=\"max-height:38px;max-width:38px;\" src=\"" + pichost+data.data[i].Pic + "\"/></div></div>" +
                              "<div style=\"float:left; margin-left: 5px;margin-top: 3px;\"><div class=\"textnone\" style=\"width:200px\"><span style=\"color:#212121;cursor:pointer;\">" + data.data[i].MaterialName + "</span></div><div style=\"color:#666666;\">" + getCurrentDate(data.data[i].InsertTime) + "</div></div>" +
                              "</a>" +
                              "<div style=\"clear:both;\"></div>" +
                              "</li>";
                    }

                }
                if (data.data.length <= 0) {
                    $("#HistoryShowMore").hide();
                }
                if (isAdd==11) {
                    $("#HistoryUl").append(li);
                } else {
                    $("#HistoryUl").html(li);
                }
                if (data.data.length < 20) {
                    $("#HistoryShowMore").hide();
                } else {
                    if (isAdd == 11) {
                        if (cDataType == 1) {
                            HistoryproIndex++;
                        }
                        if (cDataType == 2) {
                            HistorysupIndex++;
                        }
                        if (cDataType == 7) {
                            HistorystoIndex++;
                        }
                    }
                }
            }
        });
    }

    function UpdateCompanyHistory(CompanyId) {
        postJson("https://w.stonecontact.com/api/v1/history/inserthistory", JSON.stringify({ "inRequestId": "1231", "DataType": "7", "DataID": CompanyId, "From": "2", "Email": "", "Driven": "0" }), function (data) {
            if (data.success) {

            }
        });

    }

    function getCookie(name) {
        var strcookie = document.cookie;//è·åcookieå­ç¬¦ä¸²
        var arrcookie = strcookie.split("; ");//åå²
        //éåå¹é
        for (var i = 0; i < arrcookie.length; i++) {
            var arr = arrcookie[i].split("=");
            if (arr[0] == name) {
                return arr[1];
            }
        }
        return "";
    }
    function dateFtt(fmt, date) { //author: meizz   
        var o = {
            "M+": date.getMonth() + 1,                 //æä»½   
            "d+": date.getDate(),                    //æ¥   
            "h+": date.getHours(),                   //å°æ¶   
            "m+": date.getMinutes(),                 //å   
            "s+": date.getSeconds(),                 //ç§   
            "q+": Math.floor((date.getMonth() + 3) / 3), //å­£åº¦   
            "S": date.getMilliseconds()             //æ¯«ç§   
        };
        if (/(y+)/.test(fmt))
            fmt = fmt.replace(RegExp.$1, (date.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
            if (new RegExp("(" + k + ")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    }


    function getCurrentDate(time) {
        var date = new Date(time);
        var monthArray = new Array
        ("Jan", "Febr", "Mar", "Apr", "May", "June", "July", "Aug",
        "Sep", "Oct", "Nov", "Dec");
        month = date.getMonth();
        day = date.getDate();
        if (day.toString().length == 1) {
            day = "0" + day.toString();
        }
        return monthArray[month] + " " + day + ", " +
            date.getFullYear() + " " + dateFtt("hh:mm:ss",date);
    }

    function postJson(url, data, funcSuccess, funcError, funcBeforeSend) {
        $.ajax({
            url: url,
            type: 'POST',
            dataType: 'json',
            data: JSON.stringify(data),
            contentType: 'application/json; charset=utf-8',
            beforeSend: function (xhr) {
                if (funcBeforeSend) {
                    funcBeforeSend(xhr);
                }
            },
            success: function (data, status, xhr) {
                if (funcSuccess) {
                    funcSuccess(data);
                }
            },
            error: function (xhr, status, error) {
                if (funcError) {
                    funcError(error, url);
                }
            }
        });
    }



