<script type="text/javascript">
  (function() {
    loadScript('https://script.ioam.de/iam.js?m=1', function() {
      var IVWres = IVWres || {}
      IVWres.screen_width =
        typeof document.documentElement != 'undefined'
          ? document.documentElement.clientWidth
          : window.innerWidth
      var ivw_mobile = IVWres.screen_width < 785 ? true : false
      if (!ivw_mobile) {
        var iam_data = {
          mg: 'yes',
          st: 'westnach',
          cp: 'globaler_fallback',
          oc: 'globaler_fallback',
          sv: 'in',
          sc: 'yes',
        }
      } else {
        var iam_data = {
          mg: 'no',
          st: 'mobwestn',
          cp: 'mobile_globaler_fallback',
          oc: 'mobile_globaler_fallback',
          sv: 'mo',
          sc: 'yes',
        }
      }
      iom.c(iam_data, 1)
    })

    function loadScript(url, callback) {
      var loaded = false
      var script = document.createElement('script')
      script.type = 'text/javascript'
      script.src = url
      script.async = true
      script.onload = script.onreadystatechange = function() {
        if ((script.readyState && script.readyState !== 'loaded' && script.readyState !== 'complete') || loaded) return
        script.onload = script.onreadystatechange = null
        loaded = true
        callback()
      }
      var target = document.head || document.getElementsByTagName('head')[0] || document
      target.appendChild(script)
    }
  })()
</script>
