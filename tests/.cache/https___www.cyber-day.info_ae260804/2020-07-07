/*<![CDATA[*//*---->*/
if (typeof wm_select_link === 'function') wm_select_link('.mod_21145544 .liens');

$(function() {
    var ok_search = false;
    var search = document.location.pathname.replace(/^(.*)\/search\/([^\/]+)\/?(.*)$/, '$2');
    if (search != '' && search != document.location.pathname)	{ 
        ok_search = true;
    } else {
        search = document.location.search.replace(/^\?(.*)&?keyword=([^&]*)&?(.*)/, '$2');
        if (search != '' && search != document.location.search)	{
            ok_search = true;
        }
    }
    if (ok_search) {
        $('#icon-search-21145536').trigger('click');
        $('#keyword_safe_21145536').hide(); 
        $('#keyword_21145536').val(decodeURIComponent(search).replace('+', ' ')).show();
    }
});

/*--*//*]]>*/
