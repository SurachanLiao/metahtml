	var lotto_date_text = "16 พ.ค. 2564";

	function check_lotto(){
		var lottoinput = document.getElementById('lotto_check');
		console.log("lottoinput", lottoinput.value);
		ud_lotto_checklotto(lottoinput.value);
	}

	function change_lotto_check_select_date(){
		var check_select_date = document.getElementById('lotto_check_select_date').value;
		console.log("check_select_date", check_select_date);
		fetch("https://www.khaosod.co.th/get-lotto-date", {
			headers: {
				"Content-Type": "application/json",
				"Accept": "application/json",
				"X-Requested-With": "XMLHttpRequest",
				"X-CSRF-Token": ""
			},
			method: 'POST',
			credentials: "same-origin",
			body: JSON.stringify({
				check_select_date: check_select_date,
			})
		})
			.then(function (response) {
				return response.json()
			}).then(function (data) {
				console.log(data);
				if (data.success) {
					lotto_js = data.lotto_js;
					lotto_date_text = data.lotto_date_format;
				}
			});
	}

	function get_html_lotto_success(number, text, date_text){
		var html = `
			<div class="udlotto-modal">
				<br><br><br>
				<span class="udlotto-modal__icon icon-bonus"></span>
				<p class="udlotto-modal__text-1" >ยินดีด้วย</p>
				<p class="udlotto-modal__text-2" >ท่านถูกรางวัล ${text}</p>
				<p class="udlotto-modal__text-3" >ผลการตรวจหมายเลข <b>${number}</b></p>
				<p class="udlotto-modal__text-3" >งวดประจำวันที่ ${date_text}</p>
				<br>
				<a class="udbtn__btn" href="javascript:;" onclick="modal.close();" style="padding-left: 40px; padding-right: 40px;" >ตรวจสลาก</a>
				<br><br><br>
			</div>
		`;
		return html;
	}

	function get_html_lotto_fail(number, text, date_text){
		var html = `
			<div class="udlotto-modal">
				<br><br><br>
				<span class="udlotto-modal__icon icon-sad"></span>
				<p class="udlotto-modal__text-1" >เสียใจด้วย</p>
				<p class="udlotto-modal__text-2" >คุณไม่ถูกรางวัล</p>
				<p class="udlotto-modal__text-3" >ผลการตรวจหมายเลข <b>${number}</b></p>
				<p class="udlotto-modal__text-3" >งวดประจำวันที่ ${date_text}</p>
				<br>
				<a class="udbtn__btn" href="javascript:;" onclick="modal.close();" style="padding-left: 40px; padding-right: 40px;" >ตรวจสลาก</a>
				<br><br><br>
			</div>
		`;
		return html;
	}

	function ud_lotto_checklotto(lottoinput){
		if(lottoinput == ""){
			alert("กรุณากรอกตัวเลขให้ครบ 6 หลัก");
			return false;
		}
		if(lottoinput.length != 6){
			alert("กรุณากรอกตัวเลขให้ครบ 6 หลัก");
			return false;
		}
		var lotto_filter = lotto_js.filter(function(item){
			var number_length = item.number.length;
			var check = lottoinput;
			if (number_length != 6) {
				if(item.last == 0){
					check = lottoinput.substr(0, number_length);
				}else{
					check = lottoinput.substr(lottoinput.length - number_length);
				}
			}
			if (check == item.number) {
				return true;
			} else {
				return false;
			}
		});
		var success = false;
		var text = '';
		if (lotto_filter.length > 0) {
			success = true;
			lotto_filter.forEach(function(item){
				//text = text + "\r\nยินดีด้วยคุณถูก" + item.title + " จำนวนเงิน " + item.worth_text + " บาท";
				text = item.title;
			});
		}
		if (success) {
			var html = get_html_lotto_success(lottoinput, text, lotto_date_text);
			modal.setContent(html);
			modal.open();
		} else {
			var html = get_html_lotto_fail(lottoinput, text, lotto_date_text);
			modal.setContent(html);
			modal.open();
		}
		//console.log("lotto1", lotto_filter);
	}
</script>

	<!-- Start Flush Ads -->
	
    <!-- End Flush Ads -->

	<script>
	(function() {
		document.addEventListener('click',function(e){
            var url = window.location.href;

			if(e.target && (e.target.classList.contains("data-layer-track-header"))){
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_header",
					"event_action":"click",
					"event_label":"logo"
				});
				//console.log("data layer 1");
			} else if(e.target && (e.target.classList.contains("data-layer-track-footer"))){
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_footer",
					"event_action":"click",
					"event_label":"logo"
				});
				//console.log("data layer 2");
			} else if(e.target && (e.target.classList.contains("data-layer-track-menu"))){
				var category_slug = e.target.getAttribute('data-menu-slug');
				if (category_slug == 'menu-news') {
					return;
				}
				var category_parent_slug = e.target.getAttribute('data-menu-parent-slug');
				var send_slug = category_slug;
				if (category_parent_slug) {
					if (category_parent_slug != 'menu-news') {
						send_slug = category_parent_slug + '-' + category_slug;
					}
				}
				if (send_slug.startsWith("menu-news-")) {
					send_slug = send_slug.replace("menu-news-", "");
				}
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_header",
					"event_action":"click",
					"event_label": send_slug
				});
				//console.log("data layer 3 ", send_slug);
			} else if(e.target && (e.target.classList.contains("data-layer-track-footer-menu"))){
				var category_slug = e.target.getAttribute('data-menu-slug');
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_footer",
					"event_action":"click",
					"event_label": category_slug
				});
				//console.log("data layer 4 ", category_slug);
			} else if(e.target && (e.target.classList.contains("ud-mm__social-item"))){
				var social = e.target.getAttribute('alt');
				social = social.replace('social-', '');
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_header",
					"event_action":"social_button_click",
					"event_label":social
				});
				//console.log("data layer 5 ", social);
			} else if(e.target && (e.target.classList.contains("udfooter__icon"))){
				var social = e.target.getAttribute('data-social-slug');
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_header",
					"event_action":"social_button_click",
					"event_label":social
				});
				//console.log("data layer 6 ", social);
			} else if(e.target && (e.target.classList.contains("data-layer-line-top"))){
				dataLayer.push({
					"event":"track_event",
					"event_category":"line_add_friend",
					"event_action":"click_top_button",
					"event_label": url
				});
				// console.log("data layer 7 ", `line top ${url}`);
			} else if(e.target && (e.target.classList.contains("data-layer-line-bottom"))){
				dataLayer.push({
                    "event":"track_event",
					"event_category":"line_add_friend",
					"event_action":"click_bottom_button",
					"event_label": url
				});
				// console.log("data layer 7 ", `line bottom ${url}`);
            } else if(e.target && (e.target.classList.contains("data-layer-line-footer"))){
				dataLayer.push({
                    "event":"track_event",
					"event_category":"line_add_friend",
					"event_action":"click_footer_button",
					"event_label": url
				});
				// console.log("data layer 7 ", `line footer ${url}`);
			} else if(e.target && (e.target.classList.contains("data-layer-track-single"))){
				var social = e.target.getAttribute('data-social-name');
				social = social.toLowerCase();
				dataLayer.push({
					"event":"track_event",
					"event_category":"social_share_button",
					"event_action":social,
					"event_label":url
				});
				//console.log("data layer 8 ", social, url);
			} else if(e.target && (e.target.classList.contains("data-layer-track-tel"))){
				var tel = e.target.getAttribute('data-tel');
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_footer",
					"event_action":"call",
					"event_label":tel
				});
				//console.log("data layer 9 ", tel);
			} else if(e.target && (e.target.classList.contains("data-layer-track-home-section"))){
				var slug = e.target.getAttribute('data-menu-slug');
				dataLayer.push({
					"event":"track_event",
					"event_category":"homepage_section",
					"event_action":"click",
					"event_label":slug
				});
				//console.log("data layer 10 ", slug);
			} else if(e.target.parentNode && (e.target.parentNode.classList.contains("data-layer-track-home-section"))){
				var slug = e.target.parentNode.getAttribute('data-menu-slug');
				dataLayer.push({
					"event":"track_event",
					"event_category":"homepage_section",
					"event_action":"click",
					"event_label":slug
				});
				//console.log("data layer 11 ", slug);
			} else if(e.target && (e.target.classList.contains("data-layer-track-search"))){
				dataLayer.push({
					"event":"track_event",
					"event_category":"navigation_header",
					"event_action":"site_search_click",
					"event_label":null
				});
				//console.log("data layer 12 site_search_click");
			}
			//e.preventDefault();
		});
	})();
</script>


	
</body>
</html>
