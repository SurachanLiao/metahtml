$(document).on('click', 'img.close, a.down-link', function() {
downloadBannerHide();
var midnight = new Date();
midnight.setHours(23, 59, 59, 0);
$.cookie('show_app_popup', 1, { expires: midnight });
$.cookie('show_app_popup', 1, {expires: midnight, path:'/' });
});
$(window).scroll(function() {
if (screen.width < 766) {
var val = $.cookie('show_app_popup');
if ($(this).scrollTop() == 0 && (val != 'undefined' || val != null) && val != 1) {
downloadBannerShow();
} else if($(this).scrollTop() > 100) {
downloadBannerHide();
}
}
});
function downloadBannerHide() {
$("#app_banner").slideUp();
$("#page").removeClass("adjustPageTop");
$("#page").addClass("reset-page-padding");
$("header").css("top", "0px");
$("header").removeClass("adjustheaderTop");
}
function downloadBannerShow() {
setTimeout(function() {
$("#app_banner").slideDown();
$("header").addClass("adjustheaderTop");
$("#page").removeClass("reset-page-padding");
$("#page").addClass("adjustPageTop");
}, 10);
}
});
});
</script>
