document.addEventListener("DOMContentLoaded", function(event) {
$(document).ready(function() {
$.get( "/common/dynamicfootermain.html", function( hdata ) {
if (hdata && hdata.length > 0 && hdata.indexOf("Sorry, the page you are looking") === -1){
$( "#dynamicfootermain" ).html( hdata );
var $window = $(window),
$html = $('html');
var panels = $('.accordion > .newfooter_section_one_firstblocks').hide();
var buttons = $('.accordion > p > span').hide();
panels.first().show();
buttons.first().removeClass('collapsed');
$window.resize(function resize() {
var panels = $('.accordion > .newfooter_section_one_firstblocks').hide();
var buttons = $('.accordion > p > span').hide();
panels.first().show();
buttons.first().removeClass('collapsed');
//$('.accordion > p > span').click(function (e)
if($(window).width() < 1025)
{
$('.accordion > p').css('cursor','pointer');
}
$('body').on('click', '.accordion > p > span', function(e) {
//e.preventDefault();
if($(window).width() < 1025)
{
if($(this).hasClass('collapsed'))
{
var $this = $(this);
panels.hide();
buttons.addClass('collapsed');
$this.parent().next().slideDown();
$(this).removeClass('collapsed');
}
}
return false;
});
//$('.accordion > p ').click(function (e)
$('body').on('touch click', '.accordion > p ', function(e) {
//e.preventDefault();
if(e.target.localName == 'a')
{
window.location.href= e.target.a.href;
}
else
{
$(this).find('span').click();
}
return false;
});
if ($(window).width() > 1025)
{
$('.accordion > .newfooter_section_one_firstblocks').show();
$('.accordion > p > span').unbind('click');
$('.accordion > p').unbind('click');
}
}).trigger('resize');
}
});
});
});
</script>
