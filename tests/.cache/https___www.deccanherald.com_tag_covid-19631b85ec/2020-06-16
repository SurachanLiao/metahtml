<script>
document.addEventListener("DOMContentLoaded", function(e)
{
jQuery(document).ready(function($)
{
$( '.E-news-letter-box' ).css( "display","none" );
if( $(window).width() > 1200 )
{
$(document).click(function(e)
{
if($(e.target).is('.E-news-letter-close-btn'))
{
$('.E-news-letter-box').hide();
}
var container1 = $("#short_hdr_hori_menu .E-news-letter-box");
var container2 = $("#hdr_hori_menu .E-news-letter-box");
if (!container1.is(e.target) && container1.has(e.target).length === 0)
{
var email = $( '#short_hdr_hori_menu .dk_nl_email' ).val();
if( email != "" )
container1.hide();
}
if (!container2.is(e.target) && container2.has(e.target).length === 0)
{
var email = $( '#hdr_hori_menu .dk_nl_email' ).val();
if( email != "" )
container2.hide();
}
});
$("#short_hdr_hori_menu .E-news-letter-box").mouseleave(function (e)
{
var email = $( '#short_hdr_hori_menu .dk_nl_email' ).val();
if( email == "" )
$('#short_hdr_hori_menu .E-news-letter-box').hide();
});
$( "#short_hdr_hori_menu #dk_nl_signup" ).mouseover( function(){
$('#short_hdr_hori_menu .E-news-letter-box').show();
});
$( "#hdr_hori_menu #dk_nl_signup" ).mouseover(function(e) {
$('#hdr_hori_menu .E-news-letter-box').css({left: e.target.offsetLeft-15});
$('#hdr_hori_menu .E-news-letter-box').show();
});
$( "#hdr_hori_menu .E-news-letter-box" ).mouseleave(function() {
var email = $( '#hdr_hori_menu .dk_nl_email' ).val();
if( email == "" )
$('#hdr_hori_menu .E-news-letter-box').hide();
});
$(document).on('click', '#dnl_sub_btn', function()
{
if($('#short_hdr_hori_menu').is(':visible'))
{
var email = $( '#short_hdr_hori_menu .dk_nl_email' ).val();
}
if($('#hdr_hori_menu').is(':visible'))
{
var email = $( '#hdr_hori_menu .dk_nl_email' ).val();
}
var agreed = false;
var name = '';
if( email == "" || typeof email == "undefined" || email === null || email == "undefined" )
{
$('.E-news-letter-Error').empty();
$('.E-news-letter-Error').append("Email field is empty!");
$('.E-news-letter-Error').show().delay(10000).fadeOut();
}
else
{
$.ajax
({
type: 'post',
data: 'name=' + name + '&email=' + email + '&list=1&boolean=true&gdpr=' + agreed + '&Sourcewidget=header-widget',
url: '/newsletter/subscribe',
error: function(error)
{
$('.E-news-letter-Error').empty();
$('.E-news-letter-Error').append( "Subscription failed!" );
$('.E-news-letter-Error').show().delay(20000).fadeOut();
},
success: function( data )
{
var messages  = '';
var error_type = 0;
if( data == true )
{
$('.E-news-letter-box').empty();
$('.E-news-letter-box').append('<div class="E-news-letter-veri"><img src="/sites/dh/files/nl_images/varification-svg.svg"></div><div class="E-news-letter-flt-text"><span class="E-news-letter-close-btn">X</span><p>Verification email has been sent to your inbox.Please verify to complete subscription. </p></div>');
error_type = 1;
}
else
{
if( data == "Invalid email address." )
{
messages = "Please enter a valid Email id";
}
else
{
messages = data;
}
error_type = 0;
}
if( error_type == 1 )
{
$('.E-news-letter-Error').empty();
$('.E-news-letter-Error').append( messages );
$('.E-news-letter-Error').show().delay(10000).fadeOut();
}
else
{
$('.E-news-letter-Error').empty();
$('.E-news-letter-Error').append( messages );
$('.E-news-letter-Error').show().delay(10000).fadeOut();
}
}
});
}
});
}
});
});
</script>
