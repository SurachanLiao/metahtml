document.addEventListener("DOMContentLoaded", function(event) {
$(document).ready(function() {
var val = $.cookie('show_app_popup');
// downloadBannerShow();
if (screen.width < 766) {
if((val == 'undefined' || val == null || val =='') || val != 1)
{
val++;
