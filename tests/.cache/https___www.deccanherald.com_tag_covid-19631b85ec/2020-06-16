loading =0;
$('div#loadmoreajaxloader').hide();
$('.time-info').each(function(i)
{
current_card_time = this.getAttribute("data-utcdatetime");
local_time = localtimeSince( current_card_time );
this.innerHTML = local_time;
});
}).fail(function() {
loading=1; // or whatever
$('div#loadmoreajaxloader').hide();
});
}
});
});
});
</script>
