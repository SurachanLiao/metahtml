<script type="text/javascript">

	function endsWith(str, suffix) {
	    return str.indexOf(suffix, str.length - suffix.length) !== -1;
	}

	function handleSearchReturn(oEvent){
		 if(oEvent.keyCode == 13){
			 searchKeySubmit();
		 }
	}

	function searchKeySubmit(){
		
		var search = jQuery("#search").val();
		
		if(search.length > 0){
			
			if(endsWith(search, '...')){
				jQuery("#search").val(search.substr(0, search.indexOf('...')));
			}
		}

		jQuery('#companySearchLink').click();
	}
	
	function getSearchUrl(){
		var retValue = "";
		var searchKey  = jQuery("#search").val();
		if(searchKey != ""){
			jQuery.ajaxSetup({ cache:false });
			jQuery.ajaxSetup({ async:false });
			jQuery.get("/steelnetworks/company/companyContactSearch.do?method=search", 
				{ searchKey: searchKey },
				function(data){
					data = jQuery.trim(data);
					retValue = data;
				}, "text"
			);
		}
		return retValue;
	}
	
</script>






	<div class="col-sm-8 col-md-7" style="padding-left:0px; padding-right:0px;">
		<div class="well well-sm" style="padding:3px; margin-bottom:8px; background-color:#d9d9d9;">
			
			<form method="post" id="searchform" name="searchForm">
				<div class='input-group input-prepend dropdown2' data-select='true'>
					
						<input id="search" type="text" class="form-control" style=" height:40px; -webkit-border-top-left-radius: 0px; -webkit-border-bottom-left-radius: 0px;-moz-border-radius-topleft: 0px; -moz-border-radius-bottomleft: 0px;border-top-left-radius: 0px; border-bottom-left-radius: 0px;" maxlength="100" onkeydown="handleSearchReturn(event);" data-provide="typeahead" autocomplete="off" placeholder="Prezzi, articoli, aziende">
					
					
					<div class="input-group-btn">
						<button class="btn btn-primary" style=" height:40px;" id="companySearchLink">Cerca</button>
					</div>
				</div>
			</form>
		</div>
