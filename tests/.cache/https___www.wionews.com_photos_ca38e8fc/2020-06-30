<script>

$(document).ready(function() {
  var isAndroid = /(android)/i.test(navigator.userAgent);
  //IOS PLATFORM
  var isIphone = navigator.userAgent.indexOf("iPhone") != -1 ;
  var isIpod = navigator.userAgent.indexOf("iPod") != -1 ;
  var isIpad = navigator.userAgent.indexOf("iPad") != -1 ;
  var isIos = isIphone || isIpod || isIpad ;
  if(isAndroid) {
    document.getElementById("get-app1").href = "https://play.google.com/store/apps/details?id=esselgroup.zeemedia.wionews";
    document.getElementById("get-app1").target = "_blank";
    document.getElementById("get-app2").href = "https://play.google.com/store/apps/details?id=esselgroup.zeemedia.wionews";
    document.getElementById("get-app2").target = "_blank";
  } else if(isIos) {
    document.getElementById("get-app1").href = "https://apps.apple.com/us/app/wion-news/id1143018654";
    document.getElementById("get-app1").target = "_blank";
    document.getElementById("get-app2").href = "https://apps.apple.com/us/app/wion-news/id1143018654";
    document.getElementById("get-app2").target = "_blank";
  } else {
    document.getElementById("get-app1").href = "https://play.google.com/store/apps/details?id=esselgroup.zeemedia.wionews";
    document.getElementById("get-app2").href = "https://play.google.com/store/apps/details?id=esselgroup.zeemedia.wionews ";
  }
  $(".search-f").keypress(function (event) {
    if (event.which == 13) {
      location.href = '/google?search=' + $(".search-f").val();
    }
  });
  $(document).on("click", ".search-i", function () {
    location.href = '/google?search=' + $(".search-f").val();
  });
});
</script>

