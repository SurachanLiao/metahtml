// Thursday Farvardin 1 2578 (1398, 1440, 2019) 7:45:28 PM Tehran Local Time
var eventdate = new Date("March 20, 2019 17:58:27 PM EST");
 function toSt(n)
 {s=""
  if(n<10) s+="0"
  return s+n.toString();
 }
 function countdown()
 {cl=document.norooz_countdown;
  d=new Date();
  count=Math.floor((eventdate.getTime()-d.getTime())/1000);
  if(count<=0)
    {cl.days.value ="----";
     cl.hours.value="--";
     cl.mins.value="--";
     cl.secs.value="--";
     return;
    }
  //cl.secs.value=toSt(count%60);
  cl.secs.value=count;
  count=Math.floor(count/60);
  cl.mins.value=toSt(count%60);
  count=Math.floor(count/60);
  cl.hours.value=toSt(count%24);
  count=Math.floor(count/24);
  //cl.days.value=count;    
  cl.days.value= 26;   
  
  setTimeout("countdown()",500);
 }
// end script -->
</SCRIPT>
