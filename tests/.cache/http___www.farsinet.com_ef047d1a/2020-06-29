<SCRIPT>
<!-- Easter 2021 Countdown Script Copyright FarsiNet Inc. -->
<!-- hide script
// Sunday April 4 2021, 06:00:00 AM EST New York Eastern Time
var eventdate = new Date("April 4, 2021 6:00:00 AM EST ");
 function toSt(n)
 {s=""
  if(n<10) s+="0"
  return s+n.toString();
 }
 function countdown()
 {cl=document.noruz_countdown;
  d=new Date();
  count=Math.floor((eventdate.getTime()-d.getTime())/1000);
  if(count<=0)
    {cl.days.value ="----";
     cl.hours.value="--";
     cl.mins.value="--";
     cl.secs.value="--";
     return;
    }
  cl.secs.value=toSt(count%60);
  count=Math.floor(count/60);
  cl.mins.value=toSt(count%60);
  count=Math.floor(count/60);
  cl.hours.value=toSt(count%24);
  count=Math.floor(count/24);
  cl.days.value=count;    
  
  setTimeout("countdown()",500);
 }
// end script -->
</SCRIPT>
<!-- End of NoRuz Countdown Script -->
