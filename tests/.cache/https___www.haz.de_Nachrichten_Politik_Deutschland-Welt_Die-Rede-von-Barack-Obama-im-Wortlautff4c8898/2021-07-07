        },
        'gpt_rectangle-mobile-3': {
            container: 'gpt_rectangle-mobile-3',
            sizes: window.slotSizeDeclarations['rectangleMobile'],
            slotPosition: 'Inlist-3'
        },
        'gpt_rectangle-mobile-4': {
