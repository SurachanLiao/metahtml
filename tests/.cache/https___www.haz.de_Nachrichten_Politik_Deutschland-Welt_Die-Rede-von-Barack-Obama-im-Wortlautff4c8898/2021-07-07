    // Build: 2021-05-17 10:39:05

    function getSlotPosition(slotid) {
        var atfSlots = ['Top', 'Offcontent-Right', 'Rectangle-1'];
        if (atfSlots.indexOf(slotid) !== -1) {
            return 'btf';
        }

        return 'atf';
    }

    function removeUsableSlotKeys(arrKeys) {
        if (!Array.isArray(arrKeys)) return;
        arrKeys.forEach(
            function (element) {
                removeUsableSlotKey(element);
            });
    }

    function removeUsableSlotKey(key) {
        if (typeof key !== 'string') return;
        var index = window.usableSlots.indexOf(key);
        if (index > -1) {
            window.usableSlots.splice(index, 1);
        }
    }

    function getIndexExchangeSlotSize(slotId) {
        switch (slotId) {
            case ('Top'):
                if (isMobile) {
                    if (window.viewability === 'nicht lesbar') {
                        return [300, 50];
                    } else {
                        return [300, 250];
