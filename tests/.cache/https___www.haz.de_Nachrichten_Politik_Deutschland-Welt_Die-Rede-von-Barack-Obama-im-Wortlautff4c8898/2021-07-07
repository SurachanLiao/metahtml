            sizes: window.slotSizeDeclarations['rectangleMobile'],
            slotPosition: 'Inlist-9'
        },'gpt_outstream': {
            container:  'gpt_outstream',
            sizes: window.isMobile ? window.slotSizeDeclarations['rectangleMobile'] : window.slotSizeDeclarations['outOfPage'],
            slotPosition: 'Outstream'
        },
        'oms_intext_container': {
            container:  'oms_intext_container',
            sizes: window.isMobile ? window.slotSizeDeclarations['rectangleMobile'] : window.slotSizeDeclarations['outOfPage'],
            slotPosition: 'Outstream'
        },
    };

    function prepareLazy(elementId, fireServiceRequest=false) {
        window.lazyElements = window.lazyElements || [];

        if (typeof window.lazySlots[elementId] != 'undefined' && typeof window.usableSlots[elementId] == 'undefined') {
            var bidders = (typeof window.bidders[window.lazySlots[elementId].slotPosition] != 'undefined') ?
                window.bidders[window.lazySlots[elementId].slotPosition] : [];
            if (bidders.length) {
                var element = {
                    slotName: window.slotPrefix + window.lazySlots[elementId].slotPosition,
                    container: window.lazySlots[elementId].container,
                    sizes: window.lazySlots[elementId].sizes,
                    bids: bidders
                };
            } else {
                var element = {
                    slotName: window.slotPrefix + window.lazySlots[elementId].slotPosition,
                    container: window.lazySlots[elementId].container,
                    sizes: window.lazySlots[elementId].sizes,
                    amazon: false
                };
            }

            window.lazyElements.push(element);
            if (fireServiceRequest === true) {
                adHandler.service.addAdUnits(
                    {'1_10000': [element]}
                );
                adHandler.service.request();
            }
        }
    }

    document.addEventListener("DOMContentLoaded", function() {
        if (document.getElementById('oms_intext_container')) {
            prepareLazy('oms_intext_container');
        }
        if (document.getElementById('gpt_outstream')) {
            prepareLazy('gpt_outstream');
        }

        adHandler.service.addAdUnits(
            {'1_10000': window.lazyElements}
        );
        adHandler.service.request();
    });

    Object.keys(targetingData).forEach(function (item) {
        adHandler.service.setTargeting(item, targetingData[item]);
    });
    adHandler.service.request();

    adHandler.cmd.push(function () {
        try {
            googletag.cmd.push( function() {
                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    if (event.slot.getSlotElementId() === 'gpt_superbanner') {
                        var oldSkyTop = skyTop;
                        skyTop = document.getElementById('gpt_superbanner').offsetTop;
                        if (oldSkyTop != skyTop) {
                            checkSkyPosition();
                        }
                    }
                    clearInterval(window.wpIntervId);
                    window.wpIntervId = setInterval(function () {
                        if (window.wallpaperCounter++ > 9) {
                            clearInterval(window.wpIntervId);
                        }
                        var DhtmlLayer = document.getElementById('omsv_sky_DhtmlLayer');
                        if (DhtmlLayer !== null && document.getElementById('gpt_superbanner') !== null) {
                            DhtmlLayer.style.setProperty('top', document.getElementById('gpt_superbanner').offsetTop + 'px');
                            clearInterval(window.wpIntervId);
                        }
                    }, 100);
                });
            });
        }
        catch (e) {
        }
    });
</script>


    
                


        
  
