    <script>
    (function() {
      try {
        var todayDate = new Date();
        var endDate = new Date('2021-08-18');

        var datePassed = todayDate > endDate;
        var isIE = navigator.userAgent.indexOf('MSIE') !== -1 || navigator.appVersion.indexOf('Trident/') > -1;

        if (!datePassed && isIE) {
          var warningTargetElem = document.getElementById('ebWireframe');

          var ieWarningElem = document.createElement('div');
          ieWarningElem.className = 'bg--graa7 padding-xxl--tb padding-l--rl text-align--center fontsize-large fontweight-bold';
          ieWarningElem.innerHTML = 'Denne browser understøttes ikke længere.<br><a href="/teknikcheck/browser.html" class="color--red" target="_blank">Læs eventuelt mere</a> om understøttede browsere, som giver det fulde udbytte af ekstrabladet.dk';
          warningTargetElem.insertBefore(ieWarningElem, warningTargetElem.childNodes[0]);
        }
      } catch (error) {
        window.ebLog({
          component: 'IEDEPRECATIONBANNER',
