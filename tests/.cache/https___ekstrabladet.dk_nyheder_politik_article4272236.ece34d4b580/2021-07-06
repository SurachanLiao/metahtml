      function ebLog(userInput) {
        switch (userInput.level ? userInput.level : "") {
          case "ERROR":
            console.error(userInput);
            break;
          case "WARN":
            console.warn(userInput);
            break;
          case "INFO":
            console.info(userInput);
            break;
          case "DEBUG":
            console.debug(userInput);
            break;
          default:
            console.log(userInput);
            break;
        }
      }
