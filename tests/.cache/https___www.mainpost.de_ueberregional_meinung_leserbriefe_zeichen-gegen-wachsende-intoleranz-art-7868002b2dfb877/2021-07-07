<script>

	function getCookieAd(name, defaultValue) {
		match = document.cookie.match(new RegExp(name + '=([^;]+)'));
		if (match) {
			return match[1];
		} else {
			return defaultValue;
		}
	}

  var hide_pos5_for_seconds = 20000; // Sekunden, für die die Werbung nach Klick auf den Schließen-Button ausgeblendet/deaktiviert werden soll
  var hide_pos5_cookie = getCookieAd('hide_pos5', false);


  document.addEventListener('DOMContentLoaded', function(event) {
    function setFerquencyCappingCookie(name, value, seconds){         
        var d = new Date;         
        d.setTime(d.getTime() + 1000 * seconds);         
        document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString();     
    }
    var pos5_closer = document.getElementById("slot-topmobile5_closer-helper");
    
    pos5_closer.addEventListener('click', function(event){       
        setFerquencyCappingCookie('hide_pos5', 'true',  hide_pos5_for_seconds);     
    });
  })
 
</script>




