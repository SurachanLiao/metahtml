	   let keywords = "";
       let persons = "Barack Obama, Wladimir Wladimirowitsch Putin";
	   let places =  "";
       let organizations = ""; 
       function tpCheckIfEmpty(array) {
         if (array !==""){
           return array + ", "
         }
       } 
       function tpFormatTags (keywords, persons, places, organizations){
         let keywordString = tpCheckIfEmpty(keywords) + tpCheckIfEmpty(persons) + tpCheckIfEmpty(places) + tpCheckIfEmpty(organizations);
         //keywordString = keywordString.slice(0, -2); Diese funktion ist verantwortlich, das der Block nicht mehr weiter ausgeführt wird.
         return keywordString;
       }
       console.log("LLL");
       tp = window["tp"] || [];
       console.log("MMM");
       tp.push(["setSandbox", false]);
       tp.push(["setDebug", true]);
       tp.push(["setMaxCookieExpirationInDays", 365]);
		
       tp.push(["setTags", [tpFormatTags (keywords, persons, places, organizations)]]); // Tags aus Retresco oder anderem System
       console.log("NNN");
       tp.push(["setContentCreated", "2013-12-30T19:38:11+0100"]); // Erstelldatum des Artikels
       tp.push(["setContentAuthor", "Evangelischer Pressedienst"]); // Autor des Artikels
       tp.push(["setContentSection", "/ueberregional/meinung/leserbriefe/"]); // Rubrik / Ressort
       tp.push(["setContentIsNative", "false"]); // Handelt es sich um Native Advertising?       
       tp.push(["setCustomVariable", "contentDocumentId", "7868002"]); // Artikel ID
       tp.push(["setCustomVariable", "contentEncodedUrl", encodeURI("https://www.mainpost.de/ueberregional/meinung/leserbriefe/zeichen-gegen-wachsende-intoleranz-art-7868002")]); // Encoded URL
