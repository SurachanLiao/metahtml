  jQuery(document).ready(addDimensions());

  function addDimensions(){
    var dimDate='2020-01-17';
    var dimAuthor='AFP';
    var dimMainCity='monde';
    var dimCities='toulon';
    var dimMainThematic='Faits de société';
    var dimThematics='';
    var dimMainTag='';
    var dimTags='';
    var dimTypePage='Live déconnecté';
    dataLayer.push({'DimDate': dimDate});
    dataLayer.push({'DimJournaliste': dimAuthor});
    dataLayer.push({'DimLocalitePrim': dimMainCity});
    dataLayer.push({'DimLocaliteSecond': dimCities});
    dataLayer.push({'DimThemePrim': dimMainThematic});
    dataLayer.push({'DimThemeSecond': dimThematics});
    dataLayer.push({'DimTagPrim': dimMainTag});
    dataLayer.push({'DimTagSecond': dimTags});
    dataLayer.push({'DimTypePage': dimTypePage});
  }
</script>
                  
