<script type="text/javascript">
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];

  var refreshReady = {}; // test des services

  const addService = function(key) { // fonction de déclaration des services
    refreshReady[key] = false;
  }
  addService('DFP');
  addService('IAS');
  addService('W366');
  addService('Didomi');

   // check le consent DFP
   var isConsentDFP = function() {
        try {
            if (typeof Didomi != 'undefined') {
                return Didomi.getUserConsentStatusForVendor('google');
            } else {
                return false;
            }
        } catch (e) {
            return false;
        }
    }
    
    // check les états des services
    var checkIfShowAds = function(key) {

        if(refreshReady.hasOwnProperty(key)) {
            refreshReady[key] = true;
        }

        var readyToRefresh = true;
        for (var property in refreshReady) {
            
            console.log("ADS:: "+property+" is ready ?"+refreshReady[property]);

            if (refreshReady.hasOwnProperty(property) && refreshReady[property] != true) {
                readyToRefresh = false;
            }
        }

         if(readyToRefresh) {
            var consentPub =  isConsentDFP();
            googletag.pubads().setRequestNonPersonalizedAds(consentPub ? 0 : 1);
            googletag.pubads().refresh();
        }
   }


function callIAS()
{
    var IAS_PET_TIMEOUT = 700;

    //var iasDataHandler;
    var __iasPET = window.__iasPET || {};
    __iasPET.queue = __iasPET.queue || [];
    __iasPET.pubId = '9748';

       iasDataHandler = function () {
        console.log("IAS :: Callback function");
        requestAds();
    }   

    requestAds = function() {
        console.log("IAS :: Request Ads function");
        
        // EBE : Quick fix for FireFox browser, needs to be improved later
        if(typeof __iasPET !=='undefined' && typeof __iasPET.setTargetingForGPT === 'function'){
            __iasPET.setTargetingForGPT();
        }
        clearTimeout(iasTimeOut);
        checkIfShowAds('IAS');
    }

    var iasTimeOut = setTimeout(function() {
        requestAds();
    }, IAS_PET_TIMEOUT);

    googletag.cmd.push(function() {
      console.log('dedans');
        var gptSlots = googletag.pubads().getSlots();
        console.log(gptSlots);
        var iasPETSlots = [];

        for (var i = 0; i < gptSlots.length; i++) {
            var sizes = gptSlots[i].getSizes().map(function(size) {
                if (size.getWidth && size.getHeight)
                    return [size.getWidth(), size.getHeight()];
                else
                    return [1, 1];
            });
            iasPETSlots.push({
                adSlotId: gptSlots[i].getSlotElementId(),
                size: sizes,
                adUnitPath: gptSlots[i].getAdUnitPath()
            });
        }

        console.log(iasPETSlots);

        __iasPET.queue.push({
            adSlots: iasPETSlots,
            dataHandler: iasDataHandler
        });
    });
}

</script>
