        var contador = 0;
        var win = $(document);
        win.scroll(function () { 

            contador = parseInt($('#inputContador').val());
            var check = $('#inputCheck').val();
            var oTop = $('#mainContainerNote' + contador).offset().top - window.innerHeight;
            var pTop = $(window).scrollTop();
            if (check == 'true') {
                if (pTop > oTop && contador <= 10) {
                    $('#inputCheck').val('false');
                    geturltitulo();
                    if (strUrl != undefined && strUrl != "") {
                        $("#next-page").append("<div class='lds-facebook'><div></div><div></div><div></div></div>");
                        $.ajax({
