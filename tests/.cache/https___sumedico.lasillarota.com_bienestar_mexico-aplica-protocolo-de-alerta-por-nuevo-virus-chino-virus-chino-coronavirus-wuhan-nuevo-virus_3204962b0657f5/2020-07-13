        function geturltitulo() {
            strUrl = undefined;
            title = undefined;
            claseIds = "#inputNota" + contador;
            url = $(claseIds).attr("data-url");
            strUrl = $(claseIds).attr("data-url");
            title = $(claseIds).attr("data-title");
            authors = $(claseIds).attr("data-authors");
            postid = $(claseIds).attr("data-postid");
            pubdate = $(claseIds).attr("data-pubdate");
            sections = $(claseIds).attr("data-sections");
            tags = $(claseIds).attr("data-tags");
        }

        function trackContentInsight(mainNumber) {
            _ain.authors = authors;
            _ain.url = window.location.protocol + "//" + window.location.host + strUrl;
            _ain.postid = postid;
            _ain.maincontent = "div#mainContainerNote" + mainNumber;
            _ain.title = title;
            _ain.pubdate = pubdate;
            _ain.sections = "Sumedico>" + sections;
            _ain.tags = tags;
            _ain.track();
        }

        function setPositionRedesSociales(index) {
            setTimeout(function () {
                var sectionF = $("#mainContainerNote0 > .section").attr("class");
                var red = sectionF.replace(/section-Notas/g, "").replace(/section/g, "").replace(/ /g, '') + " .bottom-share-1 > .uk-sticky-below";
                $(red).attr("style", "z-index: 1; position: fixed; top: 0px; width: 60px;");
            }, 200);
        }


        function getCadidate() {
            var rand = Math.ceil((Math.random() * 10000));
            var _url = "/candidate.txt?_" + rand;
            $.ajax({
                url: _url,
                success: function (data) {
                    console.log(data);
                }
            })
        }


        function pushGoogle(href, title) {
            if (window.location.pathname != href) {
                history.pushState({}, '', href);
                document.title = title;
                clicks(href);
                ga('send', 'pageview', href);
            }
        }

        function clicks(url) {
            var id = url.replace(/(.*\/)/g, '')
            $(document).ready(function () {
                var strUrlPost = '/xstatic/sumedico/template/submitaddclickstory.aspx?id=' + id;
                $.post(strUrlPost, function (data) {
                    var valretorno;
                    valretorno = data;
                });
            });
        }

        function taboolawgt(id) {

            window.page_counter++;
            window._taboola = window._taboola || [];
            _taboola.push({ mode: 'thumbnails-b', container: id, placement: 'below the article', target_type: 'mix' });
            _taboola.push({ article: 'auto', url: 'pass-canonical-url-here' });

        }
    });
