                          i.contentWindow.document.body.style.padding = '0';
                          i.contentWindow.document.body.style.margin = '0';

                      }
                  }
              }
              if (a.tagId == "sas_50322") {
                  var bid = pbjs.getHighestCpmBids("sas_50322")[0];
                  if (bid) {
                      var i = document.createElement("iframe");
                      i.style.width = 300;
                      i.style.height = 250;
                      i.style.overflow = "hidden";
                      i.setAttribute('frameBorder', 0);
                      i.setAttribute('scrolling', 'no');
                      document.getElementById("sas_50322").appendChild(i);
                      pbjs.renderAd(i.contentWindow.document, bid.adId);
                      i.contentWindow.document.body.style.padding = '0';
                      i.contentWindow.document.body.style.margin = '0';

                  }
              }

              if (a.tagId == "sas_50382") {
                  var bid = pbjs.getHighestCpmBids("sas_50382")[0];
                  if (bid) {
                      var i = document.createElement("iframe");
                      i.style.width = 300;
                      i.style.height = 250;
                      i.style.overflow = "hidden";
                      i.setAttribute('frameBorder', 0);
                      i.setAttribute('scrolling', 'no');
                      document.getElementById("sas_50382").appendChild(i);
                      pbjs.renderAd(i.contentWindow.document, bid.adId);
                      i.contentWindow.document.body.style.padding = '0';
                      i.contentWindow.document.body.style.margin = '0';

                  }
              }

              if (a.tagId == "sas_52019") {
                  var bid = pbjs.getHighestCpmBids("sas_52019")[0];
                  if (bid) {
                      var i = document.createElement("iframe");
                      i.style.width = 300;
                      i.style.height = 250;
                      i.style.overflow = "hidden";
                      i.setAttribute('frameBorder', 0);
                      i.setAttribute('scrolling', 'no');
                      document.getElementById("sas_52019").appendChild(i);
                      pbjs.renderAd(i.contentWindow.document, bid.adId);
                      i.contentWindow.document.body.style.padding = '0';
                      i.contentWindow.document.body.style.margin = '0';

                  }
              }

              if (a.tagId == "sas_52020") {
                  var bid = pbjs.getHighestCpmBids("sas_52020")[0];
