<div class="footer-ruler" style="width: 100%;bottom: 0;background-color: white;position: fixed;transition: height 1.5s;box-shadow: 0 0 7px #5a5a5a;z-index: 999;display:none;">
    <div class="footer-ruler--containter" style="width: 970px;margin: auto;transition: height 1.5s;">
		<button class="js-modal-close modal__close h-fl close--ruler" style="margin-top: 0;">
                <i class="btn--icn--b [ icn icn--brand ]" aria-hidden="true" data-icon="x"></i>
                <span class="icn__alt ">סגור</span>
        </button>
        <div class="expanded"  data-banner-id="hp-ruler" style="position: fixed;height: 0px;bottom: 0;transition: height 1.5s;"> </div>
        <div class="collapsed-ruler" data-banner-id="hp-ruler" style="position: fixed;height: 0px;bottom: 0;transition: height 1.5s;"></div>
    </div>
</div>
<script>
    (function() {
        var base;
		var rulerDiv = $(".footer-ruler");
        var expandedDiv = rulerDiv.find(".expanded");
        var collapsedDiv = rulerDiv.find(".collapsed-ruler");
  
  		expandedDiv.click(function() {
    		var linkEl = $(this).find('a');
    		var link = linkEl.attr('href');
    		if(link) {
      			GaTm.eventManager.fireBannerClickEvent({
        			id: "hp-ruler",
                    name: "Understand the middle east - 1$",
                    creative: "Understand the middle east - 1$",
                    position: "homepage footer"
      			}, link);
    		}
  		});
  
  		collapsedDiv.click(function() {
    		var linkEl = $(this).find('a');
    		var link = linkEl.attr('href');
    		if(link) {
      			GaTm.eventManager.fireBannerClickEvent({
        			id: "hp-ruler",
                    name: "hp-ruler name",
                    creative: "hp-ruler creative",
                    position: "hp-ruler position"
      			}, link);
    		}
  		});

        var Ruler = {            
			settings: {
                "timeout": "10000",
          		"maxImpression": "1",
				"interval": "3,7,10,12,15",
                "expandedImg": "https://www.themarker.com/st/media/Media18/HDC/ruler/15/970x150.jpg",
                "collapsedImg": "https://www.themarker.com/st/media/Media18/HDC/ruler/15/970x60.jpg",
				"link": "http://pubads.g.doubleclick.net/gampad/clk?id=5243447948&iu=/9401/Haaretz.com.ClickTracker"
            },
      	    init: function() {
                base = this;
                if (UserUtil.getLoginInfo().userType != 'paying') {
                    $(".close--ruler").click(function() {
                      Ruler.collapsed();
                    });
                    $(".close--ruler").hover(function() {
                      $(".close--ruler").css('transform', 'rotate(180deg)').css('transition', 'transform 1s');
                    });
                    rulerDiv.show();
                    Ruler.expanded();
                    setTimeout(Ruler.collapsed, parseInt(Ruler.settings.timeout));
				}else{
          			var event = new Event('noRulerDetected');
					window.dispatchEvent(event);
        		}
            },
            expanded: function() {
                (rulerDiv[0]).style.height = "150px";
                (expandedDiv[0]).style.height = "150px";
                expandedDiv.html('<a data-stat-action="3" data-stat-campaign-name="Understand the middle east" data-stat-campaign-details="Subscribe" data-stat-feature="bottom strip subscribe" data-stat-feature-type="Marketing" href="'+Ruler.settings.link.replace("pubads.g.doubleclick.net","rmkz.themarker.com") +'"><img alt = "promotional content" title = "לרכישת מינוי הארץ" src="' + Ruler.settings.expandedImg + '"/></a>');
            },
            collapsed: function() {
                (expandedDiv[0]).style.height = "0px";
                (collapsedDiv[0]).style.height = "60px";
                (rulerDiv[0]).style.height = "60px";
                $(".close--ruler").hide();
                collapsedDiv.html('<a data-stat-action="3" data-stat-campaign-name="Understand the middle east" data-stat-campaign-details="Subscribe" data-stat-feature="bottom strip subscribe" data-stat-feature-type="Marketing" href="'+Ruler.settings.link.replace("pubads.g.doubleclick.net","rmkz.themarker.com") +'"><img alt = "promotional content" title = "לרכישת מינוי הארץ" src="' + Ruler.settings.collapsedImg + '"/></a>');
            },
			shouldbeShownToday: function() {
				var today = (new Date());
				today.setHours(0);
                today.setMinutes(0);
                today.setSeconds(0);
				if (!localStorage.dayInterval){
					var dateTimeInterval = [];
					var dayInterval = Ruler.settings.interval.split(",");	
					$.each(dayInterval, function( index, value ) {
						var nextIntervalDay = new Date();
						nextIntervalDay.setHours(0);
						nextIntervalDay.setMinutes(0);
						nextIntervalDay.setSeconds(0);
						var nextDay = (nextIntervalDay).setDate(today.getDate() + parseInt(value));
						dateTimeInterval.push(nextDay);
					});
					localStorage.dayInterval = JSON.stringify(dateTimeInterval); 
					return true;
				} else {
					var dateTimeInterval =  JSON.parse(localStorage.dayInterval)
					if (dateTimeInterval.length > 0){
						var nextDateTimeInterval = dateTimeInterval[0];
						if (nextDateTimeInterval <= today){
							dateTimeInterval.shift();
							localStorage.dayInterval = JSON.stringify(dateTimeInterval);
							return true;
						}
					} else{
						var nextIntervalDay = new Date();
						nextIntervalDay.setHours(0);
						nextIntervalDay.setMinutes(0);
						nextIntervalDay.setSeconds(0);
						dateTimeInterval.push((nextIntervalDay).setDate(today.getDate() + 1));
						localStorage.dayInterval = JSON.stringify(dateTimeInterval);
						return false;
					}
					return false;
				}
			}
		}
		window.addEventListener('noPopupDetected', Ruler.init);
    })();
</script>


					      
                	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		
                
