var screenw = $(window).width();
	try {
        var topDiv = document.getElementById("topdivad");
	var mobadDiv = document.getElementById("mobiletopad");
	var BottomDiv = document.getElementById("bottomsmallad");
	if (screenw > 767) {
	topDiv.innerHTML = document.getElementById("topad").innerHTML;
	}
