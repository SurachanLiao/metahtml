	$(window).bind('load', function() {
		container31210670 = $('#mod_31210670 .container_pint').masonry({
			itemSelector : '.celpint',
			columnwidth: function(containerWidth) { return containerWidth / 2; },
			isAnimated: true
		});
		container31210670.masonry( 'on', 'layoutComplete', function( laidOutItems ) { resize_height(); } );
		resize_height();
	});

