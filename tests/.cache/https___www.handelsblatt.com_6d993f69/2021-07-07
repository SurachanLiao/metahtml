        "use strict";
        var param = '';
        if(location.search.substr(1).length > 0){
            param = '&' + location.search.substr(1);
        }
        window.location.replace(this.url + ((this.url.indexOf('?') === -1) ? '?' : '&') + 'mwl=ok' + param);
    }
};
