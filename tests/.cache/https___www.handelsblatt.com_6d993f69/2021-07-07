            widgetId: moduleId,
            targetElementId: "cx_paywall",
          };
          let pianoRun = false;
          const callback = function () {
            pianoRun = true;
            console.log('PIANO CALLBACK', arguments);
            if (cnt) {
              cnt.style.visibility = "";
            }
            // window.commonAjaxify();
          };

          if (typeof cX.CCE.run === "function") {
            // run sync, and we do not need to check against adblocker - we have Piano!111
            console.log('run piano SYNC', widgetParams);
            cX.CCE.run(widgetParams, {}, callback);
          } else {
            console.log('run piano ASYNC', widgetParams);
            cX.CCE.callQueue.push(["run", widgetParams, {}, callback]);
            setTimeout(function () {
              console.log('PIANO TIMEOUT');
              if (!pianoRun) {
                if (cnt) {
                  cnt.style.visibility = "";
                }
              }
            }, 3000);
          }
        }
      }
    } catch (e) {
      console.error("PW Err", e);
    }
  }, { vendor: '5eec8924b8e05c699567f398' })
})();
