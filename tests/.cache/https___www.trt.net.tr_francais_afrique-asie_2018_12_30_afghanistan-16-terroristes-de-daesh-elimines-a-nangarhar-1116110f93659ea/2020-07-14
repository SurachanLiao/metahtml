                $.ajax({
                    url: `/francais/next/${lastArticle[0]}`,
                    type: "post",

                    success: function(data) {
                        $('#next-news').append(data);
                        let hiddenId = $(data).filter('#hiddenId').html();
                        if (hiddenId === undefined ) {
                            return false;
                        }
                        let hiddenPath = $(data).filter('#hiddenPath').html();
                        let hiddenTitle = $(data).filter('#hiddenTitle').html();
                        let hiddenImg = $(data).filter('#hiddenImg').html();
                        let anchor = '#anchoredId' + hiddenId;
                        $currentViewedNews = anchor;
                        $currentViewedNewsId = hiddenId;

                        addCurrentArticleToArray(hiddenId, hiddenPath, hiddenTitle, hiddenImg);
                        processing = false;
                    }
                })
            }
        });
    });
    function addCurrentArticleToArray(id, path, title, hiddenImg) {
        $scroll_array[id] = {
            url: window.location.origin + path,
            title: title,
            image: hiddenImg
        };
    }
    function gAnalytics(url) {
        ga("set", "page", url);
        ga("send", "pageview");
    }
</script><script>
    $(document).ready(function(){
  $("#myVideo").on("loadstart", function () { 
    $('#myVideo').on('ended',function(){
     $.featherlight.close();
    });
  }); 
});
</script></body></html>