				<script type="text/javascript">
				
					function endsWith(str, suffix) {
					    return str.indexOf(suffix, str.length - suffix.length) !== -1;
					}
				
					function handleSearchReturn(oEvent){
						 if(oEvent.keyCode == 13){
							 searchKeySubmit();
						 }
					}
				
					function searchKeySubmit(){
						
						var search = jQuery("#search").val();
						
						if(search.length > 0){
							
							if(endsWith(search, '...')){
								jQuery("#search").val(search.substr(0, search.indexOf('...')));
							}
						}
				
						jQuery('#companySearchLink').click();
					}
					
					function getSearchUrl(){
						var retValue = "";
						var searchKey  = jQuery("#search").val();
						if(searchKey != ""){
							jQuery.ajaxSetup({ cache:false });
							jQuery.ajaxSetup({ async:false });
							jQuery.get("/steelnetworks/company/companyContactSearch.do?method=search", 
								{ searchKey: searchKey },
								function(data){
									data = jQuery.trim(data);
									retValue = data;
								}, "text"
							);
						}
						return retValue;
					}
					
					function closeCoronaBar(close) {
						jQuery.ajaxSetup({ cache:false });
						jQuery.ajaxSetup({ async:false });
						jQuery.get("/welcome.do?method=closeCoronaBar", 
					  			  { },
					  			 function(data){}, 
					  			 "json"
					  	);
					}
				</script>
				
				
				<form method="post" id="searchform" name="searchForm">
					<div class='input-group input-prepend dropdown2' data-select='true'>
						<input id="search" type="text" class="form-control input-lg" maxlength="100" onkeydown="handleSearchReturn(event);" data-provide="typeahead" autocomplete="off" placeholder="Prices, articles, companies">
						<div class="input-group-btn">
							<button class="btn btn-search btn-lg" id="companySearchLink"><i class="fas fa-search" aria-hidden="true"></i></button>
						</div>
					</div>
					<div class="clearfix"></div>
				</form>
