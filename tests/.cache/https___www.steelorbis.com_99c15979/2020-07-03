				<script src="/so/js/typeahead.bundle.js"></script>
				<style>
				.tt-dropdown-menu {padding-bottom:10px; align:left; width:100% !important; min-width:290px; max-width:500px; text-align:left;}
				.tt-suggestion.tt-cursor {color: #fff; background-color: #e9e9e9;}
