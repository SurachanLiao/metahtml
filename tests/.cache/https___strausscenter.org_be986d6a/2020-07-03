  $(document).ready(function(){
    // Change new tab links
    $('.mega-menu-new-tab a').attr('target', '_blank');
    
    // Check to run mobile menu
    if ($( window ).width() <= 992) {
      runMobileMenu();
    }
    $(window).on('resize', function() {
        if ($(window).width() <= 992) {
          runMobileMenu();
        }
     });
  });
  function runMobileMenu() {
       	$(document).off("click touchend");
          $(document).on('click', '#mega-menu-header .mega-menu-link, .mega-sub-mob-cnt .mega-menu-link', function(e) {
          	if ($(this).parent().hasClass('mega-menu-item-has-children')) {
              $('#mega-menu-header .mega-sub-mobile.deadmenu').remove();
            $('#mega-menu-header').append('<div class="mega-sub-mobile"><div class="mega-sub-mobile-head">'+$(this)[0].outerHTML+'</div><div class="mega-sub-mobile-back">Back <i class="fa fa-angle-right"></i></div><div class="mega-sub-mob-cnt">'+$(this).parent().children('.mega-sub-menu')[0].outerHTML+'</div></div>')
            $('#mega-menu-header .mega-sub-mobile:last-child').animate({right: '0px'}, 'fast');
          }
          
        });
    	$(document).on('click', '.mega-sub-mobile-back', function(e) {
          e.preventDefault();
           $('#mega-menu-header .mega-sub-mobile.deadmenu').remove();
          $('#mega-menu-wrap-header #mega-menu-header > li.mega-menu-item.mega-toggle-on').removeClass('mega-toggle-on');
          $(this).parent().animate({right: '-300px'}, 'fast', "linear",function() { $('#mega-menu-header .mega-sub-mobile:last-child').addClass('deadmenu'); });
          e.stopPropagation();
        });
  }
})(jQuery);</script>  <script id="tmpl-nf-layout" type="text/template"><span id="nf-form-title-{{{ data.id }}}" class="nf-form-title">
