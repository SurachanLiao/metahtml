        };

        $(window).load(function() {
          loadAdditionalFunctionality();
        });


      // Load additionnal css
      $(window).load(function() {
        loadCSS("//public.slidesharecdn.com/ss_foundation/stylesheets/slideview.css?6dfb870e5c39ebaa8689373239eeb0d5307d48e4");
