          <script type="text/javascript">
              function setCookie(b, h, c, f, e) {
                  var a;
                  if (c === 0) {
                      a = ""
                  } else {
                      var g = new Date();
                      g.setTime(g.getTime() + (c * 24 * 60 * 60 * 1000));
                      a = "expires=" + g.toGMTString() + "; "
                  }
                  var e = (typeof e === "undefined") ? "" : "; domain=" + e;
                  document.cookie = b + "=" + h + "; " + a + "path=" + f + e
              }
          </script>

