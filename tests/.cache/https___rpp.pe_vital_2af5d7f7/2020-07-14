      <div class="col-secondary">
       <div class="box-mod box-herramientas">
  <div class="box-inner">
    <div class="mdl-medir">
      <div class="title-herramientas">
        <h3>HERRAMIENTAS</h3>
      </div>
      <div class="title-mdl">¿Tu peso es saludable?</div>
      <p class="sub-rf">Calcula tu IMC (Indice de Masa Corporal)</p>
      <form  class="x-validate form-con-peso" id="form-consult_wg2" data-show-message="true">
        <div class="row">
          <div class="mdl-left">
            <div class="row-mdl x-row">
              <label for="w-altura">Ingresa tu altura</label>
              <input type="text" class="imc_altura" placeholder="en metros, ej: 1.65" maxlength="4" data-restrict="decimal" data-require="true" data-message="Ingrese su altura">
            </div>
            <div class="row-mdl x-row">
              <label for="w-peso">Ingresa tu peso</label>
              <input type="text" class="imc_peso" placeholder="en kilogramos,  ej: 60" maxlength="3" data-restrict="integer" data-require="true" data-message="Ingrese su peso">
            </div>
          </div>
          <div class="mdl-right"><span class="icon-mdl"><i class="icon-balanza"></i></span></div>
        </div>
        <input type="submit" value="MEDIR" class="send">
      </form>
    </div>
  </div>
</div>
<script type="text/javascript">

(function () {

  if(window.widgetPesoLoaded === true) return false;

 function init(){
   var $formValidate = $(".form-con-peso");

   $formValidate.each(function(){
     var $form = $(this), $mdl = $form.closest('.mdl-medir');

     function renderTotal(obj){

      var $box = $(['<div class="box-w-peso"><div class="w-row"><span class="imc-text">Su IMC es: </span><div class="main-num-t"><span class="number-imc">'+ obj.imc +'</span><a href="#" class="btn-recalcula">Recalcular</a></div></div>',
                  '<div class="w-row"> Tu clasificación de IMC(kg/m2) es:<br>',
                  '<b class="text-status">'+ obj.status +'</b></div>',
                  '<a href="#" class="link-to">Ver clasificación</a>',
                  '</div>'].join(''));

     var $table = $(['<div class="box-w-table">',
                  '<div class="heading-table">Clasificación IMC (kg/m2) <a href="#" class="x-close"><i class="icon-cancel"></i></a></div>',
                  '<table width="100%" cellpadding="0" cellspacing="0" class="table-refers">',
                  '<tr data-range="p-bajo"><td>Bajo de Peso</td><td>Menor de 18,5</td></tr>',
                  '<tr data-range="p-normal"><td>Saludable</td><td>Entre 18,6 y 24,9</td></tr>',
                  '<tr data-range="p-preobeso"><td>Peso en exceso</td><td>Entre 25 y 29,9</td></tr>',
                  '<tr data-range="p-obesidad-1"><td>Obesidad Grado I</td><td>Entre 30,0 y 34,9</td></tr>',
                  '<tr data-range="p-obesidad-2"><td>Obesidad Grado II (severa)</td><td>Entre 35,0 y 39,9</td></tr>',
                  '<tr data-range="p-obesidad-3"><td>Obesidad Grado III (mórbido)</td><td>por arriba de 40</td></tr>',
                  '</table>',
                  '</div>'].join('')).hide();

      $box.find('.link-to').on('click', function(e){
        e.preventDefault();
        $table.show();
      });

      $table.find('.x-close').on('click', function(e){
        e.preventDefault();
        $table.hide();
      });

      $box.find('.btn-recalcula').on('click', function(e){
        e.preventDefault();
        $form.trigger('reset');
        $box.remove();
        $table.remove();
      });
      
      $table.find('[data-range='+ obj.class +']').addClass('active');
      $mdl.append($table);
      $mdl.append($box);

     }

     new xsl.fn.validate($form, function(){ 
      var result=null;
      var peso=$('.imc_peso', $form).val();
      var altura=$('.imc_altura', $form).val();

      peso = parseInt(peso.replace(/\,/g, '.'));
      altura = altura.replace(/\,/g, '.');

      var alt=altura*altura; 
      var IMC=peso/alt; 
      var imc1 =  (Math.ceil(IMC * 10)/10);
      
      if (imc1<=18.50) result = {imc: imc1, status: 'Bajo de Peso', class: 'p-bajo'}
      else if (imc1>18.50 && imc1<=24.99) result = {imc: imc1, status: 'Saludable', class: 'p-normal'}
      else if (imc1>24.90 && imc1<=29.99) result = {imc: imc1, status: 'Preobeso', class: 'p-preobeso'}
      else if (imc1>29.99 && imc1<=34.99 ) result = {imc: imc1, status: 'Obeso Grado I', class: 'p-obesidad-1'}
      else if (imc1>34.99 && imc1<=39.99 ) result = {imc: imc1, status: 'Obeso Grado II', class: 'p-obesidad-2'}
      else if (imc1>39.99) result = {imc: imc1, status: 'Obeso Grado III', class: 'p-obesidad-3'}
      renderTotal(result);
     });
   });
 }

 var interval = setInterval(function(){
       if(typeof $ === 'function'){
         clearInterval(interval);
         init();
       
       }
     },100);

 window.widgetPesoLoaded = true;

})();


</script>       </div>    
    </div>
     </div>
</div>


