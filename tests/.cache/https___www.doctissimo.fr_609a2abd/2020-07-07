        'pageInfo': {
            'contentType': "home",
            'section': aufSectionDetected,
            'subsection': "homepageprincipale",
            'rubrique': aufSectionDetected,
                        'ops_uaid': 'UA-1345247-7',
                        'tag': null,
            'ads': null,
            'content_object_id': "59",
            'content_node_id': "61",
        },
        'userInfo': {
            'du': 'none',
            'logged': false,
            'device': device_ua,
            'referer': document.referrer,
            'userAgent': navigator.userAgent,
            'adblocker': 'N/A',
            'consentInternetTargeting': (af_consentInternetTargeting ? 1 : 0),
            'consentAdsTargeting': (af_consentAdsTargeting ? 1 : 0),
            'segment': window.afSession ? afSession.getSegment() : 0,
            'screenResolution': (i.innerWidth || e.clientWidth || b.clientWidth || 0) + 'x' + (i.innerHeight || e.clientHeight || b.clientHeight || 0)
        },
        'campaignInfo': {
            'utmSource': window.afSession ? afSession.getUtmSource() : "none",
            'utmMedium': window.afSession ? afSession.getUtmMedium() : "none",
