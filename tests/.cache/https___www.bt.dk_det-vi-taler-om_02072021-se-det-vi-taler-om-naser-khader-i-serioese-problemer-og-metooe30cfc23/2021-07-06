                        ConsentHandlerVideo.handle(function(consent) {
                            if (!consent) {
                                return;
                            }

                            let playerLoaded = false;
                            let prerollAdvertisementUrl = '';
                            let postrollAdvertisementUrl = '';
                            let failSafeTimeout = 5000;
                            let settingsEmbedded;
                            let skipAutoplay;
                            const scribbleFeed = document.getElementsByClassName('scribble-post-feed')[0];

                            let waitForBidsTimeout = setTimeout( () => {
                                prepareJWDataAndPlayer();
                            }, failSafeTimeout);

                            function aspectRatio(width, height) {
                              let ratio = width / height;
                              if (ratio < 1.7 && ratio > 1) {
                                return {
                                  width: '100%',
                                  stretching: 'none'
                                }
                              }
                              else if (ratio === 1) {
                                return {
                                  aspectratio: '1:1',
                                }
                              }
                              else if (ratio < 1) {
                                if (window.md.phone()) {
                                  return {
                                    stretching: 'uniform',
                                    aspectratio: width + ':' + height
                                  }
                                } else {
                                    return {
                                      stretching: 'uniform'
                                    }
                                }
                              }
                              else {
                                return {
                                  aspectratio: width + ':' + height
                                }
                              }
                            }

                            window.invokeVideoPlayer = function(prerollTag, postrollTag) {
                                if (playerLoaded !== undefined && playerLoaded || prerollTag === false && postrollTag === false) {
                                    return;
                                }
                                if (waitForBidsTimeout !== undefined) {
                                    clearTimeout(waitForBidsTimeout);
                                }
                                if (prerollAdvertisementUrl !== undefined) {
                                    prerollAdvertisementUrl = prerollTag;
                                }
                                if (postrollAdvertisementUrl !== undefined) {
                                    postrollAdvertisementUrl = postrollTag;
                                }
                                prepareJWDataAndPlayer();
                            };

                            if (!playerLoaded && (typeof prerollTag !== 'undefined' && prerollTag || typeof postrollTag !== 'undefined' && postrollTag)) {
                                invokeVideoPlayer(prerollTag, postrollTag);
                            }

                            function prepareJWDataAndPlayer () {
                                playerLoaded = true;
                                                                    settingsEmbedded = false;
                                
                                                                    skipAutoplay = (window.md.mobile()) ? 1 : 0;
                                
                                const jwData = {
                                    id: 'mediaplayer-1514079-1231244712',
                                    embedded: settingsEmbedded,
                                    audio: false,
                                                                        options: {
                                        nodeId: "1514079",
                                        title: "Det Vi Taler Om 2021-07-02 at 11:47",
                                        mediaid: "rugGE8Np",
                                        autostart: !skipAutoplay,
                                        mute: !skipAutoplay,
                                        autoplayadsmuted: !skipAutoplay,
                                        vpaidcontrols: true,
                                                                                    recommended: "fgxIZN39",
                                                                                events: {
                                            visualQuality: function(e) {
                                                if (e.level && e.level.width) {
                                                    this.setConfig(
                                                        aspectRatio(e.level.width, e.level.height)
                                                    );
                                                }
                                            }
                                        },
                                        captions: {
                                            fontFamily: "SequelSansBlack",
                                        },
                                                            image: "https://bt.bmcdn.dk/media/cache/resolve/image_940x528_jwplayer_thumbnail/image/168/1685689/23794073-kjb.jpg",
    
    controls: true,
            "fwassetid": "DUMMY_ASSET",
        "advertising": {
            "client": "freewheel",
            "freewheel": {
                "networkid": 386345,
                "serverid": "https://5e529.v.fwmrm.net/ad/g/1",
                "profileid": "386345:DiscoverySBS_html5_live_secure",
                "sectionid": "DK_BT_ENTERTAINMENT",
                "adManagerURL": "https://mssl.fwmrm.net/libs/adm/6.39.0/AdManager.js"
            },
            "adscheduleid": "Az87bY12",
            "schedule": [
                {
                    "offset": "pre",
                    "tag": "fw-preroll"
                },
                {
                    "offset": "post",
                    "tag": "fw-postroll"
                }
            ],
            "skipoffset": 10000,
            "vpaidcontrols": true,
        },
        playlist: "//content.jwplatform.com/v2/media/rugGE8Np"

                                    }
                                };

                                if (window.md.phone()) {
                                    if (!!jwData.options.advertising) {
                                        jwData.options.advertising.client = "googima";
                                        jwData.options.advertising.schedule = {
                                            "preroll": {
                                                "tag": prerollAdvertisementUrl,
                                                "offset": "pre"
                                            }
                                        };

                                        if (postrollAdvertisementUrl) {
                                            jwData.options.advertising.schedule.postroll = {
                                                "tag": postrollAdvertisementUrl,
                                                "offset": "post"
                                            }
                                        }

                                        if (!!jwData.options.advertising.freewheel) {
                                            delete jwData.options.advertising.freewheel;
                                        }

                                        if (!!jwData.options.advertising.adscheduleid) {
                                            delete jwData.options.advertising.adscheduleid;
                                        }
                                    }

                                    if (!!jwData.options.fwassetid) {
                                        delete jwData.options.fwassetid;
                                    }
                                }

                                const initPlayer = function () {
                                    if ('function' !== typeof jwplayer) {
                                        setTimeout(initPlayer, 100);
                                        return;
                                    }
                                    Video.initializeJWVideo(jwData, window.md.phone() ? true : false);
                                };

                                initPlayer();
                            };
                        });

