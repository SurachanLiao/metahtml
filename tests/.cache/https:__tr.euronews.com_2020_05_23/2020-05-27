42}],&quot;technicalTags&quot;:[],&quot;allViews&quot;:0,&quot;allViewsMeta&quot;:{&quot;pointOfView&quot;:{&quot;quotation&quot;:null,&quot;description&quot;:null,&quot;author&quot;:null},&quot;survey&quot;:[],&quot;tweetId&quot;:null,&quot;tweet2NdId&quot;:null,&quot;contentType&quot;:null,&quot;displayOverlay&quot;:0},&quot;displayType&quot;:&quot;default&quot;,&quot;video&quot;:0,&quot;videos&quot;:[],&quot;liveStream&quot;:[{&quot;startDate&quot;:0,&quot;endDate&quot;:0}],&quot;isLiveCoverage&quot;:0,&quot;scribbleLiveId&quot;:0,&quot;scribbleLiveRibbon&quot;:0,&quot;sourceId&quot;:1,&quot;sources&quot;:[],&quot;freeField2&quot;:null,&quot;type&quot;:&quot;&quot;,&quot;program&quot;:{&quot;id&quot;:&quot;world&quot;,&quot;urlSafeValue&quot;:&quot;dunya&quot;,&quot;title&quot;:&quot;d\u00fcnya&quot;,&quot;online&quot;:1,&quot;url&quot;:&quot;\/programlar\/dunya&quot;},&quot;vertical&quot;:&quot;news&quot;,&quot;verticals&quot;:[{&quot;id&quot;:1,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;haber&quot;,&quot;title&quot;:&quot;Haber&quot;}],&quot;primaryVertical&quot;:{&quot;id&quot;:1,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;haber&quot;,&quot;title&quot;:&quot;Haber&quot;},&quot;themes&quot;:[{&quot;id&quot;:1,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;,&quot;translation&quot;:&quot;D\u00fcnya&quot;,&quot;url&quot;:&quot;\/\/tr.euronews.com\/haber\/uluslararasi&quot;}],&quot;primaryTheme&quot;:{&quot;id&quot;:1,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;},&quot;advertising&quot;:0,&quot;advertisingData&quot;:{&quot;startDate&quot;:0,&quot;endDate&quot;:0,&quot;type&quot;:null,&quot;isDfp&quot;:null,&quot;slug&quot;:null,&quot;title&quot;:null,&quot;sponsor&quot;:null,&quot;sponsorName&quot;:null,&quot;sponsorUrl&quot;:null,&quot;sponsorLogo&quot;:&quot;&quot;,&quot;sponsorLogoReverse&quot;:&quot;&quot;},&quot;geoLocation&quot;:{&quot;lat&quot;:0,&quot;lon