<script>
$(document).ready(function(){

  $(".sui-DropdownBasic").hover(function(){
      if($( window ).width() >= 800){
        $(this).toggleClass("is-expanded");  //Toggle the active class to the area is hovered
      }
  });
    $(".sui-DropdownBasic").click(function(){
        if($( window ).width() < 800){
            $(this).toggleClass("is-expanded");  //Toggle the active class to the area is hovered
        }
    });
  $(".sui-TopbarUser-toggle, .sui-TopbarUser-navWrap.is-expanded").click(function(){
    $("body").toggleClass("body-has-scroll-disabled");
    $("html").toggleClass("body-has-scroll-disabled");
    $(".sui-TopbarUser-navWrap").toggleClass("is-expanded");
  });

});

</script>
