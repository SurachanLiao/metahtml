<script>
  !function() {
    function loadScript(url) {
      return new Promise(function(resolve, reject) {
        var element = document.createElement('SCRIPT');

        Object.assign(element, {
          onload: function() {
            return resolve(url);
          },
          onerror: function() {
            return reject(url);
          },
          async: true,
          charset: 'UTF-8',
          src: url
        });

        window.onload = function() {
          window.document.body.appendChild(element);
        };
      });
    }

    var ANALYTICS_VERSION = '2.3.0';

    loadScript('https://unpkg.com/@s-ui/segment-wrapper@' + ANALYTICS_VERSION + '/umd/index.js')
        .then(function() {
          var analytics = window.sui.analytics;

          analytics && analytics.track('Page Viewed', {
            vertical: 'motor',
            site: 'debates.coches.net',
            platform: 'web',
            channel: 'forum',
            page_type: 'list',
            page_category_level1: 'all',
            page_name: 'pagina del foro'
          });
        })
        .catch(function(error) {
            console.error(error);
        });
  }();
