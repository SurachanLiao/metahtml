    <script>
    function downloadURI(uri, name) {
        var link = document.createElement("a");
        link.download = name;
        link.href = uri;
        link.click();
    }
</script>
    <script>
        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {
                $('nav').addClass("fixed").fadeIn();
                $('#logomin').empty();
                $('#logomin').prepend('<img src="../img/logomin.png"  alt="#">');
            }
            else {
                $('#logomin').empty();
                $('nav').removeClass("fixed");
            }
        });
    </script>
