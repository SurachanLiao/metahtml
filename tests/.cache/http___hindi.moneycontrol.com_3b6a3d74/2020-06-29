				<script type="text/javascript" >
				 $(document).ready(function(){
					 $('#a1').css('height','110');
					lastBlock = $("#a1");
					 maxWidth = 350;
					 minWidth = 96;
					
					 $(".ancherbg ul li a").hover(
					   function(){
						 $(lastBlock).animate({width: minWidth+"px",height:96}, {queue:false, duration:400});
						$(this).animate({width: maxWidth+"px",height:110}, { queue:false, duration:400});
						lastBlock = this;
					   }
					 );
				 });
				 function show_anchors(id,total_slides,direction){
					for(var k=1;k<=total_slides;k++){
							if(direction=="next"){
								if(id<total_slides){z = id+1;}
							}else if(direction=="previous"){
								if(id>1){z = id-1;}
							}
						if(z==k){
							$('#ancher'+z).show();
						}else{
							$('#ancher'+k).hide();
						}
					}
					$('.ancherbg ul li  a').animate({width: minWidth+"px",height:96}, {queue:false, duration:400});
					$('#a'+z).animate({width: maxWidth+"px",height:110}, { queue:false, duration:400});
					lastBlock = $("#a"+z);
				 }


				function validate_mp_poll_1(){
						
						var radio_choice = false;

						for (counter = 0; counter < document.form_mp_poll_1.radio_mp_poll.length; counter++){
						
						if (document.form_mp_poll_1.radio_mp_poll[counter].checked){
								radio_choice = true; 
								var check_var=counter+1;
							}

						
						}

						if (!radio_choice){alert("Please select a option.")
						return (false);
						}
						
						
						if(check_var==1)
							pass_val="opt1";
						if(check_var==2)
							pass_val="opt2";
						if(check_var==3)
							pass_val="opt3";

						pollid=document.getElementById('pollid').value;
						window.open('http://hindi.moneycontrol.com/india/messageboard/marketpulse/poll_inter.php?op=poll&poll_id='+pollid+'&radio_mp_poll='+pass_val,'Polldetails'); 
						return false;
						
					}
				 </script>
