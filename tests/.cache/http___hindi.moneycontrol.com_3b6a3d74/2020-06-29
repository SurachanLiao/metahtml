				<!-- DFP Initialization -> Start -->
				<script type='text/javascript'>
					var is_dfp_ads_visible = true; /* Use to check for DFP Ads visibility */

					/* Read DFP cookie function -> Start */
						function get_dfp_cookie( cname ) {
							var name = cname + '=';
							var ca = document.cookie.split( ';' );
							for( var i = 0; i < ca.length; i++ ) {
								var c = ca[i];
								while( c.charAt(0) == ' ' ) {
									c = c.substring(1);
								}
								if( c.indexOf( name ) == 0 ) {
									return c.substring( name.length, c.length );
								}
							}
							return '';
						}
					/* End <- Read DFP cookie function */

					/* Initialization DFP  -> Start */
						var googletag = googletag || {};
						googletag.cmd = googletag.cmd || [];
					/* End <- Initialization DFP */

					/* Normal DFP -> Start */
						function normal_dfp() {
							(function() {
								var gads = document.createElement('script');
								gads.async = true;
								gads.type = 'text/javascript';
								var useSSL = 'https:' == document.location.protocol;
								gads.src = (useSSL ? 'https:' : 'http:') + 
								'//securepubads.g.doubleclick.net/tag/js/gpt.js';
								var node = document.getElementsByTagName('script')[0];
								node.parentNode.insertBefore(gads, node);
							})();
						}
					/* End <- Normal DFP */

					/* send Ad server Request function -> Start */
						function sendAdserverRequest() {
							try {
								if (rtbpbjs && rtbpbjs.adserverRequestSent) return;
									googletag.cmd.push(function() {
										googletag.pubads().refresh();
									});
							} catch (e) {
								googletag.cmd.push(function() {
									googletag.pubads().refresh();
								});
							}
						}
					/* End <- send Ad server Request function */

					/* Launch dfp ads function -> Start */
						function launch_dfp_ads() {
							/*  Normal DFP -> Start */
								normal_dfp();
							/* End <- Normal DFP */

							/* Initialized DFP Define Slots -> Start */
								define_dfp_slot();
							/* End <- Initialized DFP Define Slots */
						}
					/* End <- Launch dfp ads function */

					function define_dfp_slot() {
						googletag.cmd.push(function() { 
							googletag.pubads().disableInitialLoad();
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_300x250', [300,250], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_300x250').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_300x600', [300,600], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_300x600').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_728x90', [[728,90],[768,90],[468,60]], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_728x90').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_180x300', [180,300], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_180x300').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_970x250', [[728,90],[970,90],[970,250]], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_970x250').addService(googletag.pubads()); 
											googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_HP_Native1', [430,60], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_HP_Native1').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_EARPANEL_LEFT_210x70', [210,70], 'Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_EARPANEL_LEFT_210x70').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_EARPANEL_RIGHT_210x70', [210,70], 'Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_EARPANEL_RIGHT_210x70').addService(googletag.pubads()); 
							googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_Gutter', [1, 1], 'Hindi_Moneycontrol/Hindi_Moneycontrol_HP/Hindi_Moneycontrol_HP_Gutter').addService(googletag.pubads());
							googletag.pubads().enableSingleRequest();
							googletag.pubads().collapseEmptyDivs(true);
							googletag.enableServices();
						});

						setTimeout(function() {
							sendAdserverRequest();
						}, 5000);
					}

					var user_token_normal = get_dfp_cookie( 'token-normal' );
					if( user_token_normal != '' && get_dfp_cookie( 'mcpro' ) == '1' ) {
						var ajax_link = 'https://www.moneycontrol.com/monitoring/mc_user_entitlements.php?user_token=' + user_token_normal;
						var opts = {
							method: 'GET',
							headers: {}
						};
						fetch( ajax_link, opts ).then( function ( response ) {
							return response.json();
						})
						.then( function ( json_obj ) {
							if( !json_obj.hasOwnProperty( 'entitlements' ) ) {
								/* console.log( 'Normal User' ); */
								launch_dfp_ads();
							} else {
								/* console.log( 'MC PRO User' ); */
								is_dfp_ads_visible = false;
								document.getElementById( 'is_dfp_ads_visible' ).value = is_dfp_ads_visible;
							}

						});
					} else {
						/* console.log( 'Free User' ); */
						launch_dfp_ads();
					}
