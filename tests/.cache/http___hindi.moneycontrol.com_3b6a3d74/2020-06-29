<script>
function mcloginsignin(signinform){
	if(signinform == 'register'){
		$("#ifval").val("285px");
    	$("#myframe").attr('src','https://accounts.moneycontrol.com/mclogin/?formname=register');
	}else{
		$("#ifval").val("495px");
        $("#myframe").attr('src','https://accounts.moneycontrol.com/mclogin/?d=2');
	}
	$("#LoginModal").modal();
}
</script>
</body>
</html>		

	<script>
	hot_stk('1');
	</script>
