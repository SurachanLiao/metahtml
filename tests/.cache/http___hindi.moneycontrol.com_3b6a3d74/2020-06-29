				<script type="text/javascript" >
				 $(document).ready(function(){
					 $('#a1').css('height','110');
					lastBlock = $("#a1");
					 maxWidth = 350;
					 minWidth = 96;
					
					 $(".ancherbg ul li a").hover(
					   function(){
						 $(lastBlock).animate({width: minWidth+"px",height:96}, {queue:false, duration:400});
						$(this).animate({width: maxWidth+"px",height:110}, { queue:false, duration:400});
						lastBlock = this;
					   }
					 );
				 });
				 function show_anchors(id,total_slides,direction){
					for(var k=1;k<=total_slides;k++){
							if(direction=="next"){
								if(id<total_slides){z = id+1;}
							}else if(direction=="previous"){
								if(id>1){z = id-1;}
							}
						if(z==k){
							$('#ancher'+z).show();
						}else{
							$('#ancher'+k).hide();
						}
					}
					$('.ancherbg ul li  a').animate({width: minWidth+"px",height:96}, {queue:false, duration:400});
					$('#a'+z).animate({width: maxWidth+"px",height:110}, { queue:false, duration:400});
					lastBlock = $("#a"+z);
				 }
				 </script>
