<script>

var IE = document.all?true:false;

function topnewstab(passtab)
{
	
	var tabhtmlfiles = new Array("", "hmkt_top_news.html", "hmkt_latest_news.html", "mc_top_news.html");


	for (tabcnt=1; tabcnt<=3; tabcnt++)
		{
			if(passtab==tabcnt)
				document.getElementById("toptab_"+tabcnt).className='active';			
			else
			{
				
				document.getElementById("toptab_"+tabcnt).className='';			
			}
		}	

if(passtab != 1)
	{
	$('#top_view_story').fadeOut(1);
	}
else
	$('#top_view_story').fadeIn(1000);
			
			$('#topnews').fadeOut(1);
			$('#topnews').html("<div style='margin:50px 180px 50px 180px;'><img src='https://img-d02.moneycontrol.co.in/images/markets_new_loader.gif' ></div>");	
			
//			$('#topnews').load('/mccode/markets/mkt_elements/tabbedtopnews_'+passtab+'.html ', '', function(data) 
	
			$('#topnews').load('/mccode/markets/'+tabhtmlfiles[passtab], '', function(data) 
				{					
					//$('#topnews').fadeOut(1);
					$('#topnews').fadeIn(1000);
				});
}

function hot_stk(sttab)
{			
   var htmlfiles = new Array("", "h_mostactive.html", "h_topgainers.html", "h_toplosers.html", "h_52wkhigh.html", "h_52wklow.html");

  
	for (tabcnt=1; tabcnt<=5; tabcnt++)
		{
			if(sttab==tabcnt)
				document.getElementById("hotstock_"+tabcnt).className='current';			
			else
				document.getElementById("hotstock_"+tabcnt).className='';
		}	

			$('#hotstk_dat').fadeOut(1);
			$('#hotstk_dat').html("<div style='margin:50px 180px 50px 180px;'><img src='https://img-d02.moneycontrol.co.in/images/markets_new_loader.gif' ></div>");	
				      
			$('#hotstk_dat').load('/mccode/markets/hotstocks/new_2010/'+htmlfiles[sttab], '', function(data) 
				{	
				   
					//$('#topnews').fadeOut(1);
					$('#hotstk_dat').fadeIn(1000);
				});
}	

function bsensetool(bntool)
{
	if (bntool == '1')
	{
		if( $("#sensex_toggle_home_hindi").val() == "1" )
		{

		$("#bsegraphs").empty();
		$("#bsegraphs").attr("src","https://www.moneycontrol.com/mccode/common/indices_chart/indices_chart.php?classic=true&market=i&period=1d&ind_id=4&height=100&width=280");

		$("#sensex_toggle_home_hindi").attr("value","2");
		}

	}else
	{

	}	
	for (bncnt=1; bncnt<=2; bncnt++)
	{
		if(bntool==bncnt)
		{
			if(document.getElementById("bsense_"+bncnt).className=='plus')
			{
				document.getElementById("bsense_"+bncnt).className='minus';	
				document.getElementById("bsensediv_"+bncnt).className='table2';
				$("#bsensediv_"+bncnt).slideUp(1);
				$("#bsensediv_"+bncnt).slideDown(500);
			}
			else
			{
				document.getElementById("bsense_"+bncnt).className='plus';
				$("#bsensediv_"+bncnt).slideUp(500);
				document.getElementById("bsensediv_"+bncnt).className='table2 dnone';
			}
		}
		else
		{
			document.getElementById("bsense_"+bncnt).className='plus';
			$("#bsensediv_"+bncnt).slideUp(500);
			document.getElementById("bsensediv_"+bncnt).className='table2 dnone';
		}
	}
}


function mktroundtool_2(mkttool)
{
	
	for (mktcnt=1; mktcnt<=4; mktcnt++)
	{
		if(mkttool==mktcnt)
		{
			if(document.getElementById("mktround_"+mktcnt).className=='')
			{
				document.getElementById("mktround_"+mktcnt).className='active';	
				if(mkttool==3)
				{
				document.getElementById("mktrounddiv_"+mktcnt).className='';
				}
				else
				{
				document.getElementById("mktrounddiv_"+mktcnt).className='table2';
				}
				$("#mktrounddiv_"+mktcnt).slideUp(1);
				$("#mktrounddiv_"+mktcnt).slideDown(500);
			}
			else
			{
				document.getElementById("mktround_"+mktcnt).className='';
				$("#mktrounddiv_"+mktcnt).slideUp(500);
				if(mkttool==3)
				{
				document.getElementById("mktrounddiv_"+mktcnt).className='dnone';
				}
				else
				{
				 document.getElementById("mktrounddiv_"+mktcnt).className='table2 dnone';
				}
			}
		}
		else
		{
			document.getElementById("mktround_"+mktcnt).className='';
			$("#mktrounddiv_"+mktcnt).slideUp(500);
			    if(mkttool==3)
				{
				document.getElementById("mktrounddiv_"+mktcnt).className='dnone';
				}
				else
				{
				 document.getElementById("mktrounddiv_"+mktcnt).className='table2 dnone';
				}
		}
	}

   if(mkttool==4)
	{
		document.getElementById('yworld').style.display="block";
		//document.getElementById('nworld').style.display="none";
	}
	else
	{
		document.getElementById('yworld').style.display="none";
		//document.getElementById('nworld').style.display="block";
	}

}


function open_tools(tool_div)
{
	for(i=1; i < 5; i++)
	{
		document.getElementById("tool_"+i).style.display = "none";
		document.getElementById("tool_"+i+"_img").innerHTML = "<a href='javascript:void(0);' onclick=\"javascript:open_tools('"+i+"');\"><img src='https://img-d02.moneycontrol.co.in/images/mf_revamp/plus.gif' border='0' /></a>";
	}
	document.getElementById("tool_"+tool_div).style.display = "block";
	document.getElementById("tool_"+tool_div+"_img").innerHTML = "<a href='javascript:void(0);' onclick=\"javascript:close_tools('"+tool_div+"');\"><img src='https://img-d02.moneycontrol.co.in/images/mf_revamp/minus.gif' border='0' /></a>";
}

function close_tools(tool_div)
{
	document.getElementById("tool_"+tool_div).style.display = "none";
	document.getElementById("tool_"+tool_div+"_img").innerHTML = "<a href='javascript:void(0);' onclick=\"javascript:open_tools('"+tool_div+"');\"><img src='https://img-d02.moneycontrol.co.in/images/mf_revamp/plus.gif' border='0' /></a>";
}

function post_mfmsearch()
{
	var input_val = document.mfmeter_frm.mfmeter_str.value;
	if(input_val == "Enter Scheme Name" || input_val == "" || input_val.length < 3)
	{
		alert("Please enter minimum 3 characters for search");
		document.mfmeter_frm.mfmeter_str.focus();
		return false;
	}
	else
	{
		document.mfmeter_frm.AMCname.value = input_val;
		document.mfmeter_frm.submit();
		return false;
	}
}

function livemarketmap(maptab)
{
	for (tabcnt=1; tabcnt<=2; tabcnt++)
		{
			if(maptab==tabcnt)
				document.getElementById("marketsmap_"+tabcnt).className='on';			
			else
				document.getElementById("marketsmap_"+tabcnt).className='';
		}	

		if(maptab==1)
		{
			marketmapdata="<iframe id='bsegraph' name='bsegraph' src='https://www.moneycontrol.com/mcplus/marketmap/bse/square_mapsmall.php' framespacing='0' frameborder='no' scrolling='no' width='264px' height='230px'></iframe>";
		}
		else
		{
			marketmapdata="<iframe id='bsegraph' name='bsegraph' src='http://www.moneycontrol.com/mcplus/marketmap/nse/square_mapsmall.php' framespacing='0' frameborder='no' scrolling='no' width='264px' height='230px'></iframe>";			
		}
		
		$('#bsense_map').fadeOut(1);

		$('#bsense_map').html(marketmapdata);

		$('#bsense_map').fadeIn(1000);
}

function post_hpstkscreener()
{
	var sel_sector = document.getElementById("sel_code").value;
	var sel_criteria = document.getElementById("screen_crit").value;
	if(sel_sector == "")
	{
		alert("Please select a sector to screen");
		document.getElementById("sel_code").focus();
		return false;
	}

	if(sel_criteria == "")
	{
		alert("Please select a criteria to screen companies on");
		document.getElementById("screen_crit").focus();
		return false;
	}

	if(sel_criteria == "pe" || sel_criteria == "cashpe")
	{
		sector = sel_sector;	// -1;
		if(sel_criteria == "pe")
			document.getElementById("mktinfo_scr_frm").action = "https://www.moneycontrol.com/stocks/marketinfo/pe/bse/index.php?indcode="+sector+"&sortcode=0";
		else
			document.getElementById("mktinfo_scr_frm").action = "https://www.moneycontrol.com/stocks/marketinfo/pe/bse/index.php?indcode="+sector+"&sortcode=2";

		document.getElementById("indcode").value = sector;
		document.getElementById("optex").value = "BSE";

	}
	else if(sel_criteria == "marketcap" || sel_criteria == "netsales" || sel_criteria == "netprofit" || sel_criteria == "totassets")
	{
		var sel_sector = document.getElementById("sel_code").options[document.getElementById("sel_code").options.selectedIndex].text;
		document.getElementById("mktinfo_scr_frm").action = "https://www.moneycontrol.com/stocks/marketinfo/"+sel_criteria+".php?indcode="+sel_sector+"&optex=BSE";
		document.getElementById("indcode").value = sel_sector;
		document.getElementById("optex").value = "BSE";

	}
	else if(sel_criteria == "qtrgrowth")
	{
		sector = sel_sector - 1;
		document.getElementById("mktinfo_scr_frm").action = "https://www.moneycontrol.com/stocks/marketinfo/"+sel_criteria+"/index.php?indcode="+sector+"&call_option=N";
		document.getElementById("indcode").value = sel_sector;
		document.getElementById("call_option").value = "N";
	}
	else
	{
		document.getElementById("mktinfo_scr_frm").action = "https://www.moneycontrol.com/stocks/marketinfo/"+sel_criteria+"/bse/index.php?indcode="+sel_sector;
		document.getElementById("indcode").value = sel_sector;
	}

	document.getElementById("mktinfo_scr_frm").submit();
	return false;
}

function post_indexchart()
 {
 	var ind_id = document.getElementById("sel_techindex").value;
 	if(ind_id == "")
 	{
 		alert("Please select an Index to view the chart!!!");
 		return false;
 	}
 	document.getElementById("index_id").value = ind_id;
 	document.index_tech_frm.submit();
 	return false;
 }
function openchart()
{
	var str="";
	len=document.techchart_frm.compname.value.length;
	if(len < 3)
	{
		alert("Please enter minimum 3 characters for search");
		return false;
	}
	if((document.techchart_frm.compname.value != "") && (document.techchart_frm.compname.value != "à¤à¤à¤ªà¤¨à¥ à¤¨à¤¾à¤®"))
		str="compname="+document.techchart_frm.compname.value;
	else
	{
		alert("Please enter Stock code or Company Name");
		document.techchart_frm.compname.focus();
		return false;
	}
	document.techchart_frm.action="/stocks/cptmarket/charting_tech.php?"+str;
	document.techchart_frm.submit();
	return true;
}

function validate_mp_poll()
{
	
	// set var radio_choice to false
	var radio_choice = false;

	// Loop from zero to the one minus the number of radio button selections
	for (counter = 0; counter < document.form_mp_poll.radio_mp_poll.length; counter++)
	{
	// If a radio button has been selected it will return true
	// (If not it will return false)
	if (document.form_mp_poll.radio_mp_poll[counter].checked)
		{
			radio_choice = true; 
			var check_var=counter+1;
		}

	
	}

	if (!radio_choice)
	{
	// If there were no selections made display an alert box 
	alert("Please select a option.")
	return (false);
	}
	
	
	if(check_var==1)
		pass_val="opt1";
	if(check_var==2)
		pass_val="opt2";
	if(check_var==3)
		pass_val="opt3";

	pollid=document.getElementById('pollid').value;

	


	window.open('http://hindi.moneycontrol.com/india/messageboard/marketpulse/poll_inter.php?op=poll&poll_id='+pollid+'&radio_mp_poll='+pass_val,'Polldetails'); 
	//window.open('poll_graph.php?op=poll&pollid='+pollid+'&radio_mp_poll='+pass_val,'Polldetails'); 
	
	//return true;
	return false;
	
}

function postquote(keysearch)
{
	len = keysearch.companyname.value.length;
	if(len < 3)
	{
		alert("Please enter minimum 3 characters for search");
		return false;
	}

	var quot = keysearch.companyname.value;
	if(quot=="Stock Quote" || quot=="Get Quote" || quot=="à¤à¤à¤ªà¤¨à¥ à¤à¤¾ à¤¨à¤¾à¤® à¤¦à¤°à¥à¤ à¤à¤°à¥à¤")
	{
		alert("Please Enter Keyword");
		keysearch.companyname.focus();
		return false;
	}

	if((quot.charAt(0)==" " && quot.charAt(1)==" ") || (quot.charAt(1)==" " && quot.charAt(2)==" "))
	{
		alert("Please Enter Keyword");
		keysearch.companyname.focus();
		return false;
	}
	
	if(quot=="")
	{
		alert("Please Enter Keyword");
		keysearch.companyname.focus();
		return false;
	}

	var chk=searchcomp(keysearch);
	if(chk=="false")
	{
		return false;
	}
	return false;
}

function searchcomp(keysearch)
{
	var result="";
	var result1="";
	var result2="";
	var result3="";
	var tempvar="";
	var tempvar1="";
	var preval="";
	var flag="";
	var flag1=""
	var tempvar="";
	var len,resultlen,i,fname,preval1,preval2;
	sel=0;

	companyname= new String(keysearch.companyname.value);
	len=companyname.length;
	for(i=0;i<=len-1;i++)
	{
		if(len>10)
		{
			tempvar=companyname;
			flag1="true";
			break;
		}
		else
		{
			j=i+1;
			result= companyname.charAt(i);
			preval=companyname.charAt(j);
			if(result==" " || preval==" " || result=="." || preval=="." || len<6)
			{
				tempvar="";
				for(k=0;k<=len-1;k++)
				{
					result1="";
					result1=companyname.charAt(k);
					//if(result1!=" " && result1!=".")
					if(result1 != ".")
					{	
						tempvar+=result1;
					}
				}
				flag="true";
				flag1="true";
				break;
			}
			else
			{
				for(l=0;l<=len-1;l++)
				{
					result3="";
					result3=companyname.charAt(l);
					if(result3!="." && l!=len)
					{	
						tempvar+=result3;
					}
				}
				flag1="true";
				break;
			}
			if(flag=="true")
			{
				flag1="true";
				break;
			}
		}
	}
	if(flag1=="true")
	{
		compname=new String(tempvar);
		rExp="&";
		newString = new String("*") ;
		if(compname.replace(rExp,newString))
		{
			companyname=compname.replace(rExp,newString);
		}
		else
		{
			companyname=tempvar;
		}
		
		if(companyname=="TV18" || companyname=="tv18")
		{
			companyname="TV 18";
		}
		keysearch.action="/stocks/cptmarket/compsearchnew.php";
		keysearch.submit();
		
		return true;
	}
	else
	{
		document.compsearch.companyname.focus();
	}
	return false;
}

function ass_pval(sval)
{
  document.getElementById("fname").value=sval;
}

function c_h(){
	document.getElementById('floatad1').style.width="180px";
	document.getElementById('flashcontent').className="ad_float_off";
}
function rc_h(){
	document.getElementById('floatad1').style.width="490px";
	document.getElementById('flashcontent').className="ad_float_on";
}

function post_indextech(ind_id)
{
	if(ind_id == "4")
		document.indextech_frm.action = "https://www.moneycontrol.com/sensex/bse/sensex-live/";
	else if(ind_id == "9")
		document.indextech_frm.action = "https://www.moneycontrol.com/nifty/nse/nifty-live";
	else
		document.indextech_frm.action= "/stocks/cptmarket/indices_tech.php";

	document.getElementById("index_id").value=ind_id;
	document.indextech_frm.submit();
	return false;
}


function iposnaptab(passtab)
{
	
	var tabhtmlfiles = new Array("", "ipo_sanp_tab_11.html", "ipo_sanp_tab_12.html", "ipo_sanp_tab_13.html", "ipo_sanp_tab_14.html");

	for (tabcnt=1; tabcnt<=4; tabcnt++)
		{
			if(passtab==tabcnt)
				document.getElementById("ipo_sanp_tab_"+tabcnt).className='active';			
			else
			{
				
				document.getElementById("ipo_sanp_tab_"+tabcnt).className='';			
			}
		}	

			
			$('#iposapshot_ajax').fadeOut(1);
			$('#iposapshot_ajax').html("<div style='margin:50px 180px 50px 180px;'><img src='https://img-d02.moneycontrol.co.in/images/markets_new_loader.gif' ></div>");	
	
			$('#iposapshot_ajax').load('/mccode/ipo/ipo_hp_element/'+tabhtmlfiles[passtab], '', function(data) 
				{					
					$('#iposapshot_ajax').fadeIn(1000);
				});
}

function ipo_pop(passtab)
{
	for (tabcnt=1; tabcnt<=3; tabcnt++)
		{
		   if(passtab==tabcnt)
			{
				document.getElementById("vpop"+tabcnt).className='active';	
				document.getElementById("ipo_watch_"+tabcnt).style.display='block';	
			}
			else
			{
				
				document.getElementById("vpop"+tabcnt).className='';
				document.getElementById("ipo_watch_"+tabcnt).style.display='none';	
			}
		}
}

function wealthArticle(num)
{
	for (tabcnt=1; tabcnt<=6; tabcnt++)
	{
		if(num==tabcnt){
		   document.getElementById("warticle_tab_"+tabcnt).className='active';			
		   document.getElementById("wartcles_div_"+tabcnt).style.display='block';			
		}else
		{
		   document.getElementById("warticle_tab_"+tabcnt).className='';	
		   document.getElementById("wartcles_div_"+tabcnt).style.display='none';	
		}
	}
}
function changerhstab(tabname) {
	if(tabname == 'budget') {
		document.getElementById('budget_head').className='act';
		document.getElementById('budget').style.display='block'; 
		document.getElementById('budget_expect_head').className='nact';
		document.getElementById('budget_expect').style.display='none'; 
	}
	if(tabname == 'budget_expect') {
		document.getElementById('budget_head').className='nact';
		document.getElementById('budget').style.display='none'; 
		document.getElementById('budget_expect_head').className='act';
		document.getElementById('budget_expect').style.display='block'; 
	}
}




