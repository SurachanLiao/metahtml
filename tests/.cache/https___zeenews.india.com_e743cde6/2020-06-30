(function ($) {		
	$(document).ready(function(){			
		
		function fillElementWithNativeAd($elN, slotCodeN, sizeN, targetingN){									
			if (typeof targetingN === 'undefined') { 
				targetingN = {}; 
			} else if ( Object.prototype.toString.call( targetingN ) !== '[object Object]' ) { 
				targetingN = {}; 
			}
			var elIdN = $elN.attr('id');	
			//console.log("elId:" + elId);			
			googletag.cmd.push(function(){ 										
				var slotN = googletag.defineSlot(slotCodeN, sizeN, elIdN); 
				for (var tN in targetingN){ 
					slotN.setTargeting(tN, targetingN[tN]); 
				}
				slotN.addService(googletag.pubads());
				googletag.display(elIdN);
				//googletag.pubads().refresh([slotN]);
				googletag.pubads().enableSingleRequest();
				googletag.pubads().collapseEmptyDivs();
				googletag.enableServices();
			});
		}
		
					//var dN = $('<div id="div-hp-native1"></div>');
			//$(".mini-lead-block > .row > .view-id-zn_hp_mc_lead_news_2018 > .view-content > .mini-pad-l > .mini-list-story").replaceWith(dN);
			//fillElementWithNativeAd(dN, '/11440465/Zeenews_English_Web/Zeenews_English_HP_Native1', [[300, 100], 'fluid'], {});
			
			var dN2 = $('<div id="div-hp-native2" class="video-story clearfix"></div>');
			$(".vd-story-block .view-id-zn_hp_mc_top_video_2017 >.view-content").prepend(dN2);
			$(".vd-story-block .view-id-zn_hp_mc_top_video_2017 >.view-content > .video-story:last").remove();
			
			fillElementWithNativeAd(dN2, '/11440465/Zeenews_English_Web/Zeenews_English_HP_Native2', ['fluid', [300, 100]], {});
			});
})(jQuery);
</script>

