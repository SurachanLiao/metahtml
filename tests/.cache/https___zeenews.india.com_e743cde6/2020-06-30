<script>
$(function() {
	$(document).ready(function(){ 
		var ccontainer = document.getElementById('co-container');	
		var btn = document.getElementById("myBtn");
		var myClsBtn = document.getElementById("myCloseBtn");
		gdpr_is_europe = 0;
		function checkCookieValues(){
		  console.log("GDPRFlag: " + localStorage.getItem('gdprFlag'));			
		  if(gdpr_is_europe == 1){	
			//console.log("GDPRFlag: " + localStorage.getItem('gdprFlag'));
			if (localStorage.getItem('gdprFlag') != '1'){
				ccontainer.style.display = "block";									
			}else{
				ccontainer.style.display = "none";
			}						
		  }else{
			  localStorage.setItem('gdprFlag', '1');
		  }
		}
		function setCookieValues(){	
			localStorage.setItem('gdprFlag', '1');
			checkCookieValues();						
		}
		$(btn).click(function(){	  
			setCookieValues();          
		});
		$(myClsBtn).click(function(){      
			setCookieValues();
			ccontainer.style.display = "none";
		});
		checkCookieValues();
	});
})
</script> 
