<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
<script type="text/javascript">
  function setntfCookie(e, t, n) {
      var i = new Date;
      i.setTime(i.getTime() + 24 * n * 60 * 60 * 1000);
      var o = "expires=" + i.toUTCString();
      document.cookie = e + "=" + t + "; " + o;
  }

  function getntfCookie(e) {
      for (var t = e + "=", n = document.cookie.split(";"), i = 0; i < n.length; i++) {
          for (var o = n[i]; " " == o.charAt(0); )
              o = o.substring(1);
          if (0 == o.indexOf(t))
              return o.substring(t.length, o.length);
      }
      return "";
  }

  $(document).ready(function () {

      var cmsId = $("#snippet").data("cmsid");

      if (getntfCookie("ntf-hide") != "hide") {

          $("#snippet .global-button").on("click", function () {
              dataLayer.push({'event': 'snippetClick', 'snippetCmsId': cmsId});
          });

          setTimeout(function () {
              $("#snippet").addClass("active");
              dataLayer.push({'event': 'snippetImpression', 'snippetCmsId': cmsId});
          }, 2000);

          $(".snippet-close").on("click", function () {
              $("#snippet").hide()
          });

          	setntfCookie("ntf-hide", "hide", 30);
      }

  });
</script>

			
