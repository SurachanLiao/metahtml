else{$(this).removeClass('disabled')}});$(window).on('resize',function(){if(window.innerWidth>992)$('body').removeClass('opened_burger_menu');$(this).next($('.sub-menu')).slideToggle()});$(document).ready(function(){$('#new-subnav-PM .dropdown:first .dropdown-menu').each(function(){var o=$(this).html();$('.dropdown-menu-placeholder').data('placeholder',o).html(o)});$('.dropdown').hover(function(){$(this).children().each(function(){$(this).toggleClass('show-dropdown');if(!$(this).hasClass('show-dropdown')){return};var o=$(this).html().trim();if(''!==o&&o.indexOf('<li')>-1){$('.dropdown-menu-placeholder').html(o).show()}})});var e=!1,o=!1;$('#new-subnav-PM .dropdown, #new-subnav-PM .dropdown *, .dropdown-menu-placeholder').mouseout(function(n){e=!0;if(o){clearTimeout(o)};o=setTimeout(function(){if(e){$('.dropdown-menu-placeholder').each(function(){$(this).html($(this).data('placeholder')).show()})}},100)}).mouseover(function(d){e=!1;if(o){clearTimeout(o)};var n=$('.dropdown-menu.show-dropdown').html();if('undefined'!==typeof n){n.trim();$('.dropdown-menu-placeholder').html(n).show()}});$('.hideDropDown').hover(function(){$(this).parent().toggleClass('HideFirst');$('.dropdown-menu-placeholder').hide().html('')})});</script><section id="main"><div class="container"><div class="row"><div class="col-md-12"><div class="sitefre" id="match2_Actu_fre"></div><ol class="breadcrumb"><li><a href="https://www.parismatch.com/"><span>Paris Match</span></a></li><li><a href="https://www.parismatch.com/Actu"><span>Actu</span></a></li></ol></div><div class="col-md-12"><h1><span>Match</span> Actualités</h1></div><div id="pm_actu_zone_actu" class=""><div id="pm_actu_zone_actu_alaune_alaune_pub_occ_1" class="alaune"><div class="" id="pm_actu_zone_actu_alaune_alaune_pub_occ_1_e_1"><div class="col-md-8"><a href="https://www.parismatch.com/Actu/International/Coronavirus-reconfinement-de-plus-de-140-millions-d-Indiens-1694168" id="pm_actu_zone_actu_alaune_alaune_pub_occ_1_e_1_1"><img class="im