<path d="M438.8 67.3c0-8 5.4-12.5 11.1-12.5A9.614 9.614 0 01458 59V46.3h5.5v32.9H458v-4c-1.8 2.5-4.2 4.5-8.1 4.5-5.7 0-11.1-4.5-11.1-12.4zm19.2 0c0-4.7-3.2-7.7-6.9-7.7s-6.9 2.9-6.9 7.6v.1c0 4.6 3.2 7.6 6.9 7.6 3.7.1 6.9-3 6.9-7.6z" data-name="Path 6987" />
<path d="M467.7 70.5V55.4h5.5v13.5c0 3.7 1.8 5.8 5.1 5.8 3.2 0 5.4-2.2 5.4-5.9V55.4h5.5v23.8h-5.5v-3.7a8.367 8.367 0 01-7.4 4.2c-5.5 0-8.6-3.6-8.6-9.2z" data-name="Path 6988" />
<path d="M492.4 67.4a12.233 12.233 0 0112.4-12.5 11.968 11.968 0 019.5 4.1l-3.4 3.7c-1.7-1.8-3.4-3-6.1-3-4 0-6.9 3.4-6.9 7.6v.1c0 4.2 2.9 7.6 7.1 7.6a8.488 8.488 0 006.2-2.9l3.3 3.2a12.031 12.031 0 01-9.7 4.5 12.21 12.21 0 01-12.4-12.4z" data-name="Path 6989" />
