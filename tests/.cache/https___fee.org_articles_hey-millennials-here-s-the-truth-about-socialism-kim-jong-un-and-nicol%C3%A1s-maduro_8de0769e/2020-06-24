6 -4.716)" fill-rule="evenodd" fill="url(#a)" /></g><g transform="translate(26.16)"><path d="M31.816,7.534h-.94V1.48h2.4a1.812,1.812,0,0,1,2.005,1.863A1.745,1.745,0,0,1,34.1,5.072l1.359,2.463H34.376L33.142,5.227H31.816Zm0-3.109h1.352a1.007,1.007,0,0,0,1.146-1.062,1.033,1.033,0,0,0-1.171-1.058H31.816V4.425Z" transform="translate(-30.876 -1.182)" /><path d="M40.361,6.3a1.853,1.853,0,0,1-1.98,1.321,2.075,2.075,0,0,1-2.11-2.357,2.107,2.107,0,0,1,2.106-2.388c1.271,0,2.039.868,2.039,2.3V5.5H37.189v.051a1.207,1.207,0,0,0,1.217,1.309A1.1,1.1,0,0,0,39.493,6.3h.868ZM37.189,4.828H39.5a1.1,1.1,0,0,0-1.125-1.184A1.168,1.168,0,0,0,37.189,4.828Z" transform="translate(-30.876 -1.182)" /><path d="M41.359,6.242c0-.822.613-1.3,1.7-1.364L44.3,4.807V4.412c0-.482-.319-.755-.936-.755-.5,0-.852.185-.953.508h-.873c.092-.785.831-1.288,1.867-1.288,1.146,0,1.792.57,1.792,1.535V7.534h-.869V6.893h-.072a1.538,1.538,0,0,1-1.372.718A1.381,1.381,0,0,1,41.359,6.242ZM44.3,5.852V5.47l-1.116.071c-.629.042-.915.256-.915.659s.357.65.848.65A1.077,1.077,0,0,0,44.3,5.852Z" transform="translate(-30.876 -1.182)" /><path d="M46.373,5.247c0-1.443.743-2.357,1.9-2.357a1.507,1.507,0,0,1,1.4.8h.067V1.182h.9V7.534h-.864V6.813H49.7a1.586,1.586,0,0,1-1.435.8C47.107,7.611,46.373,6.7,46.373,5.247Zm.931,0c0,.97.458,1.553,1.221,1.553s1.229-.592,1.229-1.548S49.281,3.7,48.525,3.7,47.3,4.286,47.3,5.247Z" transform="translate(-30.876 -1.182)" /><path d="M54.36,5.247a2.164,2.164,0,1,1,4.309,0,2.165,2.165,0,1,1-4.309,0Zm3.382,0c0-.99-.445-1.569-1.225-1.569s-1.226.579-1.226,1.569.441,1.574,1.226,1.574S57.742,6.242,57.742,5.247Z" transform="translate(-30.876 -1.182)" /><path d="M59.872,2.965h.869v.726h.067a1.368,1.368,0,0,1,1.364-.813,1.486,1.486,0,0,1,1.582,1.7V7.534h-.9V4.8c0-.734-.319-1.1-.986-1.1a1.048,1.048,0,0,0-1.091,1.158V7.534h-.9V2.965Z" transform="translate(-30.876 -1.182)" /></g></g></svg>
