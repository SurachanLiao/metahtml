<path d="M196.2 47.6h23.4v5h-17.9v8.2h15.8v5h-15.8v8.5h18.1v5h-23.6z" data-name="Path 6978" />
<path d="M221.8 67.4a12.233 12.233 0 0112.4-12.5 11.968 11.968 0 019.5 4.1l-3.4 3.7c-1.7-1.8-3.4-3-6.1-3-4 0-6.9 3.4-6.9 7.6v.1c0 4.2 2.9 7.6 7.1 7.6a8.488 8.488 0 006.2-2.9l3.3 3.2a12.032 12.032 0 01-9.7 4.5 12.21 12.21 0 01-12.4-12.4z" data-name="Path 6979" />
<path d="M244.6 67.4a12.70009842 12.70009842 0 0125.4-.1v.1a12.7 12.7 0 01-25.4 0zm19.9 0c0-4.3-3-7.8-7.3-7.8-4.4 0-7.2 3.4-7.2 7.6v.1a7.285 7.285 0 007.3 7.6c4.4.1 7.2-3.4 7.2-7.5z" data-name="Path 6980" />
<path d="M273.3 55.4h5.5v3.7a8.572 8.572 0 017.4-4.2c5.4 0 8.5 3.6 8.5 9.1v15.2h-5.5V65.7c0-3.7-1.8-5.8-5.1-5.8-3.2 0-5.4 2.2-5.4 5.9v13.4h-5.5V55.4z" data-name="Path 6981" />
<path d="M297.7 67.4a12.70009842 12.70009842 0 0125.4-.1v.1a12.7 12.7 0 01-25.4 0zm19.9 0c0-4.3-3-7.8-7.3-7.8-4.4 0-7.2 3.4-7.2 7.6v.1a7.285 7.285 0 007.3 7.6c4.4.1 7.2-3.4 7.2-7.5z" data-name="Path 6982" />
<path d="M326.4 55.4h5.5V59c1.5-2.1 3.6-4.1 7.3-4.1a7.528 7.528 0 017.1 4.2 9.617 9.617 0 018-4.2c5.2 0 8.3 3.3 8.3 9.1v15.2h-5.5V65.7c0-3.8-1.8-5.8-4.8-5.8s-5.1 2.1-5.1 5.9v13.4h-5.5V65.6c0-3.7-1.8-5.8-4.8-5.8s-5.1 2.3-5.1 5.9v13.4h-5.5V55.4z" data-name="Path 6983" />
<path d="M367.1 46.5h5.9v5.2h-5.9zm.2 8.9h5.5v23.8h-5.5z" data-name="Path 6984" />
<path d="M376.4 67.4a12.233 12.233 0 0112.4-12.5 11.968 11.968 0 019.5 4.1l-3.4 3.7c-1.7-1.8-3.4-3-6.1-3-4 0-6.9 3.4-6.9 7.6v.1c0 4.2 2.9 7.6 7.1 7.6a8.488 8.488 0 006.2-2.9l3.3 3.2a12.032 12.032 0 01-9.7 4.5 12.21 12.21 0 01-12.4-12.4z" data-name="Path 6985" />
