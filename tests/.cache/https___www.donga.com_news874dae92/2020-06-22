        <script>
        var imgCurPos = -1 ;

        function callCSset(key, val)
        {
            var targetUrl = "/HUB/common/cst.php?s="+key+"&u="+val ;
            $.ajax( {
                type: "GET",
                url: targetUrl,
                dataType: "html",
                success: function (result) {
                }
            });
        }

        function changeImgBack ()
        {
            imgCurPos = ( imgCurPos - 1 + NewImg.length ) % NewImg.length ;
            $(".IM").html( "<img src='"+NewImg[imgCurPos]+"' usemap='#NewsMap"+imgCurPos+"'>" ) ;
            callCSset('wpp', imgCurPos-1)  ;
        }

        function changeImg ()
        {
            callCSset('wpp', imgCurPos)  ;
            imgCurPos = ( imgCurPos + 1 ) % NewImg.length ;
            $(".IM").html( "<img src='"+NewImg[imgCurPos]+"' usemap='#NewsMap"+imgCurPos+"'>" ) ;
        }

        $(document).ready ( function(){
            changeImg() ;
            setInterval(changeImg, 10000);
        });
        </script>    <!-- footer -->
