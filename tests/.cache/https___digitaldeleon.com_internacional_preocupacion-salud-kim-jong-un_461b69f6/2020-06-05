                    if ( td_screen_width < 768 ) {
                        /* Phones */
                        document.write('<ins class="adsbygoogle" style="display:inline-block;width:320px;height:50px" data-ad-client="ca-pub-1447679990359568" data-ad-slot="8069718982"></ins>');
                        (adsbygoogle = window.adsbygoogle || []).push({});
                    }
                </script>
