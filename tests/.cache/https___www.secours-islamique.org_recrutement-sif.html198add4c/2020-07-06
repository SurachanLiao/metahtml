			var exp_date = new Date();
			exp_date.setTime(exp_date.getTime() + (1000*3600*24*365));
			document.cookie = 'cookie_996729cb2f31c029bc168ac031d455b6=accepted;expires='+exp_date.toUTCString()+';path=/';
			jQuery('#easycookieinfo').slideUp('slow');
			
			return false;
		});
    });
</script>		</div>
