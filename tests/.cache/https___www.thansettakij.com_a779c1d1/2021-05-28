<script type="text/javascript">

                                            $(function(){

                                                $(".pic-gallery").lightGallery();
                                                $('#index-slide').flexslider({
                                                    animation: "slide",
                                                    controlNav: "thumbnails"
                                                });
                                                $('.slide-ad ul').slick({
                                                    dots : true
                                                });

                                                $('.slider').slick({
                                                    infinite: true,
                                                    slidesToShow: 4,
                                                    variableWidth: true,
                                                    slidesToScroll: 1,
                                                    autoplay:true,
                                                });

                                                $('.slide2').slick({
                                                    autoplay:false,
                                                    useCSS : true,
                                                    responsive: [
                                                    {
                                                        breakpoint: 899,
                                                        settings: {
                                                            slidesToShow: 3
                                                        }
                                                    },
                                                    {
                                                        breakpoint: 767,
                                                        settings: {
                                                            slidesToShow: 2
                                                        }
                                                    },
                                                    {
                                                        breakpoint: 479,
                                                        settings: {
                                                            slidesToShow: 1
                                                        }
                                                    }
                                                    ]
                                                });

                                                $('.slide3').slick({
                                                    autoplay:false,
                                                    useCSS : true,
                                                    slideMargin : 10,
                                                    responsive: [
                                                    {
                                                        breakpoint: 999,
                                                        settings: {
                                                            slidesToShow: 2
                                                        }
                                                    },
                                                    {
                                                        breakpoint: 499,
                                                        settings: {
                                                            slidesToShow:1
                                                        }
                                                    }
                                                    ]
                                                });

                                                $('.section-covid-list-item, .custom-option').click(function(el){
                                                    var header = this.innerHTML;
                                                    var cate_id = $(this).data('value');
                                                    $('.section-covid-box .heading').text(header);
                                                    $('.section-covid-list .custom-select__trigger span').text(header);
                                                    $.ajax({
                                                        url: 'https://www.thansettakij.com/api/news_category/'+cate_id,
                                                        dataType: 'json',
                                                        success: function (res) {
                                                            if(res.news.length == 4){
                                                                for(i in res.news){
                                                                    $('#section-covid-link-'+i).prop('href',res.news[i].link);
                                                                    $('#section-covid-image-'+i).prop('src',res.news[i].thumbnail);
                                                                    $('#section-covid-title-'+i).html(res.news[i].title);
                                                                    $('#section-covid-date-'+i).text(res.news[i].date);
                                                                }
                                                            }
                                                        },
                                                        fail: function (){
                                                            // console.log('load m33 ajax fail')
                                                        }
                                                    });
                                                });
                                            });

                                            document.querySelector('.section-covid-list.--mobile').addEventListener('click', function() {
                                                this.querySelector('.custom-select').classList.toggle('open');
                                            })

                                            for (const option of document.querySelectorAll(".custom-option")) {
                                                option.addEventListener('click', function() {
                                                    if (!this.classList.contains('selected')) {
                                                    this.parentNode.querySelector('.custom-option.selected').classList.remove('selected');
                                                    this.classList.add('selected');
                                                    this.closest('.custom-select').querySelector('.custom-select__trigger span').textContent = this.textContent;
                                                    }
                                                })
                                            }

                                            window.addEventListener('click', function(e) {
                                                const select = document.querySelector('.custom-select')
                                                if (!select.contains(e.target)) {
                                                    select.classList.remove('open');
                                                }
                                            });
                                        </script>
