<script>
jQuery("#footer_toggle").toggle(
	function() 
	{
		//jQuery("#footer_close").hide( "blind", {},500);
		jQuery("#footer_close").css("display","none");
		//jQuery("#footer_close").hide();
		jQuery("#footer_toggle").addClass("open");
	},
	function() 
	{
		//jQuery("#footer_close").show( "blind", {},500);
		jQuery("#footer_close").css("display","block");
		//jQuery("#footer_close").show();
		jQuery("#footer_toggle").removeClass("open");
	}
);
</script>





