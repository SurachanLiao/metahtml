        <div id="lv-container" data-id="khan" data-uid="Ni82LzQ=">
<!--         <div id="lv-container" data-id="khan" data-uid="Ni8zOTMzNS8xNTg2Mg=="> -->
<!--         <div id="lv-container" data-id="khan" data-uid="Ni8zMzA3Ny8xNTg2Mg=="> -->
            <script type="text/javascript">
                window.livereOptions = {
                    customMessage: {m_khan: '경향신문'},
                    refer: 'news.khan.co.kr/kh_news/khan_art_view.html?artid=202005220600035',
                    site: 'http://news.khan.co.kr/kh_news/khan_art_view.html?artid=202005220600035&utm_source=livere&utm_medium=social_share',
                };

                // Hooking
                window.livereHooks = {
                    write: function(data) {
                        doHooking('write', data);
                    },

                    modify: function(data) {
                        doHooking('modify', data);
                    },

                    remove: function(data) {
                        doHooking('remove', data);
                    }
                };

                function doHooking(mode, data)
                {
                    var pars = {
                        mode: mode,
                        comment_seq: data.reply.replySeq
                    };

                    if (mode == 'write') {
                        pars.parent_seq = data.reply.parentSeq;
                        pars.content = data.reply.content;
                        pars.time = data.reply.regdate;
                        pars.member_icon = data.reply.writer.profileImageUrl;
                        pars.member_domain = data.reply.writer.strategy;
                        pars.is_mobile = data.reply.environment.isMobile;
                        pars.member_id = data.reply.writer.snsId;
                        pars.name = data.reply.writer.name;
                        pars.user_agent = data.reply.environment.agent;
                        pars.member_seq = data.reply.writer.id;
                        pars.site = data.reply.site.url;
                        pars.page = location.href;
                        pars.title = $( 'meta[property="og:title"]' ).attr( 'content' );
                        if(pars.title == '' || pars.title == undefined) pars.title = "[단독]“한국인들이 미얀마 법 지켰으면”…한국 업체, ‘코로나’ 이유로 미얀마 노조 대량 해고";
                        pars.art_img = $( 'meta[property="og:image"]' ).attr( 'content' );
                    }

                    else if (mode == 'modify') {
                        pars.content = data.reply.newContent;
                        pars.time = data.reply.editDate;
                    }

                    else if (mode == 'remove') {
                        pars.time = data.reply.deletedDate;
                    }

                    jQuery.ajax({
                        url : "/livere_hook.html",
                        type : "POST",
                        dataType : "json",
                        data  : pars,
                        cache : false,
                        success: function(json) {},
                        error: function(xhr) {}
                    });

                    console.log('hooking', pars);
                }


               (function(d, s) {
                   var j, e = d.getElementsByTagName(s)[0];

                   if (typeof LivereTower === 'function') { return; }

                   j = d.createElement(s);
                   j.src = 'https://cdn-city.livere.com/js/embed.dist.js';
                   j.async = true;

                   e.parentNode.insertBefore(j, e);
               })(document, 'script');
            </script>
            <noscript>>Please activate JavaScript for write a comment in LiveRe.</noscript>
        </div>




