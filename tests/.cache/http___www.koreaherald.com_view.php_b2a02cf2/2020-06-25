<script language="javascript">
function getCookie(cookieName){
	var cookieValue=null;
	if(document.cookie){
		var array=document.cookie.split((escape(cookieName)+'='));
		if(array.length >= 2){
			var arraySub=array[1].split(';');
			cookieValue=unescape(arraySub[0]);
		}
	}
	return cookieValue;
}

var strCookieName = "";
var strUrl = "";
var nW = "";
var nH = "";
var nT = "";
var nL = "";

/* popup 1
strCookieName = "KhPopup_1";
strUrl = "/popup.php";
nW = "400";
nH = "425";
nT = "50";
nL = "900";

if( getCookie(strCookieName) == null )
{
	var objImsiPopWin = window.open(strUrl, "HDWPopUpWin_"+strCookieName, "width="+nW+", height="+nH+", top="+nT+", left="+nL+", scrolling=no");
}
*/

/* popup 2
strCookieName = "KhPopup_2";
strUrl = "/popup2.php";
nW = "400";
nH = "680";
nT = "50";
nL = "1100";

if( getCookie(strCookieName) == null )
{
	var objImsiPopWin = window.open(strUrl, "HDWPopUpWin_"+strCookieName, "width="+nW+", height="+nH+", top="+nT+", left="+nL+", scrolling=no");
}
*/

</script>

