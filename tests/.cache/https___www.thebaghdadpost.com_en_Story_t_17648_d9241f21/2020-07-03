<script>
        $(document).ready(function () {

            var from;
            var to;
            var mount;

            $("#convert").click(function () {

                //var cul = 'en';
                var serviceURL = 'ar/Home/GetCurrencyConvert1'; 

                from = $('#from').val();
                to = $('#to').val();
                mount = $('#mount').val();
                var serviceURL = 'https://free.currencyconverterapi.com/api/v5/convert?q=' + from + '_' + to + '&compact=y' ;  
                console.log(serviceURL);
                $.ajax({
                    type: "GET",
                    url: serviceURL,
                    dataType: "json",
                    success: successFunc, 
                    error: errorFunc
                });

                // alert(action);
            });


            function successFunc(data, status)
            {
                var t = from + '_' + to  ;
                //console.log(data[t]["val"]);
                var total = data[t]["val"] * mount;
                //var from_txt = $("#from").text();
                //var to_txt = $("#to option:selected").text();  
                //var result = total + " " + to_txt;    
                //alert(total); 

                $('#repon').val(total);  

        }

        function errorFunc() {
            console.log('error'); 
            alert("sucess");
        }
    });

    </script>
</div>
