		});
	}

	if ('function' === typeof ga && 'function' === typeof cookie && 'function' === typeof cookie.setItem) {
		ga(function (tracker) {
			var clientId = tracker.get('clientId');
			cookie.setItem('gr_ga', clientId); // Modificar para usar la librearía de establecer cookies de freepik
		});
	}
