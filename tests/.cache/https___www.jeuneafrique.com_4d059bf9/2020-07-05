<!-- Board -->
<section id="board-economie" class="board economie actu">
	<!-- Block Title -->
	
<h1 class="board-title">
	<a href="/rubriques/economie/">
		Economie	</a>
</h1>
	<!-- end block title -->

	<!-- Block body -->
	<div class="board-body">
	<div class="clearfix">
		<div class="col-sm-12 col-md-8">
			
<article class="board-lead-art"  itemscope itemtype="http://schema.org/NewsArticle">
    <a href="/1010435/economie/affaire-adesina-les-etats-unis-se-rangent-au-mandat-du-panel-de-revue/" itemprop="url mainEntityOfPage" >

        
        <img src="/medias/2020/07/03/640px-steve_mnuchin_swearing_in-320x160-1593789916.jpg" alt="Steven Mnuchin, le secrétaire américain au Trésor, lors de sa prestation de serment."  class="art-thumbnail"  itemprop="image" />

        
                <span class="subtitle" itemprop="keywords">Banque de développement</span>
                <h2 class="reserved-content art-title">
            <span itemprop="name" class="js-no-orphans">Affaire Adesina : les États-Unis se rangent au mandat du panel de « revue »</span>
                    </h2>
                <p itemprop="description">Washington, qui réclamait une nouvelle enquête sur les allégations contre le président de la BAD, s’est félicité de la sélection du "panel indépendant", malgré les limites de son mandat.</p>
        
    </a>
            <!-- hidden itemprops for google -->
        <span style="display: none">
            <span itemprop="author">Joël Té-Léssia Assoko</span>
            <span itemprop="headline">Affaire Adesina : les États-Unis se rangent au mandat du panel de « revue »</span>
            <time itemprop="datePublished" datetime="2020-07-03T15:35:47Z" pubdate="2020-07-03T15:35:47Z"></time>
            <time itemprop="dateModified" datetime="2020-07-03 15:51:01"></time>
            <span itemprop="publisher" itemscope itemtype="http://schema.org/Organization">
                <span itemprop="name">Jeune Afrique</span>
                <span itemprop="logo" itemscope itemtype="https://schema.org/ImageObject">
                    <span itemprop="url" content="//www.jeuneafrique.com/wp-content/themes/ja-3.0.x/assets/img/favicon.png"></span>
                </span>
            </span>
        </span>
        </article>

