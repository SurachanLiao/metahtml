	<script type='text/javascript'>
		var sectionPageContentGroup =  {
			"Author"     :"Section-Page",
			"categType"	 :"Misc" ,
			"openType"	 :"100",
			"layout"	 :"Section-Page"
		}
		GaTm.articlePage.initContentGroup(sectionPageContentGroup,UserUtil.getLoginInfo());
		//GaTm.eventManager.fireArticlePageEvent();
		//GaTm.eventManager.fireSectionPageEvent();
		ga('set', 'dimension1', GaTm.articlePage.getusrType()); //User type
		ga('set', 'dimension2', 'Section-Page'); //Author Name - Author
		ga('set', 'dimension3', "100"); //Article Mode - openType
		ga('set', 'dimension4', 'Section-Page'); //Page Type - layout
		//ga('send', 'pageview');
	</script>



      
          	    

      
  
      </html>							