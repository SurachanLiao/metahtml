  <script class="consent-independent" type="text/javascript" src="https://static.jyllands-posten.dk/app/data/jp/ng/inline.f1c3d82f196d7f2549c0.bundle.js"></script>
  <script class="consent-independent" type="text/javascript" src="https://static.jyllands-posten.dk/app/data/jp/ng/polyfills.776877468f2e98307d60.bundle.js"></script>
  <script class="consent-independent" type="text/javascript" src="https://static.jyllands-posten.dk/app/data/jp/ng/styles.52894a819c3772585f66.bundle.js"></script>
  <script class="consent-independent" type="text/javascript" src="https://static.jyllands-posten.dk/app/data/jp/ng/main.7af806251ab8de57f4a7.bundle.js"></script>
  <script src="https://static-cdn.jyllands-posten.dk/jquery/jquery.js" class="consent-independent"></script> 
  <script class="consent-independent">

  ;
var pageRoleData = (
    
    {
      page: 'Article',
      loginCookieName: 'article_last_login_adjustment_attempt',
      updateProfileAdjustmentCookieName: 'article_last_profile_update_adjustment_attempt',
    }
    
);

// 
if (pageRoleData) {
  try {
    FS.authStateHandler.init(
      () => JP.oauth2.loginAndSetCookies(pageRoleData.page),
      () => JP.oauth2.logoutAndSetCookies(pageRoleData.page)
    );
  } catch (e) {
    console.warn(e);
  }

  window.setAuthState = function(state) {
    try {
      FS.authStateHandler.reflect(state);
    } catch (e) {
      console.warn(e);
    }
  };
}
// 

var authCallback = function () {
  JP.oauth2.events.subscribe(function (e) {
    if(e.message === 'Attempting immediate login') {
      var authLoadSpinner = document.getElementById('authLoadSpinner');
      authLoadSpinner && (authLoadSpinner.style.display = 'block');
    }

    if(e.message === 'Error starting immediate login') {
      var authLoadSpinner = document.getElementById('authLoadSpinner');
      authLoadSpinner && (authLoadSpinner.style.display = '');
    }
  });

  JP.oauth2.user.subscribe(function (user) {
    function reflectAuthState(state) {
      if (window.setAuthState) {
        window.setAuthState(state);
      }
    }

    var authState = {
      isLoggedIn: false,
      subscription: { isActive: false }
    };
    if (user) {
      authState = {
        isLoggedIn: true,
        userName: user.name,
        userEmail: user.email,
        subscription: {
          isZuora: true,
          isActive: !!(
            user &&
              user.attributes &&
              user.attributes.access &&
              user.attributes.access.includes("Jyllands-Posten LOCKED ARTICLE")
          )
        }
      };
    }

    reflectAuthState(authState);
    if(!window.premiumInfo__) {
      window.premiumInfo__ = {};
    }

    if (user) {
      if (window.premiumInfo__) {
        premiumInfo__.ssoId = user.id;
        premiumInfo__.name = user.name || '';
        premiumInfo__.email = user.email;
      }

      url = '/api/jps/user/subscription_info/' + user.id + '/?ncc=1&email=' + encodeURIComponent(user.email);
      $jp.get(url, "", function(info) {

        if(premiumInfo__ &&  premiumInfo__.ssoId) {
          var $mySideLink = $jp('#mySideLink');
          if(info.jps && !info.sos) {
            $mySideLink.attr('href', 'https://abonnement.jyllands-posten.dk/min-side.aspx?jpref=');
          }
        }

        authState.subscription.isActive = info.sos ||  info.jps;
        authState.subscription.isZuora  = info.sos && !info.jps;
        reflectAuthState(authState);
      });
    }
  });

  if (pageRoleData) {
    JP.oauth2.adjustToCookiesOrAutologin(
      pageRoleData.page,
      pageRoleData.loginCookieName,
      pageRoleData.updateProfileAdjustmentCookieName
    );
  } else {
    JP.oauth2.processAutologin();
  }
}

setTimeout(authCallback, 0);
</script> 
  <script class="consent-dependent" type="text/plain" data-consent-vendor="5f044b7c8b962910bbae35b4" data-consent-purposes="5eeb505ad578652a8a1f706c" data-consent-id="exponea-script">
    window.addEventListener("load", () => {
        if (localStorage.getItem("ssoid") !== window.user_id && window.user_id !== "anon") {
            localStorage.setItem("ssoid", window.user_id)
            exponea.identify({"sso_id":window.user_id});
        }
    });
</script> 
