$(document).ready(function() {
	$('body').append('<a id="toTop" href="#">Back to Top</a>');
	
	$(window).scroll(function() {
		if ($(this).scrollTop() > 4000) {
			$('#toTop').fadeIn();
			$('#toSpicy').fadeIn();
		} else {
			$('#toTop').fadeOut();
			$('#toSpicy').fadeOut();
		}
	});
	$('#toTop').live({
	'click': function(){
		$(window).scrollTop(0);
	}
	})
});

</script>
