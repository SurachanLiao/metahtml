$(function(){

var banner_rotator_timer_sec = 60;
var banner_rotator_code = '<script src="//static.newsmaxfeednetwork.com/web-clients/bootloaders/qdaACZWjvDd6SpiXX3uv9L/bootloader.js" async="true" data-version="3" data-url="[PAGE_URL]" data-zone="[ZONE]" data-organic-clicks="[ORGANIC_TRACKING_PIXEL]" data-paid-clicks="[PAID_TRACKING_PIXEL]"></scr'+'ipt>';


function rotateBanner() {	
  $("#banner_code_rotator").html("<iframe style='border:0'></iframe>");
  var iframeDoc = $("#banner_code_rotator iframe")[0].contentDocument;
  iframeDoc.open();
  iframeDoc.write(banner_rotator_code);
  iframeDoc.close();
}

if (typeof window.rotateBannerInterval !== 'undefined') clearInterval(rotateBannerInterval);
window.rotateBannerInterval = setInterval(function(){rotateBanner();}, banner_rotator_timer_sec*1000);
rotateBanner();
});

</script>
