		  		var minutes = 180;
		  		expireDate.setTime(expireDate.getTime() + (minutes * 60 * 1000));
		  		$.cookie('popupfundappeal', 'seen', {
			  		expires: expireDate,
			  		path: '/',
			  		domain: 'wsws.org',
			  		secure: false
			  	});
  				setTimeout(function() {
	  				$('#popupfundappeal').addClass('active');
	  			}, 6500);
  			}
  		}
  		/*
