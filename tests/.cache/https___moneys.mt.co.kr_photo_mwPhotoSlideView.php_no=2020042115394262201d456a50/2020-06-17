<script type="text/javascript">
    function imgResize() {
        var myImage = new Image();
        myImage.src = $("#articleBigImg").attr('src');
        var ori_width = myImage.width;			// 이미지 원래 가로 사이즈
        var ori_height = myImage.height;			// 이미지 원래 세로 사이즈
        var ratio = ori_width / ori_height;	// 현재 이미지의 비율

        if (ori_width > ori_height) { // 가로 이미지일 경우, 가로 사이즈 530px -> 비율대로 세로 설정
            if (ori_width > 530) {
                var new_width = 530;
                var new_height = Math.round(530 / ratio);
            } else {
                var new_width = ori_width;
                var new_height = ori_height;
            }
        } else if (ori_height > ori_width) { // 세로 이미지일 경우, 세로 사이즈 500px -> 비율대로 가로 설정
            if (ori_height > 500) {
                var new_height = 500;
                var new_width = Math.round(500 * ratio);
            } else {
                var new_width = ori_width;
                var new_height = ori_height;
            }
        } else {
            var new_width = ori_width;
            var new_height = ori_height;
        }

        $('#articleBigImg').attr({width: new_width, height: new_height});
    }

    function cutByLen(str, maxByte) {
        for(b=i=0;c=str.charCodeAt(i);) {
            b+=c>>7?2:1;
            if (b > maxByte)
            break;
            i++;
        }
        return str.substring(0,i);
    }

    $(document).ready(function () {
        var slide_no = $('#slide_no').val();
        var cate = $('#cate').val();

        // 상단 포토슬라이드 리스트 만들기
        makeTopSlideList = function (movePage) {
            if (movePage) {
                var topSlideListStr = getDataByAjax('getTopList', '', movePage, '');
            } else {
                var topSlideListStr = getDataByAjax('getTopList', '', '', '');
            }

            if (topSlideListStr == 'no_more_slide_page') {
                alert('마지막 슬라이드입니다.');
            } else {
                var topSlideInfoArr = topSlideListStr.split('<<end>>');
                var topSlideListArea = '';
                for (var i = 0; i < topSlideInfoArr.length; i++) {
                    if (i == 0) {
                        var slidePageInfoArr = topSlideInfoArr[0].split('|');
                        var slidePage = slidePageInfoArr[0]; // 현재 슬라이드 페이지
                        var currentPosition = slidePageInfoArr[1]; // 페이지리스트에서의 현재 스라이드 위치 (1~6)

                        $('#currentTopListPage').val(slidePage); // 현재 슬라이드 리스트 페이지 저장
                    } else {
                        var slideInfoArr = topSlideInfoArr[i].split('|');
                        var slideNo = slideInfoArr[0];
                        var slideTitle = slideInfoArr[1];
                        var slideThumb = slideInfoArr[2];
                        var slideOnClass = (slide_no == slideNo) ? ' class="on"' : '';

                        if (slideNo) {
                            topSlideListArea += '<li id="divPageList_' + i + '"><a href="/photo/mwPhotoSlideView.php?no=' + slideNo + '"' + slideOnClass + '><span class="thum"><img src="' + slideThumb + '" alt=""><span class="edge_on"></span></span><span class="tit">' + slideTitle + '</span></a></li>';
                        }
                    }
                }

                $('#topSlideListArea').html(topSlideListArea);
            }
        }

        // 기사정보 설정
        setArticleInfo = function (articleId) {
            var articleInfoStr = getDataByAjax('getArticleInfo', articleId, '', '');

            var infoArr = articleInfoStr.split('<<end>>');
            var adjacentArr = infoArr[0].split('|');
            $('#prevArticleId').val(adjacentArr[0]);
            $('#nextArticleId').val(adjacentArr[2]);
            $('#currentArticleId').val(adjacentArr[1]);
            $('#currentPhotoNo').text(adjacentArr[3]);

            var articleInfo = infoArr[1].split(';:;');
            $('#photoArticleTitle').html(articleInfo[0]);		// 기사 제목
            $('#pn_giza_name').text(articleInfo[1]);			// 기사 작성자
            $('#pn_article_date').text(articleInfo[2]);			// 작성일자
            $('#articleSummary').html(articleInfo[3]);			// 요약글 (본문내용)
            $('#pn_reply_cnt').html(articleInfo[5]);			// 조회수
            var giza_sosok = articleInfo[6];                // 소속
            var gisa_keyword = articleInfo[7];                // 키워드

            $('#articleBigImg').css('display', 'none');
            $('#articleBigImg').attr('src', articleInfo[4]);	// 본문 이미지
            $('#articleBigImg').fadeIn(500);

            /* 수집안된다고 하여 제거
            var dableCustom1 = 'count';
            if ( gisa_keyword.indexOf("스타패션") != -1 ) {
                dableCustom1 = 'not';
            }
            
            // meta 변경
            $('meta[property="article:published_time"]').prop("content", (new Date(articleInfo[2])).toISOString());
            $('meta[name="author"]').prop("content", giza_sosok+articleInfo[1]);
            $('meta[property="article:author"]').prop("content", giza_sosok+articleInfo[1]);
            $('meta[property="dable:author"]').prop("content", giza_sosok+articleInfo[1]);
            $('meta[property="article:section"]').prop("content", '포토슬라이드');
            $('meta[property="article:tag"]').prop("content", '포토슬라이드');
            $('meta[name="keywords"]').prop("content", gisa_keyword);
            $('meta[property="dable:custom1"]').prop("content", dableCustom1);

            var description = articleInfo[3];
            description = description.replace(/<(\/)?([a-zA-Z]*)(\s[a-zA-Z]*=[^>]*)?(\s)*(\/)?>/ig, "");
            var descriptionLen = description.length;
            description = cutByLen(description,80);
            if (descriptionLen > description.length) {
                description = description+'...';
            }
            $('meta[name="description"]').prop("content", description);
            $('meta[itemprop="description"]').prop("content", description);
            $('meta[name="twitter:description" ]').prop("content", description);
            $('meta[property="og:description"]').prop("content", description);
            */
        }


        // 슬라이드 내에서 사진 한 장 씩 이동
        moveArticle = function (direction) {
            // 마지막 사진일 경우, 이전/이후 슬라이드로 페이지 이동
            var moveArticleId = $('#' + direction + 'ArticleId').val();

            // prev_slide 또는 next_slide 값인 경우, 이전/다음 슬라이드로 이동
            if (moveArticleId == 'prev_slide' || moveArticleId == 'next_slide') {
                //alert('다음 슬라이드로 넘어갑니다.');
                moveSlidePage(moveArticleId);
            } else { // 다음 기사 아이디가 있는 경우, 슬라이드 내의 다음 기사 데이터를 보여줍니다.
                setPhotoCnt(direction);
                setArticleInfo(moveArticleId);
            }
        }

        // 포토슬라이드 이미지 상단 카운트 변경
        setPhotoCnt = function (direction) {
            var currentPhotoCnt = parseInt($('#currentPhotoNo').text());
            var newPhotoCnt = (direction == 'prev') ? currentPhotoCnt - 1 : currentPhotoCnt + 1;

            $('#currentPhotoNo').text(newPhotoCnt);
        }

        // 다음 슬라이드로 이동
        moveSlidePage = function (move_slide) {
            var currentSlideNo = $('#slide_no').val();
            var moveSlideUrl = getDataByAjax('getMoveSlideNo', move_slide, '', '');

            if (moveSlideUrl == 'no_more_slide') {
                alert('마지막 슬라이드입니다.');
            } else {
                location.href = moveSlideUrl;
            }
        }

        // 상단 슬라이드 리스트 페이지 이동
        moveTopSlideList = function (direction) {
            var currentListPage = parseInt($('#currentTopListPage').val());
            var newListPage = (direction == 'prev') ? currentListPage - 1 : currentListPage + 1;
            newListPage = (!newListPage || newListPage == 0) ? 'no' : newListPage;

            makeTopSlideList(newListPage);
        }

        // 슬라이드 중 첫번째로 보여줄 기사 아이디 가져오기
        getFirstArticleId = function (idx) {
            if ($.trim(idx) == '' || idx == 0) {
                // idx값이 빈값이거나 0일 경우, 슬라이드 내 첫번째 기사를 보여줍니다.
                var firstArticleId = '';
            } else {
                // idx값이 있는 경우, 해당하는 기사의 아이디 값을 설정합니다.
                var firstArticleId = getDataByAjax('getFirstArticleId', '', '', idx);
            }

            return firstArticleId;
        }

        /********************************************************************
         * ajax 이용해서 데이터 가져오기
         * mode :
         *     - getTopList : 포토슬라이드 본문 상단 리스트 데이터 가져오기
         *     - getArticleInfo : 슬라이드 본문용 데이터 가져오기
         *     - getMoveSlideNo : 슬라이드 내에서 다음 사진으로 넘어갈 때
         *                        마지막 사진일 경우 다음 슬라이드로 넘어감.
         *     - getFirstArticleId : 슬라이드 내에서 idx번째 기사의 아이디
         *                           가져오기. idx 변수 필수
         * article_id :
         *     - getTopList에서는 사용안함
         *     - getArticleInfo에서는 해당 아이디에 해당되는 내용 리턴
         *     - getMoveSlideNo에서는 prev_slide / next_slide 두 값만 존재
         * move_page :
         *     - getTopList에서만 사용. 명시할 경우, 해당 페이지 리스트 리턴
         * idx :
         *     - getFirstArticleId에서만 사용. idx번째 기사 아이디 가져옴
         *******************************************************************/
        getDataByAjax = function (mode, article_id, move_page, idx) {
            $.ajax({
                url: 'getPhotonewsInfos.php',
                data: 'mode=' + mode + '&cate=' + cate + '&slide_no=' + slide_no + '&article_id=' + article_id + '&topPage=' + move_page + '&idx=' + idx,
                async: false,
                dateType: 'html',
                error: function (request, status, error) { //통신 에러 발생시 처리
                    returnStr = error;
                },
                success: function (response, status, request) { //통신 성공시 처리
                    returnStr = response;
                },
                complete: function () { //통신이 완료된 후 처리
                }
            });
            return returnStr;
        }

        makeTopSlideList(); // 상단 포토슬라이드 리스트 만들기

        var firstArticleId = getFirstArticleId($('#idx').val());
        setArticleInfo(firstArticleId); // 슬라이드 내의 첫번째 기사 정보 출력

    });

    // // 중앙이미지 좌우버튼 스크롤 액션
