    <script type="text/javascript">
        function content_mouse2(num, tab) {
            for (var i = 0; i <= tab; i++) {
                document.getElementById('covercon' + i).style.display = 'none';
            }
            document.getElementById('covercon' + num).style.display = 'block';

            left_num = (num * 1) - 1;
            right_num = (num * 1) + 1;

            if (left_num < 0) {
                left_num = '2';
            }
            if (right_num > '2') {
                right_num = 0;
            }

            document.getElementById('cover_tab').innerHTML = '<a href="javascript:content_mouse2(\'' + right_num + '\',\'2\')" class="next"><img src="https://menu.mt.co.kr/new_moneyweek/images/button/btn_left.gif" alt="이전" /></a><a href="javascript:content_mouse2(\'' + left_num + '\',\'2\')" class="prev"><img src="https://menu.mt.co.kr/new_moneyweekmages/button/btn_right.gif" alt="다음" /></a>';
        }
    </script>


