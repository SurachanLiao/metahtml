<script type="text/javascript">
    $(document).ready(function () {
        $('#clothesTab li a').on('mouseover', function () {
            $('#clothesTab li a').removeClass('on');
            $(this).addClass('on');
        });
    });

    // 슬라이드 이동
    function moveSlide(slideNo) {
        $('ul[id^="clothesImgArea"]').hide();
        $('#clothesImgArea' + slideNo).show();

        $('div[id^="clothesTitleArea"]').hide();
        $('#clothesTitleArea' + slideNo).show();
    }

    // 슬라이드 내의 이미지 이동
    function moveSlideImg(slideId, direction) {
        var maxCnt = parseInt($('#clothesImgArea' + slideId + ' li').length);
        var maxCnts = parseInt($('#clothesTitleArea' + slideId + ' div').length);

        var currentNo = '';
        $('#clothesImgArea' + slideId + ' li').each(function () {
            if ($(this).css('display') == 'block') {
                currentNo = parseInt($(this).attr('alt'));
            }
        });

        if (direction == 'prev') {
            var targetNo = ((currentNo - 1) < 0) ? (maxCnt - 1) : (currentNo - 1);
        } else {
            var targetNo = ((currentNo + 1) > (maxCnt - 1)) ? 0 : (currentNo + 1);
        }

        $('#clothesImgArea' + slideId + ' li').each(function () {
            if ($(this).attr('alt') == targetNo) {
                $(this).css('display', 'block');
            } else {
                $(this).css('display', 'none');
            }
        });


        $('#clothesTitleArea' + slideId + ' div').each(function () {
            if ($(this).attr('alt') == targetNo) {
                $(this).css('display', 'block');
            } else {
                $(this).css('display', 'none');
            }
        });
    }
</script>

