<script type="text/javascript">
$('.tennis-game-tabs .tab_item').click(function() {
    if ($(this).text() == '男子') {

        //日程・結果のurl修正
        var s_url = $('.tennis-score a:contains("日程・結果")').attr('href').replace(/wta$/g,'atp');
        $('.tennis-score a:contains("日程・結果")').attr('href', s_url); 

        //ランキングのurl修正
        var r_url = $('.tennis-score a:contains("ランキング")').attr('href').replace(/\/women\/point$/g,'/men/point');
        $('.tennis-score a:contains("ランキング")').attr('href', r_url); 

        //プレイヤー名とurl修正
        var p_url = $('.tennis-score a:contains("最新結果")').attr('href').replace(/66476$/g,'15733');
        $('.tennis-score a:contains("最新結果")').attr('href', p_url); 

        var p_name = $('.tennis-score a:contains("最新結果")').text().replace('大坂なおみ','錦織 圭');
        $('.tennis-score a:contains("最新結果")').text(p_name); 
    } else {

        //日程・結果のurl修正
        var url = $('.tennis-score a:contains("日程・結果")').attr('href').replace(/atp$/g,'wta');
        $('.tennis-score a:contains("日程・結果")').attr('href', url); 

        //ランキングのurl修正
        var r_url = $('.tennis-score a:contains("ランキング")').attr('href').replace(/\/men\/point$/g,'/women/point');
        $('.tennis-score a:contains("ランキング")').attr('href', r_url); 

        //プレイヤー名とurl修正
        var p_url = $('.tennis-score a:contains("最新結果")').attr('href').replace(/15733$/g,'66476');
        $('.tennis-score a:contains("最新結果")').attr('href', p_url); 

        var p_name = $('.tennis-score a:contains("最新結果")').text().replace('錦織 圭','大坂なおみ');
        $('.tennis-score a:contains("最新結果")').text(p_name); 
    }
})
</script> 


