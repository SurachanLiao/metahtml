<!-- アプリ送客コード  -->
<script>
function getCookie(name) {
        var result = null;

        var cookieName = name + '=';
        var allcookies = document.cookie;

        var position = allcookies.indexOf( cookieName );
        if( position != -1 ) {
                var startIndex = position + cookieName.length;

                var endIndex = allcookies.indexOf( ';', startIndex );
                if( endIndex == -1 ) {
                        endIndex = allcookies.length;
                }
                result = decodeURIComponent(
                        allcookies.substring( startIndex, endIndex )
                );
        }

        return result;
}

(function () {
    var ua = {};
    ua.name = window.navigator.userAgent.toLowerCase();
    ua.isAndroid = ua.name.indexOf('android') >= 0;
    ua.isIOS     = ua.name.indexOf('iphone') >= 0;
    isApp = getCookie('goonews_app_from');

    if ((ua.isAndroid)&&(isApp === null)) {
        $(".gn-article,.gn-topics").after('<div style="margin-bottom:20px;"><a href="https://app.appsflyer.com/jp.ne.goo.app.news?pid=goo_news&c=sp"><img src="https://img.news.goo.ne.jp/topics/app/banner/app_banner.png" style="width:100%;"></a></div>');
        $(function() {
            $.smartbanner({ title: '今日のニュースがパッとわかる！', author: 'gooニュース', price: '無料', inGooglePlay: '公式アプリ', icon: '//img.news.goo.ne.jp/topics/app/banner/newsapp_icon_android.png', url: 'https://app.appsflyer.com/jp.ne.goo.app.news?pid=goo_news&c=sp', button: 'ダウンロード', daysHidden: 0, daysReminder: 0, hideOnInstall: true });
        });

    } else if ((ua.isIOS) && (isApp === null)) {

        $(".gn-article,.gn-topics").after('<div style="margin-bottom:20px;"><a href="https://app.appsflyer.com/id1351572933?pid=goo_news&c=sp"><img src="https://img.news.goo.ne.jp/topics/app/banner/app_banner.png" style="width:100%;"></a></div>');
        $(function() {
            $.smartbanner({ title: '今日のニュースがパッとわかる！', author: 'gooニュース', price: '無料', inAppStore: '公式アプリ', icon: '//img.news.goo.ne.jp/topics/app/banner/newsapp_icon_android.png', button: 'ダウンロード', daysHidden: 0, daysReminder: 0, hideOnInstall: true,
                    onClose: function() {
                        document.getElementById("smartbanner").style.display = "none";
                    }
                });
        });
        }
  })();
</script>
