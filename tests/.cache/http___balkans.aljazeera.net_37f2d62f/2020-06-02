  <li class="first odd last">
    <section id="block-panels-mini-mega-menu-news" class="block block-panels-mini clearfix">

      
  <div class="panel-display panel-1col clearfix" id="mini-panel-mega_menu_news">
  <div class="panel-panel panel-col">
    <div class="inside">
      <div class="row">
        <div class="panel-pane pane-views-panes pane-news-taxonomies-news-taxonomies-panel-pane clearfix"  >
  <div class="container">
    <div class="pane-wrapper">
      
                  
      
