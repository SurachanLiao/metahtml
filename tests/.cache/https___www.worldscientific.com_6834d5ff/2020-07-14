        <div data-widget-def="ux3-layout-widget" data-widget-id="0fb12cac-9d40-49fa-8b08-02cdfcd4e5f9" class="background-light-gray grid-sm-2 home-sections bottom-margin">
        



        
            <h3 class="section__header border-bottom wspcpaddingtop">
                <a href="/action/showPublications?PubType=book&amp;sortBy=Earliest&amp;startPage=&amp;Ppub=[20190301%20TO%2020200731]" style="text-decoration:none;">WHAT'S NEW</a>
            </h3>
        
        <ul class="rlist--inline"><li class=" col-sm-12">



        
        <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script async src="//static.zotabox.com/d/d/dd13e181780e401e8a2258e54c92eb84/widgets.js"></script>
<style>
#slideronstaticpage { overflow: hidden; }
.catalogues { width: 300px; float: left; text-align: center; }
#controller { 
  position: relative;
  margin: 0;
  left: 0;
  text-align: center;
}
#arrowback
{
  position: absolute;
  left: 0;
  top: 0;
  cursor: pointer;
  display: block;
  z-index: 9999;
  color: white;
  background: #d6d6d6;
  border-radius: 3px;
  font-size: 14px;
  font-weight: 700;
  padding: 4px 7px;
  line-height: 1;
}
#arrowback:hover
{
  background: #869791;
}
#arrowfor
{
  position: absolute;
  right: 0;
  top: 0;
  cursor: pointer;
  display: block;
  z-index: 9999;
  color: white;
  background: #d6d6d6;
  border-radius: 3px;
  font-size: 14px;
  font-weight: 700;
  padding: 4px 7px;
  line-height: 1;
}
#arrowfor:hover
{
  background: #869791;
}
.animate {
  transition: all .5s ease-out;
}
</style>
<script>
var boxwidth = 300;
var backandfor = 0;
var sliderwidth = 0;
var controllerwidth = 0;
var numberofslides = 0;
var currentslide = 0;
var ctime = 500;//complete time for flipping over
var interwidth;
var countchild;
var numberofboxes;

function restartall()
{
  interwidth = $(".col-sm-12").width();// get the width of the content div
  //console.log(interwidth);
  countchild = document.getElementById("controller").childElementCount;// count number of book
  numberofboxes = interwidth / boxwidth;// currently the box width is 300px
  if(countchild == Math.floor(numberofboxes) || countchild < Math.floor(numberofboxes))
  {
    sliderwidth = interwidth;
    controllerwidth = interwidth;
    $(".catalogues").width(interwidth/countchild);// make sure the boxes are on the center of the content div
    numberofslides = 1;
    document.getElementById("arrowback").style.display = "none";
    document.getElementById("arrowfor").style.display = "none";
    currentslide = 1;
  }
  else
  {
    numbooksperrow = Math.floor(interwidth/boxwidth);//find out one page can hold how many books per frame
    expectedwidth = Math.ceil(interwidth/numbooksperrow);// find out the catalogues expected width
    $(".catalogues").width(expectedwidth);
    sliderwidth = interwidth;
    controllerwidth = expectedwidth*countchild;
    backandfor = controllerwidth - sliderwidth;
    if(backandfor > sliderwidth){backandfor = sliderwidth;}
    numberofslides = Math.round(controllerwidth / sliderwidth);
    //console.log(numberofslides);
    currentslide = 1;
    document.getElementById("arrowback").style.display = "none";
    document.getElementById("arrowfor").style.display = "block";
  }
  conheight = $(".catalogues").height();
  //con = $("#controller").offset();
  //console.log(controllerwidth);
  document.getElementById("arrowback").style.top = (conheight/2)+"px";
  document.getElementById("arrowfor").style.top = (conheight/2)+"px";
  $("#slideronstaticpage").width(sliderwidth);
  $("#controller").width(controllerwidth);
}
function next(move)
{
  currentslide++;
  if(move != "no"){$("#controller").animate({left: "-="+backandfor+"px"}, ctime);}
  if(currentslide == numberofslides){document.getElementById("arrowfor").style.display = "none";}
  if(currentslide <= numberofslides && numberofslides > 1){document.getElementById("arrowback").style.display = "block";}
}
function back(move)
{
  currentslide--;
  if(move != "no"){$("#controller").animate({left: "+="+backandfor+"px"}, ctime);}
  if(currentslide == 1){document.getElementById("arrowback").style.display = "none";}
  if(currentslide < numberofslides){document.getElementById("arrowfor").style.display = "block";}
}
function refreshframe()
{
  //if(currentslide > 1)
  //{ $("#controller").animate({left: ((currentslide-1)*backandfor)+"px"}, 500); console.log((currentslide-1)*backandfor);}
  restartall();
}
</script>

<center>

