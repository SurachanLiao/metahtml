		<script>
			function LancerSimulationPC(){
				var revenu_monsieur = jQuery('#revenu_monsieur').val();
				var revenu_madame = jQuery('#revenu_madame').val();
				var annees_mariage = jQuery('#annees_mariage').val();
			
				if (revenu_monsieur !="" && revenu_madame !="" && annees_mariage != ""){	
					var montant_total_revenu = parseInt(revenu_monsieur)+parseInt(revenu_madame)
					var difference_revenu = Math.abs(parseInt(revenu_monsieur)-parseInt(revenu_madame));
					var tier_difference = difference_revenu/3;
					var demiduree_mariage = annees_mariage/2;
					var resultat_calcul = tier_difference*demiduree_mariage;
					jQuery("#montant_compensation").val( parseInt(resultat_calcul) );
					jQuery("#resultat_simulateur").attr("style","display:block");
					console.log(montant_total_revenu);
					if (montant_total_revenu > 95000){   
						jQuery("#contact_simulateur").html(resultat_calcul);
					}else{
						jQuery("#contact_simulateur").html(resultat_calcul);
					}
					jQuery("#erreurmessage").attr("style","display:none");
				}else{
					//alert("Tous les champs sont obligatoires ! ");
					jQuery("#erreurmessage").text("Merci de remplir tous les champs !");
					jQuery("#erreurmessage").attr("style","display:block;text-align:center;color:red;");
				}
			}
		</script>
