                            locationId: '_content_diagnostics_ch_de_home_jcr_content_par_carousel',
                            variants: [{"path":"/ch/de/home/default","name":"default","title":"Default","campaignName":"","thumbnail":"/ch/de/home.thumb.png","url":"/ch/de/home/_jcr_content/par/carousel.default.html","tags":[]}],
                            strategy: '',
                            trackingURL: null
                        });

