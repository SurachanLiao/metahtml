<script>
        $(document).ready(function () {
            loadPartialView(); // first time
        }
         function loadPartialView() {

            var cul = 'en';
            var serviceURL = '/' + cul + '/Home/GetWeather1';
            $.ajax({
            type: "GET",
            url: serviceURL,
            dataType: "json",
            success: successFunc
            //error: errorFunc
            });
    }

    function successFunc(data, status) {

    jsonObj = JSON.parse(data);
    // alert(jsonObj.data.current_condition[0].temp_C);
    var degree = jsonObj.data.current_condition[0].temp_C;
    var cloudcover = jsonObj.data.current_condition[0].cloudcover;
    var humidity = jsonObj.data.current_condition[0].humidity;

    var observation_time = jsonObj.data.current_condition[0].observation_time;
    var city = jsonObj.data.request[0].query;
    var time_zone = jsonObj.data.request[0].time_zone;
    var date = jsonObj.data.weather[0].date;
    var visibility = jsonObj.data.current_condition[0].visibility;

    $('#res').text(degree);
    }
    </script>
