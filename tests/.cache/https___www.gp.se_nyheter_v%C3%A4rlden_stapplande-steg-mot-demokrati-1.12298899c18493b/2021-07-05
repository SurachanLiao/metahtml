  // The command "_trackContentPath" is optional but strongly recommended, it should 
  // be replaced with the section path of the displayed page e.g. "/Home/News"
  //********************************************************************************

    var _cInfo = _cInfo || [];
    (function () {
      var contentPath = "";
      if(typeof slm.category != "undefined"){
        if(slm.category.main != "" && slm.category.main != "uncategorized"){
          contentPath = "/" + slm.category.main;
        }
        if(slm.category.sub != ""){
          contentPath += "/" + slm.category.sub;
        }
      }

      if (document.cookie.match("__codnt") === null) {
        window._cInfo.push(
          { cmd: "_trackContentPath", val: contentPath },
          { cmd: "_executeTracking" }
        );

        var ca = document.createElement('script');
        ca.type = 'text/javascript';
        ca.async = true;
        ca.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'analytics.codigo.se/j/75bcfc66e54540209af0656e0139176e.js';
