$( document ).ready(function( $ ) {
	var wsize = 360;
	var hsize = ($(window).width() * 0.2625) - 36;
	if($(window).width()<640){
		wsize = $(window).width() - 16;
		hsize= 180;
	}
	twsize = (wsize / 2) - 2;
	$('#hometop_fandarea').sliderPro({
		width:wsize,
		height:hsize,
		autoplayDelay:12000,
		arrows: false,
