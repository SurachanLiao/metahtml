 importantes."},{"_id":"QJ74EMAAPJERRAWGTKIQUYOAWM","type":"header","level":2,"additional_properties":{"comments":[],"inline_comments":[]},"content":"<mark class=\"hl_yellow\"><b>ESTE VIDEO TE PUEDE INTERESAR</b></mark>"},{"type":"video","_id":"4c2dee38-5495-45ce-94a1-6de735a52061","version":"0.8.0","canonical_url":"/2020/03/28/al-menos-200-peruanos-estan-varados-en-hotel-de-miami-y-con-pocas-probabilidades-de-abordar-vuelos/","canonical_website":"peru21","short_url":"/2020/03/28/al-menos-200-peruanos-estan-varados-en-hotel-de-miami-y-con-pocas-probabilidades-de-abordar-vuelos/","created_date":"2020-03-28T15:20:07Z","last_updated_date":"2020-03-28T15:22:55Z","publish_date":"2020-03-28T15:23:09Z","first_publish_date":"2020-03-28T15:23:09Z","display_date":"2020-03-28T15:23:09Z","headlines":{"basic":"Al menos 200 peruanos están varados en hotel de Miami y con pocas probabilidades de abordar vuelos"},"credits":{},"taxonomy":{"tags":[{"text":"sample"}],"primary_site":{"type":"site","_id":"/videos","version":"0.5.8","name":"Videos","path":"/videos","primary":true},"sites":[{"type":"site","_id":"/videos","version":"0.5.8","name":"Videos","path":"/videos","primary":true}],"primary_section":{"type":"section","_id":"/videos","_website":"peru21","version":"0.6.0","name":"Videos","path":"/videos","primary":true},"sections":[{"type":"section","_id":"/videos","_website":"peru21","version":"0.6.0","name":"Videos","path":"/videos","primary":true}],"seo_keywords":["sample"]},"promo_items":{"basic":{"type":"image","version":"0.5.8","credits":{},"url":"https://dvgnzpfv30f28.cloudfront.net/03-28-2020/t_d5844eadf6b641ac8252cdc7c097e1fe_name_file_640x360_600_v3_.jpg","width":640,"height":360}},"related_content":{"redirect":[],"basic":[]},"owner":{"name":"","sponsored":false},"planning":{"scheduling":{}},"revision":{"published":true},"syndication":{"search":true},"source":{"name":"","system":"video center","edit_url":"https://elcomercio.arcpublishing.com/goldfish/video/5e7f6b2746e0fb00017