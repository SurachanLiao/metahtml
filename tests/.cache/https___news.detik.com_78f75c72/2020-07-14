<style>
.BB_push{
    margin-top: 280px;
}
.BB_sticky{
    position: fixed !important;
    left: 0px!important;
    right: 0px!important;
    z-index: 999!important;
    cursor: pointer;  
}
</style>

<script>  
var bill_gpt = $("#div-gpt-ad-1534319180227-0");
            console.log( "masuk" + bill_gpt); 
function BB_Sticky_noTop(){$('#billboard_banner').css({ top: '37px' });}
function BB_Sticky_withTop(){$('#billboard_banner').css({ top: '110px' });}
function Settimeout_BBSticky(){
    setTimeout(function(){
         $('.header').removeClass('BB_push');
         $('#billboard_banner').removeClass('BB_sticky');
         $('#billboard_banner').removeAttr('style');
         $('.bb_banner').css({ height: '250px' }); 
     }, 3000);
}   
    
function Billboard_Sticky(){
    $('.header').addClass('BB_push');
    $('#billboard_banner').addClass('BB_sticky');
    $('.close_tb').click(function(){
        if  ($('#billboard_banner').hasClass('BB_sticky')){
              BB_Sticky_noTop();
            Settimeout_BBSticky();
        }
    })
    if ($('.top_banner_bar').length > 0) {
        BB_Sticky_withTop();
        Settimeout_BBSticky();

    }
    else{
        BB_Sticky_noTop();
        Settimeout_BBSticky();
    }
}
</script><!-- E:billboard -->
