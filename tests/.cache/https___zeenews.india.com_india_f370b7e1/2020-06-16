<script type="text/javascript">
(function ($) {		
	$(document).ready(function(){			
		
		function fillElementWithNativeAd($elN, slotCodeN, sizeN, targetingN){									
			if (typeof targetingN === 'undefined') { 
				targetingN = {}; 
			} else if ( Object.prototype.toString.call( targetingN ) !== '[object Object]' ) { 
				targetingN = {}; 
			}
			var elIdN = $elN.attr('id');	
			//console.log("elId:" + elId);			
			googletag.cmd.push(function(){ 										
				var slotN = googletag.defineSlot(slotCodeN, sizeN, elIdN); 
				for (var tN in targetingN){ 
					slotN.setTargeting(tN, targetingN[tN]); 
				}
				slotN.addService(googletag.pubads());
				googletag.display(elIdN);
				//googletag.pubads().refresh([slotN]);
			});
		}
		if($(".mini-lead-block > .row div:nth-child(2) > .col-md-4 > .list-story").length){
			var dN = $('<div id="div-al-native1"></div>');
			$(".mini-lead-block > .row div:nth-child(2) > .col-md-4 > .list-story").replaceWith(dN);
			fillElementWithNativeAd(dN, '/11440465/Zeenews_English_Web/Zeenews_English_AL_Native1', ['fluid', [300, 100]], {});
		}
		
	});
})(jQuery);
</script>

<script type="text/javascript">
(function ($) {		
	$(document).ready(function(){			
		
		function fillElementWithNativeAd($elN, slotCodeN, sizeN, targetingN){									
			if (typeof targetingN === 'undefined') { 
				targetingN = {}; 
			} else if ( Object.prototype.toString.call( targetingN ) !== '[object Object]' ) { 
				targetingN = {}; 
			}
			var elIdN = $elN.attr('id');	
			//console.log("elId:" + elId);			
			googletag.cmd.push(function(){ 										
				var slotN = googletag.defineSlot(slotCodeN, sizeN, elIdN); 
				for (var tN in targetingN){ 
					slotN.setTargeting(tN, targetingN[tN]); 
				}
				slotN.addService(googletag.pubads());
				googletag.display(elIdN);
				//googletag.pubads().refresh([slotN]);
				googletag.pubads().enableSingleRequest();
				googletag.pubads().collapseEmptyDivs();
				googletag.enableServices();
			});
		}
		
		var dN = $('<div id="div-al-native1"></div>');
		$(".mini-lead-block > .row div:nth-child(2) > .col-md-4 > .list-story").replaceWith(dN);
		fillElementWithNativeAd(dN, '/11440465/Zeenews_English_Web/Zeenews_English_AL_Native1', ['fluid', [300, 100]], {});

		
	});
})(jQuery);
</script>

