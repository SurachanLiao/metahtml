  <script>
    if (typeof window.readCookie === 'undefined') {
      window.readCookie = function(cookieName) {
      var nameEQ = cookieName + '=';
      var ca = document.cookie.split(';');
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ')
          c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) == 0)
          return c.substring(nameEQ.length, c.length);
      }
      return null;
      }
    }
  </script>

  <script>
    window.dn = window.dn || {};
    window.dn.datalayer = window.dn.datalayer || {};

    var stat = window.readCookie('DN_Stat');
    var visitor = { type: '', id: '' };
  
    if (stat != null) {
      var tokens = decodeURIComponent(escape(window.atob(stat))).split(',');
      var decoded = tokens && tokens.length > 0 ? tokens[0].split('|') : null;

      if (decoded != null && decoded.length > 1) {
        visitor.type = decoded[1];
        visitor.id = decoded[0];
      }
    }

    var data = {
      'context': 'article',
      'visitor': visitor,
      'pageName': 'nyhetsstudio',
      'issue': {
        'extent': 'Full',
        'release': 'Free'
      },
      'article': {
        'authors': 'DN'.split(', '),
        'category': {
          'default': 'nyheter',
          'others': []
        },
        'publishDate': '2020-09-28T11:15:00Z',
        'tags': [],
        'topic': 'nyheter',
        'id': '202009-nyhetsstudio'
      }
    };

    for (var prop in data) {
      if (data.hasOwnProperty(prop)) {
        window.dn.datalayer[prop] = data[prop];
      }
    }
  </script>

