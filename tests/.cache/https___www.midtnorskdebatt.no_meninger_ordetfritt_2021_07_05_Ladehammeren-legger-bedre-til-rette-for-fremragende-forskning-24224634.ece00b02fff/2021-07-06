    if (tw) {
        tw.addEventListener('click', function () {
            var url = "https://twitter.com/intent/tweet?text=" + encodeURIComponent(document.querySelector('title').innerText + " " + location.href)
            var win = window.open(url, '_blank')
            win.focus();
        });
    }
var ep = document.querySelector('.epost');
    if (ep) {
        ep.addEventListener('click', function () {
            var url = "mailto:?subject=Nyhetstips fra midtnorskdebatt.no : " + encodeURIComponent(document.querySelector('title').innerText) + "&body=Nyhetstips fra midtnorskdebatt.no : %0D%0A%0D%0A" + encodeURIComponent(document.querySelector('title').innerText) + "%0D%0A" + encodeURIComponent(location.href)
            var win = window.open(url, '_blank')
            win.focus();
        });
    }
});
</script>
