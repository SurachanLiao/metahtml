</div><script type="text/javascript">
        (function() {
	var elm = document.querySelector(".article-top h1 span");

	// Dersom det er brukt byline-overstyring, splitt ved fÃ¸rste komma
	document.querySelectorAll(".profile .authorName").forEach(elm => {
		var splitChar = ",";
		var splitted = elm.innerText.split(splitChar);
		var first = splitted.shift();
		splitted = splitted.join(splitChar).trim();
		elm.innerHTML = '<span class="name">' + first + '</span><span class="title">' + splitted + '</span>';
	})

	// Dersom det er flere bylines i en polarisStories, tell opp overskytende
	document.querySelectorAll(".text").forEach(elm => {
		var profiles = elm.querySelectorAll(".profile")
		if (profiles.length > 1) {
			var plus = document.createElement("span");
			plus.className="plus";
			plus.innerText= "+" + (--profiles.length);
			profiles[0].querySelector(".name").appendChild(plus);
		}
	});

// Generer header pÃ¥ tag-seksjon
	const tagHeader = document.querySelector(".sec .tag-header")
	if (tagHeader) {
		var url = window.location.href.split("/");
		var tagName = decodeURIComponent(url[url.length-2].replace("_", " "));
		var tagSpan = document.querySelector(".tag-header span span");
		tagSpan.innerHTML = tagName;
	}

	// Distribute side-relation images throughout body text
	const sideRelations = document.querySelector(".article-body .side-relations")
	if (sideRelations) {
		const images = sideRelations.querySelectorAll(".media");
		const body = document.querySelector(".article-body .body");
		const bodyNodes = body.children;
		const distance = Math.floor(bodyNodes.length / (images.length + 1));

		images.forEach(elm => {
			elm.classList.add("old-side");
			let increment = distance + 1;
			if (distance > 0) {
				body.insertBefore(elm, bodyNodes[increment]);
				increment += distance + 1;
			};
		});

		sideRelations.parentNode.removeChild(sideRelations);
	}

	if (!elm) return;

	HTMLElement.prototype.wrapElement = function(string) {
		var wrapper = document.createElement(string);
		this.parentNode.replaceChild(wrapper, this);
		wrapper.appendChild(this);
	}


	/**
	 * Wrap text from word index to word index
	 */
	HTMLElement.prototype.wrapWordInRange = function(wrapper, from, to) {
		var text = this.innerText.trim();
		var index = text.split(" ");

		if (from < 0) from = (index.length + 1) + from;
		if (to < 0) to = (index.length + 1) + to;

		var from_index = index.slice(0, from).join(' ').length;
		var to_index = index.slice(0, to).join(' ').length;

		this.innerHTML = text.slice(0, from_index) + " <" + wrapper + ">" + text.slice(from_index, to_index).trim() + "</" + wrapper + ">" + text.slice(to_index, text.length);

		return this.querySelector('span');
	}

	var loop = function(elm, i, index, startH) {
		// 40 iteration safty
		if (i > 40) return;
		var span = elm.wrapWordInRange('span', --index, -1);

		if (startH == undefined) startH = span.offsetHeight;
		if (startH !== span.offsetHeight) {
			elm.wrapWordInRange('span', ++index, -1);
			return;
		}

		loop(elm, ++i, index, startH);
	}
	elm.wrapElement('span');
	loop(elm, 0, -1);

})();</script>

