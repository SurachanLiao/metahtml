    if (document.location.protocol === 'https:') {
      let noz_logged_in = false,
          c1_logged_in = false;

      function gaC1NozCall(action) {
        if (!document.cookie.match(new RegExp('(^| )tracking_contradict=([^;]+)'))) {
          if (typeof MSOdataLayer == 'undefined') {
            MSOdataLayer = [];
          }

          let userId = 'no',
              trackingId = c1_headers['C1-Meter-Tracking-ID'] ? c1_headers['C1-Meter-Tracking-ID'] : '',
              status = c1_headers['C1-Meter-User-Status'] ? c1_headers['C1-Meter-User-Status'] : '';

          let label = 'id: ' + userId + ', c1: ' + trackingId + ', status: ' + status;

          MSOdataLayer.push({
            'event': 'NOZ.de falsch eingeloggt',
            'action': action,
            'label': label
          });
        }
      }
      if (noz_logged_in && !c1_logged_in) {
        gaC1NozCall('NOZ logged in and C1 not logged in');
      } else if (!noz_logged_in && c1_logged_in) {
        gaC1NozCall('NOZ not logged in and C1 logged in');
      }
              document.addEventListener('DOMContentLoaded', function () {
        if (noz_logged_in && !c1_logged_in) {
