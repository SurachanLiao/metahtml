    $(&#39;input[name*=&quot;[user_id]&quot;]&#39;).val(userID)

    $(&#39;#rescue-button&#39;).click(function(e){
      $(&#39;#rescue-blog-id-select-field&#39;).removeAttr(&#39;disabled&#39;);
      $.ajax({
        type: &#39;POST&#39;,
        url: $(&#39;#rescue-form&#39;).attr(&#39;action&#39;),
        data:$(&#39;#rescue-form&#39;).serialize(),
        success: function() {
          $(&#39;.rescue-toggle&#39;).popover(&#39;hide&#39;);
          location.reload();
        }
        // failure should be handled
      });
      e.preventDefault();
    });
  })();

//]]&gt;
