          </a>
          {{else}}
          {{#ifCanFlagComment id author_id story_author_id is_too_old_to_recommend}}
          <a href="#" class="flag"><span class="icon dkons icon-flag"></span>
            <span class="requires-view-flag-data">({{n_trollrates}})</span>
            Flag
          </a>
          {{else}}
          <span class="flag-disabled"><span class="icon dkons icon-flag"></span>
            <span class="requires-view-flag-data">({{n_trollrates}})</span>
            Flag
          </span>
          {{/ifCanFlagComment}}
          {{/if}}
        </div>
        {{/ifCommentFlaggable}}
</script>

