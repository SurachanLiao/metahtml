  &lt;form class=&quot;edit_story&quot; id=&quot;update-form&quot; action=&quot;/stories/156663/append_liveupdate&quot; accept-charset=&quot;UTF-8&quot; method=&quot;post&quot;&gt;&lt;input name=&quot;utf8&quot; type=&quot;hidden&quot; value=&quot;&amp;#x2713;&quot; /&gt;&lt;input type=&quot;hidden&quot; name=&quot;_method&quot; value=&quot;patch&quot; /&gt;&lt;input type=&quot;hidden&quot; name=&quot;authenticity_token&quot; value=&quot;VoI+P0VVokg0OWrpOERDu+MXW6ziRDSgibnesQWML7MDxCHVUnz+8rLwf1AX8K+zalxtRWCG2/CjKceE16ENEA==&quot; /&gt;
    &lt;label for=&quot;story_Update&quot;&gt;Update&lt;/label&gt;
    &lt;div class=&#39;pop-over-cke-container&#39;&gt;
      &lt;textarea class=&quot;form-control&quot; placeholder=&quot;Write your update...&quot; cols=&quot;20&quot; rows=&quot;6&quot; name=&quot;story[update_text]&quot; id=&quot;story_update_text&quot;&gt;
&lt;/textarea&gt;
    &lt;/div&gt;
    &lt;button name=&quot;button&quot; type=&quot;submit&quot; id=&quot;update-button&quot;&gt;Update this story&lt;/button&gt;
&lt;/form&gt;&lt;/div&gt;
&lt;script&gt;
//&lt;![CDATA[

  (function(){
    $(&#39;#story_update_text&#39;).ckeInline();
  })();

  // this is garbage, needs much better
  (function() {
    var disabledButton = false;

    $(&#39;#update-button&#39;).click(function(e){
      e.preventDefault();

      if (disabledButton) return;
      disabledButton = true

      $.ajax({
        type: &#39;PUT&#39;,
        url:  $(&#39;#update-form&#39;).attr(&#39;action&#39;),
        data: $(&#39;#update-form&#39;).serialize(),
        success: function() {

          $(&#39;#update-toggle&#39;).popover(&#39;hide&#39;);

          var cacheInvalidator = +(new Date())
          var newUrl = location.origin + location.pathname + &#39;?t=&#39; + cacheInvalidator
          location.replace(newUrl);
        },
        error: function(response) {
          disabledButton = false;

          DailyKos.showAlert(&#39;Update error. Please check your connection and try again.&#39;);
          console.log(&quot;Live Update Error&quot;);
          console.log(response);
        }
      });
    });
  })();

//]]&gt;
&lt;/script&gt;' data-html='true' data-placement='right' data-toggle='popover' href='#' id='update-toggle'>
<span class='share-icon-wrapper'>
<span class='dkons clipboard-pencil social-icon'></span>
</span>
Update
</a>
