      <div class='author_info_row'>
        Comment Count: {{n_comments}}
      </div>
      <div class='author_info_row'>
        Popular Tags: {{showTags popular_tags}}
      </div>
      <div class='author_info_row badges'>
        {{#each badges}}
        {{badgeUrl .}}
        {{/each}}
      </div>
    </div> <!-- .right_author_info -->
    <div class='bottom_author_info'>
      {{authorSig user_sig}}
    </div> <!-- .bottom_author_info -->
  </div> <!-- author_bio_inner -->
  <div class="author_arrow"></div>
</script>

<script id="comment-actions-partial" type="text/x-handlebars-template">
  {{#if canReply}}
  {{#unless replying}}
  <div class="pull-left">
    <a href="#" class="reply requires-comment">
      <span class="icon dkons glyphicons-reply"></span>
      Reply
    </a>
  </div>
  {{/unless}}
  {{/if}}
  {{#ifcanRecComment author_id}}
  {{#if recommenders_text}}
  <div class="recommend_buttons recommended pull-left" data-comment-id="{{id}}">
    {{else}}
    <div class="recommend_buttons pull-left" data-comment-id="{{id}}">
      {{/if}}
      <a href="#" class="recommend-wrapper unrecommend requires-recommend">
        <span class="icon dkons glyphicons-star"></span>
        <span class="count rec-count">{{n_recrates}}</span>
      </a>
      <a href="#" class="recommend-wrapper recommend">
        <span class="icon dkons glyphicons-star"></span>
        <span class="count rec-count">{{n_recrates}}</span>
      </a>
      <span class='inline_notification comment_recommend_login'>
        Please <a href='/login?return_to=/stories/2005/10/14/156663/-&return_anchor=comment_{{id}}'>log in</a> or <a href="/signup?return_to=/stories/2005/10/14/156663/-&return_anchor=comment_{{id}}&return_topic={{truncatedSubject}}">sign up</a> to continue.
      </span>
      {{else}}
      {{#if recommenders_text}}
      <div class="recommend_buttons recommended pull-left" data-comment-id="{{id}}">
        {{else}}
        <div class="recommend_buttons pull-left" data-comment-id="{{id}}">
          {{/if}}
          <span class="recommend-wrapper recommend-disabled">
            Recommended <span class="count rec-count">{{n_recrates}}</span> time{{simplePlural n_recrates}}
          </span>
          {{/ifcanRecComment}}
        </div>
        {{#ifCommentFlaggable author_id story_author_id}}
        <div class="flag-controls requires-hide-perm pull-left">
          {{#if flagged}}
          <a href="#" class="unflag"><span class="icon dkons icon-flag"></span>
            <span class="requires-view-flag-data">({{n_trollrates}})</span>
