    <script>
      //actualizar ad top
      function updateAd(){
        // console.log('update ad')
        if ($('.add-ajust-top').length) {
          // console.log('exist div ad')
          if ($('.add-ajust-top').find('img').length) {
            // console.log('existe img')
            $('.add-ajust-top').parent('ins').removeAttr('data-revive-loaded');
            reviveAsync['a4b6dc755d16d963fc228b92512e7492'].refresh(); 
          }
        }
      }
      function updateAdM(){
        // console.log('update ad')
        if ($('.add-ajust-top-m').length) {
          // console.log('exist div ad')
          if ($('.add-ajust-top-m').find('img').length) {
            // console.log('existe img')
            $('.add-ajust-top-m').parent('ins').removeAttr('data-revive-loaded');
            reviveAsync['a4b6dc755d16d963fc228b92512e7492'].refresh(); 
          }
        }
      }
      function updateAdSideB(){
        // console.log('update ad')
        if ($('.add-ajust-side-down').length) {
          // console.log('exist div ad')
          if ($('.add-ajust-side-down').find('img').length) {
            // console.log('existe img')
            $('.add-ajust-side-down').parent('ins').removeAttr('data-revive-loaded');
            reviveAsync['a4b6dc755d16d963fc228b92512e7492'].refresh(); 
          }
        }
      }
      function updateAdSideT(){
        // console.log('update ad')
        if ($('.add-ajust-side-top').length) {
          // console.log('exist div ad')
          if ($('.add-ajust-side-top').find('img').length) {
            // console.log('existe img')
            $('.add-ajust-side-top').parent('ins').removeAttr('data-revive-loaded');
            reviveAsync['a4b6dc755d16d963fc228b92512e7492'].refresh(); 
          }
        }
      }
        window.addEventListener('load', function() {
            $( document ).ready(function() {
                setInterval(function(){
                updateAd();
                updateAdM();
                updateAdSideB();
                updateAdSideT();
                }, 10000); // se actualiza cada 10 segundos
            });
        });
    </script>

