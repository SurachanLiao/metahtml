    <script type="text/javascript">
        window.addEventListener('load', function() {
            simpleStickySidebar('#sidebar-l', {
                container: '#side-l-out',
                topSpace: document.querySelector('.header').getBoundingClientRect().height + 20, // or any no i.e 20
                bottomSpace : 20
            });
        });
    </script>
    <script>
      // sidebar r fixed
            userLogged = 0;
      // console.log('usuario logeado? ', userLogged);
        window.addEventListener('load', function() {
            jQuery(window).on('scroll', function () {
                
                scrollTop = $(window).scrollTop();
                elementOutOffset = $("#sidebar-r-out").offset().top;
                elementOffset = $('#sidebar-r').offset().top;

                changeToRelative = 1550;
                topRelative = changeToRelative - elementOutOffset;
                
                publiBottomOffset = $("#publi-side-con").offset().top;

                if (userLogged == 1) {
                adminBarH = $( "#wpadminbar" ).height();
                distance = (elementOffset - scrollTop);
                } else {
                navBarH = $( "header" ).height();
                // fixed and relative all sidebar
                if (scrollTop > elementOutOffset && scrollTop < changeToRelative) {
                    setWidth = $('#sidebar-r-cont').innerWidth() + 'px';
                    addNavBarH = navBarH + 'px';
                    $('#sidebar-r-cont').addClass('position-fixed').css( {'top': addNavBarH, 'width': setWidth} );
                } else if (scrollTop <= elementOutOffset) {
                    $('#sidebar-r-cont').removeClass('position-fixed').removeAttr("style");
                } else if (scrollTop > changeToRelative) {
                    $('#sidebar-r-cont').removeClass('position-fixed').addClass('position-relative').css({'top': topRelative+navBarH+'px',});
                    // fixed last publicidad
                    if (scrollTop > publiBottomOffset) {
                    navBarH = $( "header" ).height();
                    addNavBarH = navBarH + 'px';
                    setWidth = $('#sidebar-r-cont').innerWidth() + 'px';
                    $('#publi-side-in').addClass('position-fixed').css( {'top': addNavBarH, 'width': setWidth} );
                    /* $('#sidebar-l').css("top", '-100px');
                    $('#sidebar-l').css("transition", 'top .8s cubic-bezier(0.77, 0, 0.175, 1) ');*/
                    } else {
                    $('#publi-side-in').removeClass('position-fixed').removeAttr("style");
                    /* $('#sidebar-l').css("top", '50px'); */
                    }
                }
                }
                // console.log('distancia entre el top y el sidebar: ',distance);
            });
        });
    </script>
    <!-- new audio player -->
    <script>
        window.addEventListener('load', function() {
            jQuery(document).ready(function($){
                var ipaddress = "172.69.34";
                /* var blog_id = 1; */
                if(ipaddress === "170.80.24"){
                // ips internas
                var tracks = [
                    {'track':0,'name':'Durango 96.5 FM','file':'http://172.16.8.30:8010/Durango'},
                    {'track':1,'name':'Parral 96.9 FM','file':'http://172.16.8.30:8040/Parral'},
                    {'track':2,'name':'Santiago 89.3 FM','file':'http://172.16.8.30:8060/Santiago'},
                    {'track':3,'name':'Jiménez 105.1 FM','file':'http://172.16.8.30:8070/Jimenez'}
                ];
                } else {
                // ips externas
                var tracks = [
                    {'track':0,'name':'Durango 96.5 FM','file':'//radio.latremenda.com.mx/Durango'},
                    {'track':1,'name':'Parral 96.9 FM','file':'//radio.latremenda.com.mx/Parral'},
                    {'track':2,'name':'Santiago 89.3 FM','file':'//radio.latremenda.com.mx/Santiago'},
                    {'track':3,'name':'Jiménez 105.1 FM','file':'//radio.latremenda.com.mx/Jimenez'}
                ];
                }
                $.each(tracks, function (i, track) {
                $('#Radios').append($('<option>', { 
                    value: track.track,
                    text : track.name 
                }));
                });
                var playing = false;
                if (playing == false) {
                $('#pButton').addClass('fa-play');  
                }
                
                $('#Radios').on('change', function() {
                if (playing == true) {
                    playing = false;
                    $('#pButton').removeClass('fa-stop').addClass('fa-play');
                    $('#playlist-audio')[0].pause();
                    $('#playlist-audio')[0].load();
                } 
                src = tracks[this.value].file
                $('#playlist-audio').prop({'src': src});
                });
