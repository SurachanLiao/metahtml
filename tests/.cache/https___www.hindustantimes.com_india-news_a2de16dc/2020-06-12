	<script>

	function getCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for (var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ')
				c = c.substring(1, c.length);
			if (c.indexOf(nameEQ) == 0)
				return c.substring(nameEQ.length, c.length);
		}
		return null;
	}

	$(document)
			.ready(
					function() {
						var cookie_token = getCookie("token");
						var user_name = getCookie("name");

						if (cookie_token && user_name) {

							$("#divsso").removeClass("sign-in");
							$("#divsso").addClass("sign-in on");

							var len = user_name.length;
							var u_name = "";
							if (len <= 15)
								u_name = user_name;
							else {
								var res = user_name.split(" ");
								var uName = res[0] + " " + res[1];

								if (uName.length <= 15)
									u_name = uName;
								else {
									if (res[0].length <= 15)
										u_name = res[0];
									else
										u_name = user_name.substring(0, 13)
												+ "..";
								}
							}
							$("#divSignIn")
									.html(
											"<a class=\"sign-in-link\">"
													+ u_name
													+ "<i class=\"fa fa-angle-down\"></i></a>");
						}
					});

	$(document).ready(function() {

		$(".sign-in.on .sign-in-link").click(function() {
			$("#divSignOut").toggleClass("block");

		});

	});

	function changeIconCSS() {
		$("#searchIcon").removeClass();
		$("#searchIcon").addClass("fa fa-times");

		$("#liSearch").attr("onclick", "undoClass()");

	}

	function undoClass() {
		$("#searchIcon").removeClass();
		$("#searchIcon").addClass("fa fa-search");
		$("#liSearch").attr("onclick", "changeIconCSS()");

	}
	
	function SubmitMyForm() {
		var txtSearchValue = document
		.getElementById("newsearchbox_new").value;	
		if (txtSearchValue != "" && txtSearchValue != null && txtSearchValue.length > 1) {
		var finalSearchKeywords = encodeURIComponent(txtSearchValue);
		setTimeout(function(){window.location.href = "https://www.hindustantimes.com/search?q="
		+ finalSearchKeywords},50);
		}
		else{
		alert("minimum 2 characters are required.");
		return false;
		}
		}

	$(document).ready(function() {
		$('#newsearchbox_new').keydown(function(e) {
			if (e.which === 13) {
				if ($('#newsearchbox_new').val() != '') {
					SubmitMyForm();
					return false;
				}
			}
		});
	});
</script>
