<script>    
    var stopStatus=["11","12","13","14","15","16","111","116","123","124","126"];
    var statusAbandoned=["113","130","131"];
    var activeTeamId="4";
    var teamAMatchData={
    		"teamInfo":"",
    		"innings":[]
     };
     var teamBMatchData={
    			"teamInfo":"",
    			"innings":[]
    	 };
    var scoreCardURL=""; 
	var isLive;

    function getStickyLiveScore()
    {
    	$.ajax({
    		url:"https://www.hindustantimes.com/static-content/10s/cricket-liupre.json",
    		//url:"http://www.preview.prd.hindustantimes.com:3701/eom/PortalConfig/Common/external/cricket-liupre.json",
    		dataType:"json",
    		success:function(response){ 
    			var worldCupMatch=response.live.filter(function(item){return (item.matchdetail.team_Home==activeTeamId || item.matchdetail.team_Away==activeTeamId) && item.matchdetail.status_Id!=="120";;});	 
    			if(worldCupMatch.length>0){    		
    	    		isLive=true;
    	    		showInfinite=false;
    	    		showsStickyLiveMatch(worldCupMatch);
    	    	}
    	    	else{
    	    		var matchUrl = "/sticky-feed/india-vs-australia";
    	    		isLive=false;
    	    		$.ajax({
    	    			url:matchUrl,
    	    			dataType:"json",
    	    			success:function(responseID){
    	    			var matchId= responseID.matchID;
    	    			if(matchId!=0){
    	    			worldCupMatch=response.results.filter(function(item){return (item.matchdetail.team_Home==activeTeamId || item.matchdetail.team_Away==activeTeamId) && (item.matchdetail.match.code==matchId);});
    	    			if(worldCupMatch.length>0){
    	    			showInfinite=false;
    	    			}else{
    	    				showInfinite=true;
    	    			}
    	    			showsStickyLiveMatch(worldCupMatch);
    	    			}
    	    			else{
    	    				showInfinite=true;
    	    			}
    	    			}
    	    		});			
    	    	}
    			
    		}		
    	});
    	
    }    	
    	
    function showsStickyLiveMatch(worldCupMatch)
    {    	
    	var liveMatchDiv="";  
    	var isDouble=false;
    	if(worldCupMatch.length>1){
    		isDouble=true;
    	}
    	if(worldCupMatch.length>0){
	        response=worldCupMatch;
	    	liveMatchDiv+="<div class='sticky-bottom'>";
	        liveMatchDiv+="<div class='stickyfooter'>";
	        liveMatchDiv+="<div class='container'>";
	        liveMatchDiv+="<div class='stickyslider'>";
	        liveMatchDiv+="<div class='swiper-wrapper'>";
	        	        
	        $.each(response,function(index, liveData){	
	    		if(index>1){return false;}
	    		var wickets="";
	    		var matchNo=liveData.matchdetail.match.number;
	    		var matchID=liveData.matchdetail.match.code;
	    		var teamHomeId=liveData.matchdetail.team_Home;
	    		var teamAwayId=liveData.matchdetail.team_Away;
	    		var equation=liveData.matchdetail.equation;
	    		var matchType=liveData.matchdetail.match.type;
	    		var statusID=liveData.matchdetail.status_Id;
	    		var statusMsg, msgColor="";
	    		var team_Home = liveData.matchdetail.team_Home == liveData.teamlist[0].team_Id ? liveData.teamlist[0].name_Full : liveData.teamlist[1].name_Full;
	    		var team_Away = liveData.matchdetail.team_Away == liveData.teamlist[0].team_Id ? liveData.teamlist[0].name_Full : liveData.teamlist[1].name_Full;
	    		var team_Home_Slug = team_Home.toLowerCase().replace(/ /g,'-');
	    		var team_Away_Slug = team_Away.toLowerCase().replace(/ /g,'-');
	    		scoreCardURL="https://www.hindustantimes.com/cricket/live-full-scorecard-"+team_Home_Slug+"-vs-"+team_Away_Slug+"-"+matchID+"/";
	    		
	    		getTeamWiseData(liveData,teamHomeId,teamAwayId);
	    		var teamA=teamAMatchData.teamInfo.name_Short;
	    		var teamB=teamBMatchData.teamInfo.name_Short;
	    		var teamFullA=teamAMatchData.teamInfo.name_Full;
	    		var teamFullB=teamBMatchData.teamInfo.name_Full;
	    		var teamImgA="https://www.hindustantimes.com/images/app-images/cricket/world-cup/"+teamA+".svg";
	    		var teamImgB="https://www.hindustantimes.com/images/app-images/cricket/world-cup/"+teamB+".svg";
	    	
	    		
	    		if(stopStatus.includes(statusID)){
	    			statusMsg="Play interrupted";
	    			msgColor="stopped-rain";
	    		}
	    		else if(statusAbandoned.includes(statusID)){
	    			statusMsg="Match Abandoned";
	    			msgColor="stopped-rain";
	    		}			
	    		else if(statusID=="110"){
	    			statusMsg="Innings Break";
	    		}
	    		else if(statusID=="118"){
	    			statusMsg="Play stopped due to rain";
	    			msgColor="stopped-rain";
	    		}
	    		else{
	    			statusMsg="";
	    		}   		
	    		
		        liveMatchDiv+="<div class='swiper-slide'>";
		        if(isDouble){
		        	liveMatchDiv+="<div class='flex-divs flex-100' onclick=\"sendURL('"+scoreCardURL+"')\">";
		        }else{
		        	liveMatchDiv+="<div class='flex-divs flex-pd-100' onclick=\"sendURL('"+scoreCardURL+"')\">";	
		        }
		        liveMatchDiv+="<div class='match-heading'>";
		        if(isLive){
			        liveMatchDiv+="<div class='match-live'>";
			        liveMatchDiv+="<i class='fa fa-circle'></i><span>LIVE</span>";
			        liveMatchDiv+="</div>";
		        }else{
			        liveMatchDiv+="<div class='result'>";
			        liveMatchDiv+="<span>RESULT</span>";
			        liveMatchDiv+="</div>";
		        }
		        liveMatchDiv+="<strong>"+response[0].matchdetail.series.name+"</strong>";
		        liveMatchDiv+="</div>";
	        
		        liveMatchDiv+="<ul class='team-centre flex-divs'>";
		        	
		        liveMatchDiv+="<li class='team'>";
		        liveMatchDiv+="<div class='team-thumb'>";
		        liveMatchDiv+="<img src='"+teamImgA+"' alt='' title=''>";
		        liveMatchDiv+="<span class='team-name'>"+teamFullA+"</span>";
		        liveMatchDiv+="</div>";
		        liveMatchDiv+="<div class='score'>";
		        if(teamAMatchData.innings.length>0){
			        var teamAInnings=teamAMatchData.innings.reverse();
			        if(matchType==='Test'){		
				        var total=teamAMatchData.innings.length;
				        var showFollow=false;
				        $.each(teamAInnings,function(i, item){
				        	showFollow=item.hasFollowedOn || false;
				        });
				        $.each(teamAInnings,function(i, item){
				        	if(i===0 && showFollow && total>0){
				        		showFollow ? followOn = "<span class='follow-score'>(f/O)</span>, " : followOn="";
				        	}else{
				        		followOn='';
				        	}
				        	item.wickets==='10' ? wickets = "" : wickets = "/"+item.wickets;
				        	item.isdeclared ? declared = "<span class='follow-score'>(D)</span>" : declared ="";
							liveMatchDiv+="<strong>";
							liveMatchDiv += item.total + wickets + declared;
							if(i<total-1 && followOn===''){
								liveMatchDiv +=", ";
							}
							liveMatchDiv+= followOn;
							liveMatchDiv+="</strong>";
							
				        });
				        
					    //liveMatchDiv+=inningRuns.substring(0,(inningRuns.length-2));
				        
			        }else{
				        liveMatchDiv+="<strong>"+teamAMatchData.innings[0].total+"/"+teamAMatchData.innings[0].wickets+"</strong>";
				        liveMatchDiv+="<span class='mr-0'>"+teamAMatchData.innings[0].overs+" ovs</span>";
			        }
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        } else{
			        liveMatchDiv+="<div class='score'>";
			        liveMatchDiv+="<strong class='mar-0'>Yet To Bat</strong>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
			        if(!isLive){
				   		liveMatchDiv+="<li>";
				        liveMatchDiv+="<div class='beat-team'>"+equation+"</div>";
				        liveMatchDiv+="</li>";
			        }
			        else{
			        if(statusMsg!=""){
		    			liveMatchDiv+="<li>";
		    			liveMatchDiv+="<div class='"+msgColor+"'>"+statusMsg+"</div>";
		    			liveMatchDiv+="</li>"; 
		    		}
		    		else{
		    			liveMatchDiv+="<li><div class='vs'>VS</div></li>"; 
		    		}
		        }
		       
		        liveMatchDiv+="<li class='team team-b'>";
		        liveMatchDiv+="<div class='team-thumb'>";
		        liveMatchDiv+="<img src='"+teamImgB+"' alt='' title=''>";
		        liveMatchDiv+="<span class='team-name'>"+teamFullB+"</span>";
		        liveMatchDiv+="</div>";
		        liveMatchDiv+="<div class='score'>";
			    if(teamBMatchData.innings.length > 0){
			    	var teamBInnings=teamBMatchData.innings.reverse();
			        if(matchType==='Test'){
			        var declared="";
			        var followOn="";
			        var total=teamBMatchData.innings.length;
			        var showFollow=false;
			        $.each(teamBInnings, function(i, item){
			        	showFollow=item.hasFollowedOn || false;
			        });
			        $.each(teamBInnings, function(i, item){
			        	if(i===0 && showFollow && total>0){
				        	showFollow ? followOn = "<span class='follow-score'>(f/O)</span>, " : followOn="";
				        	}else{
				        		followOn='';
				        	}
			        	item.wickets==='10' ? wickets = "" : wickets = "/"+item.wickets;
			        	item.isdeclared ? declared = "<span class='follow-score'>(D)</span>" : declared ="";
						liveMatchDiv+="<strong>";
						liveMatchDiv += item.total + wickets + declared;
						if(i<total-1 && followOn===''){
							liveMatchDiv +=", ";
						}
						liveMatchDiv+= followOn;
						liveMatchDiv+="</strong>";
						
			        });
			        }else{
				        liveMatchDiv+="<strong>"+teamBMatchData.innings[0].total+"/"+teamBMatchData.innings[0].wickets+"</strong>";
				        liveMatchDiv+="<span>"+teamBMatchData.innings[0].overs+" ovs</span>";
				    }
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
		        else{
			        liveMatchDiv+="<div class='score'>";
			        liveMatchDiv+="<strong class='mar-0'>Yet To Bat</strong>";
			        liveMatchDiv+="</div>";
			        liveMatchDiv+="</li>";
		        }
		        liveMatchDiv+="</ul>";
		       	        
		        
		        liveMatchDiv+="<a class='match-centre'>Match Centre</a>";
		        liveMatchDiv+="</div>";
		        liveMatchDiv+="</div>";
	        
	        });
	        
	        liveMatchDiv+="</div>";			
	    	//liveMatchDiv+="<div class='swiper-button-next swiper-button-black'></div>";
	    	//liveMatchDiv+="<div class='swiper-button-prev swiper-button-black'></div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	liveMatchDiv+="</div>";
	    	
	    	$("#sticky-match-card").html(liveMatchDiv);
    	}  
    }

    function getTestInnings(innings,t1,t2){ 
    	teamAInning=[];
    	teamBInning=[];
      	 $.each(innings,function(index,elm){
    		if (elm.battingteam === t1) {
    			teamAInning.push(elm);
    		} else if (elm.battingteam === t2) {
    			teamBInning.push(elm);
    		}
    		});
    		teamAInning = teamAInning.reverse();
    		teamBInning = teamBInning.reverse();
    	}
    
    function getTeamWiseData(data,homeID,awayID){
   	 teamAMatchData={
   			"teamInfo":"",
   			"innings":[]
   	 };
   	 teamBMatchData={
   				"teamInfo":"",
   				"innings":[]
   		 };
   	 $.each(data.teamlist,function(index,elm){
   			if (elm.team_Id === homeID) {
   				teamAMatchData.teamInfo=elm;
   			} else if (elm.team_Id === awayID) {
   				teamBMatchData.teamInfo=elm;
   			}
   		});
   	 $.each(data.innings,function(index,elm){
   			if (elm.battingteam === homeID) {
   				teamAMatchData.innings.push(elm);
   			} else if (elm.battingteam === awayID) {
   				teamBMatchData.innings.push(elm);
   			}
   		});
    }
    
    $(document).ready(function(){
    getStickyLiveScore();
   	setInterval(function(){getStickyLiveScore(); }, 30000);
    	
    });
   
    function sendURL(url){
    	if(url==""){
    		window.location=scoreCardURL;
    	}
    	else{
    		window.location=url;
    	}
    	
    }
   
