<script>/*<![CDATA[*/ var dataLayer = window.dataLayer || [];
        var infoArray = {};
        gooten_photo_collection = window.gooten_photo_collection || [];
        PIO.config({
            // set the widget endpoint url
            url: "https://widget.print.io/widget/b4309023-78ee-487b-9112-58da8835bacb/",
            // api key
            recipeId: "b4309023-78ee-487b-9112-58da8835bacb",
            helpUrl: "https://www.photosales.bloxcms.com/site/forms/contact_us/?domain=dentonrc.com",
            // preload data into localstorage
            preLoad: { on: true},
            // disable upload
            canUseUpload: false,
            // restore image collection
            preserveCollection: true, // does nothing
            // hide the resume item popup
            hideResumeItemPopup: true,
            // show all products available for the region
            useDefaultProductSettings: true,
            // hide Products that are coming soon
            hideComingSoonProducts: true,
            // disable user's progress in the browser URL
            usePushState: false, // disabled to allow widget fix to function
            fns: {onEvent: function(key, val){
        // Grab user cart data
        if (window.localStorage) {
            infoArray.userCart = JSON.parse( localStorage.getItem('pio.1.cart') );
        }
        if(key==="section-init"){
            // store template name
            infoArray.tplName = val.tplName;
            
            if (val.tplName == 'tpl-product-b' && "name" in infoArray && "productId" in infoArray) {
                // Product detail impression
                dataLayer.push({
                    'ecommerce': {
                        'detail': {
                            'actionField': {'list': ''},
                            'products': [{
                                'name': infoArray.name,
                                'id': infoArray.productId,
                                'price': '',
                                'brand': 'Denton Record-Chronicle',
                                'category': 'Photo sales',
                                'variant': 'Gooten'
                            }]
                        }
                    },
                    'event':'tncms.event.trigger',
                    'tncms': {
                       'event': {
                          'trigger': {
                             'action': 'product detail view',
                             'category': 'Photo sales',
                             'label': 'viewed product options'
                          }
                       }
                    }
                });
            }
        } else if (key==="address-change"){
            // Checkout Workflow: Step 2 - Shipping address
            dataLayer.push({
                'event': 'checkout',
                'ecommerce': {
                    'checkout': {
                        'actionField': {'step': 2, 'option': 'shipping address'},
                        'products': infoArray.checkoutItems
                    }
                },
                'event':'tncms.event.trigger',
                'tncms': {
                   'event': {
                      'trigger': {
                         'action': 'checkout',
                         'category': 'Photo sales',
                         'label': 'shipping address'
                      }
                   }
                }
            });
            
        } else if(key==="product-focus-change"){
            // Add values to infoArray for later use
            if (val && val.name && val.id) {
                infoArray.name = val.name;
                infoArray.productId = val.id;
            }
        } else if(key==="onCartChange"){
            var removedItem = null;
            var addedItem = null;
            
            // Cart content has changed 
            if (infoArray.eventAction == 'Customization finished' 
                || infoArray.eventAction == 'Product quantity increased'
                || infoArray.eventAction == 'Item removed from cart'
                || infoArray.eventAction == 'Product quantity decreased') {
                
                // New item added
                if (infoArray.eventAction == 'Customization finished') {
                    addedItem = infoArray.userCart.items[infoArray.userCart.items.length - 1];
                    addedItem.change = 1;
                }
                // Increase or decrease in item quantity
                else {
                    // Run compare to find the change
                    if (infoArray.items[0] && infoArray.userCart.items[0]) {
                        // Run through infoArray items and find difference
                        for (var i = 0; i < infoArray.items.length; i++) {
                            if (!infoArray.userCart.items[i] 
                                || (infoArray.items[i].id != infoArray.userCart.items[i].id)) {
                                // Item removed
                                removedItem = infoArray.items[i];
                                removedItem.change = infoArray.items[i].quantity;
                                break;
                            }
                            if (infoArray.items[i].quantity != infoArray.userCart.items[i].quantity) {
                                // Increase or drecrease in item quantity
                                
                                if (infoArray.items[i].quantity < infoArray.userCart.items[i].quantity) { 
                                    // Quantity increase
                                    infoArray.eventAction = 'Product quantity increased';
                                    addedItem = infoArray.userCart.items[i];
                                    addedItem.change = (infoArray.userCart.items[i].quantity - infoArray.items[i].quantity);                                                                    break;
                                } else if (infoArray.items[i].quantity > infoArray.userCart.items[i].quantity) {
                                    // Quantity decrease
                                    infoArray.eventAction = 'Product quantity decreased';
                                    removedItem = infoArray.userCart.items[i];
                                    removedItem.change = (infoArray.items[i].quantity - infoArray.userCart.items[i].quantity);
                                    break;
                                }
                            }
                        }
                    } else {
                        // Only item was removed
                        infoArray.eventAction = 'Product quantity decreased';
                        removedItem = infoArray.items[0];
                        removedItem.change = removedItem.quantity;
                    }
                }
            }
            // Update infoArray items list
            infoArray.items = val.items;
            
            if (removedItem != null) { 
                // Removed product from cart
                dataLayer.push({
                    'ecommerce': {
                        'currencyCode': 'USD',
                            'remove': {
                            'products': [{
                                'name': removedItem.sku,
                                'id': removedItem.productId,
                                'price': '',
                                'brand': 'Denton Record-Chronicle',
                                'category': 'Photo sales',
                                'variant': 'Gooten',
                                'quantity': removedItem.change
                            }]
                        }
                    },
                    'event':'tncms.event.trigger',
                    'tncms': {
                        'event': {
                            'trigger': {
                                'action': 'remomve from cart',
                                'category': 'Photo sales',
                                'label': 'removed product from cart'
                            }
                        }
                    }
                });
            } else if (addedItem != null) {
                if (addedItem != 'undefined'){
                    // Added product to cart
                    dataLayer.push({
                        'ecommerce': {
                            'currencyCode': 'USD',
                                'add': { 
                                'products': [{ 
                                    'name': addedItem.sku,
                                    'id': addedItem.productId,
                                    'price': '',
                                    'brand': 'Denton Record-Chronicle',
                                    'category': 'Photo sales',
                                    'variant': 'Gooten',
                                    'quantity': addedItem.change
                                }]
                            }
                        },
                        'event':'tncms.event.trigger',
                        'tncms': {
                           'event': {
                              'trigger': {
                                 'action': 'add to cart',
                                 'category': 'Photo sales',
                                 'label': 'added product to cart'
                              }
                           }
                        }
                    });
                }
            }
            // Update cart count
            updateCartCount();
        } else if(key==="analytics-event"){
            // Store event action and category
            infoArray.eventCategory = val.eventCategory;
            infoArray.eventAction = val.eventAction;
            if (val.eventAction === 'Checkout button pressed'){
                // Checkout Workflow: Step 1 - Start

                // Set up final products array
                var checkoutArray = [];
                for (var i = 0; i < infoArray.userCart.items.length; i++) {
                    var item = infoArray.userCart.items[i];
                    checkoutArray.push({ 
                        "name": item.sku,
                        "id": item.productId,
                        "price": '', // why not item.price?!
                        "brand": 'Denton Record-Chronicle',
                        'category': 'Photo sales',
                        'variant': 'Gooten',
                        'quantity': item.quantity
                    });
                }                
                infoArray.checkoutItems = checkoutArray;
                dataLayer.push({
                    'event': 'checkout',
                    'ecommerce': {
                        'checkout': {
                            'actionField': {'step': 1, 'option': 'start'},
                            'products': infoArray.checkoutItems
                        }
                    },
                    'event':'tncms.event.trigger',
                    'tncms': {
                       'event': {
                          'trigger': {
                             'action': 'checkout',
                             'category': 'Photo sales',
                             'label': 'start'
                          }
                       }
                    }
                });
            } else if (val.eventAction === 'Shipping finished'){
                // Checkout Workflow: Step 3 - Shipping options
                dataLayer.push({
                    'event': 'checkout',
                    'ecommerce': {
                        'checkout': {
                            'actionField': {'step': 3, 'option': 'shipping options'},
                            'products': infoArray.checkoutItems
                        }
                    },
                    'event':'tncms.event.trigger',
                    'tncms': {
                       'event': {
                          'trigger': {
                             'action': 'checkout',
                             'category': 'Photo sales',
                             'label': 'shipping options'
                          }
                       }
                    }
                });
            }
        } else if(key==="analytics-addTransaction"){
            // Purchase transaction
            // https://developers.google.com/analytics/devguides/collection/analyticsjs/ecommerce
            dataLayer.push({
                'ecommerce': {
                    'purchase': {
                        'actionField': {
                            'id': val.id,
                            'affiliation': 'Denton Record-Chronicle',
                            'revenue': val.revenue,
                            'tax': val.tax,
                            'shipping': val.shipping,
                        },
                        'products': infoArray.checkoutItems
                    }
                },
                'event':'tncms.event.trigger',
                'tncms': {
                   'event': {
                      'trigger': {
                         'action': 'purchase',
                         'category': 'Photo sales',
                         'label': 'purchase'
                      }
                   }
                }
            });
            // Leaving logs in place for duration of experiment
            __tnt.log('\nEvent: analytics-addTransaction');
            __tnt.log(val);
        } else if(key==="analytics-send"){
            __tnt.log('\nEvent: analytics-send');
            __tnt.log(val);
        } else if(key==="analytics-addItem"){
            __tnt.log('\nEvent: analytics-addItem');
            __tnt.log(val);
        } else if(key==="onOrderSubmit"){
            __tnt.log('\nEvent: onOrderSubmit');
            __tnt.log(val);
        }
    }},
            // custom widget style
            style: {
                showHeader: true,
                cssUrl: 'https://bloximages.newyork1.vip.townnews.com/dentonrc.com/shared-content/art/tncms/templates/libraries/flex/components/utility/resources/styles/photo.sales.gooten.d923772f9879bbb606b2f68b8a7a2623.css'     
            }
        });
        // open on purchase click
        document.addEventListener("click", function(e) {
            if (e.target.matches(".tnt-photo-purchase")) {
                e.preventDefault();
                var photoMeta = JSON.parse(e.target.dataset.photo);
                gooten_photo_collection.unshift(photoMeta.url);
                PIO.open({
                    // pass the photo collection
                    images: gooten_photo_collection,
                    // order-level metadata
                    meta: photoMeta
                });
                // gooten iframe hack/fix
                document.getElementById("__pio").src = "https://widget.print.io/widget/b4309023-78ee-487b-9112-58da8835bacb/";
            }
        });
        // Cart count
        function updateCartCount(){
            var cartCounts = document.querySelectorAll(".tnt-photo-sales-cart .cart-count");
            for (i = 0; i < cartCounts.length; i++) {
                if ( PIO.getNumItems() > 0 ) {
                    cartCounts[i].style.display = 'block';
                } else {
                    cartCounts[i].style.display = 'none';
                }
            }
        } /*]]>*/</script>
