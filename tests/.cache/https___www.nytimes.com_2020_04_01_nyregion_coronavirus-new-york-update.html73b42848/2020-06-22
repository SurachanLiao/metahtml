<script>
  var pathname
  if (window.asset && window.asset.url) {
    // hybrid
    pathname = window.asset.url;
  } else {
    // vi
    pathname = window.location.pathname;
  }
  // remove section label from menu if on corona virus section front
  if (document.querySelector('#masthead-section-label') && window.location.pathname.endsWith("news-event/coronavirus")) {
    var css = '#masthead-section-label { display: none; } ';
    var parent = document.querySelector("#nytslm");
    style = document.createElement('style');
    parent.appendChild(style);

    style.type = 'text/css';
    if (style.styleSheet) {
      // This is required for IE8 and below.
      style.styleSheet.cssText = css;
    } else {
      style.appendChild(document.createTextNode(css));
    }
  }

  var tags = document.getElementsByClassName('nytslm_li_link');
  var menu = document.getElementById('nytslm-scroll-container');
  var menuScrollWidth = menu.scrollWidth;
  var isClippedMenu = menuScrollWidth > menu.clientWidth; 
  var title = document.getElementById('nytslm-title');
  var titleWidth = title.getBoundingClientRect().width;
  var firstItem = document.getElementsByClassName('nytslm_li')[0];
  var pathnameElement; 
  var isPathnameInMenu = false;


  for (var index = 0; index < tags.length; index++) {
    var el = tags[index];
    var elRect = el.getBoundingClientRect();

    if (pathname.length && el && el.href && el.href.includes(pathname)) {
      el.classList.add("nytsMenu_li_current");
      isPathnameInMenu = true;
      pathnameElement = elRect;
      break;
    }
  }

  /* scroll user to current menu item */
  if(isClippedMenu && isPathnameInMenu){
    var elementXPositionCentered = Math.round(pathnameElement.x + (pathnameElement.width / 2) - titleWidth - ((menu.clientWidth - titleWidth)/2));
    menu.scrollLeft = elementXPositionCentered; //center current menu item in view
    stickTitle();
    setTimeout(() => menu.addEventListener('scroll', throttle(handleTitleDisplayOnScroll, 30)), 80)
  }

  function removeLiveBug() {
    var found = false;
    var spans = document.querySelectorAll('span');
    for (var index = 0; index < spans.length; index++) {
      if (spans[index].textContent === 'LIVE UPDATES') {
        found = true;
        break;
      }
    }

    if (found) {
      var live = document.querySelector('#live');
      if (live) live.remove();
    }
  }

  removeLiveBug();
  document.addEventListener('DOMContentLoaded', removeLiveBug);

 /* SCROLLING FUNCTIONS */
 function throttle(fn, wait) {
    var time = Date.now();
    return function() {
      if ((time + wait - Date.now()) < 0) {
        fn();
        time = Date.now();
      }
    }
  }
  function handleTitleDisplayOnScroll(){
    if(isTitleInViewport()){
        showTitle();
    }
    else{
      if(menu.oldScroll < menu.scrollLeft || menu.oldScroll > menu.scrollLeft){
        hideTitle();
      }
    }
    menu.oldScroll = menu.scrollLeft;
  }
  function isTitleInViewport() {
    return menu.scrollLeft < titleWidth;
  }
  function hideTitle() {
    title.style.visibility = 'hidden';
    title.style.opacity = '0';
  }
  function showTitle(){
    title.style.visibility = 'visible';
    title.style.opacity = '1';
  }
  function stickTitle(){
    title.style.margin = '0 auto';
    title.style.position = '-webkit-sticky';
    title.style.position = 'sticky';
    title.style.backgroundColor = 'white';
    title.style.boxShadow = '4px 0px 7px 10px white';
    title.style.paddingLeft= '1.25em';
    title.style.left= '0';
    title.style.opacity= '1';
    title.style.transition= 'visibility linear 300ms, opacity 300ms';
    menu.style.paddingLeft = "0px";
    firstItem.style.paddingLeft = "15px";
  }
</script><!-- Pipeline: 2020-01-23-styln-menu-wuhan-coronavirus | June 22, 2020, 09:01AM | 3fa3fbecc6486a254b5a59935b1baa6b15b12e73 -->
