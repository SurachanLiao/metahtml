    window.slots = {
        'gpt_top': {
            container: 'gpt_top',
            sizes: window.slotSizeDeclarations['top'],
            slotPosition: 'Top'
        },
        'gpt_superbanner': {
            container:  'gpt_superbanner',
            sizes: window.slotSizeDeclarations['superbanner'],
            slotPosition: 'Top'
        },
        'gpt_skyscraper': {
            sticky: true,
