        $(function(){
            var ad_signin_form = $(".ad_signin");
            var ad_subscribe_form = $(".ad_subscribe");

            var ad_signin_button = $("button[name='ad_sign_in']");
            ad_signin_button.on("click", function() {
                ad_subscribe_form.hide();
                ad_signin_form.toggle();
            });

            var ad_subscribe_button = $("button[name='ad_register']");
            ad_subscribe_button.on("click", function() {
                ad_signin_form.hide();
                ad_subscribe_form.toggle();
            });
        });

        function playDemo() {
            var id = "5e093131e3899153ca493df2";
            const http = new XMLHttpRequest();
            const url = "/track-content-download/" + id;
            http.open("GET", url);
            http.send();
            $(".demoConfirm").hide();
            $(".demoPlayer").show();
            $(".videoAboveText").show();
            $(".videoBelowText").show();
        };
