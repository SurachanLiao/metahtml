if( document.getElementById("newsletter_widget_email_input").value == '' && sessionUser_email != '') {
	document.getElementById("newsletter_widget_email_input").value = sessionUser_email;
}


function ds_layover(action) {
	action = action || 'open';
	var wrapp = document.getElementById('ds_layover_wrapper');
	
	if( action == "open" ) {
		wrapp.style.display = 'block';
	} else if( action == "close" ) {
		wrapp.style.display = 'none';
	} 
	
	return;
}


var forms = document.querySelectorAll('.newsletter-signin');
for (var i = 0, ii = forms.length; i < ii; ++i) {
	
	form_cookie_check = getCookie( forms[i].getAttribute( 'data-type' ) );
	if( form_cookie_check == 'true' ) {
		forms[i].style.display = 'none';
	}
	
	nlform = forms[i].querySelector('form');
	
	nlform.onsubmit = function (e) {
		
		var formAction = this.action;
        e.preventDefault();
        var errors = 0;
        var data = new Array;

        var formData = new FormData(this);
        var es, e, pair;
        for (es = formData.entries(); !(e = es.next()).done && (pair = e.value);) {
        	data.push(pair[0]+ '=' + pair[1]); 
        }
        postdata = data.join('&');
        
        this.style.display = 'none';
        this.parentNode.getElementsByClassName('nlspinn-elip')[0].style.display = 'block';

        var xhr;
        if (window.XMLHttpRequest) {
            // code for IE7+, Firefox, Chrome, Opera, Safari
            xhr = new XMLHttpRequest();
        }
        else {
            // code for IE6, IE5
            xhr = new ActiveXObject("Microsoft.XMLHTTP");
        } 
        
        
        var thisform = this;
        xhr.onload = function () {
            if (xhr.status >= 200 && xhr.status < 300) {
                console.log('done');
                if( xhr.responseText.indexOf('CHECKOUTNL-OK') ) {
                	console.log('OK');                	
                	thisform.parentNode.getElementsByClassName('nlspinn-elip')[0].style.display = 'none';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].style.display = 'block';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].getElementsByTagName('h3')[0].innerHTML = 'BITTE BESTÄTIGEN SIE DIE ANMELDUNG';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].getElementsByTagName('p')[0].innerHTML = 'In Kürze erhalten Sie zum Abschluss Ihrer Newsletter-Anmeldung einen Aktivierungslink an Ihre E-Mail-Adresse geschickt.<br />' + 
                	'<br />' +
                	'Es kann bis zu 5 Minuten dauern, bis Sie die E-Mail erhalten. Sollten Sie danach immer noch keine E-Mail in Ihrem Posteingang finden können, prüfen Sie bitte Ihren Spam-Ordner.<br />' +
                	'<br /><strong>Wichtig: Sie erhalten den Newsletter erst, nachdem Sie auf den Aktivierungslink geklickt haben. Dieser Link ist nur 24 Stunden gültig.</strong><br />' +
                	'<br />' +
                	'Vielen Dank!';
                	
                } else {
                	
                	thisform.parentNode.getElementsByClassName('nlspinn-elip')[0].style.display = 'none';
                	thisform.style.display = 'block';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].style.display = 'block';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].getElementsByTagName('h3')[0].innerHTML = '<span style="color: red !important">ACHTUNG</span>';
                	thisform.parentNode.getElementsByClassName('nlfeedback')[0].getElementsByTagName('p')[0].innerHTML = 'Es ist ein Fehler bei der Anmeldung aufgetreten.<br />' +
                	'Bitte prüfen Sie die Eingaben und versuchen Sie es erneut.';
                }
            } 
        };

        xhr.open('POST',formAction, true);
        xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
        xhr.send(postdata);
    };
	
}

function setCookie(name, value, days) {
    var d = new Date;
    d.setTime(d.getTime() + 24*60*60*1000*days);
    document.cookie = name + "=" + value + ";path=/;expires=" + d.toGMTString();
}

function getCookie(name) {
    var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
    return v ? v[2] : null;
}

var newsletter_close_icons = document.querySelectorAll('.newsletter-signin .close');
for (var i = 0, ii = newsletter_close_icons.length; i < ii; ++i) {
	newsletter_close_icons[i].addEventListener("click", function (e) {
		
		setCookie( this.parentNode.getAttribute('data-type'), 'true', 90);
		form = document.querySelector('.' + this.parentNode.getAttribute('data-type') );
		form.style.display = 'none';
	
	});
}

</script>






