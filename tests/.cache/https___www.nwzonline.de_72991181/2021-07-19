       var NWZPublisherTag=function(){function e(e){this.debugConsole=!1,this.adSlots=[],this.lazyAdSlots=[],this.lazyAdLoaderOffset=1e3,this.zone="",this.pageConfig={},this.targets={},this.debugConsole=e.debug||!1,this.zone=e.zone||"test",this.adSlots=e.adSlots||[],this.pageConfig.product=e.pageConfig.product||"NWZonline",this.pageConfig.sectionId=e.pageConfig.sectionId||"/",this.pageConfig.eomType=e.pageConfig.eomType||"",this.pageConfig.channel=e.pageConfig.channel||"c9",this.pageConfig.facet=e.pageConfig.facet||"",this.pageConfig.keyword=e.pageConfig.keyword||[],this.init()}return e.prototype.init=function(){SDG.Publisher.setZone(this.zone),this.setTargeting(),this.prepareSlots(),this.lazyAdLoader()},e.prototype.almostVisible=function(e){if(!e)return!1;var t=window.pageYOffset-this.lazyAdLoaderOffset,o=window.pageYOffset+window.innerHeight+this.lazyAdLoaderOffset;return e.offsetTop+e.clientHeight>t&&e.offsetTop<o},e.prototype.debug=function(e){this.debugConsole&&console.log(e)},e.prototype.lazyAdLoader=function(){var e=this;this.lazyAdSlots.length>0&&this.lazyAdSlots.forEach(function(t,o){e.almostVisible(document.getElementById(t.DOMContainerId))&&(e.debug(t.name+" is (almost) visible and loads now ..."),e.loadLazyAd(t),e.lazyAdSlots.splice(o,1))}),0==this.lazyAdSlots.length&&(this.debug("Unregister listener ... all Adslots triggered and loaded"),window.removeEventListener("scroll",lazyAdObserver))},e.prototype.loadLazyAd=function(e){this.debug("callback fired for: "+e.name+"/"+e.DOMContainerId),SDG.Publisher.registerSlot(e.name,document.getElementById(e.DOMContainerId)).setTargeting(this.targets),SDG.Publisher.loadSlot(e.name)},e.prototype.prepareSlots=function(){var e=this;this.debug("Prepare slots, load those NOT beeing lazy"),this.adSlots.length>0&&(this.adSlots.forEach(function(t){SDG.Publisher.registerSlot(t.name,t.DOMContainerId).setTargeting(e.targets)}),SDG.Publisher.loadAllSlots(),SDG.Publisher.finalizeSlots(),this.debug("executed SDG.Publisher.loadAll