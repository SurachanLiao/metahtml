<script type="text/javascript">
    var shouldDisplayModal = false;
    $(function () {
        if (!shouldDisplayModal)
            return;
        $("#preferredLanguageModal").css("visibility", "hidden");
            $("#preferredLanguageModal").css("visibility", "visible");
            $("#preferredLanguageModal").dialog({
                autoOpen: true,
                dialogClass: 'preferredlanguage',
                resizable: true,
                width:'auto',
                closeOnEscape: false,
                open: function( event, ui ) {
                    $(".preferredlanguage").css("top", "");
                    $(".preferredlanguage").css("left", "");
                    $(".preferredlanguage").css("position", "");
                    $('.l-viewport').css({'pointer-events' : 'none'});
                }
            });
    });

    function setLanguage(lang) {
        $.ajax({
            url: "Redesign/Site/StorePreferredLanguage",
            type: 'POST',
            dataType: "json",
            traditional: true,
            data:  { language: lang },
            success: function (val) {
                $("#preferredLanguageModal").dialog('close');
                $('.l-viewport').css({'-webkit-filter' : ''});
                $('.l-viewport').css({'filter' : ''});
                window.location.href = val;
            },
            error: function(jqxhr, textStatus, errorThrown) {
                console.error(errorThrown);
            }
        });
    }

</script>

        





