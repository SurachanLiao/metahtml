  (function ($) {
    $(function () {
      var jcarousel = $("#jcarousel-subiecte_fierbinti-b74d94ec01e02d3b6e7f3125da4f4f8d");
      var width_trick = $('.view.view-subiecte-fierbinti.view-id-subiecte_fierbinti > .view-content');
      jcarousel
        .on('jcarousel:reload jcarousel:create', function () {
          var width = width_trick.innerWidth();

          if (width >= 600) {
            width = width / 4;
          } else if (width >= 300) {
            width = width / 1;
          }

          jcarousel.jcarousel('items').css('width', width + 'px');
        })
        .jcarousel({
          wrap: 'circular'
        });

      $(".jcarousel-prev-subiecte_fierbinti-b74d94ec01e02d3b6e7f3125da4f4f8d")
        .jcarouselControl({
          target: '-=1'
        });

