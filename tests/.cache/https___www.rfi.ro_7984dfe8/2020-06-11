      var jcarousel = $("#jcarousel-subiecte_fierbinti-4c3647b5db4d4107f483708c60a9a312");
      var width_trick = $('.view.view-subiecte-fierbinti.view-id-subiecte_fierbinti > .view-content');
      jcarousel
        .on('jcarousel:reload jcarousel:create', function () {
          var width = width_trick.innerWidth();

          if (width >= 600) {
            width = width / 4;
          } else if (width >= 300) {
            width = width / 1;
          }

