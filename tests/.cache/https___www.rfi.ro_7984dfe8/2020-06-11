  (function ($) {
    $(function () {
      var jcarousel = $("#jcarousel-galerie_foto-7a795fcdb706211dcdbcea83513863cb");
      jcarousel
        .on('jcarousel:reload jcarousel:create', function () {
          var width = jcarousel.innerWidth();

          if (width >= 250) {
            width = width / 2;
          } else if (width >= 100) {
            width = width / 1;
          }

          jcarousel.jcarousel('items').css('width', width + 'px');
        })
        .jcarousel({
          wrap: 'circular'
        });

      $(".jcarousel-prev-galerie_foto-7a795fcdb706211dcdbcea83513863cb")
        .jcarouselControl({
          target: '-=1'
        });

      $(".jcarousel-next-galerie_foto-7a795fcdb706211dcdbcea83513863cb")
        .jcarouselControl({
          target: '+=1'
        });
    });
  })(jQuery);
