<script>
jQuery(document).ready(function() {
function string_to_slug (str) {
    str = str.replace(/^\s+|\s+$/g, '');
    str = str.toLowerCase();
    var from = "àáäâèéëêìíïîòóöôùúüûñç·/_,:;";
    var to   = "aaaaeeeeiiiioooouuuunc------";
    for (var i=0, l=from.length ; i<l ; i++) {
        str = str.replace(new RegExp(from.charAt(i), 'g'), to.charAt(i));
    }

    str = str.replace(/[^a-z0-9 -]/g, '')
        .replace(/\s+/g, '-')
        .replace(/-+/g, '-');
    return str;
}

var sectionNewsletter = window.digitalData.page.category.primaryCategory;
sectionSlug = string_to_slug(sectionNewsletter);
sectionSlug = (sectionSlug=='inicio') ? 'home' : sectionSlug; 
console.log('newsLetter: '+sectionSlug);

  if (jQuery('#widgetMicuenta').length) {
    var urlw = jQuery("#widgetMicuenta").data("urlw");
    jQuery.ajax({
        url: urlw,
        context: document.body,
        data : { seccion : 'nacion' },
        success: function(responseText) {
            jQuery( "#widgetMicuenta" ).html(responseText);
        }
    });
  }

});
</script> </div>
