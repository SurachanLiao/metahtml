<script>
var observer_rekomendasi = new IntersectionObserver(function(entires){
    entires.forEach(function(entry) {
        if(entry.intersectionRatio > 0){
            show_rekomendasi(entry.target)                
        }
    });
}, {
    threshold: 0
});        

observer_rekomendasi.observe(document.getElementById('rekomendasi-inject'));
function show_rekomendasi(element){
console.log('show_rekomendasi');

var rekomendasi_xhr = new XMLHttpRequest();
rekomendasi_xhr.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
        var rekomendasi = JSON.parse(rekomendasi_xhr.responseText); 
        var rekomendasi_html = '<div class="title clearfix"><div class="title__content">Rekomendasi untuk anda</div><div class="jixie__logo jixie__logo--right"><span>Powered by &nbsp; </span><div class="jixie__logo__img" data-type="tooltips" data-text="Jixie mencari berita yang dekat dengan preferensi dan pilihan Anda. Kumpulan berita tersebut disajikan sebagai berita pilihan yang lebih sesuai dengan minat Anda."><img src="https://asset.kompas.com/data/2017/widget/images/jixie-logo.png" alt=""> <div class="tooltips"><span>Jixie mencari berita yang dekat dengan preferensi dan pilihan Anda. Kumpulan berita tersebut disajikan sebagai berita pilihan yang lebih sesuai dengan minat Anda.</span></div></div></div></div><div class="article__wrap__grid--flex col-offset-fluid mt2">';
        var rekomendasi_no = 1;
        rekomendasi.docs.result.forEach(function(value){
            if(rekomendasi_no <= 8){
                img_link = value.img_link[0];
                if(value.img_link[0].indexOf('/crop/') != -1){
                    img_link = img_link.replace("750x500/data/photo", "170x113/data/photo");
                    img_link = img_link.replace("780x390/data/photo", "170x113/data/photo");
                }
                ads = '';
                img_ads = '';
                if(value.kanal[0]=="<span style='text-decoration:underline'>ADVERTORIAL</span>" || rekomendasi_no == 4){
                    ads = ' jixie_advbox';
                    if(typeof value.img_ads_link !== "undefined" && value.img_ads_link != ''){
                        img_ads = '<img src="'+value.img_ads_link+'" width="1" height="1" alt="" style="height:0px;">';
                    }
                }
                value.path = value.path[0].replace('http://', 'https://');
                original_title = value.title[0];
                value.title = original_title.split(" ").splice(0,6).join(" ")
                if(value.title != original_title){
                    value.title += '...';
                }
                rekomendasi_html += '<div class="col-bs12-3">'
                rekomendasi_html += '<div class="article__grid'+ads+'">'
                rekomendasi_html += '<div class="article__boxsubtitle" id="'+value.path+'">'
                rekomendasi_html += '<div class="article__subtitle">'+value.kanal+'</div>'
                rekomendasi_html += '</div>'					
                rekomendasi_html += '<div class="article__asset">'
                rekomendasi_html += '<a title="'+original_title+'" href="'+value.path+'" position="'+rekomendasi_no+'">'
                rekomendasi_html += '<img id="'+value.path+'" src="'+img_link+'" alt="'+original_title+'">'+img_ads
                rekomendasi_html += '</a>'
                rekomendasi_html += '</div>'
                rekomendasi_html += '<div class="article__box">'
                rekomendasi_html += '<div class="article__title">'
                rekomendasi_html += '<a class="article__link" title="'+original_title+'" position="'+rekomendasi_no+'" href="'+value.path+'" style="font-size: 14px;">'+value.title+'</a>'
                rekomendasi_html += '</div>'
                rekomendasi_html += '</div>'
                rekomendasi_html += '</div>'
                rekomendasi_html += '</div>'
            }
            rekomendasi_no += 1;
        });
        rekomendasi_html += '</div>';
        document.getElementById("rekomendasi").innerHTML = rekomendasi_html;
    }
};
rekomendasi_xhr.open("GET", "https://apis.kompas.com/api/related?json=1&url=2010/10/06/06492112", true);
rekomendasi_xhr.send(); 
}
