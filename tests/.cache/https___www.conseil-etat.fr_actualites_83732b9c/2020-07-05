        <script type="text/javascript">
            $( function() {
                $('#search_term').autocomplete({
                    source : function( request, response ) {
                        $.ajax({
                            url: '/autocomplete',
                            dataType: 'json',
                            data: {
                                name_startsWith: $('#search_term').val() 
                            },
                            success: function(data){
                                response( data );
                            }
                        });
                    },
                    select: function( event, ui ) {
                        $('#search_term').val(ui.item.value);
                        $('#search_term').closest('form').submit();
                    }
                }).focus(function() {
                    $(this).autocomplete("search");
                });
            });
            $( function() {
                $('#aside_search_term').autocomplete({
                    source : function( request, response ) {
                        $.ajax({
                            url: '/autocomplete',
                            dataType: 'json',
                            data: {
                                name_startsWith: $('#aside_search_term').val() 
                            },
                            success: function(data){
                                response( data );
                            }
                        });
                    },
                    select: function( event, ui ) {
                        $('#aside_search_term').val(ui.item.value);
                        $('#aside_search_term').closest('form').submit();
                    }
                }).focus(function() {
                    $(this).autocomplete("search");
                });

            });
        </script>
    
