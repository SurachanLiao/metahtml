                  var container = document.getElementById('search-results-mosaic-grid');
                  var styleEl = document.createElement('style');
                  var width = container.clientWidth - 1;
                  var rect;
                  function selectHeightBasedOnWidth(heights, width) {
  // Find the last height (iterate right to left)
  for (let i = heights.length - 1; i > 0; i -= 1) {
    const h = heights[i];

    if (width >= h.containerWidth) {
      return h.height;
    }
  }

  return (heights[0] || {
    height: 260
  }).height;
};
                  function pack(rectangles, rowWidth, targetRowHeight, {
  maxRows = null,
  usePercentageWidths = false,
  spacing = 0
} = {}) {
  let rowIndex = 1;
  let row = null;
  const tgtRowHeight = targetRowHeight - spacing;

  const newRow = () => ({
    width: 0,
    height: tgtRowHeight,
    aspect: 0,
    index: rowIndex,
    rectCount: 0
  }); // Step 1: Scale images to have height equal to targetRowHeight
  // and build up row objects. O(n) time and space.


  rectangles.forEach((rectangle, index) => {
    row = row || newRow();
    rectangle.height = tgtRowHeight;
    rectangle.width = calcWidth(rectangle.height, rectangle.aspect);
    const compareWidth = Math.min(rectangle.width, rowWidth);
    const remainingSpace = rowWidth - row.width; // Can't add the current rectangle to the current row; create a new row.

    if (row.width > 0 && remainingSpace < compareWidth * Math.min(1, tgtRowHeight / compareWidth)) {
      rowIndex += 1;
      row = newRow();
    }

    rectangle.row = row;
    row.width += rectangle.width;
    row.aspect = calcAspect(row.width, row.height);
    row.rectCount += 1;
    if (index === rectangles.length - 1) row.last = true;
  }); // Step 2: Scale images to their final size using the information
  // saved in the row objects. O(n) time, O(1) space. Skip last row if
  // the images would look too big if they were scaled up.

  for (let i = 0; i < rectangles.length; i += 1) {
    const rectangle = rectangles[i];
    const {
      row: currentRow
    } = rectangle;
    if (!maxRows) delete rectangle.row; // delete reference so row object can be garbage collected

    const finalRowWidth = rowWidth - spacing * currentRow.rectCount; // Normalize the current row if it's not the last row, OR
    // normalize the last row its contents take up at least 60 percent of the finalRowWidth

    if (!currentRow.last || currentRow.last && currentRow.width >= finalRowWidth * 0.6) {
      // Consider (micro) optimizing this step so it only happens when it needs to
      currentRow.width = finalRowWidth;
      currentRow.height = calcHeight(currentRow.width, currentRow.aspect);
    }

    rectangle.height = currentRow.height;
    rectangle.width = calcWidth(rectangle.height, rectangle.aspect);
    rectangle.rowIndex = currentRow.index;
    if (usePercentageWidths) rectangle.width = asPercentageOf(rectangle.width, finalRowWidth);
  }

  if (maxRows) {
    for (let i = rectangles.length - 1; i >= 0; i -= 1) {
      if (rectangles[i].row.index === maxRows) {
        rectangles.splice(i + 1);
        return;
      }
    }
  }
};
                  var height = selectHeightBasedOnWidth(heights, width);
                  pack(
                    packed,
                    width,
                    height,
                    { usePercentageWidths: false, maxRows: null, spacing: 5 }
                  );
                  for (var i = 0; i < packed.length; i++) {
                    rect = packed[i];
                    styleStr += [
                      '#search-results-mosaic-grid .mosaic-grid-item-', i, ' {',
                      'width: ', rect.width, 'px !important;',
                      'height: ', rect.height, 'px !important;',
                      'margin: 2.5px !important;',
                      'padding: 0 !important;',
                      '}'
                    ].join('');
                  }
                  styleEl.innerText = styleStr;
                  container.appendChild(styleEl);
                  container.style.visibility = 'visible';
                }())
              </script></div><div class="z_b_a k_b_c k_b_eg"><div class="k_b_O k_b_fo k_b_fn k_b_b"><div class="z_b_g"><a data-automation="BottomNav_NextButton" href="/search?page=2" class="o_button_theme_button oc_u_f b_h_a b_h_E b_h_B b_h_A b_h_y b_h_w b_h_s o_button_theme_flat o_button_theme_button o_button_theme_squared b_h_C o_button_theme_neutral oc_u_j b_h_x z_b_e oc_u_g b_h_q oc_u_k" data-react-toolbox="button">Suivant</a></div></div><div class="k_b_O k_b_fn k_b_b z_b_f"><div class="z_b_b" data-automation="BottomNav_pagination_container"><div class="b_aE_a"><a data-track-section="paginationBottom" data-track-action="click" data-track-label="previousPage" data-track-value="{&quot;page&quot;:&quot;0&quot;}" data-automation="undefinedPreviousPage" disabled="" aria-label="Previous page" href="/search" class="o_button_theme_toggle o_button_theme_button oc_G_b b_E_a b_E_G b_E_H o_button_theme_neutral b_E_b oc_G_c b_E_j oc_G_g b_E_e" data-react-toolbox="button"><span aria-label="Previous page" class="b_B_g b_B_a oc_G_j b_E_c b_aE_b" data-icon="chevron-left"></span></a><div data-react-toolbox="input" class="o_input_theme_input oc_K_h b_J_f oc_R_a b_aE_c number-input"><input type="number" class="o_input_theme_input-element oc_K_f b_J_b oc_R_b b_aE_f o_input_theme_filled oc_K_m" min="1" step="1" max="3250960" data-track-section="paginationBottom" data-track-action="change" data-track-label="inputField" data-track-value="{&quot;page&quot;:&quot;1&quot;}" data-automation="undefinedInputPage" value="1" aria-label="Change page"/><span class="o_input_theme_bar oc_K_p b_J_e"></span></div><a data-track-section="paginationBottom" data-track-action="click" data-track-label="nextPage" data-track-value="{&quot;page&quot;:&quot;2&quot;}" data-automation="undefinedNextPage" aria-label="Next page" href="/search?page=2" class="o_button_theme_toggle o_button_theme_button oc_G_b b_E_a b_E_G b_E_H o_button_theme_neutral b_E_b oc_G_c b_E_j oc_G_g b_E_e" data-react-toolbox="button"><span aria-label="Next page" class="b_B_g b_B_a oc_G_j b_E_c b_aE_b" data-icon="chevron-right"></span></a><div class="b_aE_g z_b_c">sur 3 250 960</div></div></div></div></div></div></main> </div></div><div class="oc_N_p"> <div data-automation="UserFeedbackBanner_Div" class="U_c_a">Aidez-nous à améliorer votre expérience de recherche.<a rel="noopener noreferrer" target="_blank" href="https://shutterstockcx.az1.qualtrics.com/jfe/form/SV_elZUFWpRLIcSI0R?searchTerm=&amp;site=image" class="o_button_theme_button oc_u_f b_h_a b_h_E b_h_B b_h_A b_h_y b_h_w b_h_s o_button_theme_flat o_button_theme_button o_button_theme_squared b_h_C U_c_b oc_u_l b_h_F oc_u_k" data-react-toolbox="button">Envoyer des commentaires<span aria-label="Open in new" class="U_c_c b_B_c b_B_a" data-icon="open-in-new"></span></a></div><footer role="contentinfo" data-automation="SiteFooter_footer"><div class="q_g_e q_f_c k_b_c"><div data-automation="SiteFooter_stats_text" class="k_b_O k_b_b q_g_c">Plus de 324 291 335 images libres de droits avec 873 268 nouvelles images ajoutées chaque semaine.</div><div class="k_b_cT k_b_O k_b_cE k_b_b q_f_m"><div tabindex="0" class="K_i_b q_f_j" data-automation="SiteFooter_languageRoot_mobile" aria-controls="SiteFooter_LanguageSelect-mobile-menu" aria-expanded="false" aria-haspopup="true"><div class="K_i_e q_f_l" tabindex="-1"><span class="K_i_h"><span aria-label="language icon" class="b_B_g b_B_a" data-icon="language"></span><span>Français</span></span><span aria-label="Chevron down" class="b_B_g b_B_a" data-icon="chevron-down"></span></div><div data-react-toolbox="menu" class="o_menu_theme_menu oc_P_a b_az_f o_menu_theme_max-height K_i_c q_f_n" style="max-height:336px" tabindex="-1" id="SiteFooter_LanguageSelect-mobile-menu" role="menu"><div class="o_menu_theme_outline oc_P_p b_az_k"></div><div class="o_menu_theme_menu-inner b_az_i K_i_f q_f_o" role="none"><a href="/cs/search/" class="o_menu_theme_menu-item oc_P_h b_az_a K_i_a" value="cs" tabindex="-1" data-automation="Si