				var $list2 = '<span><a href="https://news.mt.co.kr/theissue/yearlyPlanList.html?hid=201912191422533111&YP2020_2"><img src="https://menu.mt.co.kr/common/etc/banner_plan2020_5.jpg" alt="그린수소시대, 미래를 선점하라"></a></span>';
				var $list3 = '<span><a href="https://news.mt.co.kr/theissue/yearlyPlanList.html?hid=202003161248533761&YP2020_4"><img src="https://menu.mt.co.kr/common/etc/banner_plan2020_6.jpg" alt="대한민국 4.0 타락한 진영의식의 고개를 너머"></a></span>';
				var $listArr = [$list1,$list2,$list3];

				function RandomArr(d) {
					for(var c = d.length - 1; c > 0; c--){
						var b = Math.floor(Math.random() * (c + 1));
						var a = d[c]; d[c] = d[b]; d[b] = a;
					}
					return d
				}
				$listArr = RandomArr($listArr);
				$('.bnslick').html($listArr.join());
				
				bnSlickStart();
			});
			/* //추가 200506 세종 / 2020신년기획 랜덤정렬 */
			/* 수정 200506 세종 / 랜덤정렬 이후 적용을 위해 함수로 변경 */
			function bnSlickStart(){
				$(".bnslick").slick({
					infinite: true,					/* 맨끝이미지에서 끝나지 않고 다시 맨앞으로 이동 */
					dots: true,						/*  아래점  */
					arrows: false,					/* 화살표  */
					autoplay: true,					/* 자동으로 다음이미지 보여주기 */
					autoplaySpeed:3000,		/* 다음이미지로 넘어갈 시간 */
					speed:1000 ,					/* 다음이미지로 넘겨질때 걸리는 시간 */
					pauseOnHover:true			/* 마우스 호버시 슬라이드 이동 멈춤 */
				});
			}
			/* //수정 200506 세종 */
		</script>

		<!-- float해제 -->
		<span class="cboth"></span>
	</div>
</div>            <!-- 2번세트 실시간속보, 메인탑 sub,메인탑 밸류에이션  -->
            
