			<script type="text/javascript">
				function cdNewstab(direction){
					var idx = document.getElementById('gx').value;

					if(direction == "prev"){
						idx = parseInt(idx) - 1;

						if(idx < 0){
							idx = '2';
						}
					}
					else{
						idx = parseInt(idx) + 1;

						if(idx == '3'){
							idx = 0;
						}
					}
					document.getElementById('gx').value = idx;

					for(i=0; i<3; i++){
						document.getElementById('card'+i).style.display = "none";
						//document.getElementById('ttime'+i).style.display = "none";
						document.getElementById('infog'+i).style.display = "none";
					}
					document.getElementById('card'+idx).style.display = "block";
					//document.getElementById('ttime'+idx).style.display = "block";
					document.getElementById('infog'+idx).style.display = "block";
				}
			</script>
