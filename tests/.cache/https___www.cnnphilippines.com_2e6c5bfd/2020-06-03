$(document).ready(function() {
var screenWidth = $(window).width();
if (screenWidth < 600) {
$('.carousel-multiple .owl-carousel').addClass('mobile');
$('.carousel-multiple .owl-carousel').removeClass('desktop');
var owl = $('.carousel-multiple .owl-carousel');
owl.owlCarousel({
nav : true,
lazyLoad: true,
lazyLoadEager: 4,
navText: ["<img src='/.resources/demo-cnn-new/webresources/img/slider-arrow-left.png'>","<img src='/.resources/demo-cnn-new/webresources/img/slider-arrow-right.png'>"],
dots: false,
rewindNav : true,
margin: 15,
loop: true,
responsive:{
0:{
items:1
},
600:{
items:1
},
1000:{
items:1
}
}
})
}
else {
$('.carousel-multiple .owl-carousel').addClass('desktop');
$('.carousel-multiple .owl-carousel').removeClass('mobile');
var owl = $('.carousel-multiple .owl-carousel');
owl.owlCarousel({
nav : true,
lazyLoad: true,
lazyLoadEager: 1,
slideBy:3,
navText: ["<img src='/.resources/demo-cnn-new/webresources/img/slider-arrow-left.png'>","<img src='/.resources/demo-cnn-new/webresources/img/slider-arrow-right.png'>"],
dots: false,
margin: 10,
loop: true,
stagePadding: 50,
responsive:{
0:{
items:1
},
600:{
items:3
},
1000:{
items:4
}
}
})
}
})
</script>
