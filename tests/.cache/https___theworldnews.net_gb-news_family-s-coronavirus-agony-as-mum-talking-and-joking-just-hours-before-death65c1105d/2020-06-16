		function onProgress( imgLoad, image ) {
			var item = $(image.img);

			if (item.width() < 100 || !image.isLoaded) {
				item.hide(0);
			}
			else if(item.width() > 100 && item.width() < 300){
				item.addClass('img-float');
			}
		}

	    $('.mn_content__text').imagesLoaded().progress( onProgress );

