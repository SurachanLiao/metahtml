<script type="text/javascript" language="javascript">
<!--
window.onunload=function(){};
-->
jQuery(function($){
	const strLStrageKey = 'dnews_keyword_follow';
	var cLStrage = new LStrage(strLStrageKey);
	
	
	{
		hashKeyword = cLStrage.getEntityLStarge();
		if(hashKeyword == null){
			hashKeyword = {};
		}
		var load_values = [];
		for(key in hashKeyword){
		load_values.push(decodeURIComponent(key));
		}
		$('input[type="checkbox"][name="topic_id"]').each(function(){
		this.checked = $.inArray(this.value, load_values) != -1;
		});
	}
	$('input[type="checkbox"][name="topic_id"]').change(function(){
			var save_values = cLStrage.getEntityLStarge();
			if(save_values == null){
				save_values = {};
			}
			$('input[type="checkbox"][name="topic_id"]').each(function(){
				var save_key = encodeURIComponent(this.value)
				if(save_key in save_values){
					delete save_values[save_key];
				}
				if(this.checked){
					var val = $(this).parent().prev().text();
					save_values[save_key] = encodeURIComponent(val);
				}
			});
		cLStrage.reverseEntityLStrage(save_values);
	});
	
	$('.check_btn').click(function(){
		if(($(this).prev().prop('checked'))){
			$(this).prev().prop('checked', true).change();
		} else {
			$(this).prev().prop('checked', false).change();
		}
	});

	$('.check_btn').click(function(){
		if(($(this).prev().prop('checked'))){
			$(this).next().fadeIn(500);
		} else {
			$(this).next().fadeOut(1000);
		}
	});

});

</script>


