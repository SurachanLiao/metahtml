<article data-history-node-id="84085" role="article" about="/fix-style-84085" class="node node--type-block node--view-mode-full clearfix hidden">
<header>
    <h2 class="node__title"><span class="field field--name-title field--type-string field--label-hidden">Fix style</span>
</h2>
  </header>
<div class="node__content clearfix">

  <div class="field field--name-body field--type-text-with-summary field--label-visually_hidden">
    <div class="field__label visually-hidden">Contenu</div>
              <div class="field__item"><style>
.hors-serie img { width: 440px !important; }
.section .view-blocs-site .views-row:last-child article {margin-bottom: 0;}
.section .view-blocs-site .pub .node__content { margin-bottom: 0;  margin-top: 0; }
</style>
<script>
jQuery(document).ready(function() {
  var today = new Date();
  var dd = today.getDate();
  var mm = today.getMonth() + 1; //January is 0!
  if(dd<10){
    dd='0'+dd;
  } 
  if(mm<10){
    mm='0'+mm;
  } 
var date = dd + "/" + mm;
  jQuery(".view-actus-en-continu .views-field-field-date-tri .field-content").each(function(index, el) {
     $el = jQuery(el);
    if($el.html().indexOf(date) != -1) {
      $el.html($el.html().replace(dd + "/" + mm, ""));
    }
  });
});
</script></div>
          </div>

</div>
</article>
</div>

    </div>
  
          </div>
