              <source srcset="/sites/default/files/styles/article_grille/public/2020-07/SIPA_00941271_000220_1.jpg?itok=zhouOlN0 1x" media="all and (min-width: 980px)" type="image/jpeg"/>
              <source srcset="/sites/default/files/styles/article_grille_narrow_680x300/public/2020-07/SIPA_00941271_000220_1.jpg?itok=l1o6fF-D 1x" media="all and (min-width: 480px) and (max-width: 979px)" type="image/jpeg"/>
