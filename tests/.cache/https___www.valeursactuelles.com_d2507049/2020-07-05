<article data-history-node-id="117806" role="article" about="/marcus-spurway-117806" class="node node--type-block node--view-mode-full clearfix hors-serie">
<header>
    <h2 class="node__title"><span class="field field--name-title field--type-string field--label-hidden">Marcus Spurway</span>
</h2>
  </header>
<div class="node__content clearfix">

  <div class="field field--name-body field--type-text-with-summary field--label-visually_hidden">
    <div class="field__label visually-hidden">Contenu</div>
              <div class="field__item"><a href="https://boutique.valeursactuelles.com/produit/solution-hydo-alcoolique/"></div>
          </div>

  <div class="field field--name-field-image field--type-image field--label-visually_hidden">
    <div class="field__label visually-hidden">Image</div>
          <div class="field__items">
