    // Get side nav opened /closed state
    function getNavState() {
      if (window.innerWidth > 958) {
        var navState = localStorage.getItem('nok_nav');
        if(!navState || navState == 'o') {
          // Default to open
          localStorage.setItem('nok_nav', 'o');
          document.body.classList.add('show-side-nav');
        }
      }
    }
    getNavState();
</script>
