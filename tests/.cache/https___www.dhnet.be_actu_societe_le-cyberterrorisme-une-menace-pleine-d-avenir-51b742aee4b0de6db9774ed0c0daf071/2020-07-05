    (function placeRectangleInsideArticleText() {

        'use strict';

        var adContainer = document.getElementById('RECTANGLE_BOTTOM---POS2')
        var articleText = document.getElementById('article-text');
        var arrayP = articleText.getElementsByTagName('p');
