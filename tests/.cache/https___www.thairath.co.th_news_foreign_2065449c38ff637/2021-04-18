one;background:white;width:46px;height:22px;z-index:11;-webkit-align-items:center;-webkit-box-align:center;-ms-flex-align:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;-ms-flex-pack:center;justify-content:center;border-radius:3px 3px 0 0;margin-top:-32px;box-shadow:0 -2px 4px #00000014;}.css-v87t20 .btn-expanse svg{width:12px;height:12px;-webkit-transition:all 0.5s;transition:all 0.5s;}@-webkit-keyframes fadein{0%{opacity:0;}100%{opacity:1;}}@keyframes fadein{0%{opacity:0;}100%{opacity:1;}}@media (max-width:767px){.css-v87t20 .btn-close a{width:22px;height:22px;margin-top:-22px;top:10px;right:0px;padding:0;}.css-v87t20 .btn-close a svg{width:8px;height:8px;}}</style><div class="hidden css-v87t20 e160b0jq7"><div style="text-align:center"></div></div></div></div><div><figure>
