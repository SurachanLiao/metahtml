             $(document).ready(function () {                
                 $('.bg-image').attr('data-counter', 0).fadeIn('500');                
                 $("#helptext").html($('<div><b>' + resources["ImageRef"] + ': ' + sliderImages[0].newdesc + ' ' + resources["stockPhoto"] + '<b></div>').text() + '<em></em>');
                 $('.slider_btn_row .previous').addClass('previous_disabled disabled');
             });
         }   

    </script>
