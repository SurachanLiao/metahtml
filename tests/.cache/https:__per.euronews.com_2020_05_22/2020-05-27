u06a9\u0646\u0634 \u0628\u0647 \u062a\u0635\u0645\u06cc\u0645 \u0622\u0645\u0631\u06cc\u06a9\u0627 \u0628\u0631\u0627\u06cc \u062e\u0631\u0648\u062c \u0627\u0632 \u067e\u06cc\u0645\u0627\u0646 \u0646\u0638\u0627\u0645\u06cc \u00ab\u0622\u0633\u0645\u0627\u0646 \u0647\u0627\u06cc \u0628\u0627\u0632\u00bb \u062a\u0627\u06a9\u06cc\u062f \u06a9\u0631\u062f \u06a9\u0647 \u062a\u0645\u0627\u0645 \u062a\u0648\u0627\u0646 \u062e\u0648\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0646\u062c\u0627\u062a \u0627\u06cc\u0646 \u0645\u0639\u0627\u0647\u062f\u0647 \u0628\u0647 \u06a9\u0627\u0631 \u062e\u0648\u0627\u0647\u062f \u06af\u0631\u0641\u062a.&lt;\/p&gt;\n&lt;p&gt;\u0648\u0632\u0627\u0631\u062a \u062e\u0627\u0631\u062c\u0647 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0627 \u0627\u0646\u062a\u0634\u0627\u0631 \u0628\u06cc\u0627\u0646\u06cc\u0647 \u0627\u06cc \u0636\u0645\u0646 \u0631\u062f \u0627\u062a\u0647\u0627\u0645 \u00ab\u0646\u0642\u0636\u00bb \u067e\u06cc\u0645\u0627\u0646 \u0646\u0638\u0627\u0645\u06cc \u00ab\u0622\u0633\u0645\u0627\u0646 \u0647\u0627\u06cc \u0628\u0627\u0632\u00bb \u062e\u0631\u0648\u062c \u0627\u06cc\u0627\u0644\u0627\u062a \u0645\u062a\u062d\u062f\u0647 \u0627\u0632 \u0627\u06cc\u0646 \u0645\u0639\u0627\u0647\u062f\u0647 \u0631\u0627 \u0636\u0631\u0628\u0647 \u0627\u06cc \u0628\u0647 \u06a9\u0644 \u0686\u0647\u0627\u0631\u0686\u0648\u0628 \u0627\u0645\u0646\u06cc\u062a\u06cc \u062f\u0631 \u0627\u0631\u0648\u067e\u0627 \u062a\u0648\u0635\u06cc\u0641 \u06a9\u0631\u062f. \u067e\u06cc\u0645\u0627\u0646 \u00ab\u0622\u0633\u0645\u0627\u0646 \u0647\u0627\u06cc \u0628\u0627\u0632\u00bb \u06a9\u0647 \u0627\u0632 \u0633\u0627\u0644 \u06f2\u06f0\u06f0\u06f2 \u0645\u06cc\u0644\u0627\u062f\u06cc \u062a\u0627\u06a9\u0646\u0648\u0646 \u0627\u062c\u0631\u0627 \u0645\u06cc \u0634\u062f \u0628\u0647 \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u0647\u0627\u06cc \u062c\u0627\u0633\u0648\u0633\u06cc \u0648 \u062a\u062c\u0633\u0633\u06cc \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0627\u062c\u0627\u0632\u0647 \u0645\u06cc \u062f\u0647\u062f \u062a\u0627 \u0628\u0631 \u0641\u0631\u0627\u0632 \u0642\u0644\u0645\u0631\u0648 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u067e\u0631\u0648\u0627\u0632 \u06a9\u0646\u0646\u062f.&lt;\/p&gt;\n&lt;p&gt;\u0627\u0632 \u067e\u06cc\u0645\u0627\u0646 \u00ab\u0622\u0633\u0645\u0627\u0646 \u0647\u0627\u06cc \u0628\u0627\u0632\u00bb \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u06cc\u06a9\u06cc \u0627\u0632 \u067e\u0627\u06cc\u0647 \u0647\u0627\u06cc \u0633\u0627\u0632 \u0648 \u06a9\u0627\u0631 \u06a9\u0646\u062a\u0631\u0644 \u0633\u0644\u0627\u062d \u0647\u0627 \u062f\u0631 \u062c\u0647\u0627\u0646 \u06cc\u0627\u062f \u0645\u06cc \u0634\u0648\u062f. \u062f\u0648\u0646\u0627\u0644\u062f \u062a\u0631\u0627\u0645\u067e\u060c \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631\u06cc \u0622\u0645\u0631\u06cc\u06a9\u0627 \u062f\u0631 \u062d\u0627\u0644\u06cc \u0631\u0648\u0632 \u067e\u0646\u062c\u0634\u0646\u0628\u0647 \u0627\u0632 \u0642\u0635\u062f \u06a9\u0634\u0648\u0631\u0634 \u0628\u0631\u0627\u06cc \u062e\u0631\u0648\u062c \u0627\u0632 \u0627\u06cc\u0646 \u067e\u06cc\u0645\u0627\u0646 \u0633\u062e\u0646 \u06af\u0641\u062a \u06a9\u0647 \u0648\u0627\u0634\u0646\u06af\u062a\u0646 \u0633\u0627\u0644 \u06af\u0630\u0634\u062a\u0647 \u0627\u0632 \u067e\u06cc\u0645\u0627\u0646 \u0645\u0646\u0639 \u0645\u0648\u0634\u06a9 \u200c\u0647\u0627\u06cc \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0645\u06cc\u0627\u0646 \u200c\u0628\u0631\u062f \u0628\u0627 \u0631\u0648\u0633\u06cc\u0647 \u062e\u0627\u0631\u062c \u0634\u062f\u0647 \u0628\u0648\u062f.&lt;\/p&gt;\n&lt;p&gt;&lt;strong&gt;&lt;a href=\&quot;https:\/\/t.me\/euronewspe\&quot;&gt;\u0628\u0647 \u06a9\u0627\u0646\u0627\u0644 \u062a\u0644\u06af\u0631\u0627\u0645 \u06cc\u0648\u0631\u0648\u0646\u06cc\u0648\u0632 \u0641\u0627\u0631\u0633\u06cc \u0628\u067e\u06cc\u0648\u0646\u062f\u06cc\u062f&lt;\/a&gt;&lt;\/strong&gt;&lt;\/p&gt;\n&lt;p&gt;\u0628\u0647 \u0627\u06cc\u0646 \u062a\u0631\u062a\u06cc\u0628 \u0627\u06a9\u0646\u0648\u0646 \u062a\u0631\u062f\u06cc\u062f\u0647\u0627\u06cc\u06cc \u062c\u062f\u06cc \u062f\u0631 \u0645\u0648\u0631\u062f \u062a\u0645\u062f\u06cc\u062f \u062f\u0648\u0631\u0647 \u067e\u06cc\u0645\u0627\u0646 \u06a9\u0627\u0647\u0634 \u062a\u0633\u0644\u06cc\u062d\u0627\u062a \u0647\u0633\u062a\u0647 \u0627\u06cc \u00ab\u0646\u06cc\u0648 \u0627\u0633\u062a\u0627\u0631\u062a\u00bb (New START) \u067e\u062f\u06cc\u062f \u0622\u0645\u062f\u0647 \u0627\u0633\u062a. \u062f\u0648\u0631\u0647 \u0627\u0639\u062a\u0628\u0627\u0631 \u067e\u06cc\u0645\u0627\u0646 \u00ab\u0646\u06cc\u0648 \u0627\u0633\u062a\u0627\u0631\u062a\u00bb \u062f\u0631 \u0645\u0627\u0647 \u0641\u0648\u0631\u06cc\u0647 \u0633\u0627\u0644 \u06f2\u06f0\u06f2\u06f1 (\u0628\u0647\u0645\u0646\/\u0627\u0633\u0641\u0646\u062f \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u0634\u0645\u0633\u06cc) \u0628\u0647 \u067e\u0627\u06cc\u0627\u0646 \u0645\u06cc \u0631\u0633\u062f. \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u06cc\u0627\u0644\u0627\u062a \u0645\u062a\u062d\u062f\u0647 \u062a\u0627\u06a9\u0646\u0648\u0646 \u0628\u0627\u0631\u0647\u0627 \u062e\u0648\u0627\u0633\u062a\u0627\u0631 \u0645\u0634\u0627\u0631\u06a9\u062a \u0686\u06cc\u0646 \u062f\u0631 \u06af\u0641\u062a\u06af\u0648\u0647\u0627\u06cc \u0622\u0645\u0631\u06cc\u06a9\u0627 \u0648 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0631\u0627\u06cc \u062f\u0633\u062a\u06cc\u0627\u0628\u06cc \u0628\u0647 \u062a\u0648\u0627\u0641\u0642\u06cc \u0628\u0631\u0627\u06cc \u062c\u0627\u06cc\u06af\u0632\u06cc\u0646\u06cc \u00ab\u0646\u06cc\u0648 \u0627\u0633\u062a\u0627\u0631\u062a\u00bb \u0634\u062f\u0647 \u0627\u0633\u062a. \u0627\u06cc\u0646 \u062f\u0631 \u062d\u0627\u0644\u06cc \u0627\u0633\u062a \u06a9\u0647 \u067e\u06a9\u0646 \u0628\u0627 \u0627\u06cc\u0646 \u067e\u06cc\u0634\u0646\u0647\u0627\u062f \u0645\u062e\u0627\u0644\u0641\u062a \u06a9\u0631\u062f\u0647 \u0627\u