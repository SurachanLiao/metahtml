 \u0647\u0633\u062a\u0647 \u0627\u06cc \u0631\u0627 \u06a9\u0627\u0647\u0634 \u062f\u0627\u062f\u0647 \u0627\u0646\u062f. \u062d\u062f\u0648\u062f \u06f4\u06f5 \u062f\u0631\u0635\u062f \u0627\u0644\u06a9\u062a\u0631\u06cc\u0633\u06cc\u062a\u0647 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f0\u06f6 \u062f\u0631 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0627\u06cc \u0647\u0633\u062a\u0647 \u0627\u06cc \u062a\u0648\u0644\u06cc\u062f \u0645\u06cc \u0634\u062f. \u0627\u06cc\u0646 \u0631\u0642\u0645 \u062f\u0631 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f8 \u0628\u0647 \u06f2\u06f8 \u062f\u0631\u0635\u062f \u06a9\u0627\u0647\u0634 \u06cc\u0627\u0641\u062a.  \n\n\u0628\u0647 \u06a9\u0627\u0646\u0627\u0644 \u062a\u0644\u06af\u0631\u0627\u0645 \u06cc\u0648\u0631\u0648\u0646\u06cc\u0648\u0632 \u0641\u0627\u0631\u0633\u06cc \u0628\u067e\u06cc\u0648\u0646\u062f\u06cc\u062f \n\n\u0628\u0627 \u0627\u06cc\u0646 \u062d\u0627\u0644 \u0647\u0645\u0647 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0639\u0636\u0648 \u0633\u06cc\u0627\u0633\u062a \u06cc\u06a9\u0633\u0627\u0646\u06cc \u0628\u0631\u0627\u06cc \u06a9\u0627\u0647\u0634 \u0633\u0647\u0645 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0627\u06cc \u0647\u0633\u062a\u0647 \u0627\u06cc \u062f\u0631 \u062a\u0648\u0644\u06cc\u062f \u0628\u0631\u0642 \u062f\u0646\u0628\u0627\u0644 \u0646\u0645\u06cc \u06a9\u0646\u0646\u062f. \u0641\u0631\u0627\u0646\u0633\u0647 \u0647\u0645\u0686\u0646\u0627\u0646 \u062f\u0647\u0647\u0627 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0633\u062a\u0647 \u0627\u06cc \u062f\u0627\u0631\u062f \u0627\u0645\u0627 \u0622\u0644\u0645\u0627\u0646 \u062f\u0631 \u0646\u0638\u0631 \u062f\u0627\u0631\u062f \u062a\u0627 \u0633\u0647\u0645 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0627\u06cc \u0647\u0633\u062a\u0647 \u0627\u06cc \u062f\u0631 \u062a\u0648\u0644\u06cc\u062f \u0628\u0631\u0642 \u0631\u0627 \u0628\u0647 \u0635\u0641\u0631 \u0628\u0631\u0633\u0627\u0646\u062f.  \n\n\u0627\u0646\u0631\u0698\u06cc \u0647\u0633\u062a\u0647 \u0627\u06cc \u0628\u0631\u0627\u06cc \u0628\u0644\u0627\u0631\u0648\u0633 \u0627\u0645\u0627 \u0646\u0647 \u062a\u0646\u0647\u0627 \u0645\u0646\u0628\u0639\u06cc \u0628\u0631\u0627\u06cc \u06a9\u0627\u0647\u0634 \u0648\u0627\u0628\u0633\u062a\u06af\u06cc \u0628\u0647 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0631 \u0632\u0645\u06cc\u0646\u0647 \u062a\u0627\u0645\u06cc\u0646 \u0627\u0644\u06a9\u062a\u0631\u06cc\u0633\u06cc\u062a\u0647 \u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f \u0628\u0644\u06a9\u0647 \u0627\u0641\u062a\u062e\u0627\u0631\u06cc \u0645\u0644\u06cc \u0646\u06cc\u0632 \u0645\u062d\u0633\u0648\u0628 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u062f\u0631 \u0645\u0642\u0627\u0628\u0644 \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u0628\u0631 \u0627\u06cc\u0646 \u0628\u0627\u0648\u0631 \u0627\u0633\u062a \u06a9\u0647 \u0627\u062d\u062f\u0627\u062b \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0627\u0648\u0633\u062a\u0648\u0631\u0648\u062a\u0633 \u062f\u0631 \u0639\u0645\u0644 \u0637\u0631\u062d \u0698\u0626\u0648\u067e\u0648\u0644\u06cc\u062a\u06cc\u06a9 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0627 \u0647\u062f\u0641 \u062d\u0645\u0627\u06cc\u062a \u0627\u0632 \u0628\u0644\u0627\u0631\u0648\u0633 \u0648 \u0636\u0631\u0628\u0647 \u0632\u062f\u0646 \u0628\u0647 \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u0627\u0633\u062a.  \n\n\u0622\u0631\u0648\u06cc\u062f\u0627\u0633 \u0633\u06a9\u0645\u0648\u06a9\u0627\u0633 \u062f\u0631 \u06af\u0641\u062a\u06af\u0648 \u0628\u0627 \u06cc\u0648\u0631\u0648\u0646\u06cc\u0648\u0632 \u062a\u0627\u06a9\u06cc\u062f \u06a9\u0631\u062f \u06a9\u0647 \u0627\u0633\u062a\u0627\u0646\u062f\u0627\u0631\u062f\u0647\u0627\u06cc \u0628\u06cc\u0646 \u0627\u0644\u0645\u0644\u0644\u06cc \u062f\u0631 \u0633\u0627\u062e\u062a \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0627\u0648\u0633\u062a\u0648\u0631\u0648\u062a\u0633 \u0646\u0642\u0636 \u0634\u062f\u0647 \u0627\u0633\u062a. \u0628\u0647 \u06af\u0641\u062a\u0647 \u0648\u0632\u06cc\u0631 \u0633\u0627\u0628\u0642 \u0627\u0646\u0631\u0698\u06cc \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u062f\u0648\u0644\u062a \u0628\u0644\u0627\u0631\u0648\u0633 \u062a\u0648\u0635\u06cc\u0647 \u0622\u0698\u0627\u0646\u0633 \u0628\u06cc\u0646 \u0627\u0644\u0645\u0644\u0644\u06cc \u0627\u0646\u0631\u0698\u06cc \u0627\u062a\u0645\u06cc \u0645\u0628\u0646\u06cc \u0628\u0631 \u0627\u062d\u062f\u0627\u062b \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0627\u06cc \u0627\u062a\u0645\u06cc \u062f\u0631 \u0641\u0627\u0635\u0644\u0647 \u0627\u06cc \u0628\u06cc\u0634 \u0627\u0632 \u06f1\u06f0\u06f0 \u06a9\u06cc\u0644\u0648\u0645\u062a\u0631 \u0627\u0632 \u0645\u0646\u0627\u0637\u0642 \u067e\u0631\u062c\u0645\u0639\u06cc\u062a \u0631\u0627 \u0646\u0627\u062f\u06cc\u062f\u0647 \u06af\u0631\u0641\u062a\u0647 \u0627\u0633\u062a.    \n\n\u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u0645\u0627\u0646\u0648\u0631\u0647\u0627\u06cc\u06cc \u0628\u0631\u0627\u06cc \u0622\u0645\u0627\u062f\u06af\u06cc \u062f\u0631 \u0628\u0631\u0627\u0628\u0631 \u062d\u0627\u062f\u062b\u0647 \u0647\u0633\u062a\u0647 \u0627\u06cc \u0627\u062d\u062a\u0645\u0627\u0644\u06cc \u062f\u0631 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0627\u0648\u0633\u062a\u0648\u0631\u0648\u062a\u0633 \u0628\u0631\u06af\u0632\u0627\u0631 \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a. \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u062d\u062a\u06cc \u062e\u0648\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0628\u062f\u062a\u0631\u06cc\u0646 \u062d\u0627\u0644\u062a \u0645\u0645\u06a9\u0646 \u06cc\u0639\u0646\u06cc \u062a\u062e\u0644\u06cc\u0647 \u0627\u062d\u062a\u0645\u0627\u0644\u06cc \u0634\u0647\u0631 \u0648\u06cc\u0644\u0646\u06cc\u0648\u0633 \u06a9\u0647 \u0628\u06cc\u0634 \u0627\u0632 \u06f5\u06f0\u06f0 \u0647\u0632\u0627\u0631 \u0646\u0641\u0631 \u062c\u0645\u0639\u06cc\u062a \u062f\u0627\u0631\u062f \u0622\u0645\u0627\u062f\u0647 \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.  \n\n\u0644\u06cc\u0646\u0627\u0633 \u0644\u06cc\u0646\u06a9\u0627\u0648\u06cc\u0686\u0633\u060c \u0648\u0632\u06cc\u0631 \u062e\u0627\u0631\u062c\u0647 \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u0628\u0631 \u0627\u06cc\u0646 \u0628\u0627\u0648\u0631 \u0627\u0633\u062a \u06a9\u0647 \u06a9\u0634\u0648\u0631\u0634 \u0628\u0627\u06cc\u062f \u0628\u0627 \u0647\u0645\u06a9\u0627\u0631\u06cc \u0627\u062a\u062d\u0627\u062f\u06cc\u0647 \u0627\u0631\u0648\u067e\u0627 \u0627\u0632 \u0627\u06cc\u0645\u0646\u06cc \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0627\u0648\u0633\u062a\u0648\u0631\u0648\u062a\u0633 \u0627\u0637\u0645\u06cc\u0646\u0627\u0646 \u062d\u0627\u0635\u0644 \u06a9\u0646\u062f.  \n\n&quot;,&quot;htmlText&quot;:&quot;&lt;p&gt;\u0622\u0631\u0648\u06cc\u062f\u0627\u0633 \u0633\u06a9\u0645\u0648\u06a9\u0627\u0633\u060c \u0648\u0632\u06cc\u0631 \u0633\u0627\u0628\u0642 \u0627\u0646\u0631\u0698\u06cc \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u062f\u0631 \u0645\u0635\u0627\u062d\u0628\u0647 \u0628\u0627 \u06cc\u0648\u0631\u0648\u0646\u06cc\u0648\u0632 \u0647\u0634\u062f\u0627\u0631 \u062f\u0627\u062f \u06a9\u0647 \u0627\u0631\u0648\u067e\u0627 \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u062f\u0631 \u0622\u06cc\u0646\u062f\u0647 \u0628\u0627\u0628\u062a \u0639\u062f\u0645 \u062c\u0644\u0648\u06af\u06cc\u0631\u06cc \u0627\u0632 \u0622\u063a\u0627\u0632 \u0628\u0647 \u06a9\u0627\u0631 \u0627\u0648\u0644\u06cc\u0646 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0633\u062a\u0647 \u0627\u06cc \u0628\u0644\u0627\u0631\u0648\u0633 \u00ab\u0647\u0632\u06cc\u0646\u0647 \u0633\u0646\u06af\u06cc\u0646\u06cc\u00bb \u0628\u067e\u0631\u062f\u0627\u0632\u062f.&lt;\/p&gt;\n&lt;p&gt;\u0627\u0648\u0644\u06cc\u0646 \u062a\u0627\u0633\u06cc\u0633\u0627\u062a \u0647\u0633\u062a\u0647 \u0627\u06cc \u0628\u0644\u0627\u0631\u0648\u0633 \u06a9\u0647 \u062f\u0631 \u0627\u0648\u0633\u062a\u0648\u0631\u0648\u062a\u0633 (Ostrovets) \u062f\u0631 \u0634\u0645\u0627\u0644 \u063a\u0631\u0628\u06cc \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0642\u0631\u0627\u0631 \u062f\u0627\u0631\u062f \u0642\u0631\u0627\u0631 \u0627\u0633\u062a \u0645\u0627\u0647 \u0698\u0648\u0626\u06cc\u0647 \u0627\u0645\u0633\u0627\u0644 (\u062a\u06cc\u0631\/\u0645\u0631\u062f\u0627\u062f) \u0641\u0639\u0627\u0644\u06cc\u062a \u062e\u0648\u062f \u0631\u0627 \u0622\u063a\u0627\u0632 \u06a9\u0646\u062f. \u0627\u06cc\u0646 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u062a\u0646\u0647\u0627 \u06f4\u06f5 \u06a9\u06cc\u0644\u0648\u0645\u062a\u0631 \u0628\u0627 \u0648\u06cc\u0644\u0646\u06cc\u0648\u0633\u060c \u067e\u0627\u06cc\u062a\u062e\u062a \u0644\u06cc\u062a\u0648\u0627\u0646\u06cc \u0641\u0627\u0635\u0644\u0647 \u062f\u0627\u0631\u062f.&lt;\/p&gt;\n&lt;p&gt;\u062f\u0648\u0644\u062a \u0628\u0644\u0627\u0631\u0648\u0633 \u062f\u0631 \u062d\u0627\u0644\u06cc \u0628\u0647 \u062f\u0646\u0628\u0627\u0644 \u0627\u0641\u062a\u062a\u0627\u062d \u0627\u0648\u0644\u06cc\u0646 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0647\u0633\u062a\u0647 \u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0627\u0633\u062a \u06a9\u0647 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0645\u0646\u0637\u0642\u0647 \u0647\u0645\u0686\u0646\u0627\u0646 \u062a\u062d\u062a \u062a\u0627\u062b\u06cc\u0631 \u0641\u0627\u062c\u0639\u0647 \u0647\u0633\u062a\u0647 \u0627\u06cc \u06f3\u06f4 \u0633\u0627\u0644 \u067e\u06cc\u0634 \u062f\u0631 \u0646\u06cc\u0631\u0648\u06af\u0627\u0647 \u0686\u0631\u0646\u0648\u0628\u06cc\u0644 \u0642\u0631\u0627\u0631 \u062f\u0627\u0631\u0646\u062f.&lt;\/p&gt;\n&lt;p&gt;\u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u0627\u0631\u0648\u067e\u0627\u06cc\u06cc \u062f\u0631 \u0633\u0627\u0644\u0647\u0627\u06cc \u0627\u062e\u06cc\u0631 \u0648 \u067e\u0633 \u0627\u0632 \u062d\u0627\u062f\u062b\u0647 \u0633\u0627\u0644 \u06f2\u06f0\u06f1\u06f1 \u062