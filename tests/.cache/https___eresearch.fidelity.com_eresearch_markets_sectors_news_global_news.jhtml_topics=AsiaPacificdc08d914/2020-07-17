 <script>
          function addCheckboxTickEvents()
          {
              $("#allregions ") .click(function () { 
            	 
            	  if($(this).is(':checked'))
           	  	  {
            		  $(this).parent().parent().find('.subregion >:checkbox').attr('checked', false);
            	  }else{//Do not allow to unchecking all regions box manually.
            		 // $(this).attr('checked', true); 
            	  }
              }); 
              $("#allcountries ") .click(function () { 
             	 
            	  if($(this).is(':checked'))
           	  	  {
            		 $(this).parent().parent().find('.country >:checkbox').attr('checked', false);
           	  	  }else{//Do not allow to unchecking all regions box manually.
            		//  $(this).attr('checked', true); 
            	  }
              }); 
              
              $(".country :checkbox ") .click(function () { 
              	 
            	  if($(this).is(':checked'))
           	  	  {
            		 $("#allcountries").attr('checked', false);
           	  	  }else{
           	  		//check whether the others are also unchecked, if they are, set the all countries to 'checked'
      		    	if ($(".country  ").find(':checked').length == 0)
      		    	{
      		    		$("#allcountries").attr('checked', true);
      		    	}
           	  	
           	  	  }
              }); 
              $(".subregion :checkbox ") .click(function () { 
               	 
            	  if($(this).is(':checked'))
           	  	  {
            		 $("#allregions").attr('checked', false);
            		 //$(this).parent().siblings('.country').find(':checkbox').attr('checked', true);
            		// $("#allcountries").attr('checked', false);
           	  	  }else{
           	  		// $(this).parent().siblings('.country').find(':checkbox').attr('checked', false);
	           	  	if ($(".subregion  ").find(':checked').length == 0)
	  		    	{
	  		    		$("#allregions").attr('checked', true);
	  		    	}
           	  	  }
              }); 
          
          }
          </script>
<script type="text/javascript">
  
$(document).ready(function() {
		
	addCheckboxTickEvents();
	displaySelectRegions();	
	//postLoadImages();
});
function actionOK(){ 
	 $(".geography-custom-selectbox").css("border","1px solid #BDBDBD").css("border-right","none");
     $("#geographySelectBox").hide();
    
     displaySelectRegions();	
}
function displaySelectRegions()
{
	
	if($("#geographySelectBox").find(':checked').length == 0)
	{
		$("#allcountries").attr('checked', true);
		$("#allregions").attr('checked', true);
	}
	
	var showContent = "";
	var isAllRegions = $("#allregions").is(":checked")  ;
	var isAllCountries = $("#allcountries").is(":checked");
	
	if(isAllRegions&&isAllCountries)
	{
		showContent = "All Regions & All Countries";
	}else{
		
		var countCountries = 0;
		
		$(".subregion :checkbox ") .each(function () { 
          	 
      	  if($(this).is(':checked'))
     	  {
      		
      		showContent = showContent+", "+ $(this).siblings('label').text();
      		countCountries++;
     	  }
        }); 
		$(".country :checkbox ") .each(function () { 
         	 
	      	  if($(this).is(':checked'))
	     	  {
	      		showContent = showContent+", "+$(this).siblings('label').text();
	      		countCountries++;
	     	  }
	    }); 
	
		//if(showContent.charAt(0)==',')
		{
			showContent = showContent.substring(1);
			
		}
	
		var limitLength = (isAllRegions||isAllCountries)? 20:30;
		if(showContent.length > limitLength)
		{
			showContent = showContent.substring(0,limitLength) + "...";
			
		}
		if(countCountries == 0)
		{
			showContent ="";
		}else{
			showContent = countCountries + " ("+showContent+")";
		}
		if(isAllRegions)
		{
			showContent = "All Regions" +(showContent.length>0?" & ":"")+showContent;
		}
		if(isAllCountries)
		{
			showContent = "All Countries" +(showContent.length>0?" & ":"")+showContent;
		}
	}
	
	$("#selectRegion").attr("value",showContent);
}
function isValidDate(date)
{
    var matches = /^(\d{2})[\/](\d{2})[\/](\d{4})$/.exec(date);
    if (matches == null) return false;
    var d = matches[2];
    var m = matches[1] - 1;
    var y = matches[3];
    var composedDate = new Date(y, m, d);
    return composedDate.getDate() == d &&
            composedDate.getMonth() == m &&
            composedDate.getFullYear() == y;
}
function dateValidation()
{
  if(!isValidDate($("#date1").attr("value")))
	{
	    alert("Please input a valid 'From Date'.");
	    return false;
	}
	if(!isValidDate($("#date2").attr("value")))
	{
	    alert("Please input a valid 'To Date'.");
	    return false;
	}
	
	var days = 0;
	var difference = 0;
	toDate = new Date($("#date2").attr("value"));
	today = new Date();
	difference = today - toDate;
	days = Math.round(difference/(1000*60*60*24));
	
	if(days<0)
	{
		$("#date2").attr("value",today.asString());
	}
	
	
	fromDate = new Date($("#date1").attr("value"));
	difference = today - fromDate;
	days = Math.round(difference/(1000*60*60*24));
	if(days <0)
	{
		var selectedRange = $('input:radio[name=pubdate]:checked').val(); 
		$("#date1").attr("value",today.addDays(-selectedRange).asString());
	}
	else if(days>90)
	{
		
		$("#date1").attr("value",today.addDays(-90).asString());
	}
	
	difference = toDate - fromDate;
	days = Math.round(difference/(1000*60*60*24));
	if(days <0)
	{
		alert('Invalid Search Criteria. "From" date cannot greater than "To" date.');
		return false;
	}
	return true;
}

function goAction(){
	if(!dateValidation())
	{
		return false;
	}
}
</script>



