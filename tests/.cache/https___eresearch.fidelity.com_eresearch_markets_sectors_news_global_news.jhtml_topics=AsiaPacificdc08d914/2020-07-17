    <script type="text/javascript" src="https://www.fidelity.com/webcontent/ap101883-markets_sectors-content/20.06.0/js/jquery.datePicker.js"></script>
    <script type="text/javascript" src="https://www.fidelity.com/webcontent/ap101883-markets_sectors-content/20.06.0/js/date.js"></script>
 <script type="text/javascript" src="https://www.fidelity.com/webcontent/ap101883-markets_sectors-content/20.06.0/js/dropdown.js"></script>
<script type="text/javascript">

		function expandFilter()
		{
			 $('#news-article-content').show();
			 
			 $('.expand-collapse-img').attr('src','https://www.fidelity.com/webcontent/ap101883-markets_sectors-content/20.06.0/images/collapse_blue.gif');
			$('.expand-collapse-img').attr('alt','Hide');
			$('.expand-collapse-img').attr('title','Hide'); 
		}
		function collapseFilter()
		{ 
			$('#news-article-content').hide();
			$('.expand-collapse-img').attr('src','https://www.fidelity.com/webcontent/ap101883-markets_sectors-content/20.06.0/images/expand_blue.gif');
			$('.expand-collapse-img').attr('alt','Show');
			$('.expand-collapse-img').attr('title','Show');
			$(".geography-custom-selectbox").css("border","1px solid #BDBDBD");
			$("#geographySelectBox").hide();
			displaySelectRegions();	
		} 
		
		$('.header').click(function(){	
			//var checkClass = $(this).hasClass('expand-collapse-img');
			var isVisible = $('#news-article-content').is(':visible');
			if(isVisible){
				collapseFilter();
			}
			else{					
				expandFilter();
			}				
		});	
		$("input:radio").change(function (event) {
			var dtm = new Date();
			var startDate = dtm.addDays(-$(this).attr("value"));
			
			var defaultDate = new Date().addDays(-91);
			$('#date1').dpSetStartDate(defaultDate);
			
			$('#date1').dpSetSelected(startDate.asString());
			
			$('#date1').dpSetStartDate( new Date().addDays(-90));
			
			$('#date2').dpSetSelected(new Date().asString());
			
		});
		
		$(document).ready(function() {
					
			var defaultDate = new Date().addDays(-90);
			$('.date-pick').dpSetStartDate(defaultDate);
			
		});
		
</script>


				</div>
