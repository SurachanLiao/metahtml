<script type="text/html" id="multimedia-thumbnail-template">

    <div class="modalcarousel__item" data-bind="click: $parentContext.$parent.navToClick.bind($index()), css: {'modalcarousel__item--active' : $parentContext.$parent.activeIndex() == $index()}, attr: { title: item.Title}, modalCarousel, css-static: 'modalcarousel__item--' + item.MediaType()">
        <div class="modalcarousel__item--wrapper">
            <div class="modalcarousel__item--imagewrapper h-nocopy" >
                <img oncontextmenu="return false;" data-bind="lazyload: {src: item.ThumbnailUrl, options: {loadOnlyInViewport: true} , updateTick: $parentContext.$parent.carouselScrollTick}, attr: {alt: item.Title }, scrollIntoView:$parentContext.$parent.activeIndex() == $index()" />
            </div>
            <div class="modalcarousel__item--text" data-bind="html: item.Title, ellipsis: item.Title"> </div>
        </div>
        <div class="modalcarousel__item--number" data-bind="text: ($index() + 1)"></div>
    </div>
</script><script type="text/javascript">
