                if (window.screen.width >= 600 && screen.width <= 999) {
                    stc = { "device": "tablet" };
                }

                let xtparamValue = "&ac=" + xt_ac + "&an=" + xt_an + xt_multc + "&tag=" + xt_tags + "&stc=" + JSON.stringify(stc);

                if (window.xtparam != null) {
                    window.xtparam += xtparamValue
                } else {
                    window.xtparam = xtparamValue;
                }
                //-->
            </script>
