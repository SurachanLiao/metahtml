	<script type="text/javascript">
	function setVote(){
	if(vote_options){
	var leftObj = document.getElementById('vote');
	var rightObj = document.getElementById('unVote');
	var opt1=vote_options['591035']+1;
	var opt2=vote_options['591036']+1;
	var sum = parseInt(opt1)+parseInt(opt2);
	var rate1 = opt1/sum?opt1/sum:0.5;
	var rate2 = opt2/sum?opt2/sum:0.5;
	var w1 = 170*rate1;
	if(rate1>rate2){
	leftObj.className = 'vote largeFont';
	rightObj.className = 'unVote smallFont'; 
	}else if(rate1<rate2){
	leftObj.className = 'vote smallFont';
	rightObj.className = 'unVote largeFont';
	}else{
	leftObj.className = 'vote sameFont';
	rightObj.className = 'unVote sameFont';
	}
	//var w2 = 120*rate2;
	leftObj.innerHTML = Math.round(rate1*100)+'%';
	leftObj.style.width = Math.round(w1-1)+'px';
	rightObj.innerHTML = Math.round(rate2*100)+'%';
	rightObj.style.width = 170-Math.round(w1-1)+'px';
	}
	};
	setVote();
	</script>

