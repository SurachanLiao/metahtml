					function $(id){return document.getElementById(id);}
					function switchImgTo(a,b,c) {
						$("img_"+b).style.display="none";
						$("img_"+c).style.display="none";
						$("img_"+a).style.display="";
					}
					var arrayIndex=["b_SHCOMP","b_HSI","b_NKY","b_RTY","b_SPX","b_CCMP","b_UKX","b_DAX","b_BE500","USDCNY","EUR","GBP"];
					var arrayIndex1=["s_sh000001","int_hangseng","int_nikkei","dji","int_sp500","int_nasdaq","int_ftse","int_dax30","int_bloombergeuropean500","USDCNY","EUR","GBP"];
					var urlHQ="http://hq.sinajs.cn/func=fillHQ();&rn=@&list=b_SHCOMP,b_HSI,b_NKY,b_RTY,b_SPX,b_CCMP,b_UKX,b_DAX,b_BE500,USDCNY,EUR,GBP,JPY,CAD";
					var urlImg="http://image.sinajs.cn/newchart/v5/worldindex/mins/@.gif";
					function loadHQ() {
						var e=document.createElement("script");
						e.type="text/javascript";
						e.charset = "gb2312";
						e.src=urlHQ.replace("@",(new Date()).getTime());
						e[document.all?"onreadystatechange":"onload"]=function(){
							if(document.all)if(this.readyState!="loaded"&&this.readyState!="complete")return;
							this[document.all?"onreadystatechange":"onload"] = null;
							this.parentNode.removeChild(this);
						};
						$("script_loader").appendChild(e);
						setTimeout("loadHQ();",15000);
					}
					loadHQ();
					function fillHQ() {
						for(var i=0;i<9;i++) {
							var arr = window["hq_str_"+arrayIndex[i]].split(",");
							$("p_"+arrayIndex[i]).innerHTML=arr[1];
							$("c_"+arrayIndex[i]).innerHTML='<span class="'+getColor(arr[3])+'">'+arr[3]+'%</span>';
						}
						for(var i=9;i<arrayIndex.length;i++) {
							var arr = window["hq_str_"+arrayIndex[i]].split(",");
							var c=((arr[8]-arr[3])/(arr[3]*1)*100).toFixed(2);
							$("p_"+arrayIndex[i]).innerHTML=arr[8];
							$("c_"+arrayIndex[i]).innerHTML='<span class="'+getColor(c)+'">'+c+'%</span>';
						}
					}
					function getColor(v) {
						var c="gary";if(v*1>0)c="green";else if(v*1<0)c="red"; return c;
					}
					function loadImg() {
						for(var i=0;i<9;i++){
							if(arrayIndex[i] == 'b_RTY') arrayIndex[i] = 'b_RTY';
							$("img_"+arrayIndex[i]).src=urlImg.replace("@",arrayIndex1[i].replace(/^(s_)|(int_)/,""))+"?"+(new Date()).getTime();
							if(arrayIndex[i] == 'b_RTY') arrayIndex[i] = 'b_RTY';
						}
						setTimeout("loadImg();",900000);
					}
					loadImg();
				</script>	
