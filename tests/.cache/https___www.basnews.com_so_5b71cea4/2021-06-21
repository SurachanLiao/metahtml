<script>
//bapala
var slideIndex3 = 1;
showDivs3(slideIndex3);

function plusDivs3(n) {
  showDivs3(slideIndex3 += n);
}

function currentDiv3(n) {
  showDivs3(slideIndex3 = n);
}

function showDivs3(n) {
  var i;
  var x = document.getElementsByClassName("bapele");
  if (n > x.length) {slideIndex3 = 1}
  if (n < 1) {slideIndex3 = x.length}
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";  
  }
  x[slideIndex3-1].style.display = "block";  

}

setInterval(function(){
    slideIndex3++;
    currentDiv3(slideIndex3);
}, 6000);

</script>

<script>
var slideIndex = 1;
showDivs(slideIndex);

function plusDivs(n) {
  showDivs(slideIndex += n);
}

function currentDiv(n) {
  showDivs(slideIndex = n);
}

function showDivs(n) {
  var i;
  var x = document.getElementsByClassName("slide");
  var dots = document.getElementsByClassName("demo");
  if (n > x.length) {slideIndex = 1}
  if (n < 1) {slideIndex = x.length}
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";  
  }
  for (i = 0; i < dots.length; i++) {
    dots[i].className = dots[i].className.replace(" w3-white", "");
  }
  x[slideIndex-1].style.display = "block";  
  dots[slideIndex-1].className += " w3-white";
}


var myVar;
function myFunction() {
myVar = setInterval(barsh, 5000);
}

function barsh(){
slideIndex++;
    currentDiv(slideIndex);
     
}
myFunction();

function myStopFunction() {
  clearInterval(myVar);
}

</script>


<script>
//lifestyle
var slideIndex1 = 1;
showDivs2(slideIndex1);

function currentDiv1(n) {
  showDivs2(slideIndex1 = n);
}

function showDivs2(n) {
  var i;
  var x = document.getElementsByClassName("mymy");
  var dots = document.getElementsByClassName("dd");
  if (n > x.length) {slideIndex1 = 1}
  if (n < 1) {slideIndex1 = x.length}
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";
  }
  for (i = 0; i < dots.length; i++) {
    dots[i].className = dots[i].className.replace(" w3-opacity-off", "");
  }
  x[slideIndex1-1].style.display = "block";
  dots[slideIndex1-1].className += " w3-opacity-off";
}


setInterval(function(){
    slideIndex1++;
    currentDiv1(slideIndex1);
}, 5000);
</script>


