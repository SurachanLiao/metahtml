        // ------------------------- / At-Internet object start / ------------------------- //
        (function () {
            var at = document.createElement('script');
            at.type = 'text/javascript';
            at.async = true;
            at.src = '/js/smarttag-production.js';
            (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0] || document.getElementsByTagName('script')[0].parentNode).insertBefore(at, null);
        })();

        // Initialize AT Internet library tag asynchronously
        window.ATInternet = {
            onTrackerLoad: function () {
                window.ATInternet.Utils.consentReceived(false);
                window.tag = new window.ATInternet.Tracker.Tag({ ignoreEmptyChapterValue: true, sendHitWhenOptOut: false});
                window.tag.page.set({
                name: 'council-preparatory-bodies',
                level2: '3',
                chapter1: 'preparatory-bodies',
                chapter2: '',
                chapter3: ''
                });

                window.tag.customVars.set({
                site: {
                    1: 'ro',
                    2: '05/10/2017',
                    3: '10',
                    4: '2017',
                    5: 'Council of European Union',
                    6: 'subsection',
                    7: '0',
                    9: 'Council preparatory bodies',
                    10: encodeURIComponent(document.URL),
                    11: encodeURIComponent(document.referrer)
                    },
                    /*REGISTER-TRACKING-PLACEHOLDER*/
                });

                window.tag.internalSearch.set({
                keyword: '',
                resultPageNumber: '0'
                });

                if (document.cookie.indexOf('atuserid=') == -1 && document.cookie.indexOf('atoptedout=') == -1) {
                    window.tag.clientSideUserId.set("Consent-NO");
                }

                window.tag.dispatch();
            }
        };

        // ------------------------- / At-Internet object end / ------------------------- //
    </script>



