    <div data-teaser-lid="34480624" data-teaser-cid="25548988" data-teaser-rlid="38e3eff04e15a536806458e90dc621cf" data-teaser-rcid="X6RQ2Q42MZT6IQKFQO4UYUENWE"></div>
    <div class="pdb-teaser3-teaser"
         data-component="linkblock"                    >
                                                                                                                                                                        

        <div class="pdb-image pdb-teaser3-image pdb-teaser3-image_teaser3"
    >
                    <div class="pdb-teaser3-image-picture" data-imagesasyncloader="wrapper">
