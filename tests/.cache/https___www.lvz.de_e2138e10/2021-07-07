                <script>
    // Build: 2021-05-17 10:39:06

    function getSlotPosition(slotid) {
        var atfSlots = ['Top', 'Offcontent-Right', 'Rectangle-1'];
        if (atfSlots.indexOf(slotid) !== -1) {
            return 'btf';
        }

        return 'atf';
    }

    function removeUsableSlotKeys(arrKeys) {
        if (!Array.isArray(arrKeys)) return;
        arrKeys.forEach(
            function (element) {
                removeUsableSlotKey(element);
            });
    }

    function removeUsableSlotKey(key) {
        if (typeof key !== 'string') return;
        var index = window.usableSlots.indexOf(key);
        if (index > -1) {
            window.usableSlots.splice(index, 1);
        }
    }

    function getIndexExchangeSlotSize(slotId) {
        switch (slotId) {
            case ('Top'):
                if (isMobile) {
                    if (window.viewability === 'nicht lesbar') {
                        return [300, 50];
                    } else {
                        return [300, 250];
                    }
                } else {
                    return [728, 90];
                }
            case ('Offcontent-Right'):
                return [120, 600];
            case ('Rectangle-1'):
            case ('Rectangle-2'):
            case ('Rectangle-3'):
            case ('Rectangle-4'):
            case ('Inlist-1'):
            case ('Inlist-2'):
            case ('Inlist-3'):
            case ('Inlist-4'):
            case ('Inlist-5'):
            case ('Inlist-6'):
            case ('Inlist-7'):
            case ('Inlist-8'):
            case ('Inlist-9'):
                return [300, 250];
            case ('Inlist-Banner-1'):
            case ('Inlist-Banner-2'):
            case ('Inlist-Banner-3'):
            case ('Inlist-Banner-4'):
                return [728, 90];
            default:
                return [];
        }
    }

