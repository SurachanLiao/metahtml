    if (adPageType === 'article') {
              window.slots['gpt_rectangle-1'].sizes = window.slotSizeDeclarations['rectangleMulti'];
        window.blockArticleRectangle = (window.blockArticleRectangle || false);
        if (window.blockArticleRectangle) {
            removeUsableSlotKey('gpt_rectangle-1');
        }
        if (window.viewability === 'nicht lesbar') {
            window.slots['gpt_top'].sizes = window.slotSizeDeclarations['topPaywall'];
            window.slots['gpt_rectangle-mobile-1'].sizes = window.slotSizeDeclarations['rectangleMobilePaywall'];
        }
        if (window.isMobile) {
            removeUsableSlotKey('gpt_outofpage');

            if (isFirstPageImpression()) {
                window.sessionStorage.setItem('firstImpressionDone', true);
            } else {
                window.usableSlots.push('gpt_interstitial');
            }
        }
    }

            if (adPageType === 'gallery') {
            window.galleryImageCount = window.galleryImageCount || 0;
            removeUsableSlotKey('gpt_outofpage');
                      if (window.galleryImageCount > 1) {
                window.usableSlots.push('gpt_rectangle-2');
            }
            if (window.galleryImageCount > 2) {
                window.usableSlots.push('gpt_rectangle-3');
            }
                  }
    
    if (adPageType === 'frontpage-plugin' || adPageType === 'poll') {
        removeUsableSlotKey('gpt_rectangle-mobile-1');
        if (adPageType === 'poll') {
            removeUsableSlotKey('gpt_rectangle-1');
        }
    }

    window.adUnits = [];
    window.usableSlots.forEach(
        function (element) {
            var adUnit = {};
            var bidders = (typeof window.bidders[window.slots[element].slotPosition]) != 'undefined' ?
                window.bidders[window.slots[element].slotPosition] : [];
            if (bidders.length) {
                adUnit = {
                    slotName: window.slotPrefix + window.slots[element].slotPosition,
                    container: window.slots[element].container,
                    sizes: window.slots[element].sizes,
                    bids: bidders
                };
                if (typeof (window.slots[element].sticky) != 'undefined' && window.slots[element].sticky) {
                    adUnit.sticky = { top: 50 }
                }
                if (typeof (window.slots[element].refresh) != 'undefined' && window.slots[element].refresh) {
                    adUnit.refresh = {
                        every: 30,
                        require: ['focus', 'visibility', 'viewable'],
                        targetingNameCounter: 'counter'
                    }
                }
            } else {
                adUnit = {
                    slotName: window.slotPrefix + window.slots[element].slotPosition,
                    container: window.slots[element].container,
                    sizes: window.slots[element].sizes,
                    amazon: false
                };
            }
            window.adUnits.push(adUnit);
        });

    window.adUnits = {'1_10000': window.adUnits};
    window.adHandler.service.addAdUnits(window.adUnits);

