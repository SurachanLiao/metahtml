					<a href="{{getURL customURL URL}}"><img src="http://cdn2.hubspot.net/hubfs/2221756/icons/offer-next-button.png"></a>
				</div>
			</div>
			<div class="right" style="background: url({{image}}) center center no-repeat;"></div>
		</div>
		{{/each}}
	</script>
	
	<script>
		var slidePosts = [];
		
		
			

			slidePosts.push({
				"playBtn":		"",
				"customURL":	"",
				"date":			"Fri  |  Jul 16, 2021  |  1:44 PM PDT",
				"URL":			"https://www.secureworld.io/industry-news/iranian-spies-facebook-target-u.s",
				"image":		"https://info.secureworldexpo.com/hubfs/Blog_images/Iran_cyber_attack_shutterstock_1329004469.jpg",
				"title":		"Iranian Cyber Ops Used Facebook to Spy on U.S. Targets",
