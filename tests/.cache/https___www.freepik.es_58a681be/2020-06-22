                pti: 'printTypeId',
                tp: 'typology',
				ris: 'ris',
				fe: 'freepikEditor',
				fs: 'freepikStories',
            }
            var link = document.getElementById("dtl-" + detailId);
            for (var k in attr) {
                link.dataset[names[k] || k] = attr[k];
            }
        }
