  <!--[IF !IE]> -->
          <aside id="park-portal-2135833785" class="park-portal" aria-hidden="true">
                                                    <style>
          @media screen and (max-width: 48em) {
            /* type is defined */
                                      #park-portal-2135833785 {
                              min-width: 100%;
                                            min-height: 250px;
