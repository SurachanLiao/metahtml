            else{
                var el = document.querySelector('.b-article__vote-content[data-vote="'+voteId+'"] .b-vote-form');
                voteForm.style.display = 'none';
                votePoll.style.display = 'block';
            }
        </script></div></div></div></div></div></div><footer class="footer"><div class="l-wrap m-oh"><div class="m-pl36"><div class="b-cal"><div class="l-wrap"><div class="l-maincolumn"><div class="b-calendar"><div class="b-title">Archivo</div><div class="b-cal__months"><div class="b-cal__months-container"><ul class="b-cal__months-list"><li class="b-cal__months-item"><a href="#" data-href="/espana/20110201/calendar.html">Febrero 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110301/calendar.html">Marzo 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110401/calendar.html">Abril 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110501/calendar.html">Mayo 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110601/calendar.html">Junio 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110701/calendar.html">Julio 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110801/calendar.html">Agosto 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20110901/calendar.html">Septiembre 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20111001/calendar.html">Octubre 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20111101/calendar.html">Noviembre 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20111201/calendar.html">Diciembre 2011</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20120101/calendar.html">Enero 2012</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20120201/calendar.html">Febrero 2012</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20120301/calendar.html">Marzo 2012</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20120401/calendar.html">Abril 2012</a></li><li class="b-cal__months-item"><a href="#" data-href="/espana/20120