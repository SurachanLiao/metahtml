      <!--   Mots clés (thématique et géographique)   -->
                <div class="contenugen-listmot contenugen-listmotcle">
              <div class="bloc-title">
                  <div class="text-resize-em">
                    Mots-clés                      <span class="border-bottom"></span>
                  </div>
              </div>

              <div class="list-motcle">
                                                          <span class="one-motcle">
					
				Daesh			</span>
                                          <span class="one-motcle">
					
				frontière			</span>
                                          <span class="one-motcle">
					
				guerre			</span>
                                          <span class="one-motcle">
					
				Terrorisme			</span>
                                  
