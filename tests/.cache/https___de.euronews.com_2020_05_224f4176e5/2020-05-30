isplayOverlay&quot;:0},&quot;displayType&quot;:&quot;default&quot;,&quot;video&quot;:1,&quot;videos&quot;:[{&quot;format&quot;:&quot;mp4&quot;,&quot;quality&quot;:&quot;md&quot;,&quot;type&quot;:&quot;normal&quot;,&quot;url&quot;:&quot;https:\/\/video.euronews.com\/mp4\/med\/EN\/IS\/SU\/20\/05\/22\/de\/200522_ISSU_11863295_11863297_480000_171436_de.mp4&quot;,&quot;editor&quot;:null,&quot;duration&quot;:480000,&quot;filesizeBytes&quot;:49510403,&quot;expiresAt&quot;:0,&quot;youtubeId&quot;:&quot;pSXd1mTbi8M&quot;},{&quot;format&quot;:&quot;mp4&quot;,&quot;quality&quot;:&quot;hd&quot;,&quot;type&quot;:&quot;normal&quot;,&quot;url&quot;:&quot;https:\/\/video.euronews.com\/mp4\/EN\/IS\/SU\/20\/05\/22\/de\/200522_ISSU_11863295_11863297_480000_171436_de.mp4&quot;,&quot;editor&quot;:null,&quot;duration&quot;:480000,&quot;filesizeBytes&quot;:80813856,&quot;expiresAt&quot;:0,&quot;youtubeId&quot;:&quot;pSXd1mTbi8M&quot;}],&quot;liveStream&quot;:[{&quot;startDate&quot;:0,&quot;endDate&quot;:0}],&quot;isLiveCoverage&quot;:0,&quot;scribbleLiveId&quot;:0,&quot;scribbleLiveRibbon&quot;:0,&quot;sourceId&quot;:1,&quot;sources&quot;:[],&quot;freeField2&quot;:null,&quot;type&quot;:&quot;&quot;,&quot;program&quot;:{&quot;id&quot;:&quot;unreported-europe&quot;,&quot;urlSafeValue&quot;:&quot;unreported-europe&quot;,&quot;title&quot;:&quot;Unreported Europe&quot;,&quot;online&quot;:1,&quot;url&quot;:&quot;\/programme\/unreported-europe&quot;},&quot;vertical&quot;:&quot;news&quot;,&quot;verticals&quot;:[{&quot;id&quot;:1,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;nachrichten&quot;,&quot;title&quot;:&quot;Nachrichten&quot;}],&quot;primaryVertical&quot;:{&quot;id&quot;:1,&quot;slug&quot;:&quot;news&quot;,&quot;urlSafeValue&quot;:&quot;nachrichten&quot;,&quot;title&quot;:&quot;Nachrichten&quot;},&quot;themes&quot;:[{&quot;id&quot;:1,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;,&quot;translation&quot;:&quot;Welt&quot;,&quot;url&quot;:&quot;\/\/de.euronews.com\/nachrichten\/international&quot;}],&quot;primaryTheme&quot;:{&quot;id&quot;:1,&quot;urlSafeValue&quot;:&quot;news&quot;,&quot;title&quot;:&quot;News&quot;},&quot;advertising&quot;:0,&quot;advertisingData&quot;:{&quot;startDate&quot;:0,&quot;endDate&quot;:0,&quot;type&quot;:null,&quot;isDfp&quot;:null,&quot;slug&quot;:null,&quot;title&quot;:null,&quot;sponsor&quot;:null,&quot;sponsorName&quot;:null,&quot;sponsorUrl&quot;:null,&quot;sponsorLogo&quot;:&quot;&quot;,&quot;sponsorLogoReverse&quot;:&quot;&quot;},&quot;geoLocation&quot;:{&quot;lat&quot;:0,&quot;lon&quot;:0},&quot;location&quot;:1,&quot;continent&quot;:{&quot;id&quot;:104,&quot;urlSafeValue&quot;:&quot;europa&quot;,&quot;title&quot;:&quot;Europa&quot;},&quot;country&quot;:{&quot;id&quot;:117,&quot;urlSafeValue&quot;:&quot;frankreich&quot;,&quot;title&quot;:&quot;Frankreich&quot;,&quot;url&quot;:&quot;\/nachrichten\/europa\/frankreich&quot;},&quot;town&quot;:[],&quot;versions&quot;:[],&quot;fullUrl&quot;:&quot;\/2020\/05\/22\/asylsuchende-hausbesetzer-und-obdachlose-im-corona-elend&quot;,&quot;lastModified&quot;:1590178950,&quot;ampUrl&quot;:&quot;\/amp\/2020\/05\/22\/asylsuchende-hausbesetzer-und-obdachlose-im-corona-elend&quot;},{&quot;id&quot;:1106112,&quot;cid&quot;:4698306,&quot;versionId&quot;:1,&quot;archive&quot;:0,&quot;housenumber&quot;:&quot;200522_NWSU_12036839&quot;,&quot;owner&quot;:&quot;euronews&quot;,&quot;isMagazine&quot;:0,&quot;channels&quot;:[{&quot;id&quot;:1},{&quot;id&quot;:2},{&quot;id&quot;:3},{&quot;id&quot;:4},{&quot;id&quot;:5}],&quot;status&quot;:2,&quot;title&quot;:&quot;Italien: Corona und die Folgen f\u00fcr die Schattenwirtschaft&quot;,&quot;titleSeo&quot;:null,&quot;titleListing1&quot;:null,&quot;titleListing2&quot;:&quot;\&quot;In drei Monaten habe ich insgesamt drei Tage gearbeitet.\&quot; #Schattenwirtschaft #Coronakrise #Italien&quot;,&quot;leadin&quot;:&quot;Die Corona-Krise hat auch Folgen f\u00fcr illegale Jobs: Mit dem Lockdown kam die Arbeitslosigkeit.&quot;,&quot;summary&quot;:&quot;Die Corona-Krise hat auch Folgen f\u00fcr illegale Jobs: Mit dem Lockdown kam die Arbeitslosigkeit.&quot;,&quot;url&quot;:&quot;italien-corona-und-die-folgen-fur-die-schattenwirtschaft&quot;,&quot;masterCms&quot;:&quot;v2&quot;,&quot;plainText&quot;:&quot;Quarticciolo, eine Stadt \u00f6stlich von Rom. 70 Prozent der Einwohner verdienen hier ihren Lebensunterhalt mit Arbeit, die nicht angemeldet ist oder mit Drogenhandel. \n\nDoch mit dem Lockdown kam auch die Arbeitslosigkeit. Schreiner, Handwerker, Reinigungskr\u00e4fte - sie alle k\u00f6nnen kaum auf Hilfen vom Staat hoffen. Zweimal pro Woche stehen sie Schlange, f\u00fcr Lebensmittelpakete oder Einkaufsgutscheine. \n\nViele Menschen in Italien haben illegale Jobs, mehr als in anderen Teilen Europas. Ganze Familien k\u00f6nnen sich so \u00fcber Wasser halten, zudem spielt es eine wichtige Rolle f\u00fcr das Wirtschaftswachstum. \n\nStefano Belmonte hatte in den vergangenen drei Monaten keine Arbeit. Er muss seine Frau und seine Tochter versorgen. Er schaut in seinen Korb: \n\n\&quot; Ich hab etwas Gem\u00fcse, Tomaten, Spaghetti, Obst, Eier. Das ist genug f\u00fcr Mittag- und Abendessen.\u00a0Es ist nicht viel, aber ich werde mein Bestes tun, damit es f\u00fcr uns drei reicht. \&quot; \n\nIn Quarticciolo sind die Menschen f\u00fcreinander da. Stefano hilft seit einigen Jahren in einer Wohlt\u00e4tigkeitsorganisation. Auf diese Weise kann er sich selbst besch\u00e4ftigen und nebenbei die \u00f6rtliche Gemeinschaft unterst\u00fctzen. \n\nUm junge Leute von der Stra\u00dfe zu holen, wurde ein Boxhalle gebaut. Doch diese hat noch nicht wiederer\u00f6ffnet, die Teenager in der Stadt f\u00fchlen sich jetzt verloren. \n\nGian Maria Fara ist Pr\u00e4sident des Institutes f\u00fcr politische, soziale und wirtschaftliche Studien ( Eurispes ). F\u00fcr ihn entstand die Schattenwirtschaft durch die langsame  B\u00fcrokratie des Landes sowie urch die hohen Einkommenssteuers\u00e4tze. Jedoch macht sie 35 Prozent des BIP Italiens aus. \n\nSpannungen in der Gesellschaft - das k\u00f6nnte letztendlich eine Folge der Corona-Krise sein. Viele Menschen in Italien werden keine andere Wahl haben, als kriminelle Organisationen zu unterst\u00fctzen. \n\n\n&quot;,&quot;htmlText&quot;:&quot;&lt;p&gt;Quarticciolo, eine Stadt \u00f6stlich von Rom. 70 Prozent der Einwohner verdienen hier ihren Lebensunterhalt mit Arbeit, die nicht angemeldet ist oder mit Drogenhandel.&lt;\/p&gt;\n&lt;p&gt;Doch mit dem Lockdown kam auch die Arbeitslosigkeit. Schreiner, Handwerker, Reinigungskr\u00e4fte - sie alle k\u00f6nnen kaum auf Hilfen vom Staat hoffen. Zweimal pro Woche stehen sie Schlange, f\u00fcr Lebensmittelpakete oder Einkaufsgutscheine.&lt;\/p&gt;\n&lt;p&gt;Viele Menschen in Italien haben illegale Jobs, mehr als in anderen Teilen Europas. Ganze Familien k\u00f6nnen sich so \u00fcber Wasser halten, zudem spielt es eine wichtige Rolle f\u00fcr das Wirtschaftswachstum.&lt;\/p&gt;\n&lt;div class=\&quot;widget widget--type-image widget--size-fullwidth widget--animation-fade-in widget--align-center\&quot;&gt;\n    &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n        &lt;div class=\&quot;widget__ratio widget__ratio--auto\&quot;&gt;\n            &lt;div class=\&quot;widget__contents\&quot;&gt;\n                &lt;figure class=\&quot;widget__figure\&quot;&gt;\n                    &lt;img class=\&quot;widgetImage__image\&quot; loading=\&quot;lazy\&quot; data-src=\&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/808x454_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg\&quot; src=\&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/808x454_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg\&quot; srcset=\&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/202x113_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 202w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/266x149_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 266w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/404x227_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 404w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/534x300_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 534w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/606x340_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 606w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/808x454_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 808w\&quot;  data-srcset=\&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/202x113_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 202w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/266x149_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 266w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/404x227_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 404w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/534x300_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 534w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/606x340_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 606w, https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/808x454_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg 808w\&quot; alt=\&quot;Euronews\&quot; \/&gt;\n                    &lt;figcaption class=\&quot;widget__caption\&quot;&gt;\n                        &lt;span class=\&quot;widget__captionWrap\&quot;&gt;\n                            &lt;span class=\&quot;widget__captionText\&quot;&gt;Arbeitssuchender Stefano Belmonte&lt;\/span&gt;\n                                    &lt;span class=\&quot;widget__captionCredit\&quot;&gt;Euronews&lt;\/span&gt;\n                        &lt;\/span&gt;\n                    &lt;\/figcaption&gt;\n                &lt;\/figure&gt;\n            &lt;\/div&gt;\n        &lt;\/div&gt;\n    &lt;\/div&gt;\n&lt;\/div&gt;\n\n\n&lt;p&gt;Stefano Belmonte hatte in den vergangenen drei Monaten keine Arbeit. Er muss seine Frau und seine Tochter versorgen. Er schaut in seinen Korb:&lt;\/p&gt;\n&lt;p&gt;\&quot;&lt;em&gt;&lt;strong&gt;Ich hab etwas Gem\u00fcse, Tomaten, Spaghetti, Obst, Eier. Das ist genug f\u00fcr Mittag- und Abendessen. Es ist nicht viel, aber ich werde mein Bestes tun, damit es f\u00fcr uns drei reicht.&lt;\/strong&gt;&lt;\/em&gt;\&quot;&lt;\/p&gt;\n&lt;p&gt;In Quarticciolo sind die Menschen f\u00fcreinander da. Stefano hilft seit einigen Jahren in einer Wohlt\u00e4tigkeitsorganisation. Auf diese Weise kann er sich selbst besch\u00e4ftigen und nebenbei die \u00f6rtliche Gemeinschaft unterst\u00fctzen.&lt;\/p&gt;\n&lt;div class=\&quot;widget widget--type-quotation\n    widget--size-fullwidth\n    widget--align-center\&quot;&gt;\n    &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n        &lt;div class=\&quot;widget__content\&quot;&gt;\n            &lt;blockquote class=\&quot;widget__quote\&quot;&gt;\n                &lt;span class=\&quot;widget__quoteText\&quot;&gt;Seit drei Monaten habe ich keinen einzigen Euro mehr nach Hause gebracht. Ich halte durch, solange ich Essenspenden bekomme. Ich habe mal hier und mal da was gemacht, aber es gibt kein richtiges Einkommen. In drei Monaten habe ich insgesamt drei Tage gearbeitet. Jobs, die nicht angemeldet sind - das ist vollst\u00e4ndig zum Erliegen gekommen. Leider mache ich aber genau diese Arbeit. In meinem Alter habe ich nicht die Chance, einen Job angeboten zu bekommen.&lt;\/span&gt;\n            &lt;\/blockquote&gt;\n            &lt;cite class=\&quot;widget__author\&quot;&gt;\n                    &lt;div class=\&quot;widget__authorText\&quot;&gt;\n                        Stefano Belmonte\n                    &lt;\/div&gt;\n                    &lt;div class=\&quot;widget__author_descriptionText\&quot;&gt;\n                        Arbeitssuchender\n                    &lt;\/div&gt;\n            &lt;\/cite&gt;\n        &lt;\/div&gt;\n    &lt;\/div&gt;\n&lt;\/div&gt;\n\n\n&lt;p&gt;Um junge Leute von der Stra\u00dfe zu holen, wurde ein Boxhalle gebaut. Doch diese hat noch nicht wiederer\u00f6ffnet, die Teenager in der Stadt f\u00fchlen sich jetzt verloren.&lt;\/p&gt;\n&lt;p&gt;Gian Maria Fara ist Pr\u00e4sident des Institutes f\u00fcr politische, soziale und wirtschaftliche Studien (&lt;a href=\&quot;https:\/\/eurispes.eu\/\&quot;&gt;Eurispes&lt;\/a&gt;). F\u00fcr ihn entstand die Schattenwirtschaft durch die langsame B\u00fcrokratie des Landes sowie urch die hohen Einkommenssteuers\u00e4tze. Jedoch macht sie 35 Prozent des BIP Italiens aus.&lt;\/p&gt;\n&lt;div class=\&quot;widget widget--type-quotation\n    widget--size-fullwidth\n    widget--align-center\&quot;&gt;\n    &lt;div class=\&quot;widget__wrapper\&quot;&gt;\n        &lt;div class=\&quot;widget__content\&quot;&gt;\n            &lt;blockquote class=\&quot;widget__quote\&quot;&gt;\n                &lt;span class=\&quot;widget__quoteText\&quot;&gt;Mit diesen Eink\u00fcnften k\u00f6nnen Familien Lebensmittel einkaufen oder umziehen. Mit anderen Worten: Sie f\u00fchren ein normales Leben. Und auch wenn es sich um ein illegales Einkommen handelt - es ist immer noch Teil der gesamten Wirtschaft, so wie auch die Einnahmen aus kriminellen Aktivit\u00e4ten zum Wirtschaftswachstum des Landes beitragen.&lt;\/span&gt;\n            &lt;\/blockquote&gt;\n            &lt;cite class=\&quot;widget__author\&quot;&gt;\n                    &lt;div class=\&quot;widget__authorText\&quot;&gt;\n                        Gian Maria Fara\n                    &lt;\/div&gt;\n                    &lt;div class=\&quot;widget__author_descriptionText\&quot;&gt;\n                        Pr\u00e4sident Eurispes\n                    &lt;\/div&gt;\n            &lt;\/cite&gt;\n        &lt;\/div&gt;\n    &lt;\/div&gt;\n&lt;\/div&gt;\n\n\n&lt;p&gt;Spannungen in der Gesellschaft - das k\u00f6nnte letztendlich eine Folge der Corona-Krise sein. Viele Menschen in Italien werden keine andere Wahl haben, als kriminelle Organisationen zu unterst\u00fctzen.&lt;\/p&gt;\n&quot;,&quot;hashtag&quot;:null,&quot;createdAt&quot;:1590130002,&quot;publishedAt&quot;:1590168584,&quot;updatedAt&quot;:1590168963,&quot;expiresAt&quot;:0,&quot;canonical&quot;:&quot;https:\/\/de.euronews.com\/2020\/05\/22\/italien-corona-und-die-folgen-fur-die-schattenwirtschaft&quot;,&quot;images&quot;:[{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/08\/{{w}}x{{h}}_cmsv2_eb406680-a3bd-5c8d-8708-fbb3a48f7772-4698308.jpg&quot;,&quot;altText&quot;:null,&quot;caption&quot;:null,&quot;captionCredit&quot;:&quot;Alessandra Tarantino\/Copyright 2020 The Associated Press. All rights reserved&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;width&quot;:3827,&quot;height&quot;:2551},{&quot;url&quot;:&quot;https:\/\/static.euronews.com\/articles\/stories\/04\/69\/83\/06\/{{w}}x{{h}}_cmsv2_fa23da0d-3a24-504e-b157-bc66e8f4bf72-4698306.jpg&quot;,&quot;altText&quot;:&quot;Arbeitssuchender Stefano Belmonte&quot;,&quot;caption&quot;:&quot;Arbeitssuchender Stefano Belmonte&quot;,&quot;captionCredit&quot;:&quot;Euronews&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;width&quot;:1920,&quot;height&quot;:1080}],&quot;authors&quot;:{&quot;journalists&quot;:[],&quot;producers&quot;:[],&quot;videoEditor&quot;:[]},&quot;keywords&quot;:[{&quot;id&quot;:158,&quot;slug&quot;:&quot;italy&quot;,&quot;urlSafeValue&quot;:&quot;italien&quot;,&quot;title&quot;:&quot;Italien&quot;,&quot;titleRaw&quot;:&quot;Italien&quot;},{&quot;id&quot;:12388,&quot;slug&quot;:&quot;workers&quot;,&quot;urlSafeValue&quot;:&quot;arbeiter&quot;,&quot;title&quot;:&quot;Arbeiter&quot;,&quot;titleRaw&quot;:&quot;Arbeiter&quot;},{&quot;id&quot;:22526,&quot;slug&quot;:&quot;coronavirus&quot;,&quot;urlSafeValue&quot;:&quot;coronavir