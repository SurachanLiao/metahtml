oto&quot;,&quot;captionUrl&quot;:null,&quot;sourceCredit&quot;:null,&quot;sourceUrl&quot;:null,&quot;width&quot;:1024,&quot;height&quot;:683}],&quot;authors&quot;:{&quot;journalists&quot;:[],&quot;producers&quot;:[],&quot;videoEditor&quot;:[]},&quot;keywords&quot;:[{&quot;id&quot;:35,&quot;slug&quot;:&quot;brazil&quot;,&quot;urlSafeValue&quot;:&quot;brasilien&quot;,&quot;title&quot;:&quot;Brasilien&quot;,&quot;titleRaw&quot;:&quot;Brasilien&quot;},{&quot;id&quot;:3863,&quot;slug&quot;:&quot;rio-de-janeiro&quot;,&quot;urlSafeValue&quot;:&quot;rio-de-janeiro&quot;,&quot;title&quot;:&quot;Rio de Janeiro&quot;,&quot;titleRaw&quot;:&quot;Rio de Janeiro&quot;},{&quot;id&quot;:22526,&quot;slug&quot;:&quot;coronavirus&quot;,&quot;urlSafeValue&quot;:&quot;coronavirus&quot;,&quot;title&quot;:&quot;Coronavirus&quot;,&quot;titleRaw&quot;:&quot;Coronavirus&quot;},{&quot;id&quot;:22728,&quot;slug&quot;:&quot;covid-19&quot;,&quot;urlSafeValue&quot;:&quot;covid-19&quot;,&quot;title&quot;:&quot;Covid-19&quot;,&quot;titleRaw&quot;:&quot;Covid-19&quot;},{&quot;id&quot;:381,&quot;slug&quot;:&quot;latin-america&quot;,&quot;urlSafeValue&quot;:&quot;lateinamerika&quot;,&quot;title&quot;:&quot;Lateinamerika&quot;,&quot;titleRaw&quot;:&quot;Lateinamerika&quot;}],&quot;widgets&quot;:[],&quot;related&quot;:[],&quot;technicalTags&quot;:[],&quot;allViews&quot;:0,&quot;allViewsMeta&quot;:{&quot;pointOfView&quot;:{&quot;quotation&quot;:null,&quot;description&quot;:null,&quot;author&quot;:null},&quot;survey&quot;:[],&quot;tweetId&quot;:null,&quot;tweet2NdId&quot;:null,&quot;contentType&quot;:null,&quot;displayOverlay&quot;:0},&quot;displayType&quot;:&quot;default&quot;,&quot;video&quot;:1,&quot;videos&quot;:[{&quot;duration&quot;:50000,&quot;editor&quot;:null,&quot;filesizeBytes&quot;:4934948,&quot;format&quot;:&quot;mp4&quot;,&quot;youtubeId&quot;:&quot;zEFfbsteOcg&quot;,&quot;type&quot;:&quot;normal&quot;,&quot;url&quot;:&quot;https:\/\/video.euronews.com\/mp4\/med\/EN\/NC\/SU\/20\/05\/22\/de\/2