    var g_Have160Tower = true;
    var g_Have300Tower = true;
    var iScreenWidth = 0; // 0 = normal, 1 = wider, 2 = widest
    var g_windowWidth = 0;
    var g_towerPromoSpotRightPos = 0;
    
    g_windowWidth =
            window.innerWidth ||
            (!document.documentElement ? 0 : document.documentElement.clientWidth) ||
            (!document.body ? 0 : document.body.clientWidth) ||
            (!document.body ? 0 : document.body.offsetWidth);
