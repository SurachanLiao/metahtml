<script>
		function updateClock() {
			const now = new Date();
			const endDate = new Date(2021, 02, 31, 12, 29, 59);
			const diffNow = endDate - now;
			if(diffNow < 0){ clearInterval(t); return false; }
			const parts = {
				d: Math.floor(diffNow / (1000 * 60 * 60 * 24)),
				h: Math.floor((diffNow / (1000 * 60 * 60)) % 24),
				m: Math.floor((diffNow / 1000 / 60) % 60),
				s: Math.floor((diffNow / 1000) % 60),
			};

			parts.d = (parts.d < 10) ? '0'+parts.d : parts.d;
			parts.h = (parts.h < 10) ? '0'+parts.h : parts.h;
			parts.m = (parts.m < 10) ? '0'+parts.m : parts.m;
			parts.s = (parts.s < 10) ? '0'+parts.s : parts.s;

			document.querySelector(".md").innerHTML = parts.d;
			document.querySelector(".mh").innerHTML = parts.h;
			document.querySelector(".mm").innerHTML = parts.m;
			document.querySelector(".ms").innerHTML = parts.s;
		}

		var t = setInterval(updateClock, 1000);
	</script>



