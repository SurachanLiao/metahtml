}
if(td_screen_width >=768&&td_screen_width < 1019){
document.write('<ins class="adsbygoogle" style="display:inline-block;width:468px;height:60px" data-ad-client="ca-pub-6189371274455157" data-ad-slot="9891476003"></ins>');
(adsbygoogle=window.adsbygoogle||[]).push({});
