  var measurementEnabled = true;
  if (false) {
    var optanonCookie =
      document &&
      document.cookie &&
      document.cookie
        .split(';')
        .map((s) => s.trim())
        .find((s) => s.startsWith('OptanonConsent'));
    var measurementCategoryEnabledRegex = /groups=[^&]*%2C2%3A1/;
    measurementEnabled = measurementCategoryEnabledRegex.test(optanonCookie);
  };
  window._comscore = [
    {
      c1: '2',
      c2: '1000004',
      options: { url_append: 'comscorekw=US_Top_News_and_Analysis' },
      cs_ucfr: measurementEnabled ? 1 : 0
    }
  ];
