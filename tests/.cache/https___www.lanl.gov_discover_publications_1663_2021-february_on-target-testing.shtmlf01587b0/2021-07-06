    <!-- SHARE BAR COLUMN -->
            
    <aside class="share-bar-column d-flex flex-column ">
        <!-- Editor Profile -->
                                            <span class="editor-name d-block align-self-center">Rebecca McDonald</span>
                                        <span class="editor-title d-block align-self-center text-dark">Science Writer</span>
                
        <!-- Share Links -->
    <span class="share align-self-center text-dark mt-3 border-top w-100 text-center py-3">SHARE</span>
    <a class="facebook-share share-icon share-icon-column align-self-center" href="#" target="_blank"><i class="fab fa-facebook-f"></i></a>
    <a class="twitter-share share-icon share-icon-column align-self-center" href="#" target="_blank"><i class="fab fa-twitter"></i></a>
