                <script>
                const shareBarRow = document.querySelector('.share-bar-row');
                const expandButton = document.querySelector('#share-expander-button');
                const expander = document.querySelector('#share-expander');
                const profile = document.querySelector('#social-profile');
                
                if ( profile !== null && profile !== undefined ) {
                    expandButton.addEventListener('click', function(){
                        if (window.innerWidth < 600) {
                            if (expander.classList.contains('open')) {
                                profile.classList.remove('hide');
                            } else {
                                profile.classList.add('hide');
                            }
                        }
                        
                        if (window.innerWidth >= 601) {
                            if (profile.classList.contains('hide')) {
                                profile.classList.remove('hide');
                            }
                        }
                        expander.classList.toggle('open'); 
                    });
                } else {
                    expandButton.addEventListener('click', function() {
                        expander.classList.toggle('open'); 
                    });
                }
                </script>
                
