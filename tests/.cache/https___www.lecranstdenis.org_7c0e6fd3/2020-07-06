<div id="warper"><script type="text/javascript">
var largeurTransition;
var currentMarginLeft	= 0;

jQuery(document).ready(function() {
	//return;
	if(jQuery('.slidecontent').size() > 2){
		largeurTransition = jQuery('.slidecontent').outerWidth(true);
		
		//Initialisation, il faut mettre la dernière diapo en première
		placerDebut();
		jQuery('#homepageslider').append('<span id="prev" class="control">Back</span><span id="next" class="control">Next</span>');
		handleClick();
	}
	
	jQuery('#homepageslider').bind('precedent', function() {
		jQuery('#homepageslider span#prev').trigger('click');
	});
	jQuery('#homepageslider').bind('suivant', function() {
		jQuery('#homepageslider span#next').trigger('click');
	});
	
	$('#homepageslider span#next, #homepageslider span#prev').mousedown(function() {
      return false;
	});
});

function animation_suivant(){
	
	currentMarginLeft = currentMarginLeft - largeurTransition;
	jQuery('#homepageslider span#next, #homepageslider span#prev').unbind('click');
	
	jQuery('ul#slider').animate({ marginLeft: currentMarginLeft }, 250, 'linear', function() {
		placerFin();
		handleClick();
	});
}

function animation_precedent(){
	
	currentMarginLeft = currentMarginLeft + largeurTransition;
	jQuery('#homepageslider span#next, #homepageslider span#prev').unbind('click');
	
	jQuery('ul#slider').animate({ marginLeft: currentMarginLeft }, 250, 'linear', function() {
		placerDebut();
		handleClick();
	});
}

function placerDebut(){
	currentMarginLeft = currentMarginLeft - largeurTransition;
	
	jQuery('.slidecontent:last').clone().prependTo('ul#slider');
	jQuery('.slidecontent:last').remove();
	jQuery('ul#slider').css({ marginLeft: currentMarginLeft });
}

function placerFin(){
	currentMarginLeft = currentMarginLeft + largeurTransition;
	
	jQuery('.slidecontent:first').clone().appendTo('ul#slider');
	jQuery('.slidecontent:first').remove();
	jQuery('ul#slider').css({ marginLeft: currentMarginLeft });
}

function handleClick(){
	jQuery('#homepageslider span#prev').bind('click', function() {
		animation_precedent();
	});
	
	jQuery('#homepageslider span#next').bind('click', function() {
		animation_suivant();
	});
}
</script>
<!--Home slider-->
