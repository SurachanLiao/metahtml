			<script>
				$('#btn-search').click(function() {
					var active = $(this).hasClass('active');

					if (active == true) {
						$(this).removeClass('active');
						$('#search-desktop').removeClass('active');
					} else {
						$(this).addClass('active');
						$('#search-desktop').addClass('active');
					}
				});

				$('#notificaciones > div.botones > div.notificacon-btn.notificacon-btn-si').click(function() {
					$('#notificaciones-campana').addClass('active');
				});
				$('#notificaciones > div.botones > div.notificacon-btn.notificacon-btn-no').click(function() {
					$('#notificaciones-campana').removeClass('active');
				});

				$('#menu-hamburguesa').click(function() {
					var active = $(this).hasClass('active');

					if (active == true) {
						$(this).removeClass('active');
						$('#menu-desplegable').removeClass('active');
					} else {
						$(this).addClass('active');
						$('#menu-desplegable').addClass('active');
					}
				});
			</script>

