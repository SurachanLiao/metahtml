			} else {
				$('#div-gpt-ad-1576883860770-0').remove();
				$('#div-gpt-ad-1576883880710-0').remove();
			}
		} else if (window.location.pathname == "/wasapea-el-heraldo") {
			if (screen.width >= 1180) {
				$('#wasapea-bloque-1 #widget_horizontal_3_noticias_mas_pauta .content-items .item-4').addClass('pauta pauta_desktop pauta_300x250').append($('#div-gpt-ad-1576935990353-0'));
				$('#wasapea-bloque-2 #widget_horizontal_3_noticias_mas_pauta .content-items .item-4').addClass('pauta pauta_desktop pauta_300x250').append($('#div-gpt-ad-1576935979645-0'));
			} else {
				$('#div-gpt-ad-1576935990353-0').remove();
				$('#div-gpt-ad-1576935979645-0').remove();
			}
		} else if (window.location.pathname == "/elecciones" || window.location.pathname == "/elecciones-colombia-2019") {
			if (screen.width >= 1180) {
				$('#elecciones-bloque-1 #widget_horizontal_3_noticias_mas_pauta .content-items .item-4').addClass('pauta pauta_desktop pauta_300x250').append($('#div-gpt-ad-1576885446457-0'));
			} else {
				$('#div-gpt-ad-1576885446457-0').remove();
			}
		}
	</script>
</section>
