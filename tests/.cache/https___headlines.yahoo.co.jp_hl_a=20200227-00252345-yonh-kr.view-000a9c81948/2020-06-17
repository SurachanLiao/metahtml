<script>
var Rapid3Impl = function() {
    return this.initialize.apply(this, arguments);
};

Rapid3Impl.prototype = {
    rapid: null,
    isInitRapid: false,
    modules: [],
    rapidCallback: function() {},

    /**
     * コンストラクタ
     */
    initialize : function(spaceId){
        this.rapid = new YAHOO.i13n.JP.simpleRapid();

        this.rapid
            .setSpaceid(spaceId)
            .setNonepv(1)
            .setService('news')
            .setPagetype('detail')
            .setApptype('web')
            .setVtgrpid('hdl')
            .setOpttype('pc');

        this.rapid.addPageParam('conttype', 'pctrart');
        this.rapid.addPageParam('shcid', '8dbdacd53f6564463af0d4e1d1f57cd1cc50d14b');

        this.rapid.addPageParam('mit_serv', 'news');



    },

    track: function() {
        this.rapid = this.rapid.trackedClass().initRapid();
        this.isInitRapid = true;
        this.rapidCallback.call(this);
        for (var i = 0, len = this.modules.length; i < len; i += 1) {
            this.addModules(this.modules[i]);
        }
    },

    addModules: function(sec) {
        if (!!!this.isInitRapid) {
            this.modules.push(sec);
            return;
        }

        this.rapid.addModules(sec, false);
    },
};

if (typeof YJNEWS === 'undefined') {
    YJNEWS = {};
}

YJNEWS.Rapid3 = new Rapid3Impl('2078710292');
</script>

