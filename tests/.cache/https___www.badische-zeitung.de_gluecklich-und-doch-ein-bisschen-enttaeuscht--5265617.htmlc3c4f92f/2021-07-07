	<script type='text/javascript'>
		if (typeof registerError == 'undefined') {
			function registerError(el,msg) {
				$targetEl 		  = $('#'+el);
				$targetLabel 	  = $('[for="'+el+'"]');
				$targetElParent = $('#'+el).parent();

				$('#erBox'+el).remove();
				$targetElParent.append('<div class="error__info-feld" id="erBox'+el+'">'+msg+'</div>');

				$targetEl.addClass('error');
				$targetLabel.addClass('error');	
			}
		}

		if (typeof clearError == 'undefined') {
			function clearError(el) {
				$targetEl 		= $('#'+el);
				$targetLabel 	= $('[for="'+el+'"]');
				$('#erBox'+el).remove();

				var formEl  = $targetEl.attr("class").replace("-error", " ");
				var labelEl = $targetLabel.attr("class").replace("-error", " ");
				$targetEl.attr("class").replace("-error", " ");
				$targetLabel.attr("class").replace("-error", " ");
				$targetEl.attr("class", formEl);
				$targetLabel.attr("class", labelEl);

				$targetEl.removeClass('error');
				$('label[for="'+el+'"]').removeClass('error');
			}
		}
		$( '#login1139293577EingabeLoginsetComplexloginLogin, [for="login1139293577EingabeLoginsetComplexloginLogin"]' ).on( "click", function() {
			$('#erBoxlogin1139293577EingabeLoginsetComplexloginLogin').hide();
		});
		$( '#login1139293577EingabeLoginsetComplexloginLogin, [for="login1139293577EingabeLoginsetComplexloginLogin"]' ).blur(function() {
			if ( $( '#login1139293577EingabeLoginsetComplexloginLogin' ).val() == "" ) {
				$('#erBoxlogin1139293577EingabeLoginsetComplexloginLogin').show();
			}
		});
	</script>










	<script type='text/javascript'>
	
	(function(){
		var element = $('#login1139293577EingabeLoginsetComplexloginLogin');
		function validateRequired(e) {
			if ( e.target.value == '' || e.target.value == "keineAngabe" || e.target.value == "---------------") { registerError('login1139293577EingabeLoginsetComplexloginLogin','Das Feld E-Mail ist eine Pflichtangabe.'); }
			else { clearError('login1139293577EingabeLoginsetComplexloginLogin'); }
		}
		if ( typeof element != 'undefined' ) {
			var myel = 'login1139293577EingabeLoginsetComplexloginLogin';
			var lastChar = myel.substr(myel.length - 1);
			if ( isNaN( parseInt(lastChar, 10) ) === false) {
				var test = myel.substr(0, myel.length - 1);
				$('input[name='+test+']').each(function(element){ 
			        $(this).change(validateRequired);
			    });
			}
			element.change(validateRequired);
		}
	})();

	</script>

	</li>
