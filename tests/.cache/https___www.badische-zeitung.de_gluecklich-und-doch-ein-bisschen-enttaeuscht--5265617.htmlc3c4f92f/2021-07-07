<script type="text/javascript">
if ( ! ('forEach' in Array.prototype) ) {
	Array.prototype.forEach = function(action, that /*opt*/) {
		for (var i= 0, n= this.length; i<n; i++)
			if (i in this)
				action.call(that, this[i], i, this);
	};
}

(function(){
	//////////////////////////////////
	// Default Form Layout
	//////////////////////////////////

	if ( typeof formlayoutDefault != 'function' ) {
		formlayoutDefault = function(formIdentity) {

			if ( ! formIdentity instanceof formidentifier )
				throw "Cannot instantiate breadcrumb layout (formidentifier)" 

			var self = this;
			
			self._formIdentity = formIdentity;
			self.navigationClass = 'x-nav-el';

			// create element references
			self.formEl = $('form[name="'+self._formIdentity.getName()+'"]');
			self.formContainerEl = self.formEl.find('div.formcontainer');
			self.buttons = {
				'nextEl': self.formEl.find('.button-holder #next'),
				'backEl': self.formEl.find('.button-holder #back'),
				'resetEl': self.formEl.find('.button-holder #reset')
			};
			
			self.buttons.nextTextEl = self.buttons.nextEl;
			self.buttons.backTextEl = self.buttons.backEl;
			self.buttons.resetTextEl = self.buttons.resetEl;

			// prevent default submit-event handling
			self.formEl.submit(function(e){
				e.preventDefault();
				return false;
			});

			self.actionCallbacks = {

				'display': function(data)
					{
						if ( data.hasOwnProperty('content') ) {
							self.drawSite( unescape( data.content ) );
						}

						self.controlButtons(data);
						self.bindCustomListener();
						
					    //var position = $(".formcontainer:parent").position();
//var position = $("#hauptformular").position();
if( $('#hauptformular').length ) {
	var position = $("#hauptformular:parent").offset().top;

	if (position){
		//scroll(0,position.top);
		$("html, body").scrollTop(position-60);
	}
}
					},
					
				'refreshPage': function(data)
					{
						location.reload();
					},
					
				'redirect': function(data)
					{
						if ( typeof data.hasOwnProperty('location') != 'undefined' ) {
							location.href = data.location;
						}
					}
				
			};

			this.clickListener = function(e) {
				e.preventDefault();

					var elIDName = $(this).closest('form').attr('id');
					//window.location = window.location.origin + window.location.pathname + '#' +elIDName;
			// Jumper Function		
				var offset = $(this).closest('form').offset();
				//if (offset){
				//	setTimeout(function() {
				//		scroll(0,(offset.top-100));
				//	}, 10);
				//}
	    
				$( document ).ajaxStart(function() {
					$("button#next").addClass("transmit"); $("button#next").attr('disabled','disabled');
				});
				$( document ).ajaxComplete(function() {
					$("button#next").removeClass("transmit"); $("button#next").removeAttr('disabled');
				});

				self.submitForm($(this),function(data) {
					if ( ! data.hasOwnProperty('action') ) {
						return;
					} else {
						self.handleAction(data.action,data);
					}
				});
			};
		};

		formlayoutDefault.prototype.handleAction = function(action,data) {
			if ( this.actionCallbacks.hasOwnProperty( action ) ) {
				this.actionCallbacks[action].call(self,data);
			}
		};

		formlayoutDefault.prototype.bindCustomListener = function() {
			this.formEl.find('.'+this.navigationClass).unbind('click').bind('click',this.clickListener);
		};

		formlayoutDefault.prototype.submitForm = function(submitBtn,onSuccess) {
			var reqPayload = {};

			// assign btn
			reqPayload[submitBtn.attr("name")] = "";

			formhandler.submit(this._formIdentity,{data:reqPayload,success:onSuccess});
		};

		formlayoutDefault.prototype.drawSite = function(siteContent) {
			this.formContainerEl.empty().append( siteContent );
		};

		formlayoutDefault.prototype.controlButtons = function(data) {

			if ( data.hasOwnProperty('prevElId') ) {
				
				if ( data.hasOwnProperty('currentEl') &&
					 typeof data.currentEl == 'object' &&
					 data.currentEl.hasOwnProperty('layout') &&
					 typeof data.currentEl.layout == "object" &&
					 data.currentEl.layout.hasOwnProperty('backbtntext')
				   )
				{
					this.buttons.backTextEl.html(data.currentEl.layout.backbtntext);
				} else {
					this.buttons.backTextEl.html("Zur&uuml;ck");
				}

				this.buttons.backEl
					.attr( "name",(data.prevElId+"-back") )
					.show()
				; 
			} else {
				this.buttons.backEl.hide();
			}


			if ( data.hasOwnProperty('nextElId') ) {
				
				if ( data.hasOwnProperty('currentEl') &&
					 typeof data.currentEl == 'object' &&
					 data.currentEl.hasOwnProperty('layout') &&
					 typeof data.currentEl.layout == "object" &&
					 data.currentEl.layout.hasOwnProperty('nextbtntext')
				   )
				{
					this.buttons.nextTextEl.html("<span></span>"+data.currentEl.layout.nextbtntext);
				} else {
					this.buttons.nextTextEl.html("<span></span>Weiter");
				}

				this.buttons.nextEl
					.attr( "name",(data.nextElId+"-next") )
					.show()
				; 
			} else {
				this.buttons.nextEl.hide();
			}

			if ( data.hasOwnProperty('prevElId') || data.hasOwnProperty('nextElId') ) {
				this.buttons.resetEl.hide();
			} else if ( data.hasOwnProperty('currentElId') && data.hasOwnProperty('noMoreSites') && data.noMoreSites === true ) {
				data.allEl.forEach( function( el,idx ) {
					if ( el.id === data.currentElId ) {
						if ( el.firstVisit === false ) {
							this.buttons.resetEl.attr( "name",(data.currentElId+"-reset") ).show();
						}
					}
				});
			}
		};
	}

	if ( 'default' == 'default' ) {
		var layoutController = new formlayoutDefault(login1139293577);
		layoutController.bindCustomListener();
	}

})();

</script>

