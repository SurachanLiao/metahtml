<script>
	$(document).ready(function() {
	// Pfeilchen
		$('.acc').on('click', '.acc__label', function() {
			if ($(this).next('.acc__input').is(":checked")) {
				$( this ).attr( "checked", false );
				$( this ).prop( "checked", false );
				$( '.acc__input' ).parent().removeClass('active');
			} else {
				if ($(this).next('.acc__input')) {
					$( '.acc__input' ).attr( "checked", false );
					$( '.acc__input' ).prop( "checked", false );
					$( '.acc__input' ).parent().removeClass('active');
				}
				$( this ).attr( "checked", true );
				$( this ).prop( "checked", true );
				$( this ).parent().addClass('active');
			}
		});

	// Slider
		const slider = tns({
			container: '.slider',
			loop: false,
			items: 1,
			slideBy: 'page',
			nav: true,
			navPosition: 'bottom',
			autoplay: false,
			autoplayButtonOutput: false,
			mouseDrag: true,
			lazyload: true,
			center: true,
			startIndex: 0,
			autoWidth: true,
			controls: false,
			gutter: 0,
			freezable: true,
			responsive: {
				600: {
					items: 2,
					autoWidth: true,
					mouseDrag: false,
					controls: false,
					nav: false,
					gutter: 0,
					center: false,
					freezable: true,
				}
			} 
		});

	// Login-Button
		$('#loginRegButton, #regLoginButton').on('click', function(e) {
				//$('#regLoginButton').text('zurück');
			$('div[rel="loginForm"]').toggle();
			$('div[rel="registerForm"]').toggle();
			e.preventDefault();
		});
	// Login-Maske Anzeigen bei Return-URL
		
		const hasLogin = ''
		if (hasLogin == 'in') {
			$('div[rel="loginForm"]').toggle();
			$('div[rel="registerForm"]').toggle();
		}
	});
</script>




     				
	

