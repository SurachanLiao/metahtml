			<script type="text/javascript">
				$("#loginField").on("input", function(){
					$('#mailRegisterd').addClass("hide-all");
					$('#mailRegisterdInActiv').addClass("hide-all");
				});
				$('#loginButton').on('click', function() {

					$.get('/api/user/exists?email='+$('#loginField').val(), function(response) {
						if ($('#loginField').val() == "") {
							$('#loginField').addClass('error');
							$('#loginField').attr("placeholder", "Bitte E-Mail-Adresse angeben");
							return false;
						} else {
							mail = $('#loginField').val();
							returnurl = location.href;

							var str = 'mode=in';

							if (returnurl.indexOf('?') == -1) {
								// Url hat kein Fragezeichen
								str = '?'+str;
							} else {
								// Url hat Fragezeichen
								str = '&'+str;
							}

							returnurl += str;

							param = 'email='+mail+'&returnurl='+returnurl;

							if (response.success === true && response.result.exists === true) {
								if (response.result.active === true) {
									$('#mailRegisterd').removeClass( "hide-all" );
								} else {
									$('#mailRegisterdInActiv').removeClass( "hide-all" );
								}
							} else {
								location.href = "https://www.badische-zeitung.de/meine-bz/registrierung?layout=registrierung_w3&"+param;
							}
						}
					});

				return false;
				});
			</script>
		
	
