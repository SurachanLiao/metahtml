<script type="text/javascript">
$(document).ready(function () {
$("#verMasGallery2417245").click(function () {
loadNoticiasContent2417245('internacional');
});
var totalItem2417245 = 3;
function loadNoticiasContent2417245(secName) {
if (secName != '') {
var url = '/ajaxConfig/?service=ajax&item=noticias&imageVersion=BASE_LANDSCAPE_SMALL&part=noticias&contentType=news,videoReference,youtubeVideo,gallery,liveVideo&startIndex=' + totalItem2417245+'&sectionUniqueName=' + secName+"&homeSectionOnly=false&maximumLenghtOfTitle=300&numberOfItemsPerPage=3";
cSection = secName;
$.ajax({
url:url,
dataType:'json',
beforeSend:function(){
// $('#verMasGallery2417245').html('');
$('#verMasGallery2417245').html('Cargando...');
$("#verMasGallery2417245").prop('disabled', true);
$('#verMasGallery2417245').css( 'cursor', 'default' );
},
success:function (data) {
var totalNumFound = data.totalNumFound;
if (totalNumFound > data.articles.length + totalItem2417245) {
$('#verMasGallery2417245').show();
}
else {
$('#verMasGallery2417245').hide();
}
totalItem2417245 = totalItem2417245 + data.articles.length;
var htmlInt = '';
for (var i = 0; i < data.articles.length; i++) {
htmlInt +='<figure>'
+'<a href="' + data.articles[i].url + '">'
+'<picture>'
+'<img alt="'+data.articles[i].title+'" src="' + data.articles[i].imageUrl + '" class="img-responsive">'
+'<div>'
+'<span class="dataction">Ver Video</span>'
+'</div>'
+'</picture>'
+'</a>'
+'<figcaption>'
+'<h3><a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a></h3>'
+'</figcaption>'
+'</figure>';
}
var htmlDiv = '<div class="carouselList">'+htmlInt+'</div>';
$("#articlesPaginationGallery2417245").append(htmlDiv);
$('#verMasGallery2417245').html('Ver más');
$("#verMasGallery2417245").prop('disabled', false);
$('#verMasGallery2417245').css( 'cursor', 'pointer' );
},
error: function (jqXHR, exception) {
var msg = '';
if (jqXHR.status === 0) {
msg = 'Not connect.\n Verify Network.';
} else if (jqXHR.status == 404) {
msg = 'Requested page not found. [404]';
} else if (jqXHR.status == 500) {
msg = 'Internal Server Error [500].';
} else if (exception === 'parsererror') {
msg = 'Requested JSON parse failed.';
} else if (exception === 'timeout') {
msg = 'Time out error.';
} else if (exception === 'abort') {
msg = 'Ajax request aborted.';
} else {
msg = 'Uncaught Error.\n' + jqXHR.responseText;
}
},
}
);
}
}
});
</script>
</div>
