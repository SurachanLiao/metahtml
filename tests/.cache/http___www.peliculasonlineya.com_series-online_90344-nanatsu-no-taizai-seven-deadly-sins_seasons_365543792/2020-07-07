<script>
function slideSimple(dir){
    var el = $(this), sel = $('#images-col'), li = sel.find('li'),ancho = li.eq(0).outerWidth(true);
    if(sel.hasClass('mov')) return false;
    sel.addClass('mov');
    if(dir == "a"){
        $('#images-col li:eq('+(li.length-1)+')').clone().insertBefore('#images-col li:eq(0)');
        sel.css('margin-left',-1*ancho+'px');
        sel.animate({'margin-left': 0+'px' },500, function(){
        $(this).removeClass('mov');
        li.last().remove();
        });
    }

    if(dir == "s"){
        $('#images-col li:eq(0)').clone().insertAfter('#images-col li:eq('+(li.length-1)+')');
        sel.animate({'margin-left': -1*ancho+'px' },500, function(){
        li.first().remove();
        $(this).removeClass('mov');
        sel.css('margin-left',0);
        });
    }
}
</script>
