		<script type="text/javascript">
    $(window).load(function(){
    var anchor = window.top.location.hash;
    if(anchor.includes('/')){
    var anchor = anchor.replace('/','');
    }
    if(anchor && false){
        $('.rs-sliding-tabs-list > li > a[href="#0"]').trigger('click');
    }
    else if(anchor){
        $('.rs-sliding-tabs-list > li > a[href="'+anchor+'"]').trigger('click');
    }
    });

        </script>

		<script type="text/javascript">
      delete window.webpackJsonp;

      function checkChatbotBootstrap() {
        if (checkChatId()
        || window.location.href.split('/')[5] === 'chat.html'
        || window.location.href.split('/')[6] === 'chat.html'
        || window.location.href.split('/')[8] === 'TestLUTProdPage.html'
        || window.location.href.split('/')[9] === 'testlutprodpage.html') {
          pxWebComponent.load(document.querySelector('[data-da-webcomponent]'));
        }
      }

      function checkChatId() {
        try {
          const key = JSON.parse(localStorage.getItem('botSettings'))['chatId'];
          return key;
        } catch(e) {
          console.log('Can not access local storage: ', e);
        }

        return false;
      }

      checkChatbotBootstrap();
        </script>

