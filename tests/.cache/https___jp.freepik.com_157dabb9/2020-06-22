<!-- YieldLove -->
	<script type = "text/javascript" >
		var switcher_id = Math.floor(Math.random() * 2);
		var yieldlove_site_id = "freepik.com";
	</script>

	<script type = 'text/javascript' src = '//cdn-a.yieldlove.com/yieldlove-bidder.js?freepik.com' defer></script>

	<script >
		var bidder_turn;

		if (switcher_id != 0) {
			bidder_turn = 'is yl';
			window.yieldlove_stop_adserver_requests = true;
			window.setTimeout(function() {
				if (typeof YLHH == "undefined") {
					googletag.cmd.push(function() {
						googletag.pubads().refresh();
					});
					window.yieldlove_prevent_autoload = true;
				}
			}, 1000);
		} else {
			bidder_turn = 'is NOT yl'
			window.yieldlove_prevent_autoload = true;
		}

		googletag.cmd.push(function() {
			googletag.pubads().getSlots().forEach(function(slot) {
				slot.setTargeting('turn', bidder_turn)
			});
		});

		var googletag = googletag || {};
			googletag.cmd = googletag.cmd || [];
			googletag.cmd.push(function() {
			googletag.pubads().disableInitialLoad();
		});
	</script>
<!-- End YieldLove -->
				
