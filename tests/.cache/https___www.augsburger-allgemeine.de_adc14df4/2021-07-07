<script>
	function getNewsletterSubscriptions(key) {
		var newsletterSubscriptions = '';
		if (uData !== 'false') {
			var newsletterSubscriptionsObject = JSON.parse(atob(uData))[key];
			if (newsletterSubscriptionsObject) {
				for (key in newsletterSubscriptionsObject) {
					if (newsletterSubscriptionsObject[key] === 1) {
						if (newsletterSubscriptions !== '') {
							newsletterSubscriptions += ','
						}
						newsletterSubscriptions += key;
					}
				}
			}
		}
		return newsletterSubscriptions;
	}

	var uData = getCookie('_uData', 'false');
	var uniqueUserHash = '';
	var customerGroup = '';
	var registrationLevel = '';
	var loggedIn = '0';
	if (uData !== 'false') {
		uData = uData.replace(/\"/g, ''); // remove quotes in the string
		loggedIn = '1';
		uniqueUserHash = JSON.parse(atob(uData)).unique_user_hash;
		customerGroup = JSON.parse(atob(uData)).crm_customergroups;
		registrationLevel = JSON.parse(atob(uData)).registration_level;
	}

	window._ti = window._ti || {};
	window._ti["customerId"] = uniqueUserHash;
	window._ti["logIn"] = loggedIn;
	window._ti["customerGroup"] = customerGroup;
	window._ti["userRegistrationLevel"] = registrationLevel;
	window._ti['newsletterabo'] = getNewsletterSubscriptions("nl_optins");
	window._ti['newsletteraboCrm'] = getNewsletterSubscriptions("crm_nl_optins");
	window._ti["fehlerseite"] = "0";
	window._ti["pageTitle"] = "Nachrichten Augsburg Bayern | Augsburger Allgemeine";
	window._ti['pagePortal'] = "NewsDesktop";
	
	window._ti["pageUrl"] = modifyUrl(window.location.href);
	window._ti["contentGroup"] = {
			1: "News",
			2: "Startseite"
	};
	
					window._ti['pageType'] = "Startseite";
				

	window._tiConfig = window._tiConfig || {
		tiDomain: "responder.wt-safetag.com",
		tiId: "915616854953982",
		option: {}
	};
	
	function modifyUrl(url) {
		const onlyParams = window.location.search;
		const urlParams = new URLSearchParams(onlyParams);
		var urlParamsToRemove = ["fbclid", "gclid"];
		for (i = 0; i < urlParamsToRemove.length; i++) {
			var urlParam = urlParamsToRemove[i];
			var urlParamString = "&"+urlParam;
			if(urlParams.has(urlParam)) {
				if(onlyParams.includes("?"+urlParam)) {
					urlParamString = "?"+urlParam;
				}
				url = url.replace(urlParamString+"="+urlParams.get(urlParam), "");
			}
		}
		return url;
	}

	(function(c,d,a,f){c.wts=c.wts||[];var g=function(b){var a="";b.customDomain&&b.customPath?a=b.customDomain+"/"+b.customPath:b.tiDomain&&b.tiId&&(a=b.tiDomain+"/resp/api/get/"+b.tiId+"?url="+encodeURIComponent(c.location.href)+"&v=5");if(b.option)for(var d in b.option)a+="&"+d+"="+encodeURIComponent(b.option[d]);return a};if(-1===d.cookie.indexOf("wt_r=1")){var e=d.getElementsByTagName(a)[0];a=d.createElement(a);a.async=!0;a.onload=function(){if("undefined"!==typeof c.wt_r&&!isNaN(c.wt_r)){var b=
	new Date,a=b.getTime()+1E3*parseInt(c.wt_r);b.setTime(a);d.cookie="wt_r=1;path=/;expires="+b.toUTCString()}};a.onerror=function(){"undefined"!==typeof c.wt_mcp_hide&&"function"===typeof c.wt_mcp_hide.show&&(c.wt_mcp_hide.show(),c.wt_mcp_hide.show=function(){})};a.src="//"+g(f);e.parentNode.insertBefore(a,e)}})(window,document,"script",_tiConfig);
