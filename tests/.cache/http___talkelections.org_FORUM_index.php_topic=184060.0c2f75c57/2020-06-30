<script type="text/javascript">
	
	smf_smileys_url = 'https://talkelections.org/FORUM/Smileys/classic';
	smf_session_id = '41ebf604be191424faae933b313d0de7';
	smf_member_id = 0;
	smf_userOptions = 0;
	smf_userStatus = 0;
	smf_current_topic = 184060;
	smf_current_board = 14;
	smf_current_board_name = 'Individual Politics';
	smf_is_using_sceditor = false;
	smf_post_editor_name = 'message';
	bbc_quote_from = 'Quote from';
	bbc_quote = 'Quote';
	bbc_search_on = 'on';
	bbc_max_image_width = 760;
	bbc_max_image_height = 780;
	ajax_notification_text = 'Loading...';
	help_popup_heading_text = 'Help';

	// This needs to be written directly to the page due to the dynamic maxHeight variable
	function isSignatureSizeTooBig(size) {
		if ( size > 475 ) {
			return true;
		}

		return false;
	}

	function loadtweet(data) {
		document.write(data.html);
	}

	
	// On document ready.
	$(function() {

		// Likes count for messages
		$(document).on("click", ".like_count a", function(e){
			e.preventDefault();
			var title = $(this).parent().text(),
				url = $(this).attr("href") + ";js=1";
			return reqOverlayDiv(url, title, "post/thumbup.png");
		});

		// Message likes
		$(document).on("click", ".msg_like", function(event){
			var obj = $(this);
			var msgID = obj.attr("data-msg-id");
			var msgWaitingWheel = $("#like_waiting_msg" + msgID);

			event.preventDefault();

			// Check if wheel is already shown, which indicates an operation is taking place
			var wheelState = $(msgWaitingWheel).css("display");
			if ( wheelState == "block" || wheelState == "inline" || wheelState == "inline-block" )
				return;

			$(msgWaitingWheel).show();

			$.ajax({
				type: "GET",
				url: obj.attr("href") + ";js=1",
				cache: false,
				dataType: "html",
				success: function(html) {
					if ( html.indexOf('"like_button"') > 0 ) {
						obj.closest("div").replaceWith(html);
					} else {
						var sAjax_indicator = '<div class="centertext"><img src="' + smf_images_url + '/waiting.gif"></div>';
						var sIcon = smf_images_url + '/' + 'post/thumbup.png';
						var sHeader = 'Error';

						// Create the div that we are going to load
						var oContainer = new smc_Popup({heading: sHeader, content: sAjax_indicator, icon: sIcon});
						var oPopup_body = $('#' + oContainer.popup_id).find('.popup_content');

						var help_content = html;
						oPopup_body.html(help_content);
					}
				},
				error: function (html) {
					$(msgWaitingWheel).hide();
				},
				complete: function () {
					$(msgWaitingWheel).hide();
				}
			});

			return false;
		});
	});

	
	function toggleQuoteHighlightBody(e, isMouseEnter)
	{
		var quoteHeaderDiv       = $(e.target);
		var highlightColor       = "rgb(233, 233, 233)";
		var highlightBorderClock = "black";

		if ( !isMouseEnter ) {
			highlightColor = "";
			highlightBorderClock = "";
		}

		if ( quoteHeaderDiv.is("b") && quoteHeaderDiv.parent().is("a") )
			quoteHeaderDiv = quoteHeaderDiv.parent().parent();
		else if ( quoteHeaderDiv.is("a") )
			quoteHeaderDiv = quoteHeaderDiv.parent();

		// Must check the parent type/class because not all quotes have the same structure, per AtlasMod update
		if ( quoteHeaderDiv.parent().is("div") && quoteHeaderDiv.parent().attr("class") === "undefined" ) {
			quoteHeaderDiv.parent()
				.children("div.quote").first()
				.css("background", highlightColor).css("border-color", highlightBorderClock);
		} else {
			quoteHeaderDiv.next(".quote")
				.css("background", highlightColor).css("border-color", highlightBorderClock);
		}
	}
	$(function() {
		// Make quotes easier to read by highlighting quote element on hover
		$(document).on("mouseenter", "div.quoteheader", function(e){
			toggleQuoteHighlightBody(e, true);
		});
		// Make quotes easier to read by highlighting quote element on hover
		$(document).on("mouseleave", "div.quoteheader", function(e){
			toggleQuoteHighlightBody(e, false);
		});
	});
</script>
