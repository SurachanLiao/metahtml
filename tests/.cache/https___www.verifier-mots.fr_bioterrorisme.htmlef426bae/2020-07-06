<script type="text/javascript">

  function showRestaurantData(url)
  {
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function () {
      if (this.readyState == 4 && this.status == 200)
      {
        document.getElementById("restaurant_list").innerHTML = this.responseText;
      }
    };
    xhttp.open("GET", url, true);
    xhttp.send();

  }

  function mouseOverRating(scrabbleid, rating) {

    resetRatingStars(scrabbleid)

    for (var i = 1; i <= rating; i++)
    {
      var ratingId = scrabbleid + "_" + i;
      document.getElementById(ratingId).style.color = "#ff6e00";

    }
  }

  function resetRatingStars(scrabbleid)
  {
    for (var i = 1; i <= 5; i++)
    {
      var ratingId = scrabbleid + "_" + i;
      document.getElementById(ratingId).style.color = "#9E9E9E";
    }
  }

  function mouseOutRating(scrabbleid, userRating) {
    var ratingId;
    if(userRating !=0) {
      for (var i = 1; i <= userRating; i++) {
        ratingId = scrabbleid + "_" + i;
        document.getElementById(ratingId).style.color = "#ff6e00";
      }
    }
    if(userRating <= 5) {
      for (var i = (userRating+1); i <= 5; i++) {
        ratingId = scrabbleid + "_" + i;
        document.getElementById(ratingId).style.color = "#9E9E9E";
      }
    }
  }

  function addRating (scrabbleid, ratingValue) {
    var xhttp = new XMLHttpRequest();

    xhttp.onreadystatechange = function ()
    {
      if (this.readyState == 4 && this.status == 200) {

        showRestaurantData('getRatingData.php?id=' + scrabbleid);

        if(this.responseText != "success") {
          alert(this.responseText);
        }
      }
    };

    xhttp.open("POST", "insertRating.php", true);
    xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
    var parameters = "index=" + ratingValue + "&dictionnaire_id=" + scrabbleid;
    xhttp.send(parameters);
  }
