        // Lazy load the "best of ABC" content.
        (function(){
        var $fragment = $('#bestofabc-script');
        var $body = $('body');
        var $window = $(window);
        $window.on('scroll.bestofabc', function(){
            if ($.inviewport($fragment, {threshold: 500})){
                $.get('https://www.abc.net.au/bestof/includes/4-col-footer2.inc.htm', function(content){
                    $fragment.closest('.page').replaceWith(content);
                });
                $window.off('scroll.bestofabc');
            }
        });
        })();
    </script>
