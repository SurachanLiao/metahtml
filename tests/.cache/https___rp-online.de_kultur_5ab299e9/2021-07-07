<script>
(function() {
    var init = function() {
        if(typeof __tcfapi === "function" && document.cookie.indexOf('OptanonAlertBoxClosed')>=0){
            var emscript = document.createElement('script');
            var emSSL = 'https:' == document.location.protocol;
                emscript.type='text/javascript';
                emscript.src = (emSSL ? 'https:' : 'http:') + '//ups.xplosion.de/loader/46505/default.js';
                document.querySelector("head").appendChild(emscript);
        }else{
            window.setTimeout(init, 50);
            return;
        };
    };
    init();
})();
    var check_npa=false;
    var consolelogads = [];
    var inisetRequestNonPersonalizedAds = function() {
        //GPT abfrage
        if (!window.googletag) {
            window.setTimeout(inisetRequestNonPersonalizedAds, 50);
            consolelogads.push("#CMP, warten auf GPT");
            return;
        }
        consolelogads.push("#CMP, GPT ist da");
        
    
        //GPT Api abfrage
        if (!window.googletag && !googletag.apiReady) {
            window.setTimeout(inisetRequestNonPersonalizedAds, 50);
            consolelogads.push("#CMP, warten auf GPT API");
            return;
        }
        consolelogads.push("#CMP, GPT API ist da");  

         //Datalayer abfrage
         if (typeof dataLayer[0].adFreeReasons!=="object") {
            window.setTimeout(inisetRequestNonPersonalizedAds, 50);
            consolelogads.push("#CMP, warten auf Datalayer");
            return;
        }
        consolelogads.push("#CMP, Datalayer ist da");  
     
        //NPA setzten
        var contenttype ="", abon="", PremiumContent="", paidUser="";
        if(dataLayer[0].contentType!== undefined){
            contenttype = dataLayer[0].contentType;
        }
        // if(dataLayer[0].actionGroups.abonnement === undefined){
        //     abon="noabo";
        // }else{
        //     //dataLayer[0].actionGroups.segment
        //     abon = dataLayer[0].actionGroups.abonnement;
        // }
        if(dataLayer[0].isPremiumContent!== undefined){
            PremiumContent = dataLayer[0].isPremiumContent.toString();
        }
        
        if(dataLayer[0].adFreeReasons!== undefined){
            var paidUserzahl = dataLayer[0].adFreeReasons.PaidUser;
            if(paidUserzahl===1){
                paidUser="paiduser";
            }else{
                paidUser="nopaiduser";
            }
        }
        googletag.cmd.push(function() {
            googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                if (event.isEmpty) {
                    var id = event.slot.getSlotElementId();
                    if(event.slot.getCollapseEmptyDiv()===true){
                        if(document.getElementById(id).parentElement.classList.length>0){
                            if(document.getElementById(id).parentElement.classList[0]=== "park-portal"){
                                consolelogads.push("### getCollapseEmptyDiv "+id);
                                document.getElementById(id).parentElement.style.display="none";
                            };
                        };
                    };
                };
                var idd = event.slot.getSlotElementId();
                if (idd==="mobilebanner_2") {
                    //consolelogads.push('Order = Campaign ID: ', event.campaignId);
                    //consolelogads.push('Order = scrollerstopp: ', window.scrollerstopp);
                    if(event.campaignId===2545697976 && window.scrollerstopp==="true"){
                        document.getElementById(idd).parentElement.style.display="none";
                        consolelogads.push('+++ SOD Stopper: ');
                    };
                };
            });
            //googletag.pubads().setRequestNonPersonalizedAds(0);
            googletag.pubads().setTargeting('contenttype', contenttype);
            //googletag.pubads().setTargeting('abonnement', abon);
            googletag.pubads().setTargeting('isPremiumContent', PremiumContent);
           googletag.pubads().setTargeting('paidUser', paidUser);
        });
        //consolelogads.push("#CMP, GPT ist da, setRequestNonPersonalizedAds(0)");
        consolelogads.push("#CMP, GPT ist da, setTargeting");
    };
    inisetRequestNonPersonalizedAds();
</script>
<script type="text/javascript">
(function() {
    function init() {
        var gpt_rpd_adslot;
        if( !window.googletag){
            console.log("warten auf GPT");
            window.setTimeout(init, 100);
        return; 
        }
        var window_width = window.getComputedStyle(document.querySelector("body")).getPropertyValue('width');
            window_width = parseInt(window_width, 10);
        if(window_width >1609){
            if (typeof document.getElementById("skyscraper_2") !=="undefined"){
                googletag.cmd.push(function() {
                    gpt_rpd_adslot = googletag.defineSlot('/1017380/rp-online.de/localnews/skyscraper_2', [[120,600],[160,600],[200,600],[300,600],[300,1050]], "skyscraper_2").setTargeting('rang', '2').addService(googletag.pubads());
                    googletag.enableServices();
                    googletag.display('skyscraper_2');
                });
            }
        }
    }
    window.setTimeout(init, 500);       
})();
</script>
