            var _sf_async_config = {};
            /* CONFIGURATION START */
            _sf_async_config.uid = 48719;
            _sf_async_config.domain = 'tribune.com.pk';
            _sf_async_config.useCanonical = true;
            _sf_async_config.sections = 'Punjab';
