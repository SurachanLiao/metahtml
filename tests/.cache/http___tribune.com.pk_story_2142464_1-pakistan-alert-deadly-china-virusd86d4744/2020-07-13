<script type="text/javascript">

      function getCurrentDate() {
        let today = new Date();
        return today.getFullYear() + '-' + (today.getMonth() + 1) + '-' + today.getDate();
      }

      function setStorageWith(key, value) {
        localStorage.setItem(key, JSON.stringify(value))
      }

      function getFromLocalStorage(key, current_day_key) {
        const itemStr = localStorage.getItem(current_day_key)
        if (!itemStr) {
          return null
        }
        return JSON.parse(itemStr);
      }

      function get_forex_rates_data(cache_key) {
        $.ajaxSetup({
          headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
          }
        });
        $.ajax({
          type: "POST",
          url: "https://tribune.com.pk/get_forex_rates",
          data: {
            cache_key: cache_key,
          },
          success: function (result) {
            if (result.data) {
              render_forex_data_for_ticker(result.data);
              //render_forex_data_for_widget(result.data);
            }
          },
          error: function (result) {
            console.log(result.message);
            return false;
          }
        });
      }

      function get_forex_rates_widget_data(cache_key) {
        $.ajaxSetup({
          headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
          }
        });
        $.ajax({
          type: "POST",
          url: "https://tribune.com.pk/get_forex_rates_widget",
          data: {
            cache_key: cache_key,
          },
          success: function (result) {
            if (result.data) {
              render_forex_data_for_widget(result.data);
            }
          },
          error: function (result) {
            console.log(result.message);
            return false;
          }
        });
      }

      function render_forex_data_for_ticker(data) {

        if($('#forex_ticeker_ul').length){
          let all_html = '';
          $.each(data, function (i, item) {
            let difference = item.differences.difference;
            let difference_percentage = item.differences.difference_percentage;
            let arrow_key = item.differences.arrow;

            if (arrow_key == 'up') {
              all_html += '<li>' +
                  '<a href="#" target="_self">' +
                  '<span class="breaking-bold">PKR-' + item.symbol + '</span>' +
                  '<span class="breaking-price">' + item.value + '</span>' +
                  '<span class="breaking-highlight">' +
                  '<img src="images/business/uparrow.png" alt="">' +
                  '<span class="greenno green_number">' + difference + '</span>' +
                  '<span class="greenno green_number_percentage">' + difference_percentage + '</span>' +
                  '</span>' +
                  '</a>' +
                  '</li>';
            }
            else if (arrow_key == 'down') {
              all_html += '<li>' +
                  '<a href="#" target="_self">' +
                  '<span class="breaking-bold">PKR-' + item.symbol + '</span>' +
                  '<span class="breaking-price">' + item.value + '</span>' +
                  '<span class="breaking-highlight">' +
                  '<img src="images/business/downarrow.png" alt="">' +
                  '<span class="greenno red_number">' + difference + '</span>' +
                  '<span class="greenno red_number_percentage">' + difference_percentage + '</span>' +
                  '</span>' +
                  '</a>' +
                  '</li>';
            }
          });

          $('#forex_ticeker_ul').empty().append(all_html);
          jQuery(".modern-ticker").modernTicker({
            effect: "scroll",
            scrollType: "continuous",
            scrollStart: "inside",
            scrollInterval: 20,
            transitionTime: 500,
            linksEnabled: true,
            pauseOnHover: true,
            autoplay: true
          });
        }
      }

      function render_forex_data_for_widget(data) {

        if($('.forex_widget_small').length){
          let all_html = '';
          $.each(data.data, function (i, item) {
            //if(i<5){
            all_html += '<tr>' +
                '            <th scope="row">PKR-' + item.currency + '</th>' +
                '            <td>' + item.buy.toFixed(2) + '</td>' +
                '            <td>' + item.sell.toFixed(2) + '</td>' +
                '        </tr>';
            //}
          });
          $('.forex_widget_small').empty().append(all_html);
        }
      }

      function videos_by_tag(tag) {

        $('.forex_video_loader').show();
        $('#vidContainerId').hide();
        $('.vid-list-container').hide();
        $('.video-top-right-content li').removeClass('active_tab');

        $.ajaxSetup({
          headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
          }
        });
        $.ajax({
          beforeSend: function(){
            $('#'+tag+'_tag').addClass('active_tab');
          },
          type: "POST",
          url: "https://tribune.com.pk/get_videos_by_tags" ,
          data: {
            tag: tag
          },
          success: function (data) {

            $('.forex_video_loader').hide();
            $('.latest-videos-section').html(data);
            $('#'+tag+'_tag').addClass('active_tab');

          },
          error: function (result) {

          }
        });
      }
    </script>

