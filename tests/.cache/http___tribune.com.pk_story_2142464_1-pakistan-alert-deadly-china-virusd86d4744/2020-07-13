<script>
  $(document).ready(function() {
    $('#scrollDiv').hide();
    $(window).scroll(function() {
        if ($(document).scrollTop() > 300) {
          $("#navbar-fixed").addClass("nav-fixed-top");
          $("#navbar-fixed").removeClass("not-to-fix");
        }
        else {
          $("#navbar-fixed").removeClass("nav-fixed-top");
          $("#navbar-fixed").addClass("not-to-fix");
        }
    });
});
   /* start scroll to fixed*/
   var prevScrollpos = window.pageYOffset;
                          window.onscroll = function() {
                          var currentScrollPos = window.pageYOffset;
                            if (prevScrollpos > currentScrollPos) {
                              document.getElementById("navbar-fixed").style.top = "0";
                            } else {
                              document.getElementById("navbar-fixed").style.top = "-75px";
                            }
                            prevScrollpos = currentScrollPos;
                          }
                        /* END:: scroll to fixed*/
</script>
