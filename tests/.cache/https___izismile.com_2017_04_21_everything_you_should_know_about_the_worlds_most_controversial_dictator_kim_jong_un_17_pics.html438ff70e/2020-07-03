	$('body').append('<a id="toSpicy" href="https://izispicy.com/" target="_blank">izispicy</a>');
	$(window).scroll(function() {
		if ($(this).scrollTop() > 4000) {
			$('#toTop').fadeIn();
			$('#toSpicy').fadeIn();
		} else {
			$('#toTop').fadeOut();
			$('#toSpicy').fadeOut();
		}
	});
	$('#toTop').live({
	'click': function(){
		$(window).scrollTop(0);
	}
	})
});

</script>
