                    <i class="inlinemedia__icon--open"></i>
                </div>
                <!-- /ko -->
            </div>
            <div class="inlinemedia__content">
                <div class="inlinemedia__image--container">
                    <img class="h-nocopy" oncontextmenu="return false;" data-bind="attr: { src: ThumbnailUrl, alt: Title }" />
                </div>

                <!-- ko if: isSideline -->
                <div class="inlinemedia__subtitle" data-bind="html: Credits"></div>
                <!-- /ko -->

                <!-- ko if: isInline() && detailsAvailable() -->
                <div class="inlinemedia__more">
                    <div class="inlinemedia__action " data-bind="css: {'inlinemedia__action--hide': !detailsVisible(), 'inlinemedia__action--show' : detailsVisible()}, click: toggleDetails">
                        <i class="inlinemedia__eye"></i>
                        <!-- ko ifnot: detailsVisible -->
                        <span class="inlinemedia__details">
                            Mostra dettagli
                        </span>
                        <!-- /ko -->
                        <!-- ko if: detailsVisible -->
                        <span class="inlinemedia__details">
                            Nascondi dettagli
                        </span>
                        <!-- /ko -->

                        <div class="inlinemedia__information" data-bind="html: Description"></div>
                        <div class="inlinemedia__credits" data-bind="html: Credits"></div>
                    </div>
                </div>
                <!-- /ko -->
            </div>
            <!-- ko ifnot: isInline -->
            <div class="multimedia__link" data-bind="component: { name: multimediaLinkComponentName, params: multimediaLinkModel }"></div>
            <!-- /ko -->
        </div>
    </div>
    <!-- /ko -->
    <!-- ko if: isText -->
