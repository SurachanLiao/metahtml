        $(document).ready(function () {
            $('.topicswitch').click(function (e) {
                e.preventDefault();
                $.ajax({
                    url: '/it/Custom/Edition/GetSwitchUrl',
                    type: "GET",
                    dataType: "json",
                    data: {
