    <script type='text/javascript'>
        var lwhb = lwhb || {cmd:[]};

        // Nedanstående callback anropas alltid då Prebid-auktionen är slutförd oavsett om något HB-material har vunnit eller ej.
        // Om Header Biddingen vinner över AppNexus anropas denna ytterligare en gång med data.win = true
        // Tyvärr så finns det inget sätt att från Prebid att koppla sig på ett event som meddelar att
        // Ad Servern vann över Header Biddingen, därför måste man göra så här.
        // Om jag kommer på ett bättre sätt så meddelar jag!

        function callback(data) {
            console.info(data);
            console.info(
               data.bid !== undefined ? "Received a bid from the Header Bidding" : "No Bid from the Header Bidding or win notification.", "-",
               data.win ? "Header Bidding Won over AppNexus internal campaigns" : "Not a Header Bidding win callback", "-",
               "InvCode:", data.apnInvCode);
        };
        var lwnative = {callback: 'adformScripts.renderPrebidNativeAds'};

    </script>
    
