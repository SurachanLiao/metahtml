    <script>
        /**
         * Renders and handles an image gallery
         * @param { String } galleryId
        */
        function runGallery(galleryId) {
            //  Select Gallery Element
            var gallery = document.getElementById(galleryId);
            var captionArea = gallery.querySelector('.c-article__top-media__caption');
            var captionTextArea = captionArea.querySelector('.caption-text');
            var imageIndexArea = captionArea.querySelector('.image-index');
            var galleryArticle = gallery.closest('article');

            // Toggles and counters
            var stepsCounter = 0;
            var stepsBreaker = 0;
            var stepsAdIndex = 0;

            // Fullscreen classes
            var bodyFullScreenClass = 'gallery-is-fullscreen';
            var galleryFullScreenClass = 'c-article__top-media--fullscreen';

            //  Calculate widths
            var widths = {};    //  Element containing imageId? for each width
                                        widths[320] = "w=320&$w=f25729b";
                            widths[610] = "w=610&$w=a64eecc";
                            widths[750] = "w=750&$w=a1569b8";
                            widths[960] = "w=960&$w=c8df749";
                            widths[1920] = "w=1920&$w=c2087ba";
            
            var galleryWidth = window.getComputedStyle(gallery).width;
            var windowSize = Math.max(window.screen.width, window.screen.height);
            if (window.devicePixelRatio) {
                windowSize = Math.round(windowSize * window.devicePixelRatio);
                galleryWidth = Math.round(galleryWidth * window.devicePixelRatio);
            }

            var inlineWidth = Number.MAX_VALUE;
            var fullWidth = Number.MAX_VALUE;
            var biggestWidth = 0;

            for (var key in widths) {
                if (widths.hasOwnProperty(key)) {
                    var width = parseInt(key);
                    if (width > galleryWidth && width < inlineWidth) {
                        inlineWidth = width;
                    }
                    if (width > windowSize && width < fullWidth) {
                        fullWidth = width;
                    }
                    biggestWidth = Math.max(biggestWidth, width);
                }
            }

            var inlineWidth = Math.min(inlineWidth, biggestWidth);
            var fullWidth = Math.min(fullWidth, biggestWidth);

            //  Form Interaction
            var previousBtn = gallery.querySelector('.js-gallery__arrow-prev');    //  Previous Button
            var nextBtn = gallery.querySelector('.js-gallery__arrow-next');    //  Next button
            var fullScreenBtn = gallery.querySelector('.c-gallery__fullscreen-btn');
            var bodyElement = document.querySelector('body');
            var activeIndex = 0;    //  Dictates which slide is in focus
            var slides = gallery.querySelectorAll('.c-gallery__item');    //  Slide elements
            
            // The article title in which the gallery is contained in
            var articleTitle = galleryArticle.getAttribute('data-article-title') || "";
            
            if (articleTitle === '') {
              var galleryArticleMeta = galleryArticle.querySelector('span[id^="article-data-"]');
              if (galleryArticleMeta !== null) {
                articleTitle = galleryArticleMeta.getAttribute('title') || '';
              }
            }

            /**
             *  Changes css classes on the involved slides, based on activeIndex
             */
            function mapSlides(currentIndex) {
                var classPrev = 'c-gallery__item-previous';
                var classCurrent = 'c-gallery__item-current';
                var classNext = 'c-gallery__item-next';

                slides.forEach(function(slide, index) {
                    if (index === currentIndex) {
                        // var captionText = slide.querySelector('.c-gallery__img').getAttribute('data-caption');
                        var captionText = slide.querySelector('.caption-holder').innerHTML;
                        //captionTextArea.textContent = captionText;
                        captionTextArea.innerHTML = captionText;
                        imageIndexArea.textContent = '' + (index + 1);
                        slide.classList.remove(classPrev, classNext);
                        slide.classList.add(classCurrent);
                    } else if (index === currentIndex + 1 || (index === 0 && currentIndex === slides.length - 1)) {
                        slide.classList.remove(classPrev, classCurrent);
                        slide.classList.add(classNext);
                    } else if (index === currentIndex - 1 || (index === slides.length - 1 && currentIndex === 0)) {
                        slide.classList.remove(classNext, classCurrent);
                        slide.classList.add(classPrev);
                    } else {
                        slide.classList.remove(classNext, classPrev, classCurrent);
                    }
                });
            }

            /** 
             *  Send GA event, if it has not been sent already, for each image
             */

            function sendGaPageview(articleTitle, galleryId) {
                window.dataLayer.push({
                    'virtualPageTitle': articleTitle,
                    'virtualPageURL': galleryId,
                    'event': 'VirtualPageview'
                });
            }

            /** 
             * Sends a new pageView event to LP for each image toggle
             */
            function sendLinkPulsePageview(articleTitle, currentImageIndex) {
                if (typeof LP4 !== 'undefined') {
                    var currentUrl = window.location.href;
                    var n = currentUrl.lastIndexOf('-');
                    var domain = ''
                    if (currentUrl.indexOf('://') > -1) {
                        domain = currentUrl.split('/', 3).join('/');
                    } else {
                        domain = currentUrl.split('/')[0];
                    }

                    var toUrl = domain + '/' + currentUrl.substring(n+1);
                    var lp4Category = '';
                    if (window.location.pathname.split('/').lenght >= 2) {
                        var pathArr = window.location.pathname.split('/');
                        lp4Category = pathArr[pathArr.length - 2];
                    }

                    LP4.logPageview({
                        url: toUrl,
                        image: gallery.querySelector('.c-gallery__item-current').getAttribute('src'),
                        title: articleTitle,
                        type: 'gallery',
                        section: lp4Category
                    });
                }
            }

            mapSlides(activeIndex)

            /**
             * Handles sliding to the next image
             * @param { string } direction //   Can be 'left' for previous image or 'right' for next image
             */
            function slide(direction) {
                if (direction === 'left') {
                    activeIndex = activeIndex === 0 ? slides.length - 1 : activeIndex - 1;
                } else {
                    activeIndex = activeIndex === slides.length - 1 ? 0 : activeIndex + 1;
                }
                mapSlides(activeIndex);

                                    var googleGalleryId = "gallery-7.60361";
                    if(googleGalleryId == "gallery-"){
                        googleGalleryId = "gallery-1.4177999";
                    }
                    console.log('GA slide');
                    sendGaPageview(articleTitle, googleGalleryId);
                                                    sendLinkPulsePageview(articleTitle, activeIndex);
                            }
            
            previousBtn.addEventListener('click', () => slide('left'));
            nextBtn.addEventListener('click', () => slide('right'));

            var swiper = {
                x: -1,
                y: {},
                xDown: null,
                yDown: null,
                swipeDirection: null,
                handleEvent: function(event) {
                    return this[event.type](event.touches.item(0));
                },
                touchstart: function(touch) {
                    this.swipeDirection = null;
                    this.xDown = touch.clientX;
                    this.yDown = touch.clientY;
                },
                touchmove: function(touch) {
                    if (!this.xDown || !this.yDown) {
                        return;
                    }

                    // Don't slide if zoomed in
                    if (typeof window.visualViewport !== 'undefined' && window.visualViewport.scale > 1) {
                        return;
                    }

                    var xUp = touch.clientX;
                    var yUp = touch.clientY;
                    var xDiff = this.xDown - xUp;
                    var yDiff = this.yDown - yUp;

                    if (Math.abs(xDiff) > Math.abs(yDiff)) {
                        if (xDiff > 0) {
                            this.swipeDirection = 'right';
                        } else {
                            this.swipeDirection = 'left';
                        }
                    }

                    this.xDown = null;
                    this.yDown = null;
                },
                touchend: function(event) {
                    if (this.swipeDirection !== null) {
                        slide(this.swipeDirection)
                    }
                }
            };
            gallery.addEventListener('touchstart', swiper);
            gallery.addEventListener('touchmove', swiper);
            gallery.addEventListener('touchend', swiper);

            fullScreenBtn.addEventListener('click', function(e) {
                siteScripts.handleFullscreen(gallery);
            });

            function isGalleryFullscreen() {
                return bodyElement.classList.contains(bodyFullScreenClass) || gallery.classList.contains(galleryFullScreenClass);
            }
        }

        runGallery('gallery-280380415');
    </script>
    
        
    
    
