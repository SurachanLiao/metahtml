									#cta.amreport #mc_embed_signup .button { border-radius: 0; box-shadow: none !important;font-family: rockingham_bold !important; font-size: 15px !important; margin: 0 0 2em 0 !important; }
									@media (max-width: 460px){
										#cta.amreport #mc_embed_signup .mc-field-group { width: 100% !important; margin-right: 0 !important; float: none !important; }
										#cta.amreport #mc_embed_signup .mc-field-group input#mce-EMAIL { padding-top: 8px; padding-bottom: 8px; width: 317px; margin-bottom: 1em; }
