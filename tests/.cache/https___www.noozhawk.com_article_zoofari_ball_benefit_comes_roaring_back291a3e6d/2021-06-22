<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script src="/js/jquery-1.10.2.min.js"></script>
<script>
	var amount_options = {
		'monthly' : {
			'1' : '10',
			'2' : '15',
			'3' : '25'
		},
		'yearly' : {
			'1' : '60',
			'2' : '180',
			'3' : '400'
		},
		'one-time' : {
			'1' : '60',
			'2' : '180',
			'3' : '400'
		}
	};

	$(document).ready(function () {
		// Won't fire if jQuery is not enabled
		// $('#hawks_club_submit').prop('type', 'button');
	}).on('focus', '.donate-amount[type="text"]', function (e) {
		$('.donate-amount').prop('checked', false);
	}).on('blur click', '.donate-amount', function (e) {setAmount(e.currentTarget);}).on('blur click', '.donate-frequency', function (e) {setFrequency(e.currentTarget);}).on('click', '#hawks_club_submit', function (e) {
		e.preventDefault();

		var can_submit = validateForm();

		if (can_submit) {setMergeField($('#hawks-email').val());}
	});

	function setFrequency (elem) {
		$('.frequency-text').text($(elem).data('text'));
		$('#actual_frequency').val($(elem).val());
		
		// Set displayed amount
		$('#amount-1').val(amount_options[$(elem).val()][1]);
		$('#amount-2').val(amount_options[$(elem).val()][2]);
		$('#amount-3').val(amount_options[$(elem).val()][3]);
		
		$('#amount-text-1').text(amount_options[$(elem).val()][1]);
		$('#amount-text-2').text(amount_options[$(elem).val()][2]);
		$('#amount-text-3').text(amount_options[$(elem).val()][3]);
		
		if ($('.donate-amount:checked').length > 0) {$('#actual_amount').val($('.donate-amount:checked').val());} else {$('#actual_amount').val($('.donate-amount[type="text"]').val());}
		
		displayMessage();
	}

	function setAmount (elem) {
		$('.donate-amount').closest('.amount').removeClass('selected');
		$(elem).closest('.amount').addClass('selected');
		$('#actual_amount').val($(elem).val());
		
		displayMessage();
	}

	function displayMessage (reset) {
		var amount = $('#actual_amount').val(),
			frequency = $('#actual_frequency').val(),
			hawks_club_level = '';

		switch (frequency) {
			case 'monthly':

				if (amount * 12 < 99) {
					hawks_club_level = 'Birds of a Feather';
				} else if (amount * 12 < 499) {
					hawks_club_level = 'Red-Tailed Hawk';
				} else {
					hawks_club_level = 'Cooper\'s Hawk';
				}

				break;

			case 'yearly':
			case 'one-time':

				if (amount < 99) {
					hawks_club_level = 'Birds of a Feather';
				} else if (amount < 499) {
					hawks_club_level = 'Red-Tailed Hawk';
				} else {
					hawks_club_level = 'Cooper\'s Hawk';
				}

				break;
		}
		if ($('.donate-amount[type="text"]').val() == 0 && $('.donate-amount:checked').length == 0) {$('.hawks_club_level').removeClass('show');} else {
		  	$('#hawks_club_level').text(hawks_club_level);
		  	$('.hawks_club_level').addClass('show');
		}
	}

	function validateForm () {
		$('.donate-amount, .donate-frequency').attr('name', '');
		
		if ($('#actual_frequency').val() == 'one-time') {
			$('#hawks_donate_form').attr('action', 'https://checkout.fundjournalism.org/donateform');
			$('#actual_frequency').attr('name', '');
		}
		
		var email = $('#hawks-email').val();
		
		$('.email-required').remove();
		if (email == '') {
			$('<span class="email-required">Please enter your email</span>').insertAfter('#hawks-email');
			return false;
		}

		return true;
	}

	function setMergeField(email) {

		if ('sendBeacon' in navigator) {
			// Set the form data
			var data = new FormData();
			data.append('list_id', '9ec8acd2c4');
			data.append('email', email);
			data.append('group_id', '28173adb40');
			data.append('csrf_token', 'e286b2160d8dcc94257e4f6e693af6c88b51eadd');

			navigator.sendBeacon('https://www.noozhawk.com/?ACT=139', data);
			$('#hawks_donate_form').submit();
		} else {
			$.ajax({
				url: 'https://www.noozhawk.com/?ACT=139',
				method: 'POST',
				data: {
					list_id: '9ec8acd2c4',
					email: email,
					group_id: '28173adb40'
				}
			}).done(function (response) {$('#hawks_donate_form').submit();});
		}
	}
</script>

		
