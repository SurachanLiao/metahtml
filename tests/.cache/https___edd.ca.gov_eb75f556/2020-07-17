<script>
$(document).ready(function(){


    $('#searchButton').click(function () {
        if ($('#address').val() != "") {
            document.cookie = 'offcSearch=' + $('#address').val() + '; path=/';
            window.location = '/Office_Locator/';
        }
		else {
			window.location = '/Office_Locator/';
		}
    });
	
	if (navigator.cookieEnabled == true) {
//		document.getElementById('#cookies_blocked').style.display = 'none'
		
		$("#cookies_blocked").hide();

	}
	else {
//		document.getElementById('#cookies_blocked').style.display = 'block'
		
		$("#cookies_blocked").show();
	}


});

</script>

