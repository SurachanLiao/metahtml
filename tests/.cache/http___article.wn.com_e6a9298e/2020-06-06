="//ecdn1.wn.com/or/assets/js/jquery.carouFredSel-6.2.1-packed.js"></script> <script src="https://widgetcdn.chartiq.com/js/ScrollMaster.js"></script>  <script type="text/javascript"></script> <style> .autocomplete-suggestions { border: 1px solid #999; background: #FFF; overflow: auto; } .autocomplete-suggestion { padding: 2px 5px; white-space: nowrap; overflow: hidden; } .autocomplete-selected { background: #F0F0F0; } .autocomplete-suggestions strong { font-weight: normal; color: #3399FF; } .autocomplete-group { padding: 2px 5px; } .autocomplete-group strong { display: block; border-bottom: 1px solid #000; } </style>  <script>
