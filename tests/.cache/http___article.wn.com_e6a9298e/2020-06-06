$(function(){
			
		$( document ).on( 'click', '.play-video', function(e) {				
						var elem = $(this);					
            mouseOverMe(elem);	
						$('.remove_player').click();		
						$("html, body").animate({ scrollTop: 0 }, "slow");				
		});		
		
		$('.video-player').hide();
					
    $( document ).on( 'click', '.play-tv', function(e) {   	
          				e.preventDefault();										
									video_id = $(this).attr('id');
									$('.icon').removeClass('active');
									$(this).addClass('active');
									$('audio').hide();
									$('#audio').attr('src', '');				
									$('.audio-data').html('');
									$('.video-content').show();									
									video_title = $(this).parent().find('h4').clone().appendTo('.audio-data');		
          				$('#audio-player').animate({bottom:'0px'},1000).css('height', '240px');
          				$('.radio-player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1&controls=0" frameborder="0" allowfullscreen="allowfullscreen"></iframe>');
		});			
										
    $( document ).on( 'click', '.stop-video-player', function(e) {   	
 				e.preventDefault();	
 				$('.video-player').html('');
 		});	
							
    $('.audio-hide ').click(function (e) {
				$('#audio-player').animate({bottom:'-240px'},1000);
				$('#audio').attr('src', '');				
				$('.radio-player').html('');
				$('.icon').removeClass('active');
    });
})           

	
	function mouseOverMe(elem){
		$('.video-player').show();
		if (elem.attr('id')){
			var video_id = elem.attr('id');			
			var data = elem.find('.video-data').html();
			$('.player-video-data').html(data);
			$('.video-player').html('<iframe id="+ video_id +" src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" style="width: 100%; height: 100%;"></iframe><div class="overlay" style="display: none;"></div>');						
		}else {	
			var query = elem.find('.keywords').html();				
				
			if (!query.length) {
	            var query = elem.find('.title_keywords').html();	
	        }  						
			$.ajax({
							context: elem,
	                        url: 'https://wn.com/api/upge/cheetah-search-adv/video',                       
	                        cache: true,
	                        data: {
	                            'query': query
	         },
	                        dataType: 'jsonp',
	                        success: function(text) {
	                        	if (text.length > 0) {
	                            video_id = text[0].id;
	                            
	                            $('.video-player').html('<iframe id="+ video_id +" src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" style="width: 100%; height: 100%;"></iframe><div class="overlay" style="display: none;"></div>'); 
	                                            
	                         	}
	                       }
	                   });  										
				
			}		
	}	


</script> <script>
				
        var MediaOn = false;
        var diffMedia = false;
			
				 

			function playAudio(sourceUrl, element) {
						$('.radio-player').html('');
						$('.icon').removeClass('active');
						$('#audio-player').animate({bottom:'0px'},1000).css('height' , '40px');
						//$('audio').show();
						var title = $(element).parent().find('h4').html();
						$('.audio-data').html(title);
            var audio = document.getElementById("audio");
            audio.src = sourceUrl;
            if (MediaOn == false) {
                audio.play();												
                $(element).addClass('active');
                MediaOn = true;
								player_state = 1;
            } else {
               if ($(element).hasClass('active')) {
                    audio.pause();
										player_state = 0;                   
                    MediaOn = false;
                } else {
                     $(element).addClass('active');
                    audio.play();
										player_state = 1;
                    MediaOn = true;

                }
            }
        }

    </script>  <script type="text/javascript">	
	
$(function(){
				
  							
      var slideOn = true	
      $(".carousel-switch").click(function() {					
                          if (slideOn == true) {
                              $('#carousel-main').carousel('pause');
                              $(this).html('<i class="fa fa-play"></i>');
                              slideOn = false;
                          } else {
                              $('#carousel-main').carousel('cycle');
                              $('#carousel-main').carousel({ interval: 5000 })
                              $(this).html('<i class="fa fa-pause"></i>');
                              slideOn = true;
                          }
      });		
  		if ($(window).width() < 500) {        
          $('#carousel-main').carousel({
          	pause: true,
          	interval: false
        	});
			} else { 
			 			$('#carousel-main').carousel({ interval: 5000 });
      }


  				
		 		$('#select-edition').on('change', function() {						 	
            		chooseEdition();					
            		activateTab('local');
            		$('#myTab .dropdown-menu').stop(true, true).delay(100).slideUp();					
            		menu_expanded = false;					
       			});	   
				$('#select-language').on('change', function() {
            		setDetectedLanguage();
								updateSelectLanguage();
								activateTab('local');
            		$('#myTab .dropdown-menu').stop(true, true).delay(100).slideUp();
            		menu_expanded = false;					
       			});   
				$('#select-language-tab').on('change', function() {
					var lang = $('#select-language-tab').val().toLowerCase();
					setPageEdition('local/'+lang);				
					loadLocalNews(window.pageEdition, 10, 1, 'local');
					
       			});

				
        // Collapse extra content to show more on Mobile view
            if ($(window).width() < 768) {
                $( "#moreRelatedVideos1" ).addClass( "collapse" );
                $( "#moreRelatedVideos2" ).addClass( "collapse" );
                $( "#moreRelatedVideos3" ).addClass( "collapse" );
                $( "#moreRelatedVideos4" ).addClass( "collapse" );
                $( "#moreRelatedVideos5" ).addClass( "collapse" );
                $( "#moreTopStories" ).addClass( "collapse" );
                $( "#moreMostViewed" ).addClass( "collapse" );               
                $( "#moreFeaturedSections" ).addClass( "collapse" );
            }
        })
        window.addEventListener("resize", function(){
            if ($(window).width() < 768) {
                $( "#moreRelatedVideos1" ).addClass( "collapse" );
                $( "#moreRelatedVideos2" ).addClass( "collapse" );
                $( "#moreRelatedVideos3" ).addClass( "collapse" );
                $( "#moreRelatedVideos4" ).addClass( "collapse" );
                $( "#moreRelatedVideos5" ).addClass( "collapse" );
                $( "#moreTopStories" ).addClass( "collapse" );
                $( "#moreMostViewed" ).addClass( "collapse" );                
                $( "#moreFeaturedSections" ).addClass( "collapse" );
							  $('#carousel-main').carousel({
          					pause: true,
										interval: false
        				});
            		} else {
                $( "#moreRelatedVideos1" ).removeClass( "collapse" );
                $( "#moreRelatedVideos2" ).removeClass( "collapse" );
                $( "#moreRelatedVideos3" ).removeClass( "collapse" );
                $( "#moreRelatedVideos4" ).removeClass( "collapse" );
                $( "#moreRelatedVideos5" ).removeClass( "collapse" );
                $( "#moreTopStories" ).removeClass( "collapse" );
                $( "#moreMostViewed" ).removeClass( "collapse" );
                $( "#moreFeaturedSections" ).removeClass( "collapse" );
            }
    });
    	  				

					 			 
			
			var fullscreenOn = false
			var playerOn = false

			 $( document ).on( 'click', '.play-float-video', function(e) {
			 	  e.preventDefault();						
					var elem = $(this);							
					if (elem.attr('id')){
					 		var video_id = elem.attr('id');
							if (playerOn == true) {
							 			reloadSmallPlayer(video_id);								 					
    								 
    					} else {		
    								triggerSmallPlayer(video_id);			
							}							
					} else {	
								var query = elem.find('.keywords').html();		
								if (!query.length) {
	            		 var query = elem.find('.title_keywords').html();									
	        			}		
								findVideo(query);
					}			
			});
			
			
			$( document ).on( 'click', '.small_player', function(e) {
				 	e.preventDefault();		
					smallerPlayer();
					fullscreenOn = false;
					playerOn = true;
			});
			
			$( document ).on( 'click', '.enlarge_player', function(e) {
		 		e.preventDefault();		
				enlargePlayer();
				fullscreenOn = true;
			});				
			$( document ).on( 'click', '.remove_player', function(e) {   	
				e.preventDefault();	
				$('.player-container').hide('slow');
				$('.player').html('');
				fullscreenOn = false;			
				playerOn = false;
			});			 
				 
		  $('#draggable').draggable();




	window.addEventListener("resize", function(){
				if (window.innerWidth > window.innerHeight) {					 				
									$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div>').css('color', '#fff');	
									$('.player-container').animate({ 'width' : '100%',  'height' : '100%' }, 500).delay("slow").css('bottom', '0px').css('right', '0px').css('left', '0px').css('background-color', '#000');	
									var height = $('.player-container').height();	
									var width = $(window).width();									
									var ratio = 16 / 9.0;				
									var lanscapePlayerWidth = Math.round(height * ratio);	
									var leftMargin = (width - lanscapePlayerWidth) / 2;		
									$('.player').css("width", lanscapePlayerWidth).css("height", height).css("margin-left", leftMargin).css('padding-bottom', '0');
								
					} else {								
							 		var width = $(window).width();
									var ratio = 9.0 / 16;
							 		var bottomPlayerHeight = Math.round(width * ratio) + 70;
									$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div><div class="enlarge_player"><i class="fa fa-expand" aria-hidden="true"></i></div>').css('color', '#000');	
							 		$('.player-container').animate({ 'width' : '100%',  'height' : bottomPlayerHeight }, 500).delay("slow").css('bottom', '0px').css('right', '0px').css('left', '0px').css('background-color', '#fff');	;	
									$('.player').css('margin-left', '0px').css('padding-bottom', '56.25%').css('height', '0px').css('width', width);												
																
					}	 
   });
			
	function triggerSmallPlayer(video_id){
							
					playerOn = true;
					
					$('.player-container').addClass('small').removeClass('fs').show();	
					
					if (($(window).width() > 500) && ($(window).height() > 500)) {						
						
						 	$('.player-container').animate({ 'width' : '420px',  'height' : '300px' }, 500).delay("slow").css('bottom', '20px').css('right', '20px').css('top', 'auto');	
							$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div><div class="enlarge_player"><i class="fa fa-expand" aria-hidden="true"></i></div>');	
					
					} else if (($(window).width() > 500) && ($(window).height() < 500)) {
					
								$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div>').css('color', '#fff');	
									$('.player-container').animate({ 'width' : '100%',  'height' : '100%' }, 500).delay("slow").css('bottom', '0px').css('right', '0px').css('left', '0px').css('background-color', '#000');								
									var width = $(window).width();																	
									var height = $(window).height() - 45;							
									var ratio = 16 / 9.0;				
									var lanscapePlayerWidth = Math.round(height * ratio);	
									var leftMargin = (width - lanscapePlayerWidth) / 2;		
									$('.player').css("width", lanscapePlayerWidth).css("height", height).css("margin-left", leftMargin).css('padding-bottom', '0');
					
					} else {						
								var ratio = 9.0 / 16;
								var width = $(window).width();
								var bottomPlayerHeight = Math.round(width * ratio) + 100;					
								
								$('.player-container').animate({ 'width' : '100%',  'height' : bottomPlayerHeight }, 			500).delay("slow").css('bottom', '0px').css('right', '0px').css('left', '0px');
								$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div>');								

				  }
					
					$('.player-container').find('.player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1&playsinline=1&modestbranding=1" frameborder="0" allowfullscreen="allowfullscreen"></iframe>');
		}	


	
	function enlargePlayer(){
  		$('.player-container').animate({ 'width' : '100%',  'height' : '100%' }, 100);			
  		$('.details-header').html('<div class="small_player"><i class="fa fa-arrow-circle-o-left" aria-hidden="true"></i><i class="arrow"></i><span>back </span></div>');
  		$('.player-container').addClass('fs').removeClass('small');	
  		$("body").css("overflow", "hidden");
  }

	function smallerPlayer(){
  		$('.player-container').addClass('small').removeClass('fs');	
  		if ($(window).width() > 500) {
  				$('.player-container').animate({ 'width' : '420px',  'height' : '300px' }, 500).delay("slow").css('bottom', '20px').css('right', '20px').css('top', 'auto').css('left', 'auto');	
  
  		} else {
  				$('.player-container').animate({ 'width' : '100%',  'height' : '40%' }, 500).delay("slow").css('bottom', '0px').css('right', '0px').css('left', '0px');	
					
  		}
  		$('.details-header').html('<div class="remove_player"><i class="fa fa-times" aria-hidden="true"></i></div><div class="enlarge_player"><i class="fa fa-expand" aria-hidden="true"></i></div>');
  				$("body").css("overflow", "scroll");
	
    }	

  	function reloadSmallPlayer(video_id){				
  		 $('.player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?autoplay=1&playsinline=1&modestbranding=1" frameborder="0" allowfullscreen="allowfullscreen"></iframe>');
  	}
						
    function findVideo(query){				
    		$.ajax({					
    	         url: 'https://upge.wn.com/api/upge/cheetah-search-adv/video',                       
    	         cache: true,
    	         data: { 'query': query },
    	         dataType: 'jsonp',
    					 jsonpCallback: 'cbLoadVideoId',
    	         success: function(text) {
    	                    if (text.length > 0) {
    	                        video_id = text[0].id;													
    												
    													if (playerOn == false) {
                        					 triggerSmallPlayer(video_id);			
                        			} else {		
                        					 reloadSmallPlayer(video_id);	
    													}	 			 															      
    	                    }
    	         }
    		});							
    }	
		
