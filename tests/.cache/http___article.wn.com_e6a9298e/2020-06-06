        $.getJSON("https://ipapi.co/json", function(data) {		  
            var city = data.city;
        		var country = data.country_name;
    				var code = data.country;
    				var country_code = code.toLowerCase();
    				var lat = data.latitude;
            var lng = data.longitude;							
								
								$('.flag').addClass('flag-'+country_code);
                if (!city.length) {
                    city = country;
                }				
                var sel = $('#select-edition option').filter(function(index) {
                    return ($(this).text() == country)
                });

                var countryArray = ["Argentina", "Australia", "Brazil", "Canada", "Colombia", "Egypt", "France",
                    "Germany", "India", "Indonesia", "Italy", "Japan", "Iran", "Mexico", "Morocco", "Nigeria",
                    "Pakistan", "Philippines", "Russia", "Saudi Arabia", "South Africa", "South Korea",
                    "Spain", "Thailand", "Turkey", "United Kingdom", "United States", "Vietnam" ];
                if( $.inArray(country, countryArray) == -1 ){
                    $('#select-edition').append('<option selected="selected" value="'+ country +'">'+ country +'</li>');
                }else{
                    $('#select-edition').val(country).attr('selected', 'selected');
                }
                // test for cookie and country selection showed before
                showCountrySelection(country_code);
                $('.user-country').html(country);
                $('.user-city').prepend(city).show();          

                var geo_obj = $('.weather-wdgt');
								var weatherTab = $('#weather-today');			

               	var table_body = "";
                window.ipData = true;
                get_weather_data(geo_obj, lat, lng, country_code);
                get_weather(weatherTab, lat, lng, city, country);
            });

        });
		
		
		
