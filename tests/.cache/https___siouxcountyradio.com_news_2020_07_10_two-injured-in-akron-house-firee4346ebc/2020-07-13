<? if (!empty($images) && sizeof($images) > 0): ?>

    <? $reset = $images->first(); ?>
    
    @if ($obj->getMeta("type") == "carousel")
    	
    
    <div class="editor-gallery <? if (!empty($obj->getMeta("logos"))) echo "has-logos"; ?>">
    	
