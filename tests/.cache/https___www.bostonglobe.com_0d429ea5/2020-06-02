  window.formatPageLayout = function formatPageLayout(_ref) {
  var layout = _ref.layout,
      loggedIn = _ref.loggedIn;
  var pageLayout = '';

  if (layout === 'ArticleRightRail') {
    pageLayout = 'Rail';
  } else if (layout === 'PageRailLess') {
    pageLayout = 'Railless';
  } else {
    pageLayout = layout;
  }

  if (pageLayout === 'Rail' || pageLayout === 'Railless') {
    if (window && window.bg && window.bg.page && window.bg.page.exemptFromMeter || loggedIn === 'true' || window.bg.globalTracking.contentRestrictions === 'meter-by-scroll') {
      pageLayout += ' | Open';
    } else {
      pageLayout += ' | Truncated';
    }
  }

  return pageLayout;
};
