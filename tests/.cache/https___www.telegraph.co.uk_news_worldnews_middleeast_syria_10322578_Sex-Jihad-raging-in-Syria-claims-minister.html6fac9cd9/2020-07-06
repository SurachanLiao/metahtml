	else if ('LinkedIn' == platforms[i]) {
		$('div.GigyaBg').append('<img src="/template/ver1-0/i/share-in.svg" class="InBG" />');
	}
	else if ('Pinterest' == platforms[i]){
		$('div.GigyaBg').append('<img src="/template/ver1-0/i/share-p.svg" class="PinBG" />');
	}
	else if ('Twitter' == platforms[i]) {
