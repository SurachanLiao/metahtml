			url: "https://www.stuttgarter-zeitung.de/cms/hitcount?portal=dec9d785-4482-493c-adae-2b00f829f5a0&topic=c93f117d-17e7-40e3-a2d1-d529f0547ca8&content=9f506242-8734-4561-a103-154eda721ae8&adcat=b86c2724-4a0d-4502-ae77-59f89a4a574a",	
			cache: false,				
			success: function(a) {
				console.log("tracking - hitount - tracked succesfully");						
			},										
			error: function(x, status, error) {
				console.log("tracking - hitount - tracked succesfully, but insecure redirect");
			}
		});	
	</script>	
