  !function() {
    // Immediately load audiences and id from local storage
    try {
          var storedAuds = window.localStorage.getItem("lotame_4335_auds") || '';
          if (storedAuds) {
            googletag.cmd.push(function() {
              window.googletag.pubads().setTargeting("audid", storedAuds.split(','));
            });
          }

          var localStoragePid = window.localStorage.getItem("_cc_id") || '';
          if (localStoragePid) {
            googletag.cmd.push(function() {
                window.googletag.pubads().setTargeting("dfp_cc_id", localStoragePid);
            });
          }
        } catch(e) {
     }

    // Callback when targeting audience is ready to push latest audience data
    var audienceReadyCallback = function (profile) {
          // Get audiences as an array
          var lotameAudiences = profile.getAudiences() || [];

          // Set the new target audiences for call to Google
          googletag.cmd.push(function() {
            window.googletag.pubads().setTargeting("audid", lotameAudiences);
          });

          var lotamePid = profile.getProfileId() || '';
          if (lotamePid) {
            googletag.cmd.push(function() {
              window.googletag.pubads().setTargeting("dfp_cc_id", lotamePid);
            });
          }
     };

      var data = null;
      // check if ldap hashed id is available
      if (_data.at) {
          data = {
            behaviors: {
              int: ["A1ldap"]
            },
            ruleBuilder: {
              st_ldapType: [_data.visitorcat.toString()]   // visitor category
            },
            thirdParty: {
              namespace: "SPHN",
              value: _data.at    // ldap hashed id
            }
          }
        }

        // Lotame config
        var lotameClientId = "4335";
        var lotameTagInput = {
            data: data,
            config: {
              clientId: Number(lotameClientId),
              audienceLocalStorage: true,
              onProfileReady: audienceReadyCallback
            }
        };

        // Lotame initialization
        var lotameConfig = lotameTagInput.config || {};
        var namespace = window["lotame_" + lotameConfig.clientId] = {};
