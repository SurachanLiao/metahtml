   jQuery('<div class="cx_paywall_placeholder" id="cx_paywall_placeholder"></div>').appendTo(".node-article .field-name-body");
}
if(document.getElementById("cx_paywall_placeholder")) {
document.getElementById("cx_paywall_placeholder").id="sph_cdp_4";
}

insertNewsletterSignupCampaign();

console.log("isLdapLoginDetailsReceived "+isLdapLoginDetailsReceived+ " isPropensityScoreReceived"+isPropensityScoreReceived);
if(isPropensityScoreReceived == true && isLdapLoginDetailsReceived == true && sphWavePaywallInitiated==false) {
console.log("Both propensity and login received => activating CDP");
sphWavePaywallInitiated = true;
SPH_CDP().init('4',' https://cdp.activation.sph.com.sg', 'c9875acb-576c-44bb-9484-aafb59ace74c').run();

}
}

window.addEventListener("SPHPropensityScoreReceived", function(evt) {
 isPropensityScoreReceived = true;
console.log("SPHPropensityScoreReceived => activating CDP");
activateCDP();          
}, false);

document.addEventListener('ldapLoginDetailsReceived', function(e) {
console.log("ldapLoginDetailsReceived => activating CDP");
      isLdapLoginDetailsReceived = true;
      activateCDP();
    },false);



       </script>
<script>
var cidd = getCookieByName('_ga');
cidd = cidd.slice(6);
var ingestScore = "https://ingest.sphdigital.com/api/score?client_id="+ cidd;
var xhttp = new XMLHttpRequest();
xhttp.open("GET", ingestScore);
var apiKey = "HsrrBKBqNf1YcZeVQXzrC8uBCdGse9Ay6Ocje1g2";
xhttp.setRequestHeader("x-api-key", apiKey);
xhttp.send();
xhttp.onreadystatechange = function(){
  if(xhttp.readyState == 4){
    var response = JSON.parse(xhttp.responseText);
    //window.dataLayer.push({ score:response.score, clientid: cidd });
    window.propenmo1 = response.score;
    var evt = new CustomEvent("SPHPropensityScoreReceived", {propenmo1: window.propenmo1});
    window.dispatchEvent(evt);
  }
};
</script>

<script>
var geSpecialArticle = false;
function isDesktopSafari() {
var desktopSafari = false;
if (navigator.userAgent) {
  var clientplatform = (navigator.userAgent).toLowerCase();
  var isSafari = false;
  if ((clientplatform.indexOf("safari") > -1) && (clientplatform.indexOf("chrome") == -1)) {
    isSafari = true;
  }
  if (isSafari && window.innerWidth >= 740) {
    console.log('this is safari desktop');
desktopSafari = true;
  }
}
return desktopSafari;
}
</script>

