    function htmlEncode(value){
      //create a in-memory div, set it's inner text(which jQuery automatically encodes)
      //then grab the encoded contents back out.  The div never exists on the page.
      return jQuery('<div/>').text(value).html();
    }

    function trySingleSignOnLogin() {
      var f = document.createElement('iframe');
      f.src = "/p/login.php";
      f.style.width = "0px";
      f.style.height = "0px";
      f.style.border = "none";
      jQuery('body').append(f);
    }

    function loadSubscriberContent() {
      jQuery.post( "/p/login_details?article_id="+_data.articleid, function( data ) {
        data = JSON.parse(data);
        // Login & Logout :)
         _data.visitorcat = data.service_type_value;
         _data.ac =  data.service_type_value;
         token_data = data;
        if ( (data.usertype == 'sub' || data.usertype == 'reg') && data.login_id != "") {

          var read_win = '<li id="my_rewards_li" class="hide"><a class="my_rewards">ST Read & Win</a></li>';
          if(data.service_type_value == 9 || data.service_type_value == 10 ){
            var read_win = '<li id="my_rewards_moe_li"><a class="my_rewards">ST Read & Win</a></li>';
          }

          var subscribe_link_for_reg = '';
          if(data.usertype == 'reg')
            subscribe_link_for_reg = '<li><a target="blank" href="https://subscribe.sph.com.sg/publications-st/?utm_campaign=st_subscription&utm_medium=sph-publication&utm_source=st&utm_content=subscribelink-menu">Subscribe</a></li>';

          var st_login_info = '<li class="nav-user"><a href="javascript:;" class="dropdown-toggle" name="login-user-name" data-toggle="dropdown"><span class="user-name-hi">Hi </span><span id="sph_user_name">'+htmlEncode(data.login_id)+'</span><span class="caret"></span></a><ul class="dropdown-menu">'+subscribe_link_for_reg+'<li><a class="mysph_user_name" href="javascript:;">Update your password</a></li>'+read_win+'<li><a class="amlogout" href="javascript:;">Log out</a></li></ul></li>';


          jQuery('.menu-user').html(st_login_info);
          if(data.ldapw){
            _data.at = data.ldapw;
            var meta = document.createElement('meta');
            meta.name = 'ldapw';
            meta.content = data.ldapw;
            document.getElementsByTagName('head')[0].appendChild(meta);
          }
          if(typeof(rewards_optimise) !== 'undefined'){
            jQuery(".my_rewards").parents('.hide').removeClass("hide");
          }
        } else if(data.usertype == "anoy") {
          var login_header_track = "if(typeof(tag) != 'undefined') return tag.click.send({elem:this, name:'click login header', level2:'"+_data.level2+"', type:'action'});";
          var st_login_info = '<li class="nav-login"><a class="mysph_login" href="javascript:;">Log in</a></li>'+
          '<li class="nav-subscribe"><a href="https://subscribe.sph.com.sg/publications-st/?utm_campaign=st_subscription&utm_medium=sph-publication&utm_source=st&utm_content=subscribelink-menu" target="_blank">Subscribe</a></li>';
          jQuery('.menu-user').html(st_login_info);
          if(typeof(rewards_optimise) !== 'undefined'){
            jQuery(".navbar-toggle.collapsed").addClass("rewards_dot");
          }
          if (jQuery(".dynamic-nav .block-user-menu").length > 0) {
            jQuery(".dynamic-nav .block-user-menu .nav-subscribe a").attr("href", "https://subscribe.sph.com.sg/publications-st/?utm_campaign=st_subscription&utm_medium=sph-publication&utm_source=st&utm_content=subscribelink-header");
          }
        }else{
          _data.visitorcat = 4; // sph annoy
        }
        //
        if(typeof(rewards_optimise) !== 'undefined'){
          jQuery(".block-menu-rewards").removeClass('hide');
        }

        if(isArticlePage && jQuery(".field-name-body .field-items .field-item .premium-read-more").length>=1 && data.full_content!= "") {
          //Remove all but imu ads container
          jQuery(".field-name-body .field-items .field-item").children().not('.ads.clearleft').remove();
          jQuery(".field-name-body .field-items .field-item").prepend(data.full_content);
        } else if(isArticlePage) {
          reloadPaywall();
        }

        if(data.service_type_value == 2){
          jQuery('.st-icon-gift').removeClass("hide").addClass("show");
        }

        // Dispatch the event
        var event = document.createEvent('Event');
        event.initEvent('ldapLoginDetailsReceived', true, true);
        document.dispatchEvent(event);

      });
    }

    function reloadPaywall() {
        var cxpageContext = { "categories": {} };
        cxpageContext["categories"]["sph-visitorcat"] = _data.visitorcat.toString(10);
      cX.CCE.callQueue.push(['run',{
          widgetId: '0290372c69e05c377ddf359350fc0bccc54daa08',
          targetElementId: 'cx_0290372c69e05c377ddf359350fc0bccc54daa08'
      },{ context: cxpageContext }]);
    }

    //to get full content of verified registered user
    function getRegisteredUserContent() {
      jQuery.post( "/p/login_details?reg_user=reg_st&article_id="+_data.articleid, function( data ) {
        data = JSON.parse(data);
        if(isArticlePage && jQuery(".field-name-body .field-items .field-item .premium-read-more").length>=1 && data.full_content!= "") {
          //Remove all but imu ads container
          jQuery(".field-name-body .field-items .field-item").children().not('.ads.clearleft').remove();
          jQuery(".field-name-body .field-items .field-item").prepend(data.full_content);
        }
      });
    }

    function getArticleContentBasedToken() {
      //send token to the backend to validate
      var post_data = {};
      var gift_token = getUrlParameter('token');
      post_data.token = gift_token;
      jQuery.post( "/p/login_details?article_id="+_data.articleid,post_data).done( function( data ) {
        data = JSON.parse(data);
        if(isArticlePage && jQuery(".field-name-body .field-items .field-item .premium-read-more").length>=1 && data.full_content!= "") {
          //Remove all but imu ads container
          jQuery(".field-name-body .field-items .field-item").children().not('.ads.clearleft').remove();
          jQuery(".field-name-body .field-items .field-item").prepend(data.full_content);
        }
      });
    }

    function handleGiftToken() {
      console.log("handle gift token");
      window.dataLayer = window.dataLayer || [];

      document.addEventListener('ldapLoginDetailsReceived', function(e) {
        var eventLabel = "";
        if(token_data.usertype == "reg"){
          if(token_data.service_type_value == 13){
                jQuery('#tokenVerifyModal').modal('show');
                eventLabel = "non-verified";
          }else if(token_data.valid_token=="yes" && (token_data.service_type_value == 12 || token_data.service_type_value == 3) && token_data.premium_quota != undefined  && token_data.premium_quota == "valid"){
                jQuery('#tokenModal').modal('show');
                eventLabel = "valid";
          }else if((token_data.service_type_value == 12 || token_data.service_type_value == 3) && token_data.premium_quota != undefined && token_data.premium_quota == "invalid"){
                jQuery('#exceedQuotaModal').modal('show');
                eventLabel = "invalid";
          }else if(token_data.valid_token == "no"){
              jQuery('#expiredModal').modal("show");
              eventLabel = "expired";
          }
        }else if(token_data.usertype == "anoy"){
            setTimeout(function(){ jQuery('#tokenanoyModal').modal("show"); }, 3000);
            eventLabel = "anoy";
        }
         window.dataLayer.push({'event':'custom_event','eventCategory':'fremium','eventAction':'lightbox','eventLabel':eventLabel});
      }, false);

      jQuery('#tokenModal .giftreadbtn').on('click', function( e ) {
        jQuery('#tokenModal').modal("hide");
        getArticleContentBasedToken();
        window.dataLayer.push({'event':'custom_event','eventCategory':'fremium','eventAction':'click','eventLabel':"read now"});
      });

    }
  </script>


  
