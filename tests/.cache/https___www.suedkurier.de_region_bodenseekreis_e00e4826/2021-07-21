<script>
  (function()
  {
    fcmsJs.scripts.load( "extlib_jquery" ).onLoad(function ()
    {
      jQuery.ajax(
      {
        method      : 'GET',
        url         : 'https://www.jobs-im-suedwesten.de/xml/jobs/bodenseekreis-0?page=0&num_results=8',
        dataType    : 'xml',
        crossDomain : true,
        success     : function(success)
        {
          console.log('--- success ---', success);

          var entry = jQuery(success).find('entry').each(function(i,e)
          {
            var logoURL  = jQuery(this).find('cm_logoURL').text();
            var locality = jQuery(this).find('cm_locality').text();
            var title    = jQuery(this).find('title').text();
            var link     = jQuery(this).find('id').text();
            var classCSS = '';

            var container = jQuery('#slider-jisw .slider-box');

            if(logoURL === '')
            {
              logoURL = '/shift17/img/logo/JISW_LOGO_rund.svg';
              classCSS = 'slider-item-fallback';
            }
            
            jQuery(container).append('<a href="' + link + '" title="' + title + '" class="slider-item ' + classCSS + '" target="_blank" rel="noopener"><figure class="vw-100"><img class="lazyload" data-src="' + logoURL + '" alt="' + title + '" data-expand="500" /></figure><header><span class="article-location">' + locality + '</span>' + title + '</header></a>');
          });
          
          jQuery('#slider-jisw .slider-item').last().addClass('slider-item-last').attr('href', 'https://jobs-im-suedwesten.de/').attr('title', 'mehr Jobs');
        },
        error       : function(error)
        {
          console.log('--- error ---', error);
        }
      });
    });
  })();
</script>

