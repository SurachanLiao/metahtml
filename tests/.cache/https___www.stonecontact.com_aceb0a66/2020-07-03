<script>
    showAll();

    function showAll() {
        var show = function (e, e_this, state, time) {
            var ul = $(e_this).children(),
				li = $(e_this).children().children(),
				ul_width = ul.width(),
			 	li_width = li.width(),
				time = time || 200,
				leftCount = (ul_width - li_width) / 5,
				places = new Array(),
				state = state;
            li.eq(state).removeClass('overs').siblings().addClass('overs');
            /**
             *åå§æ¾ç¤ºçä½ç½®
             */
            var init = function (state) {
                for (var i = 0; i < li.length; i++) {
                    if (i == 0) {
                        places[i] = 0;
                    } else if (i <= state) {
                        places[i] = leftCount * i;
                    } else {
                        places[i] = (li_width + (i - 1) * leftCount);
                    }
                    li.eq(i).animate({ left: places[i] }, 0);
                }
            }
            init(state);
            /**
             *é¼ æ æ¬æµ®äºä»¶
             */
            li.hover(function () {
                $(this).removeClass('overs').siblings().addClass('overs');
                places = [];
                li.stop();
                var index = $(this).index();
                switch (index) {
                    case 0: {
                        for (var i = 0; i < li.length; i++) {
                            if (i == 0) {
                                places[i] = 0;
                            } else if (i == 1) {
                                places[i] = li_width;
                            } else {
                                places[i] = (li_width + (i - 1) * leftCount);
                            }
                        }

                        break;
                    }
                    case 1: {
                        for (var i = 0; i < li.length; i++) {
                            if (i == 0) {
                                places[i] = 0;
                            } else if (i == index && index == 1) {
                                places[i] = leftCount * i;
                            } else {
                                places[i] = (li_width + (i - 1) * leftCount);
                            }
                        }

                        break;
                    }
                    case 2: {
                        for (var i = 0; i < li.length; i++) {
                            if (i == 0) {
                                places[i] = 0;
                            } else if (i <= index) {
                                places[i] = leftCount * i;
                            } else {
                                places[i] = (li_width + (i - 1) * leftCount);
                            }
                        }

                        break;
                    }
                    default: {
                        for (var i = 0; i < li.length; i++) {
                            if (i == 0) {
                                places[i] = 0;
                            } else if (i <= index) {
                                places[i] = leftCount * i;
                            } else {
                                places[i] = (li_width + (i - 1) * leftCount);
                            }
                        }

                        break;

                    }
                }
                //éåæ°ç»
                for (var i = 0; i < li.length; i++) {
                    li.eq(i).animate({ left: places[i] }, time);
                }
            }, function () {
                //è¿éé¼ æ ç§»åºäºä»¶
            })

        }
        show(null, ".swicth-box", 2, 800);
    }
</script>
