    $(function () {
        Happen.Scroll(200, function () { Show_TabMaterialsMenu(0); });
        var num = RandomNum(1, 5);//éæºè½®æ¢
        Show_TabProductMenu(num);
        var data = $("#MainContainer_Control_indexmap").val();
        if (data != "") {
            data = eval("(" + data + ")");
            for (var i = 0; i < data.length; i++) {
                for (var j = 0; j < data[i].Result.length; j++) {
                    var id = "container-" + data[i].Id;
                    var keyword = data[i].Result[j].Keyword;
                    if (data[i].Id == 0) {
                        indexmap(id, keyword, data[i].Result[j].Rows[0].X.split(","), data[i].Result[j].Rows[0].Y, "#2f7ed8");
                    } else {
                        indexmap(id, keyword, data[i].Result[j].Rows[0].X.split(","), data[i].Result[j].Rows[0].Y, "#8bbc21");
                    }
                }
            }
        }
     
        $("#HomeMProductBox").jCarouselLite({
            btnNext: ".HomeShowImgRight",
            btnPrev: ".HomeShowImgLeft",
            visible: 4,
            scroll: 4,
            auto: 8000,
            speed: 1000,
            beforeStart: function () {
                rightMove();
            }
        });

    });
    var tabTimer = null;
    function StopTab(n) {

        Show_TabProductMenu(n);
        clearInterval(tabTimer);
    }
    function AutoSwitchTab(n) {
        clearInterval(tabTimer);
        var tabnum = (n >= 5) ? 1 : n + 1;
        tabTimer = setInterval("Show_TabProductMenu(" + tabnum + ")", 15000);
    }
    function Show_TabProductMenu(tabadnum) {
        var tab = "tabProduct_";
        var sub = "divProduct_";
        for (var i = 1; i <= 5; i++) {
            if (i == tabadnum) {
                $("#" + sub + tabadnum).show();
                $("#" + tab + tabadnum).addClass("SelectBlockMenu");
                //$("#spn_" + tabadnum).addClass("ArrowDown");
            } else {
                $("#" + sub + i).hide();
                $("#" + tab + i).removeClass("SelectBlockMenu");
                //$("#spn_" + i).removeClass("ArrowDown");
            }
        }
        AutoSwitchTab(tabadnum);
    }
    function RandomNum(Min, Max) {
        var Range = Max - Min;
        var Rand = Math.random();
        return (Min + Math.round(Rand * Range));
    }

    function leftMove() {
        var curCir = $(".HomeImgtabSelect");
        var curPrev = curCir.prev();
        curCir.removeClass("HomeImgtabSelect").addClass("HomeImgtab");
        if (curPrev.length > 0) {
            curPrev.addClass("HomeImgtabSelect");
        } else {
            $(".HomeImgtab").last().addClass("HomeImgtabSelect");
        }
    }

    function rightMove() {
        var curCir = $(".HomeImgtabSelect");
        var curNext = curCir.next();
        curCir.removeClass("HomeImgtabSelect").addClass("HomeImgtab");
        if (curNext.length > 0) {
            curNext.addClass("HomeImgtabSelect");
        } else {
            $(".HomeImgtab").first().addClass("HomeImgtabSelect");
        }
    }

    

</script>
