<script type="text/javascript">	  
//<![CDATA[
$jsmart(document).ready(function($) {
	$("#so_slider_199").SoSlider({
            auto		: 	0,
            type		:   'theme3',
            speed		: 	1000,
            visible		: 	7,
            start		:	14,
                        circular    :   false,
            			scroll		:   7,
			btnPrev		: 	'#so_theme3_pre_199',
			btnNext		: 	'#so_theme3_next_199',
			btnPause	: 	'#so_spot_pause_199',
			navigation	:   '#so_navigation_theme3_199'
		});
        var showtitle = 1;
        var items = $(".so_title_199");
		var cssPaneContent = $(".so_content_199");
        if(typeof(items.length)!= "undefined"){
		for(i = 0; i<items.length; i++ ) {
            if(showtitle == 0){
                var paneHeightCap = height(items[i]) - 8;
            }else{
                var paneHeightCap = height(items[i]) + 6;
            }       
			$(cssPaneContent[i]).css("top", 110 - paneHeightCap-4);	
		}
        }
        $('.so_item_199').hover(function(){
            var capDesc = $(this).find(".so_content_199");
			var paneHeightDesc = $(capDesc).height() + 5;
           
			$(".so_content_199", this).stop().animate({top:110 - paneHeightDesc},{queue:false,duration:160});
		}, function() {
            var capTitleOut = $(this).find(".so_title_199");
            var paneHeightTitleOut = $(capTitleOut).height()+ 10;
            if(showtitle == 0){
                paneHeightTitleOut = 0;
            }
            $(".so_content_199", this).stop().animate({top:110 - paneHeightTitleOut},{queue:false,duration:160});
		});
})	
//]]>
</script>































