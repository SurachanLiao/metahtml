    <script type="text/javascript">
    /*
     * SEARCH WIDGET
     */

    Event.observe(document, "dom:loaded", function () {
        var dropdownLink = $$('.search-dropdown-link')[0];
        var triggerElement = $$('.search-dropdown-link')[0];
        var searchForm = $$('.search-form')[0];



        var pannelId = "search-dropdown"
        var link_w = dropdownLink.getWidth();
        var link_h = dropdownLink.getHeight();

        testFloat = new FloatingPanel(
            dropdownLink,
            pannelId,
            {
                rightOffset:'-' + (triggerElement.cumulativeOffset()[0] + link_w + 40 ),
                topOffset: '+' + (link_h + 0),
                effects: 'off',
                closeDelay:'0',
                triggerEvent: 'click',
                allowDefault: true,
                beforeOpen: function() {
                    var searchForm = jQuery('.search-form');
                    var searchInput = jQuery('#search-input');
                    searchInput.on('click',function(){
                      return false;
                    });
                    searchForm.show();
                    searchInput.focus();

                    jQuery('.search-link-text').hide();
                    jQuery('.search-wrap .arrow-down').hide();
                    jQuery('.search-wrap .arrow-up').show();
                    jQuery('.search-dropdown').width(jQuery('.search-link').width());

                    closepanefunc = function(event){
                        if(jQuery(event.target).parents('.search-wrap').length == 0){
                            console.log(event.target)
                            console.log(jQuery(event.target).parents('.search-wrap').length);
                            testFloat.closePanel();
                        }
                    }
                    jQuery(document).on('click', closepanefunc );

                    //jQuery('.arrow-down').removeClass('arrow-down').addClass('arrow-up');

                    dropdownLink.addClassName('hover');
                    Element.up(dropdownLink).addClassName('hover');
                    jQuery('.search-option a').on('click', function(e){
                        e.preventDefault();
                        testtarget = e.target;
                        console.log(e.target);
                        if(jQuery(e.target).hasClass('search-all')){
                            console.log(jQuery('.prepTextForm'));
                            jQuery('.search-form').submit();
                        } else {

                            var newLocation = jQuery(e.target).attr('href') || "";
                            window.location = newLocation + "?keywords=" + jQuery('#search-input').val() +  "&submit=Search";
                        }
                    });

                },
                beforeClose: function() {
                    var searchForm = jQuery('.search-form');
                    searchForm.hide();
                    jQuery('.search-icon').show();
                    jQuery('.search-link-text').show();
                    jQuery('.search-wrap .arrow-up').hide();
                    jQuery('.search-wrap .arrow-down').show();

                    //jQuery('.arrow-up').removeClass('arrow-up').addClass('arrow-down');

                    jQuery(document).off('click',closepanefunc);
                    dropdownLink.removeClassName('hover');
                    Element.up(dropdownLink).removeClassName('hover');
                }
            });
    });



    </script>
