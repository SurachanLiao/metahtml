  background: linear-gradient(to bottom, #d7d7d7 0%,#ffffff 100%); /* W3C */
 -webkit-box-shadow: inset 3px 0px 4px 0px #d7d7d7;
          box-shadow: inset 3px 0px 4px 0px #d7d7d7;
}

#SiteSearchBox #search-dropdown{
top: 40px !important;
left:auto !important;
position:absolute;
  display:block;
  background: #fff;
 z-index:5700003;
border: 1px solid #CCC;
border-bottom: 6px solid #c00;
box-shadow: 3px 3px 3px #666;
}

#SiteSearchBox .search-icon {
  display:inline-block;
  position:absolute;
  right:-28px;
  top: -1px;
  width: 28px;
  height: 26px;
  margin-bottom:4px;
  background: url('/images/mobile/search-white.png');
  background-color:#c00;
  background-repeat: no-repeat;
  background-position: center;
  background-size: 12px;
}

#SiteSearchBox #search-input {
    vertical-align: top;
    line-height: 1.3em;
    padding: 3px;
    background: transparent;
    width:140px;
    outline:none;
    border:none;
}
#SiteSearchBox .search-dropdown li {
  padding: 2px 5px 2px 20px;
  margin: 5px 0;
  border-left: 10px solid white;
  color: #666;
  font: bold 13px/16px 'Helvetica Neue', Arial, Verdana, sans-serif;
  cursor: pointer;

}
#SiteSearchBox .search-dropdown li a {
  color: #666;
}
#SiteSearchBox .search-dropdown li:hover {
  border-left: 10px solid #c00;
  color: #c00;
  text-decoration:none;
}
#SiteSearchBox .search-dropdown li:hover a {
  text-decoration:none;
}
</style>



  

