        --dre-image-height-ratio: var(--ratio-square) !important; }
    .measure .dre-item--sm-full {
      --dre-image-height-ratio: var(--ratio-landscape); }
    .measure .dre-item__title {
      --dre-min-font-size: 36.816;
      --dre-max-font-size: 85.904; }
    .measure .dre-item .dre-item__title {
      font-size: calc(36vw / 3.75); } }

