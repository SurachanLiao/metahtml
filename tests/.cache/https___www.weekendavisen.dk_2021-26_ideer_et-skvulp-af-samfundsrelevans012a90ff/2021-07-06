.measure {
  --ratio-landscape: 0.713235294;
  --ratio-portrait: 1.4;
  --ratio-portrait-mobile: 1.574850299;
  --ratio-square: 1; }
  .measure .dre-item {
    --dre-image-height-ratio: var(--ratio-landscape);
    --dre-image-mode: fixed-ratio; }
  .measure .dre-item--feature-imageFormat-landscape {
    --dre-image-height-ratio: var(--ratio-landscape) !important; }
  .measure .dre-item--feature-imageFormat-portrait {
    --dre-image-height-ratio: var(--ratio-portrait) !important; }
  .measure .dre-item--feature-imageFormat-square {
    --dre-image-height-ratio: var(--ratio-square) !important; }
  @media (min-width: 1024px) {
    .measure {
      --dre-font-size-unit: px; }
      .measure .dre-item--size-two-thirds-image-left.dre-item--image-left, .measure .dre-item--size-two-thirds-image-left.dre-item--image-right,
