      .measure .dre-item--size-full.dre-item--image-right,
      .measure .dre-item--size-full-image-left {
        --dre-image-height-ratio: 0.56122449; }
      .measure .dre-item--size-half.dre-item--image-left, .measure .dre-item--size-half.dre-item--image-right {
        --dre-image-height-ratio: var(--ratio-portrait); }
      .measure .dre-item--size-quarter {
        --dre-image-height-ratio: var(--ratio-square); } }
