      mobile_detect.is_phone = true;
    } else if (is_mobile_device && !mobile_detect.is_phone && (screen.width <= 768 || screen.height <= 768) ) {
      mobile_detect.is_tablet = true;
    } else {
      mobile_detect.is_desktop = true;
