setClientAd($this,false);}else{setClientAd($this);}}
function setClientAd($this,flag){flag=(flag===undefined)?true:flag;if(flag){var div_id=$($this).attr('id');var new_id=div_id.replace("_mobile_div","");$($this).attr('id',new_id);$($this).addClass('ad_displayed');}else{$($this).hide();}}
function adjustAdSlots(){if($('#block-client-ads-dfp-client-ad-dfp-mobile-lb').length){$('#block-client-ads-dfp-client-ad-dfp-mobile-lb').addClass('mobile-lb-fixed');}
if(deviceType==='mobile'&&ad_targeting.category==='home'){var slotnum=1;$('.patch-home-page #block-system-main').children('.card,.slot-row,.row:not(.slotted-ad-unit),.post-slot-container, .carousel-slot-default').each(function(){$(this).attr('slotnum',slotnum);if(slotnum==3&&$('#block-client-ads-dfp-client-ad-dfp-mobile-slot-1')!=='undefined'){var adblockcontent=$('#block-client-ads-dfp-client-ad-dfp-mobile-slot-1').html();$('#block-client-ads-dfp-client-ad-dfp-mobile-slot-1').html('');$(this).after(adblockcontent);}
