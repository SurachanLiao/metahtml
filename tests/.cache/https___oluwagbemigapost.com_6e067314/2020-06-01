						   <section class="widget gtl_mag_widget_content"><h2 class="widget-title">POLITICS</h2>                                        <!--html generate-->
			<div class="mp-feature-widget">
			    <div class="row">
			    		            <div class="col-xs-12 col-sm-6 col-md-6 left-post-content">
		                <div class="thumb-post">
		                    <figure class="post-img">
		                        <img width="558" height="318" src="https://oluwagbemigapost.com/wp-content/uploads/2020/05/coronavirus-in-kogi-is-real-i-know-a-patient-dino-melaye-558x318.jpg" class="attachment-greenturtle-mag-feature2-thumb size-greenturtle-mag-feature2-thumb wp-post-image" alt="" srcset="https://oluwagbemigapost.com/wp-content/uploads/2020/05/coronavirus-in-kogi-is-real-i-know-a-patient-dino-melaye-558x318.jpg 558w, https://oluwagbemigapost.com/wp-content/uploads/2020/05/coronavirus-in-kogi-is-real-i-know-a-patient-dino-melaye-731x417.jpg 731w" sizes="(max-width: 558px) 100vw, 558px" />		                    </figure>                                      
		                    <div class="bottom-post-content">
		                        <div class="bottom-post-content-text">
