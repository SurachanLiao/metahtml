        ||tc_vars.level3==='change-de-bebe' // 31/12/2020
    );

    var trafficing_exclu_full = (
        tc_vars.level3==='charte-donnees-personnelles-cookies'
        ||tc_vars.level3==='charte-donnees-personnelles-et-cookies-forum'
        ||tc_vars.level6==='intima-gyn-expert-soin-lavant-apaisant-200ml' // 05/08/2020
        ||tc_vars.level4==='dents-sensibles' // 01/09/2020
        ||tc_vars.level4==='cuir-chevelu-sensible' // 31/07/2020
        ||tc_vars.level4==='pellicules' // 31/07/2020
    );

    // Remove dyninfeed for pages in exclu pub
    $(document).ready(function () {
        if (trafficing_exclu_all || trafficing_exclu_full) {
            $('.doc-block-pub-dyn-infeed').remove();
        }
    });
</script>


