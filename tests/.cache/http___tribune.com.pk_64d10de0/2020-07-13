<script>
      let event = window.Echo.channel('live-blog-changed').listen('LiveBlogChanged', function (e) {
        console.log('live blog called');
        console.log(e);
        let base_path = 'https://tribune.com.pk';
        $('.liveblog-lists').empty();
        if (e.actionData.length > 0) {
          $.each(e.actionData, function (index, item) {
            let url = item.url;
            $(".liveblog-lists").append('<li><p>' + item.time + '</p><a href="' + url + '" target="_blank"><h6>' + item.title + '</h6></a></li>');
          });
        }
      });
    </script>
