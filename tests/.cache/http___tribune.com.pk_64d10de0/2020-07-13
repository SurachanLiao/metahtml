<div class="sidebar-newsletter widget-spacing">
<div class="newsletter-tribune-logo">
<div class="top10"></div>
</div>
<p>10 stories that matter, delivered to your inbox</p>
<form id="newsletter_form-1630894518" method="POST">
<input type="hidden" name="_token" value="9UcVMLlUIEZ53DqGMKUqvLXURnfm1LqIK43yVabK"> <input type="email" placeholder="Your Email Address" id="subscriber_email-1630894518" required>
<input type="submit" value="Subscribe">
</form>
<div id="success-newsletter-1630894518" style="display: none;">
<strong>Thank you for joining our mailing list.</strong>
</div>
<div style="display: none;" id="failed-newsletter-1630894518">
<strong>You are already in our mailing list!</strong>
</div>
<div style="display: none;" id="invalid-newsletter-1630894518">
<strong>Sorry! The email format is invalid.</strong>
</div>
</div>
<script>
    $(document).ready(function () {
        let id = '1630894518';
        let form = "#newsletter_form-"+id;
        $(form).submit(function (e) {
          e.preventDefault();
          var email = $('#subscriber_email-'+id).val();
          var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
          if(!email.match(mailformat))
          {
            $('#invalid-newsletter-'+id).show();
            $('#invalid-newsletter-'+id).delay(1500).fadeOut();
            return false;
          }

            var token = $('input[name="_token"]').attr('value');
            $.ajaxSetup({
                headers: {
                    'X-CSRF-TOKEN': token
                }
            });
            $.ajax({
                type: "POST",
                url: "https://tribune.com.pk/post/newsletter",
                data: {
                    email: $('#subscriber_email-'+id).val(),
                },
                success: function (result) {
                    $(form).trigger("reset");
                    if (result.data === 'exist')
                    {
                        $('#failed-newsletter-'+id).show();
                        $('#failed-newsletter-'+id).delay(1500).fadeOut();
                    }
                    else
                    {
                        $('#success-newsletter-'+id).show();
                        $('#success-newsletter-'+id).delay(1500).fadeOut();
                    }
                },
                error: function (result) {
                    alert('error');
                }
            });
        });
    });
</script>
