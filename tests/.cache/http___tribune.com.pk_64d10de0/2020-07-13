<script>
            $(document).ready(function () {
                let id = '614522929';
                let poll_id  = '14';
                let form = "#poll-widget-form-"+id;
                $(form).submit(function (e) {
                    e.preventDefault();
                    var token = $('input[name="_token"]').attr('value');
                    $.ajaxSetup({
                        headers: {
                            'X-CSRF-TOKEN': token
                        }
                    });
                    $.ajax({
                        type: "POST",
                        url: "https://tribune.com.pk/post/pollwidget",
                        data: $(form).serialize(),
                        success: function (result) {
                            if (result.data == 'failed') {
                                $(form).trigger("reset");
                                $('#failed-poll-614522929' ).show();
                                $('#failed-poll-614522929').delay(1500).fadeOut();
                            } else {
                                $('.viewresult'+poll_id).trigger('click');
                                $(form).trigger("reset");
                                $('#success-poll-614522929' ).show();
                                $('#success-poll-614522929' ).delay(1500).fadeOut();
                            }
                        },
                        error: function (result) {
                            alert('error');
                        }
                    });
                });
                $('.viewresult'+poll_id).click(function (e) {
                    e.preventDefault();
                  var token = $('input[name="_token"]').attr('value');
                  $.ajaxSetup({
                    headers: {
                      'X-CSRF-TOKEN': token
                    }
                  });
                    $.ajax({
                        type: "POST",
                        url: "https://tribune.com.pk/post/pollresult",
                        data: {poll_id: '14'},
                        success: function (result) {
                            //console.log(result);

                            //$(form).trigger("reset");
                            //$('#failed-poll-'+id).show();
                            //$('#failed-poll-'+id).delay(1500).fadeOut();
                            $('.poll_14').html(result);

                        },
                        error: function (result) {
                            alert('error');
                        }
                    });

                });
            });
        </script>
