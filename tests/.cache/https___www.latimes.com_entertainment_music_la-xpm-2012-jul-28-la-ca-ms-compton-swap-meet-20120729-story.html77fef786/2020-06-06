    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=5"><title>Wan Joon Kim of Cycadelic Records helped gangsta rappers start - Los Angeles Times</title><meta name="description" content="Wan Joon Kim of Cycadelic Records helped gangsta rappers start"><link rel="canonical" href="https://www.latimes.com/entertainment/music/la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729-story.html"><meta name="brightspot.contentId" content="00000169-9890-d37a-a1ff-9cdb32b70000"><link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png"><link rel="icon" type="image/png" href="/favicon-32x32.png"><link rel="icon" type="image/png" href="/favicon-16x16.png">
    
    <script id="head-dl">var dataLayer = {"rootId":"","pageTitle":"Wan Joon Kim of Cycadelic Records helped gangsta rappers start - Los Angeles Times","publishDate":"07-28-2012 12:00","source":"latimes_archive","sectionParentPath":"entertainment-arts","pageName":"lat:entertainment-arts:music:la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729:story.","platform":"22","sectionName":"music","cmsSlug":"la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729","ownerSite":"latimes","pageTypeCode":"story","headline":"Wan Joon Kim of Cycadelic Records helped gangsta rappers start","urlSlug":"la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729-story.html","wordCount":1371,"bspContentUuid":"00000169-9890-d37a-a1ff-9cdb32b70000","bspContentType":"story","adUnit":"GoogleDfp:/21787098806/web.latimes/entertainment-arts/music","isAccessibleForFree":false,"url":"https://www.latimes.com/entertainment/music/la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729-story.html","sectionFullPath":"entertainment-arts:music","sitePath":"/","site":"latimes","domain":"www.latimes.com","permalink":"https://www.latimes.com/entertainment/music/la-xpm-2012-jul-28-la-ca-ms-compton-swap-meet-20120729-story.html","authors":" Sam Quinones, Los Angeles Times"};</script>
