                <div class="boundSearch">
                    <input id="txtSearchAdv" onkeypress="return clickButton(event,'btnSearchAdv')" type="text"
                        onfocus="if (this.value=='Rechercher') { this.value=''; }" class="txtSearch"
                        name="txtSearchAdv" value="Rechercher" onblur="if (this.value=='') { this.value='Rechercher'; }" />
                    <img alt="Search" onclick="search();" id="btnSearchAdv"
                        class="btnSearch" src="https://www.lecourrier.vn//images/Icon/btnSearch.png" />
