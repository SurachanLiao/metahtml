    var arrTabsTop = new Array("tabtop1", "tabtop2");
    var currentTabTop = "tabtop1";
    function showTabMRightTop(tab) {
        // set current tab
        currentTabTop = tab;
        // disable all tab
        for (var i = 0; i < arrTabs.length; i++) {
            document.getElementById(arrTabsTop[i]).className = "tbaoTop";
            document.getElementById(arrTabsTop[i] + "Content").style.display = "none";
        }
        // show current tab
        document.getElementById(currentTabTop).className = "tbaoTop-active";
        document.getElementById(currentTabTop + "Content").style.display = "block";
    }

    var arrTabSubs = new Array("tabsub1", "tabsub2");
    var currentTabSub = "tabsub1";
    function showTabSub(tab) {
        // set current tab
        currentTabSub = tab;
        // disable all tab
        for (var i = 0; i < arrTabSubs.length; i++) {
            document.getElementById(arrTabSubs[i]).className = "subtab-tbao";
            document.getElementById(arrTabSubs[i] + "Content").style.display = "none";
        }
        // show current tab
        document.getElementById(currentTabSub).className = "subtab-tbao-active";
        document.getElementById(currentTabSub + "Content").style.display = "block";
    }
              
</script>

    
