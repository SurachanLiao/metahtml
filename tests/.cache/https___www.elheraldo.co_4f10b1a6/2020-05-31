<script>
//Click select jugador
$("#select-columistas select").change(function(a, e){
  res=$(this).find("option:selected").attr("data-href");
  window.location=res;
});
// Select de equipo
  $(".view-id-columnistas.view-display-id-block_2 .views-row").each(function(){
    link=$(this).find("a").attr("href");
    columistas=$(this).find("a").text();
    columistas=columistas.trim();
    html="<option data-href='"+link+"'>"+columistas+"</option>";
    $("#select-columistas select").append(html);
  });
</script>  </div>

  
