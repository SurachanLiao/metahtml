'has_unread_conversations',
function(evnt) {
placementEl.find('.global-nav-hamburger .ui_jewel.unread').removeClass('hidden');
placementEl.find('.nav-sub-link.inbox .icon-and-jewel').removeClass('hidden');
}
);
