                            <script>
  $(function(){
	let editorsPicksHeadersList = document.querySelectorAll("#content .section-top-container .in-depth .inner-panel .in-depth-articles p.news-heading-h4")
    , replaceString = 'Latest';
	editorsPicksHeadersList.forEach(child => {
	  if(child.textContent.includes(replaceString)){
		child.innerHTML = child.innerHTML.replace('Latest', '<span style="color: #e71b28;">Latest</span>');
      	console.log("latest changed to red");
	  }
	});
});
</script>
