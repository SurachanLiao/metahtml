document.querySelector('body').innerHTML = str;
}
})();
(function() {
var str = '<div style="height:1000px;position:fixed;width:100%;z-index:50000;background-color:#fff; left:0;"> <div style="margin:22px auto;width:90%;max-width:725px;border:1px solid #ccc;padding: 20px;text-align:center;"><h2 style="font-size:35px;line-height:35px; padding-bottom: 10px;">Warum sehe ich BILD.de nicht?</h2> <span style="font-size:24px;line-height:34px;color:#454545;">Ein Plugin in Ihrem Browser beschädigt die Funktion der Webseite, deshalb können wir Ihnen keinen Zugriff auf die Inhalte gewähren.<br /><br />Möglicherweise nutzen Sie Ublock Origin oder ein anderes Adblocker Plugin. Falls dies der Fall ist, versuchen Sie bitte es zu deaktivieren.</span> </div> </div>';
var func = function () { document.querySelector('body').innerHTML = str; }
try {
if(de && de.bild && !de.bild.cmsKonfig) {
func();
} else if (!de.bild.cmsKonfig.a) {
func();
} else if (!de.bild.cmsKonfig.a.b) {
func();
} else if (de.bild.cmsKonfig.a.a === undefined ) {
func();
}
} catch (err) {
console.log('catch');
func();
}
})();
</script>
