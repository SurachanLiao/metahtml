    function verif(evt) {
        var keyCode = evt.which ? evt.which : evt.keyCode;
        var accept = 'áàâäãåçéèêëíìîïñóòôöõúùûüýÿÁÀÂÄÃÅÇÉÈÊËÍÏÎÌÑÓÒÔÖÕÚÙÛÜÝabcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ';
      if(keyCode == 13) {
            this.form.submit();
      }
		 if(keyCode == 8) {
            this.form.submit();
      }
		if (accept.indexOf(String.fromCharCode(keyCode)) >= 0) {
            return true;
        } else {
            return false;
        }
    }
</script>   
