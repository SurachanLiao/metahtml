;}.css-1lpaytm:global(.fullWidth):global(.fullWidth){margin:0 auto 16px;}@media screen and (min-width: 590px){.css-1lpaytm{display:block;}}.css-1lpaytm.banner-980x300{min-height:0!important;width:auto;margin-left:-16px;margin-right:-16px;}@media (min-width: 590px){.css-1lpaytm.banner-980x300{margin-left:calc(-1 * (-3.7914691943127963vw + 38.3696682464455px));}}@media (min-width: 1012px){.css-1lpaytm.banner-980x300{margin-left:0px;}}@media (min-width: 590px){.css-1lpaytm.banner-980x300{margin-right:calc(-1 * (-3.7914691943127963vw + 38.3696682464455px));}}@media (min-width: 1012px){.css-1lpaytm.banner-980x300{margin-right:0px;}}@media screen and (min-width: 590px){.css-1lpaytm{margin:0px 0 16px;}}.css-1lpaytm.banner-980x240,.css-1lpaytm.banner-980x400,.css-1lpaytm.banner-980x500,.css-1lpaytm.banner-980x600{width:auto;max-width:980px;}@media screen and (max-width: 1012px){.css-1lpaytm.banner-980x240,.css-1lpaytm.banner-980x400,.css-1lpaytm.banner-980x500,.css-1lpaytm.banner-980x600{max-width:100%;}}.css-1lpaytm.banner-980x240::before,.css-1lpaytm.banner-980x400::before,.css-1lpaytm.banner-980x500::before,.css-1lpaytm.banner-980x600::before{padding-bottom:8px;}.css-1lpaytm.banner-320x320{width:auto;max-width:320px;}.css-1lpaytm.banner-320x320::before{padding-bottom:8px;}@media screen and (min-width: 590px){.css-1lpaytm{width:100%;}}.css-1lpaytm::before{background-color:transparent;}.css-1lpaytm::before{display:block;content:'Annons';color:#707070;font-size:10px;text-transform:uppercase;font-family:Arial,Helvetica,Verdana,Geneva,sans-serif;font-weight:bold;letter-spacing:0.4px;line-height:25px;}.css-1lpaytm>div[id*='utif']{overflow:hidden;}.css-1lpaytm>div[id*='utif']{height:auto!important;width:auto!important;position:relative;}</style><div class="disable-cogwheel css-1lpaytm" data-variants="[{&quot;invCode&quot;:&quot;se-aftonbladet-{deviceType}-article-outstream&quot;,&quot;probableWidth&quot;:320,&quot;probableHeight&quot;:185,&quot;deviceType&quot;:&quot;mobile&quo