      <script>
      jQuery(document).ready(function($) {
        var contentListingCarousel = $('#widget-carousel-news-838');

        contentListingCarousel.on('translated.owl.carousel', function(e) {
          var dots = 5;
          var dotTotal = e.item.count;

          if (dotTotal > dots) {
            var dotThis = e.item.index - e.relatedTarget._clones.length / 2;
            var dotWidth = contentListingCarousel.find('.owl-dot.active').outerWidth();

            contentListingCarousel.find('.owl-dots').animate({ scrollLeft: (dotThis - parseInt(dots / 2)) * dotWidth }, 200);
          }
        });

        $(contentListingCarousel).find('.item').first().addClass('active');

        contentListingCarousel.owlCarousel({
          responsive: {
            0: {
              nav: true,
              dots: false,
              margin: 30,
            },
            768: {
              nav: true,
              dots: true,
                              items: 1,
                margin: 30,
                          }
          },
          autoplay: true,
          autoplayTimeout: 5000,
          autoplayHoverPause: true,
          items: 1,
          loop: false,
          rewind: true,
          URLhashListener: false,
          navText: ['<i class="fa fa-angle-left"></i>', '<i class="fa fa-angle-right"></i>'],
        });
      });
      </script>
