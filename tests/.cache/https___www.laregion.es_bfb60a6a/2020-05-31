        <script>
          $(document).ready(function() {
            var mainSlider = $('#owl-carousel-891680-175');
            var thumbsSlider = $('#thumbCarousel-891680-175');

            mainSlider.on('translated.owl.carousel', function(e) {
              var dots = 5;
              var dotTotal = e.item.count;

              if (dotTotal > dots) {
                var dotThis = e.item.index - e.relatedTarget._clones.length / 2;
                var dotWidth = mainSlider.find('.owl-dot.active').outerWidth();

                mainSlider.find('.owl-dots').animate({ scrollLeft: (dotThis - parseInt(dots / 2)) * dotWidth }, 200);
              }
            });

            $(window).trigger('lookup');
            $(mainSlider).owlCarousel({
              dots: true,
              dotsEach: 5,
              nav: true,
              items: 1,
              margin: 10,
              loop: false,
              rewind: true,
              lazyLoad: false,
              responsiveRefreshRate: 200,
              center: true,
              navText: [ '<i class="fa fa-angle-left"></i>', '<i class="fa fa-angle-right"></i>' ],
            });
            $(thumbsSlider).owlCarousel({
              dots: true,
              items: 6,
              nav: true,
              navText: [ '<i class="fa fa-angle-left"></i>', '<i class="fa fa-angle-right"></i>' ],
              margin: 1,
              loop: false,
              lazyLoad: false,
              responsiveRefreshRate: 200,
              center: false,
            });

            $(thumbsSlider).on('click', '.owl-item', function(e) {
              e.preventDefault();
              var number = $(this).find('.item').data('id');

              mainSlider.trigger('to.owl.carousel', [ number, 200, true ]);
            });
            mainSlider.on('changed.owl.carousel', function(event) {
              var current = event.item.index + 1;
              var number = mainSlider.find('.owl-item:nth-child(' + current + ') .item').data('id');

              thumbsSlider.trigger('to.owl.carousel', [ number, 200, true ]);
              $(window).trigger('lookup');
            });
          });
        </script>
      </div>
    </div>
  </div>
</div>
              </div>
            </div>
            <div class="row equal-height">
              <div class="col-12 col-md-6">
                
                
                
              </div>
              <div class="col-12 col-md-6">
                
                
                
              </div>
