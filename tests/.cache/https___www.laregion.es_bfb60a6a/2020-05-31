<script>
jQuery(document).ready(function($) {
  var videoCarousel = $('#video-carousel-901069');
  var smartItemsCounter = Math.floor( ($(videoCarousel).outerWidth()/400), 1);

  var config = {
    responsive: {
      0: { items: 1, stagePadding: 15 },
      768: { items: 2, stagePadding: 15 },
      992: { items: 3, stagePadding: 15 },
      1200: { items: $(videoCarousel).closest('.wrapper-wide').length ? smartItemsCounter : 3, stagePadding: $(videoCarousel).closest('.wrapper-wide').length ? 100 : 0 }
    },
    autoPlay: true,
    stopOnHover: true,
    nav: true,
    dots: true,
    paginationSpeed: 500,
    goToFirstSpeed: 2000,
    loop: false,
    rewind: true,
    margin: 30,
    stagePadding: $(videoCarousel).closest('.wrapper-wide').length ? 100 : 0,
    navText: [ '<i class="mi material-icons">chevron_left</i>', '<i class="mi material-icons">chevron_right</i>' ]
  };

  videoCarousel.owlCarousel(config);
});
</script>
