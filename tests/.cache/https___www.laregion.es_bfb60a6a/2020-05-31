<div id="pred-meteogal">
<p id="pred-meteogal-h">Ourense - <span>Predicción para hoy</span></p>

<div id="pred-meteogal-prin">
<div id="pred-meteogal-ceo">&nbsp;</div>

<div id="pred-meteogal-temps">&nbsp;</div>
</div>

<div id="pred-meteogal-fjHs">
<ul>
</ul>
</div>

<div id="pred-meteogal-prox"><a class="btn btn-onm btn-dark btn-block bg-site-color border-site-color text-white" href="/seccion/tiempo">Próximos días <i class="mi material-icons ml-1">chevron_right</i></a></div>
</div>
<script>

  (function() {
    var now = new Date();
    var day = ('0' + now.getDate()).slice(-2);
    var month = ('0' + (now.getMonth() + 1)).slice(-2);
    var hour = ('0' + now.getHours()).slice(-2);
    var customTimestamp = now.getFullYear() + (month)  + (day) + (hour);

    var fjHsTranslate = [];
    fjHsTranslate['manha'] = 'Mañana';
    fjHsTranslate['tarde'] = 'Tarde';
    fjHsTranslate['noite'] = 'Noche';

    if (hour < 14){
      var fjH1 = 'manha';
      var fjH2 = 'tarde';
      var fjH3 = 'noite';
    }
    else if ((hour > 14) && (hour < 19)){
      var fjH1 = 'tarde';
      var fjH2 = 'noite';
    }
    else {
      var fjH1 = 'noite';
    }

    var prediccionMeteogalicia = '//files.laregion.net/meteogalicia/data/pred_ourense_cp.json?r='+customTimestamp;
    $.getJSON( prediccionMeteogalicia, {
      format: 'json'
    })
      .done(function( data ) {

        $('#pred-meteogal #pred-meteogal-prin #pred-meteogal-ceo').append('<img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH1]+'.svg" />');
        $('#pred-meteogal #pred-meteogal-temps').append('<img src="//files.laregion.net/meteogalicia/icons/thermometer_bicolor.svg" /><span> &nbsp;'+data.predConcello.listaPredDiaConcello[0].tMin+'º | '+data.predConcello.listaPredDiaConcello[0].tMax+'º</span>');
        if (!(typeof fjH3 === 'undefined' || fjH3 === null)) {
          $('#pred-meteogal #pred-meteogal-fjHs ul').append('<li><div><span>'+fjHsTranslate[fjH2]+'</span></div><div><img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH2]+'.svg" /></div></li>');
          $('#pred-meteogal #pred-meteogal-fjHs ul').append('<li><div><span>'+fjHsTranslate[fjH3]+'</span></div><div><img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH3]+'.svg" /></div></li>');
        }
        else if (!(typeof fjH2 === 'undefined' || fjH2 === null)) {
          $('#pred-meteogal #pred-meteogal-fjHs ul').append('<li><div><span>'+fjHsTranslate[fjH1]+'</span></div><div><img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH1]+'.svg" /></div></li>');
          $('#pred-meteogal #pred-meteogal-fjHs ul').append('<li><div><span>'+fjHsTranslate[fjH2]+'</span></div><div><img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH2]+'.svg" /></div></li>');
        }
        else {
          $('#pred-meteogal #pred-meteogal-fjHs ul').append('<li><div><span>'+fjHsTranslate[fjH1]+'</span></div><div><img src="//servizos.meteogalicia.gal/widget/html/images/meteoros/32x32/'+data.predConcello.listaPredDiaConcello[0].ceo[fjH1]+'.svg" /></div></li>');
        }
      });
  })();
</script></div>
                
                
