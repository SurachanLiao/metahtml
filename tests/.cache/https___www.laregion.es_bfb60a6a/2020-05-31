      </div>
    </div>
  </div>
  <script>
    jQuery(document).ready(function($) {
      var opinionOnmCarousel = $('#opinionOnmCarousel-owl-carousel-7454');

      opinionOnmCarousel.on('translated.owl.carousel', function(e) {
        var dots = 5;
        var dotTotal = e.item.count;

        if (dotTotal > dots) {
          var dotThis = e.item.index - e.relatedTarget._clones.length / 2;
          var dotWidth = opinionOnmCarousel.find('.owl-dot.active').outerWidth();

          opinionOnmCarousel.find('.owl-dots').animate({ scrollLeft: (dotThis - parseInt(dots / 2)) * dotWidth }, 200);
        }
      });

      opinionOnmCarousel.owlCarousel({
        autoplay: false,
        autoplayTimeout: 5000,
        autoplayHoverPause: true,
        loop: false,
        rewind: true,
        margin: 15,
        stagePadding: 0,
        dots: true,
        lazyLoad: true,
        nav: true,
        navText: [ '<i class="mi material-icons">chevron_left</i>', '<i class="mi material-icons">chevron_right</i>' ],
        responsive: {
          0: {
            items: 1,
            nav: true,
            stagePadding: 30,
          },
          576: {
            items: 2,
            stagePadding: 30
          },
          768: { items: 3, mouseDrag: true },
          992: { items: 3, mouseDrag: true },
        },
        URLhashListener: false,
      });
    });
  </script>
</div>
          </div>
        </div>
