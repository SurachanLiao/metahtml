var define = widgets.define.bind(widgets,'common_ad_resp','handlers',['handlers']);
define(["widget","ta/Core/TA.Event", "utils/responsive", "utils/ResponsiveEvents", "utils/resize-invalidated-cache"],
function(widget, taEvent, responsiveUtil, responsiveEvents, getCachedValue) {
var showingLargeAds;
function _resizeHandler(widgetElem, showLargeAds) {
if (showingLargeAds !== showLargeAds) {
var largeAdsContainer = widgetElem.find('.largeScreen');
var largeAds = largeAdsContainer.find('.gptAd');
largeAds.toggleClass('inactive', !showLargeAds);
largeAdsContainer.toggleClass('hidden', !showLargeAds);
// don't need to manually refresh ads on initial call aka page-load
if (showingLargeAds !== undefined) {
taEvent.fireEvent('adSizeChanged');
}
showingLargeAds = showLargeAds;
}
}
function _init($) {
var widgetElem = $(widget.element);
var isMobile = getCachedValue(responsiveUtil.mobile);
responsiveEvents.onOver('mobile', function(){_resizeHandler(widgetElem, true);});
responsiveEvents.onUnder('mobile', function(){_resizeHandler(widgetElem, false);});
_resizeHandler(widgetElem, !isMobile)
}
require(["lib/jquery-amd"],_init);
});
