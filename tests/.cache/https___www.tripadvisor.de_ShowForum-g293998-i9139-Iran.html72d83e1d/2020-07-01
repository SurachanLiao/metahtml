function showCascadeSearch(sHotelID, bFromPop, sLinkType, sArea) {
if(sHotelID == null || sHotelID == '') {return;}
var tgtUrl = "/HotelDateSearch?d=" + sHotelID + "&returnTo=https%3A__2F____2F__www__2E__tripadvisor__2E__de__2F__ShowForum__2D__g293998__2D__i9139__2D__Iran__2E__html&fromPop=" + bFromPop;
