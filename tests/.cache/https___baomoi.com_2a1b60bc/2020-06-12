<script type="text/javascript">

    function mobileDetect() {
    var e = /android|webos|iphone|ipad|ipod|blackberry|bb|playbook|iemobile|windows phone|kindle|silk|opera mini/i.test(navigator.userAgent.toLowerCase());
    if (e)
            if (window.location.hash && "bm_web" === window.location.hash.replace("#", "")) {
    var o = new Date;
    o.setTime(o.getTime() + 864e5);
    var i = "; expires=" + o.toGMTString();
    document.cookie = "bm_web=1" + i + "; path=/";
    } else {
    for (var n = null, t = "bm_web=", a = document.cookie.split(";"), l = 0; l < a.length; l++) {
    for (var r = a[l]; " " === r.charAt(0); )
            r = r.substring(1, r.length);
    if (0 === r.indexOf(t)) {
    n = r.substring(t.length, r.length);
    break;
    }
    }
    return (null === n || "1" !== n) && (window.location = window.location.href.replace("baomoi.com", "m.baomoi.com"));
    }
    }

    function checkHrefRedirect(){
    if (window.location.href.indexOf('/st/') >= 0){
    window.location = window.location.href.replace("/st/", "/top/");
    }
    }
    mobileDetect();
    checkHrefRedirect();</script>

