<script>
hpSetNbCol();
$('.hp_listbycat .slides').each(function(){

  $(this).find('.img_art').each(function(index){
	  var height = $(this).width();
	  $(this).height(height); 
  });		

  
  if($(this).find('.item').length > 0)
  {
	    $(this).isotope({
	        percentPosition: true,
	        masonry: {
	    	    columnWidth: '.grid-size-width'
	        }
	      });	    	      
  }
  
    
});




var listItems2Load = [];
var isLoadMore = false;
$('.page_content').on('scroll',function() {
    if($('.hp_listbycat:not(.loaded)').first().length > 0 && $('.hp_listbycat:not(.loaded)').first().offset().top < 2000) {
	    if(isLoadMore == false)
	    {	
		    isLoadMore = true;
		    	    	
	    	$('.hp_listbycat:not(.loaded)').each(function(idx,val){
				if(idx < 5)
				{
					$(this).addClass('loaded');
					$(this).find('.slides').addClass('wait');
					listItems2Load.push({'id':$(this).attr('relid')})
					setLoading('.hp_listbycat[relid='+$(this).attr('relid')+'] .slides');
				}
		    });

		    $.post('/s/www?isAjax=1&v=2',{'data':listItems2Load},function(ret){
			    var data = JSON.parse(ret);
			    console.log(data);
		    	for(a in data)
		    	{
			    	var $elem = $('.hp_listbycat[relid='+data[a].id+']');
			    	$elem.find('.slides').html(data[a].html)

			    	$elem.find('.slides .img_art.org').each(function(index){
			    		  $(this).removeClass('org');
			    		  var height = $(this).width(); 
			    		  $(this).height(height); 
			    	});		
			    	
			    	$elem.find('.slides').isotope({
  		    	  	  percentPosition: true,
  		    	      masonry: {
  		    		    columnWidth: '.grid-size-width'
  		    	      }
  		    	    });
		    	}
		    	
		    		
		    	listItems2Load = [];
		    	isLoadMore = false;
				initOpener();	

					
					    	
			});
	    }
    }
});
$('.hp_listbycat_linktotheme a').off('click').on('click',function(e){
	e.preventDefault();
	treatUrl($(this));	
});
</script>


					
