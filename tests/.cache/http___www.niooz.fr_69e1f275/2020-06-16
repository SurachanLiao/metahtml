<script>
  socket.on('nb_stats',function(obj){
	  if($('.shootbox .nb_users').text() == "")
	  {
		  $('.shootbox .nb_users').html("<b>0</b> nioozers en ligne");
		  $('.nb_users b').prop('number',$('.nb_users b').text()).animateNumber(    {      number: obj.all    },    2000  )
	  }	   
	  else
	   $('.nb_users b').prop('number',$('.nb_users b').text()).animateNumber(    {      number: obj.all    },    2000  )
	  delete obj;
  });
		
  socket.on('shootbox_message', function(msg){
  	var obj = jQuery.parseJSON(msg);
  	if($('.shootbox_main').length > 0)
  	{
  	  $('.shootbox_main .alone').remove();
  	  $('.shootbox_main ul').append('<li><div class="avatar"><img src="'+obj.from.avatar+'" /></div><b>'+obj.from.pseudo+'</b> ('+obj.time+') : <span>'+obj.comment+'</span>'+(obj.item_uri != "" ? ' <a href="'+obj.item_uri+'" class="color_default"><i class="fa fa-external-link" aria-hidden="true"></i></a>' : '')+'</li>');	
  	  $('.shootbox_main ul').animate({ scrollTop: $('.shootbox_main ul')[0].scrollHeight }, "slow");
  	}

  	if(obj.item_id > 0)
  	{
  	  	$('.shootbox_'+obj.item_id+' .alone').hide();
  	    $('.shootbox_'+obj.item_id+' ul').append('<li class="s'+obj.site_id+' i'+obj.item_id+'"><div class="avatar"><img src="'+obj.from.avatar+'" /></div><b>'+obj.from.pseudo+'</b> ('+obj.time+') : <span>'+obj.comment+'</span></li>');
  	  $('.shootbox_'+obj.item_id+' li').css('border','');
  	  	$('.shootbox_'+obj.item_id+' li:last:not(.alone)').css('border','0');
  	    $('.shootbox_'+obj.item_id+' ul').animate({ scrollTop: $('.shootbox_'+obj.item_id+' ul')[0].scrollHeight }, "slow");
  	    $('.shootbox_'+obj.item_id).removeClass('showTheme').addClass('showItem');
  	    $('.shootbox_'+obj.item_id+' .filterChat .selected').removeClass('selected');
  	    $('.shootbox_'+obj.item_id+' .filterChat .item').addClass('selected');
  	}
  	initSBLink();
  	delete obj;
  	delete msg;
  });
</script>

				
		<script src="http://www.niooz.fr/web/themes/niooz/js/vendor/smartbanner/jquery.smartbanner.js"></script>
		<style>
		#sas-interstitial-bg{z-index: 10000;}
		#sas-interstitial{z-index: 10001;}
		</style>
