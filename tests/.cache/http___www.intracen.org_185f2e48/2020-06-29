<script type="text/javascript">
    $(document).ready(function () {
        if ($(window).width() > 767) {
            //alert($(".footer-top-in div:first-child ul:first-child").html());
            var tmpHeight = 0;
            $(".footer-top-in div:first-child ul:first-child").children('li.footer-top-menu-item').each(function () {
                if ($(this).height() > tmpHeight) {
                    tmpHeight = $(this).height();
                }
            });
            // alert(tmpHeight);
            $(".footer-top-in div:first-child ul:first-child").children('li.footer-top-menu-item').each(function () {
                $(this).height(tmpHeight);
            });
        }
    });

</script>

            
            
