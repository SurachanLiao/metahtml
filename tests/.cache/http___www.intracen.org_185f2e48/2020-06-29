    <script type="text/javascript" defer="defer" src="/js/jquery.flexslider.js"></script>
    <script type="text/javascript">
        $(window).load(function () {
            $('.flexslider').flexslider({
                animation: "slide",
                controlNav: "thumbnails",
                reverse: false,
                useCSS: false,
                slideshowSpeed: 15000,
                before: function (slider) {
                    var $allListElements = slider.controlNavScaffold.find('li');
                    $allListElements.removeClass('flex-active-li');
                    $($allListElements[slider.animatingTo]).addClass('flex-active-li');

                },
                start: function (slider) {
				if (slider.controlNavScaffold){
					var $allListElements = slider.controlNavScaffold.find('li');
                    $($allListElements[0]).addClass('flex-active-li')
                    for (var i = 0; i <= slider.slides.length - 1; i++) {
                        if (!slider.slides.eq(i).hasClass('clone')) {
                            $($allListElements[i]).append("<p>" + slider.slides.eq(i).attr('title') + "</p>")

                            $($allListElements[i]).find('img').click(function (event) {
                                $(this).parent().parent().find('li').removeClass('flex-active-li')
                                $(this).parent().addClass('flex-active-li')
                            });

                            $($allListElements[i]).find('p').click(function (event) {
                                event.preventDefault();
                                $(this).prev().click();
                            });
                        }
                    }

                    $('body').removeClass('loading');
                }
				}
            });
        });
    </script>

