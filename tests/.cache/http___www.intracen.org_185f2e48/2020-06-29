            $('.country-list li').each(function (index) {
                // console.log("li called"+index);


                // $('ul li').each(function (index) {
                if ($(this).text().toLowerCase().trim().search(value.toLowerCase().trim()) == 0) {
                    console.log("li called" + $(this).html().toLowerCase().trim().search(value.toLowerCase().trim()));
