    <script type="text/javascript">
    jQuery("#main_top_search_btn2").on('click',function() { search_input_fn2(); });

    jQuery("#main_top_search_input2").keypress(function(e)
    {
        if (e.which == 13) { search_input_fn2(); }
    });

    function search_input_fn2() 
    {
        var main_search_val2 = jQuery("#main_top_search_btn_yn2").val();
        if (main_search_val2 == "0") 
        {
            jQuery("#main_top_search_input2").css("display","inline-block");
            jQuery("#main_top_search_btn_yn2").val("1");
            jQuery("#main_top_search_input2").val("").focus();
        }
        else 
        {
            if (jQuery("#main_top_search_input2").val()) 
            {
                jQuery("#main_top_search_input2").css("display","none");
                jQuery("#main_top_search_btn_yn2").val("0");
                search2.submit();
            }
            else 
            {
                jQuery("#main_top_search_input2").css("display","none").val("");
                jQuery("#main_top_search_btn_yn2").val("0");
            }
        }
    }
    </script>
    <!-- // floating navigation bar -->
    <script type="text/javascript">
    // 로그인/로그아웃 시 노출 될 링크
    if (document.cookie.match(/khan_uid/i) || document.cookie.match(/khan_id/i)) 
    {
        jQuery(".logIn").hide();
        jQuery(".logOut").show();
    } 
    else 
    {
        jQuery(".logIn").show();
        jQuery(".logOut").hide();
    }

    // 로그아웃
    function logout()
    {
        window.location.href = "https://member.khan.co.kr/login/logout.html?url="+ encodeURIComponent(location.href);
    }

    // 기사 검색 바로가기
    jQuery("#search").focus(function() { jQuery(this).find(".tf_search").addClass("on"); });

    // 기사 검색 - 상단
    jQuery("#main_top_search_btn").on('click',function() { search_input_fn(); });

    jQuery("#main_top_search_input").keypress(function(e) 
    {
        if (e.which == 13) { search_input_fn(); }
    });

    function search_input_fn() 
    {
        var main_search_val = jQuery("#main_top_search_btn_yn").val();
        if (main_search_val == "0") 
        {
            jQuery("#main_top_search_input").css("display","inline-block");
            jQuery("#main_top_search_btn_yn").val("1");
            jQuery("#main_top_search_input").val("").focus();
        } 
        else 
        {
            if (jQuery("#main_top_search_input").val()) 
            {
                jQuery("#main_top_search_input").css("display","none");
                jQuery("#main_top_search_btn_yn").val("0");
                search.submit();
            } 
            else 
            {
                jQuery("#main_top_search_input").css("display","none").val("");
                jQuery("#main_top_search_btn_yn").val("0");
            }
        }
    }

    // side Navigation Bar
    //jQuery(".snb").focusin(function() { jQuery(this).addClass("on"); });

    jQuery(".snb .snbToggle").focusout(function() { jQuery(".snb").removeClass("on"); });

    jQuery(".snbToggle").click(function(e) 
    {
        if (jQuery(".snb").hasClass("on")) { jQuery(".snb").removeClass("on"); } 
        else { jQuery(".snb").addClass("on"); } 
    });

    jQuery(".snb").mouseleave(function() 
    {
        if (jQuery(".snb").hasClass("over")) 
        {
            jQuery(".list_snb > li").removeClass("on");
            jQuery(this).removeClass("over");
        }
    });

    jQuery(".link_snb").on("mouseover focusin", function() 
    { 
        jQuery(".list_snb > li").removeClass("on");
        if (jQuery(this).hasClass("null")) { jQuery(".snb").removeClass("over"); } 
        else 
        {
            jQuery(this).closest("li").addClass("on");
            jQuery(".snb").addClass("over");
        }
    });

    jQuery(document).ready(function(e) 
    {
        jQuery("#comment_down").on('click',function() { window.scrollTo(0,10000); });	
    });
    </script>

	    <script type="text/javascript">
    // GNB 스크롤 이벤트
    jQuery(window).on("scroll", function () 
    { 
        var p = jQuery(window).scrollTop();

        if (jQuery(".fnb").hasClass("on") && p < 204) { jQuery(".fnb").removeClass("on"); } 
        else if (!jQuery(".fnb").hasClass("on") && p >= 204) { jQuery(".fnb").addClass("on"); }
    });
    </script>
	


<!-- AD : 에이원플랫폼 구글 광고 -->
<script type="text/javascript">
(function() {
        function A1MakeIframe(path) {
            var h = document;
            var c = h.createElement("iframe");
            c.src = path;
            c.style.margin = "0px";
            c.style.borderWidth = "0px";
            c.style.padding = "0px";
            c.scrolling = "no";
            c.frameBorder = "0";
            c.style.width = "0px";
            c.style.height = "0px";
            document.body.appendChild(c);
        }
        
    var this_page_url = document.location;
    var hidden_location = "//adv.khan.co.kr/RealMedia/ads/Creatives/khan/A1_google_gateway/hidden.html?oax_url="+this_page_url;
    A1MakeIframe(hidden_location);

})();
</script>
