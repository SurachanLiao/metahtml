                        <span class="ico_corp ico_photoslide_prev">이전 사진</span>
                    </button>
                    <button class="btn_photoslide next Fadein02" id="next_btn">
                        <span class="ico_corp ico_photoslide_next">다음사진</span>
                    </button>
                    <!-- // 버튼 -->

                    <script>
                    var pan_width = 0;
                    jQuery(document).ready(function()
                    {
                        pan_width = jQuery('#photoSlide li:first').width() * 3;
                        jQuery("#next_btn").click(function() 
                        {
                            jQuery("#photoSlide").animate(
                            {
                                left : "-="+ pan_width,
                            }, 500, function(){
                                jQuery('#photoSlide').append(jQuery('#photoSlide li:lt(3)')).css('left', '0');
                            });
                        });

                        jQuery("#prev_btn").click(function() 
                        {
                            var max = jQuery('#photoSlide li').length;
                            var gt = max -4;
                            jQuery('#photoSlide').prepend(jQuery('#photoSlide li:gt('+ gt +')')).css('left', '-'+ pan_width +'px');
                            jQuery("#photoSlide").animate(
                            {
                                left : "+="+ pan_width,
                            }, 500);
                        });
                    });
                    </script>
                </div>
            </section>
