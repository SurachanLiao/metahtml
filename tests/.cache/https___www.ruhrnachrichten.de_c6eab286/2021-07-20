				if ($(this).find("i").hasClass("rn-small-down-2")) {
					$(this).find("i").removeClass("rn-small-down-2");
					$(this).find("i").addClass("rn-small-up-2");
					$(this).css("color", "#c2def5");
				}
				else
				{
					$(this).find("i").removeClass("rn-small-up-2");
					$(this).find("i").addClass("rn-small-down-2");
					$(this).css("color", "#fff");
				}
			});

			// city selection
			$("div.CustCityModule div.CustCities div.CustCity").click(function () {				
				
				// get currently selected city
				var strCurrentSelectedCity = "";

				// hide all check marks and get current selection
				$("div.CustCityModule div.CustCities div.CustCity").each(function () {
					if ($(this).hasClass("CustCitySelected"))
					{
						strCurrentSelectedCity = $(this).attr("data-fi-locality-custid");
					}
					$(this).removeClass("CustCitySelected");
				});

				// if clicked city is also current selected city than remove cookie and decrease count for city in db
				if ($(this).attr("data-fi-locality-custid") == strCurrentSelectedCity) {

					// remove cookie
					$.cookie('CustCityModule', '', { expires: -365, path: '/' });
					
					// decrease count for city in db
					if (strCurrentSelectedCity != "") {
						FI.Util.CAjaxTool.CallServiceAsync("/Custom/Modules/CustCityModule/CustCityModuleWS.asmx/Decrease",
						{
							strLocalityCustId: strCurrentSelectedCity
						},
						function (warning, data) // success
						{

						},
						function () // error
						{
							console.log('CustCityModule Increase failed');
						});						
					}

					// change heading
					$(this).parent().parent().parent().find(".headLine").html("Meine Stadt auswÃ¤hlen <i class='rn-ico rn-small-up-2'></i>");
					$(this).parent().parent().parent().find(".headLine").removeClass("cityselected");
					$(this).removeClass("CustCitySelected");
					$(".CustCitySlogan").show();
				}
				// if clicked city is not current selected city than write cookie and increase count for city in db and decrease for prevoiusly selcted city in db
				else
				{
					// write cookie
					$.cookie('CustCityModule', $(this).attr("data-fi-locality-custid"), { expires: 365, path: '/' });

					// increase count for city in db
					FI.Util.CAjaxTool.CallServiceAsync("/Custom/Modules/CustCityModule/CustCityModuleWS.asmx/Increase",
					{
						strLocalityCustId: $(this).attr("data-fi-locality-custid")
					},
					function (warning, data) // success
					{
						
					},
					function () // error
					{
						console.log('CustCityModule Increase failed');
					});

					// decrease for prevoiusly selcted city in db
					if (strCurrentSelectedCity != "")
					{
						FI.Util.CAjaxTool.CallServiceAsync("/Custom/Modules/CustCityModule/CustCityModuleWS.asmx/Decrease",
						{
							strLocalityCustId: strCurrentSelectedCity
						},
						function (warning, data) // success
						{

						},
						function () // error
						{
							console.log('CustCityModule Increase failed');
						});
					}

					// change heading
					$(this).parent().parent().parent().find(".headLine").html($(this).text() + " <i class='rn-ico rn-small-up-2'></i>");
					$(this).parent().parent().parent().find(".headLine").addClass("cityselected");
					$(this).addClass("CustCitySelected");
					$(".CustCitySlogan").hide();
				}

				// reload page
				location.reload();
			});

			// get selected city from cookie
			var strCookieValue = $.cookie("CustCityModule");
			if (strCookieValue != null && strCookieValue != "") {

				// highlight selected city
				$("div.CustCityModule div.CustCities div.CustCity[data-fi-locality-custid='" + strCookieValue + "']").addClass("CustCitySelected");

				// change heading
				$("div.CustCityModule div.headLine").html($("div.CustCityModule div.CustCities div.CustCity[data-fi-locality-custid='" + strCookieValue + "']").text() + " <i class='rn-ico rn-small-down-2'></i>");
				$("div.CustCityModule div.headLine").addClass("cityselected");
				$("div.CustCityModule div.headLine").css("color", "#fff");
				$(".CustCitySlogan").hide();
			}
			else
			{
				// change heading
				$("div.CustCityModule div.headLine").html("Meine Stadt auswÃ¤hlen <i class='rn-ico rn-small-down-2'></i>");
				$("div.CustCityModule div.headLine").removeClass("cityselected");
				$("div.CustCityModule div.headLine").css("color", "#fff");
				$(".CustCitySlogan").show();
			}
		});
	</script>

		</div>
