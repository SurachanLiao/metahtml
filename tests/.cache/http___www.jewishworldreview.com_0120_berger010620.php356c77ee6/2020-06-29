<!-- Feedback API - Added 3.31.17 -->
<script>
(function () {
	var resource = 'http://jwr.api.ykstudios.com/', elem = document.createElement ( 'div' ), xhr = new XMLHttpRequest ();
	elem.id = 'jwr-collect-feedback-module';
	elem.innerHTML = '<div style="z-index:99999;position:fixed;bottom:25px;right:30px;font:16px/1.5 \'Franklin Gothic Book\',Arial"><div style=position:relative><button style=position:absolute;bottom:18px;right:18px;padding:0;height:50px;width:50px;cursor:pointer;color:#3a3a3a;background:url(http://jwr.api.ykstudios.com/img/feedback.png);border:none;border-radius:50%;outline:0 class=toggleFeedbackUI title=Feedback></button><form action=# class=form style="display:none;padding:0 7px;min-height:245px;width:450px;background:#fff;border:1px solid #ccc"><h2 style="margin:0;padding:10px;font-weight:500;font-size:17px;color:#5b5b5b;border-bottom:1px solid #ccc">What can we do to improve?</h2><div style=padding:10px><h3 style="margin:17px 0 7px;font-size:17px;color:#3e3e3e">Are you enjoying JWR?</h3><div class=rating-listener><span><input id=jwr-rating-1 name=rating style=display:none type=radio value=1><label class=jwr-rating for=jwr-rating-1 style=display:inline-block;height:25px;width:26px;cursor:pointer;background:url(http://jwr.api.ykstudios.com/img/star-unselected.png)></label></span><span><input id=jwr-rating-2 name=rating style=display:none type=radio value=2><label class=jwr-rating for=jwr-rating-2 style=display:inline-block;height:25px;width:26px;cursor:pointer;background:url(http://jwr.api.ykstudios.com/img/star-unselected.png)></label></span><span><input id=jwr-rating-3 name=rating style=display:none type=radio value=3><label class=jwr-rating for=jwr-rating-3 style=display:inline-block;height:25px;width:26px;cursor:pointer;background:url(http://jwr.api.ykstudios.com/img/star-unselected.png)></label></span><span><input id=jwr-rating-4 name=rating style=display:none type=radio value=4><label class=jwr-rating for=jwr-rating-4 style=display:inline-block;height:25px;width:26px;cursor:pointer;background:url(http://jwr.api.ykstudios.com/img/star-unselected.png)></label></span><span><input id=jwr-rating-5 name=rating style=display:none type=radio value=5><label class=jwr-rating for=jwr-rating-5 style=display:inline-block;height:25px;width:26px;cursor:pointer;background:url(http://jwr.api.ykstudios.com/img/star-unselected.png)></label></span></div></div><div style="padding:0 10px 10px"><h3 style="margin:17px 0 7px;font-size:17px;color:#3e3e3e">How can we make it even better?</h3><textarea style="height:172px;width:100%;resize:none;font:16px/1.5 \'Franklin Gothic Book\',Arial;border:1px solid #ccc;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;outline:0"></textarea><small style="display:block;margin:-2px 0 0 3px;font-size:12px;color:#444">Minimum 30 characters</small></div><div style="display:none;margin:10px;padding:10px 22px;font-size:14px;background:#f7f7f7;border:1px solid #b9b9b9"class=message-feedback></div><div style=padding:10px><button style="margin-bottom:15px;font-weight:700;padding:10px 50px;border:1px solid #ccc;background:0 0;cursor:pointer;outline:0"data-action=submit-feedback>Submit</button></div></form></div></div>';
	document.body.appendChild ( elem );
	window.onresize = function () { updateFeedbackVisibility (); }
	function updateFeedbackVisibility () {
		if ( window.innerWidth < 530 ) elem.style.display = 'none';
		else elem.style.display = 'block';
	}
	var
		btn_toggle_form = elem.getElementsByClassName ( 'toggleFeedbackUI' )[0],
		feedback_form = elem.getElementsByTagName ( 'form' )[0],
		submit_btn = feedback_form.getElementsByTagName ( 'button' )[0],
		textarea = feedback_form.getElementsByTagName ( 'textarea' )[0],
		rating_listener = elem.getElementsByClassName ( 'rating-listener' )[0],
		rating_items = rating_listener.getElementsByTagName ( 'span' ),
		message_elem = elem.getElementsByClassName ( 'message-feedback' )[0];
	btn_toggle_form.onclick = function () { feedback_form.style.display = feedback_form.style.display === 'none' ? 'block' : 'none'; };
	rating_listener.addEventListener ( 'click', function ( e ) {
		if ( e.srcElement.tagName === 'LABEL' ) {
			var label = e.srcElement, elem_index = parseInt ( e.srcElement.htmlFor.replace ( /^[a-zA-Z\-]+/, '' ));
			for ( var i = 0; i < elem_index; i++ ) rating_items[i].children[1].style.background = 'url(http://jwr.api.ykstudios.com/img/star.png)';
			for ( i = elem_index; i < rating_items.length; i++ ) rating_items[i].children[1].style.background = 'url(http://jwr.api.ykstudios.com/img/star-unselected.png)';
		}
	});
	feedback_form.onsubmit = function ( e ) {
		var form = {}, required_fields = [ 'rating', 'feedback' ], success = true;
		for ( var i = 0; i < rating_items.length; i++ ) if ( rating_items[i].children[0].checked ) form.rating = parseInt ( rating_items[i].children[0].value );
		form.referral = window.location.href;
		form.feedback = textarea.value;
		form.browser = navigator.userAgent;
		form.language = navigator.language;
		if ( form.feedback.length < 30 || typeof form.rating !== 'number' ) success = false;
		if ( success ) {
			submit_btn.disabled = true;
			submit_btn.style.cursor = 'wait';
			submit_btn.innerText = 'Sending...';
			textarea.disabled = true;
			message_elem.style.display = 'none';
			submit ( form );
		}
		else {
			message_elem.innerHTML = 'All fields are required, and feedback should be a minimum of 30 characters.';
			message_elem.style.display = 'block';
		}
		return false;
	};
	function submit ( form ) {
		xhr.open ( 'post', resource + 'api/feedback' );
		xhr.setRequestHeader ( 'Content-Type', 'application/json' );
		xhr.onload = function () {
			var resp = JSON.parse ( xhr.response );
			if ( xhr.status === 201 ) {
				message_elem.innerHTML = resp.message;
				message_elem.style.display = 'block';
				for ( var i = 0; i < feedback_form.children.length; i++ ) if ( feedback_form.children[i].className !== 'message-feedback' ) feedback_form.children[i].style.display = 'none';
			}
			else {
				message_elem.innerHTML = resp.message;
				message_elem.style.display = 'block';
				submit_btn.disabled = false;
				submit_btn.style.cursor = 'pointer';
				submit_btn.innerText = 'Submit';
				textarea.disabled = false;
			}
		};
		xhr.send ( JSON.stringify ( form ));
	}
})();
</script>
