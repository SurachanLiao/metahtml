    function getCookieByName(cname) {
        var c_name = cname + "=";
        var c_a = document.cookie.split(';');
        for (var i = 0; i < c_a.length; i++) {
            var c_c = c_a[i];
            while (c_c.charAt(0) == ' ') {
                c_c = c_c.substring(1);
            }
            if (c_c.indexOf(c_name) == 0) {
                return c_c.substring(c_name.length, c_c.length);
            }
        }
        return "";
    }

    $(document).ready(function () {
        try {
            console.log(getCookieByName("rennabtuohtiweikooc").length);
            if (getCookieByName("rennabtuohtiweikooc").length > 0) {
                $(".is-hide-for-premium").remove();


                console.log('SPOTIM_ADS_DISABLED');
                window.__SPOTIM_ADS_DISABLED__ = true;
                window.__SPOTIM_DISABLE_ADS__ && window.__SPOTIM_DISABLE_ADS__()  

            }
        }
        catch (err) { console.log(err);}
    });

</script>
<script type="text/javascript" async="">

    var num = 420000;

    var refreshSiteNew;
    refreshSiteInterval();

   var MAX_REFRESH = 100;
   if (sessionStorage.getItem('numRefresh') == null) {
       sessionStorage.setItem("numRefresh", "1");
   }

    function refreshSiteInterval() {
        var nextRefresh = new Date(Date.now() + (7 * 60 * 1000));
console.log('Last refreshSite in: ' + new Date());
console.log('Next refreshSite in: ' + nextRefresh);

        refreshSiteNew = setInterval(function () {
            if (typeof (Storage) !== "undefined") {
                sessionStorage.setItem("yPosition" + articleID, window.pageYOffset);
            } else {
                // Sorry! No Web Storage support..
            }
            
          
        var numRefreshInSession = parseInt(sessionStorage.getItem("numRefresh"));
        if (numRefreshInSession <= MAX_REFRESH) {
            sessionStorage.setItem("numRefresh", (numRefreshInSession + 1));
             document.location = document.location;                    
         }
            
        }, num);

        if (typeof (Storage) !== "undefined") {
            var yPosition = sessionStorage.getItem("yPosition" + articleID);
            if (yPosition != null && yPosition > 0) {
                window.scrollTo(0, yPosition);
            }
        } else {
            // Sorry! No Web Storage support..
        }

    };
    function stopRefreshSite() {
        clearInterval(refreshSiteNew);
console.log('Stop Refresh Site activated');
    }
        
    function success() {
        console.log('success');
    }

    function fail() {
        console.log('fail');
    }

        </script><!-- Twitter Script -->
