	   let keywords = "";
       let persons = "";
	   let places =  "";
       let organizations = ""; 
       function tpCheckIfEmpty(array) {
         if (array !==""){
           return array + ", "
         }
       } 
       function tpFormatTags (keywords, persons, places, organizations){
         let keywordString = tpCheckIfEmpty(keywords) + tpCheckIfEmpty(persons) + tpCheckIfEmpty(places) + tpCheckIfEmpty(organizations);
         //keywordString = keywordString.slice(0, -2); Diese funktion ist verantwortlich, das der Block nicht mehr weiter ausgeführt wird.
         return keywordString;
       }
       console.log("LLL");
       tp = window["tp"] || [];
       console.log("MMM");
       tp.push(["setSandbox", false]);
       tp.push(["setDebug", true]);
       tp.push(["setMaxCookieExpirationInDays", 365]);
		
       tp.push(["setTags", [tpFormatTags (keywords, persons, places, organizations)]]); // Tags aus Retresco oder anderem System
       console.log("NNN");
       tp.push(["setContentCreated", ""]); // Erstelldatum des Artikels
       tp.push(["setContentAuthor", ""]); // Autor des Artikels
       tp.push(["setContentSection", ""]); // Rubrik / Ressort
       tp.push(["setContentIsNative", "false"]); // Handelt es sich um Native Advertising?       
       tp.push(["setCustomVariable", "contentDocumentId", ""]); // Artikel ID
       tp.push(["setCustomVariable", "contentEncodedUrl", encodeURI("https://www.mainpost.de/")]); // Encoded URL
                            tp.push(["setCustomVariable", "contentState", "free"]); // Artikel ist paid oder free
       //tp.push(["setCustomVariable", "contentArchiveState", "false"]); // Falls notwendig. Arikel ist ein Archivartikel?     Ist wichtig Artikelstatus! Suche Token  
              tp.push(["setCustomVariable", "contentIsStory", "false"]); // Artikletyp ist Multimedia                     tp.push(["setCustomVariable", "contentIsFrontpage", "true"]); // Artikeltyp ist Ressortseite       tp.push(["setCustomVariable", "contentLength", ""]); // Länge des Artikels
       tp.push(["setCustomVariable", "contentCategory", ""]); // Stoffkategorie
	   tp.push(["setCustomVariable", "contentArticleScore", ]); // Artikelscore formel von Flo anfordern (eventuell gar nicht möglich)  brauchen wir auch!

       tp.push(["setCustomVariable", "articleDPAAttribut", ""]);
       tp.push(["setCustomVariable", "articleDPAFormat", ""]); 
       tp.push(["setCustomVariable", "articleDPATopic", ""]); 
       tp.push(["setCustomVariable", "articleDPALymbicSystem", ""]); 
       tp.push(["setCustomVariable", "articleDPALifeTime", ""]); 
