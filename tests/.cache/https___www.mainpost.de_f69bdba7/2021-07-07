<script type="text/javascript">
 fcmsJs.applications.register('fcmsUser_WebPush_Application', 'webpush-app', {
     'applicationServerKey' : 'BLZdmLDQ7demqsQoleTgHqUZXhTlD6k2Lq7-l40VPJqG0xkGjF_NdZ0Vu6lRZpN8PvDQ9z4x9_IpwH9AWc7WDeU',
     'autoPopup': false

 });

document.addEventListener('DOMContentLoaded', function(){
    mobilePushDisplay()
});

 function mobilePushDisplay(){
  var isMobilepush = false; //initiate as false
// device detection
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
	isMobilepush = true;
}
if(navigator.maxTouchPoints &&
      navigator.maxTouchPoints > 2 &&
      /MacIntel/.test(navigator.platform)){
      isMobilepush = true;
      }
      
 if (isMobilepush == true) {
        document.getElementById("webpush-app").style.display = "none";
    }
 }
 
 function isIpadOS() {
  return navigator.maxTouchPoints &&
    navigator.maxTouchPoints > 2 &&
    /MacIntel/.test(navigator.platform);
}
 
 // Opera 8.0+
var isOpera = (!!window.opr && !!opr.addons) || !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;

// Firefox 1.0+
var isFirefox = typeof InstallTrigger !== 'undefined';

// Safari 3.0+ "[object HTMLElementConstructor]" 
var isSafari = /constructor/i.test(window.HTMLElement) || (function (p) { return p.toString() === "[object SafariRemoteNotification]"; })(!window['safari'] || (typeof safari !== 'undefined' && safari.pushNotification));

// Internet Explorer 6-11
var isIE = /*@cc_on!@*/false || !!document.documentMode;

// Edge 20+
var isEdge = !isIE && !!window.StyleMedia;

// Chrome 1 - 79
var isChrome = !!window.chrome && (!!window.chrome.webstore || !!window.chrome.runtime);

// Edge (based on chromium) detection
var isEdgeChromium = isChrome && (navigator.userAgent.indexOf("Edg") != -1);

// Blink engine detection
var isBlink = (isChrome || isOpera) && !!window.CSS;

if(isChrome==true){
document.getElementById('browser-note-off').innerHTML = '<img data-src="https://www.mainpost.de/images/push-off/push-chrome.png" src="/_/pics/spacer.gif" class="lazyload" />';
}
else if (isFirefox==true){
document.getElementById('browser-note-off').innerHTML = '<img data-src="https://www.mainpost.de/images/push-off/push-firefox.png" src="/_/pics/spacer.gif" class="lazyload" />';
}
else if (isSafari==true){
document.getElementById('browser-note-off').innerHTML = '<ol><li>this is Safari</li></ol>';
}
else if (isIE==true){
document.getElementById('browser-note-off').innerHTML = '<ol><li>this is IE</li></ol>';
document.getElementById("webpush-app").style.display = "none";
}
else if (isEdge==true){
document.getElementById('browser-note-off').innerHTML = '<img data-src="https://www.mainpost.de/images/push-off/push-edge.png" src="/_/pics/spacer.gif" class="lazyload" />';
}

</script>

