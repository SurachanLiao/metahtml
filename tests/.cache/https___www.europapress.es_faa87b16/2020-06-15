<script type="text/javascript">
    var urlUltimaHoraJson = "https://www.europapress.es/ws/UltimaHora_JSON.ashx?formato=1"; 
    getJSON(urlUltimaHoraJson, function (err, json) {
        if (err !== null) { console.log('Error al cargar ultima hora ' + err) } else {
            if (json !== null) {
                var ultimaHoraHtml = "";
             
                ultimaHoraHtml += "<div id='tablaultimahora' class='full ultimahora-urgente'>";ultimaHoraHtml += "<div class='display-table'>";ultimaHoraHtml += "<div class='cabecera'><h3>Última Hora</h3></div>";ultimaHoraHtml += "<div class='titulo'>";ultimaHoraHtml += "<a href='" + json.enlace + "'><span>" + json.title + "</span></a>";if (json.lead !== "") {ultimaHoraHtml += "<p>" + json.lead + "</p>";}ultimaHoraHtml += "</div></div></div>";
                document.getElementById("urgente").innerHTML = ultimaHoraHtml;
             
            }
        }
    });
</script>



                
                

    
    
    
    


    


    

    



                

                
    
   



                

