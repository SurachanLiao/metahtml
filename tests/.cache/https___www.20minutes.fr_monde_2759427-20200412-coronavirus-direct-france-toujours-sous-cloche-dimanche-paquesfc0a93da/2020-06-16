            performance.mark('20mn:NextAcpm:End:Async');
        };
        aS.src = 'https://tag.audience.acpm.fr/js/on-1.0.min.js';

        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(aS, s);

        // 2. Sending the measurement
        if (aS.addEventListener){
            aS.addEventListener('load', function(){
                aSloaded();
            }, false)
        } else {
            aS.onreadystatechange = function(){
                if (aS.readyState in {
                        complete: 1,
                        loaded: 1
                    }){
                    aSloaded();
                }
            };
        }

        aSloaded = function(){
            var tag = new Acpm.Tag("296096216482"); // Insert the serial provided by ACPM
            tag.send();
        };
    })();
</script>


                
<script async="true">
    performance.mark('20mn:Ns1:Start:Async');

    var __nspid = "e1wveo";
    var __nsptags = [];
    (function(w, d) {
        var x = function() {
        var j = d.createElement("script");
        j.type = "text/javascript";
        j.async = true;
        j.src = "http" + ("https:" === d.location.protocol ? "s://cs" : "://c") + ".ns1p.net/p.js?a=" + __nspid;
        j.onload = function () {
            performance.mark('20mn:Ns1:End:Async');
        };
        d.body.appendChild(j);
    };
    if (w.addEventListener) {
        w.addEventListener("load", x, false);
    } else if (w.attachEvent) {
        w.attachEvent("onload", x); }
    else {
        w.onload = x;
    }}(window, document));
</script>

                
<script>
    WonderPush.ready(function(WonderPushSDK){
        // Paramètre à modifier :
        var categorie = document.location.pathname.split("/")[1]; // catégorie visitée
        // Utility function
        var appendUniqueBounded = function(list, maxLength, value){
            var pos = list.indexOf(value);
            if (pos >= 0){
                return list.slice(0, pos).concat(list.slice(pos + 1)).concat([value]);
            } else {
                return list.slice(-maxLength + 1).concat([value]);
            }
        };
        // Fetch previous values
        WonderPushSDK.getInstallationCustomProperties().then(function(installation){
            var fieldValue = installation.string_lastVisitedCategories || [];
            // Append the new value
            fieldValue = appendUniqueBounded(fieldValue, 3, categorie);
            // Update the field with the new value
            WonderPushSDK.putInstallationCustomProperties({string_lastVisitedCategories: fieldValue});
        });
    });
</script>

                                    
<script>
  performance.mark('20mn:Chartbeat:Start');
  performance.mark('20mn:ChartbeatMab:Start');

  (function(){
    /** CONFIGURATION START **/
    var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});
    _sf_async_config.uid = 34533;
    _sf_async_config.domain = "20minutes.fr";
    _sf_async_config.flickerControl = false;
    _sf_async_config.useCanonical = true;
    _sf_async_config.useCanonicalDomain = true;
    _sf_async_config.sections = window.tc_vars['article_section'];
    _sf_async_config.authors = window.tc_vars['signature'];
    var _cbq = window._cbq = (window._cbq || []);
    var user_type = ('Connecte' == window.tc_vars['user_status']) ? 'lgdin' : 'anon';
    _cbq.push(['_acct', user_type]);
    /** CONFIGURATION END **/
    function loadChartbeat() {
        var e = document.createElement('script');
        var n = document.getElementsByTagName('script')[0];
        e.type = 'text/javascript';
        e.async = true;
        e.src = 'https://static.chartbeat.com/js/chartbeat.js';
        n.parentNode.insertBefore(e, n);
        e.onload = function(){
            performance.mark('20mn:Chartbeat:End');
        };
    }
    function loadChartbeatmab() {
        var e = document.createElement('script');
        var n = document.getElementsByTagName('script')[0];
        e.type = 'text/javascript';
        e.async = true;
        e.src = 'https://static.chartbeat.com/js/chartbeat_mab.js';
        n.parentNode.insertBefore(e, n.nextSibling);
        e.onload = function(){
            performance.mark('20mn:ChartbeatMab:End');
        };
    }
    loadChartbeat();
    loadChartbeatmab();
  })();
</script>

                            
                            
                
                
<script async="true">
    (function(){
        var loadOwnpageScript = function(e){
            var t="https://script.ownpage.fr/v1/",a=document.createElement("script"),n=document.getElementsByTagName("script")[0];a.type="text/javascript",a.defer=1,a.async=1,a.src=t+"ownpage.js",a.onload=e,a.readyState?a.onreadystatechange=function(){
                ("loaded"==a.readyState||"complete"==a.readyState)&&(a.onreadystatechange=null,e&&"function"==typeof e&&e())
            }:a.onload=function(){
                e&&"function"==typeof e&&e()
            },n.parentNode.insertBefore(a,n)
        };
        loadOwnpageScript(function () {
            Ownpage.trackPage("9932310c1c44463b");
        });
    })();
</script>
            
