        $('.communitylink a').click(function() {
      $.cookie('customView','community', { path: '/' });
      window.location.href = "https://gcphn.org.au";
    });

    $('.healthlink a').click(function() {
      $.cookie('customView','healthProfessionals', { path: '/' });
