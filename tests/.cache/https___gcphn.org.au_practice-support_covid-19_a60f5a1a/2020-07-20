  document.addEventListener('DOMContentLoaded', function() {
    const menuRightElem = document.getElementById('test-menu-right');
    const allEvents = [
      'sm.back',
      'sm.back-after',
      'sm.close',
      'sm.close-after',
      'sm.forward',
      'sm.forward-after',
      'sm.navigate',
      'sm.navigate-after',
      'sm.open',
      'sm.open-after',
    ];
    if(SlideMenu) {
      const menuRight = new SlideMenu(menuRightElem, {
        keyClose: 'Escape',
        submenuLinkAfter: '<span class="menuicon" style="margin-left: 1em;"></span>',
        backLinkBefore: '<span class="backicon" style="margin-right: 1em;"></span>',
      });
    }
  });
