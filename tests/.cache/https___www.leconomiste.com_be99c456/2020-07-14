var ETAT=true;$( document ).ready(function() {
console.log( "ready!"+ETAT );
var pubEtat = localStorage.getItem('pub');
console.log( "PUB ETAT !"+pubEtat );
if(ETAT==true && (pubEtat==null || pubEtat==0)){
$('#boxes').removeClass('hidden');
localStorage.setItem('pub', 1);
