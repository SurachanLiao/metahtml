<script type="text/javascript">
	$(function() {
		var params = {
				// Request parameters
		};
	
		$.ajax({
			url: "https://oneottprodapi.azure-api.net/news-analytics/news/pageviews",
			beforeSend: function(xhrObj){
				// Request headers
				xhrObj.setRequestHeader("Ocp-Apim-Subscription-Key","d3958ffcd988470aa6dfa85f9af0ea19");
				xhrObj.setRequestHeader("Access-Control-Allow-Origin","https://news.abs-cbn.com");
				xhrObj.setRequestHeader("Access-Control-Allow-Methods","POST, GET");
			},
			type: "GET"
		})
		.done(function(data) {
			if(data.length < 1) {
				 $("a.most-read").hide();
			} else {
				 $("a.most-read").show();
				 for(var i = 0; i < 11; i++){
					if(data[i][1]!="news.abs-cbn.com/"){
					 var t = data[i][0];
					 var u = data[i][1];
					 $("#mostread").append("<article class=\"post-content post-item\"><h2 class=\"post-title\"><a href=\"https://"+u+"\">"+t.replace(" | ABS-CBN News", "")+"</a></h2></article>");
					}
				 }
			 }
		})
		.fail(function() {
			$("a.most-read").hide();  
		});
 });
</script>

