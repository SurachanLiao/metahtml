        //cuando es un dispositivo movil no se hace sticky del banner - Quitado: usagent.indexOf("ipad") == -1 &&
        if (usagent.indexOf("iphone") === -1 && usagent.indexOf("android") === -1) {
            $(".bannerTerceraColumna").stick_in_parent({ parent: $(".bannerlimite"), offset_top: 80, spacer: '.wrapper-sticky' });
            //$(".bannerTerceraColumna").stick_in_parent({ parent: $(".bannerlimite"), spacer: '.wrapper-sticky', offset_top: 80 }).on('sticky_kit:bottom', function(e) {$(this).parent().css('position','static');}).on('sticky_kit:unbottom', function(e) {$(this).parent().css('position', 'relative')});

        }
        //$(".UltimaHoraScroll").stick_in_parent({ parent: $(".row.flex"), offset_top: 80 });

        var btnSeguir = document.querySelector("#btnSeguir");

        btnSeguir.addEventListener("click",
            function (e) {
                e.preventDefault();
                var popup = btnSeguir.nextElementSibling;
                popup.classList.toggle("hidden");
            });
    </script>


