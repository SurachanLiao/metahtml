                            this._asData.justMuted = true;
                            this._sendHit("mute",
                                false)
                        }
                    } else if (this._asData.justMuted) {
                        this._asData.justMuted = false;
                        this._sendHit("unmute", false)
                    }
                },
                _vidFullScreen: function() {
                    if (document.fullscreenElement || document.mozFullScreen || document
                        .webkitIsFullScreen || document.msFullscreenElement || document.fullScreen) this
                        ._sendHit("full screen", false)
                },
                _sendHit: function(ev, ni) {
                    this._asData.eventType = ev;
                    this._asData.hitValue = ev.match(/milestone|complete/i) ? this._asData.hitValue :
                        null;
                    this._asData.nonInteraction = ni;
                    _asSendHit(this._asData)
                },
                setListeners: function() {
                    this.addEventListener("play",
                        function() {
                            this._vidPlay()
                        }, true);
                    this.addEventListener("pause", function() {
                        this._vidPause()
                    }, true);
                    this.addEventListener("seeked", function() {
                        this._vidSeek()
                    }, true);
                    this.addEventListener("ended", function() {
                        this._vidComplete()
                    }, true);
                    this.addEventListener("timeupdate", function() {
