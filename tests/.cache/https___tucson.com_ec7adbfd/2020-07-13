<script>
    $('#site-search-1788344-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1788344').submit();
        return false;
    });

    $('#site-search-1788344').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1788344 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1788344 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1788344-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
    $('.dropdown-toggle-1788344').click(function(){
        setTimeout(function(){
            $('#site-search-1788344-term').focus();
        }, 50);
    });</script></div></div>
                                
