<script>
    $('#site-search-1788381-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-1788381').submit();
        return false;
    });

    $('#site-search-1788381').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-1788381 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-1788381 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-1788381-term').val() ){
            return true;
        }else{
            return false;
        }
    });
    
