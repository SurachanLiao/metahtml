                        $('#circularhub_module_7279').append('<script async src="//api.circularhub.com/7279/9dc6e977337e457b/circularhub_module.js"><\/script>');
                        window.removeEventListener('scroll', throttledFlip);
                    }
                }   
            }, 100);
            window.addEventListener('scroll', throttledFlip);
