<script type="text/javascript">
var gSP_local_pass = true;
var gSP_data = false;
function gSP_local_callback(data) {
    gSP_data = data;
    if (gSP_data.meta.status != 200 || !gSP_data.meta.isAuthenticatedInCurrentContext || !gSP_data.response.user.hasMarketAccess)  gSP_local_pass = false;
    else if (window.dataLayer)
    {
        dataLayer.push(
            {'user_id': gSP_data.response.user.fullName},
            {'user_state': "logged in"}
        );
    }
}

jQuery(function() {
    if (gSP_local_pass && typeof(gSP_data) == 'object')
    {
        jQuery('.gannett-tn-user-screenname').html(jQuery('<div/>').text(gSP_data.response.user.fullName));
        jQuery('.gannett-logged-in').show();

        jQuery('.lee-track-top-promo:eq(1) a:eq(1)').text('Thank you for being a subscriber and supporting quality local journalism.');
        jQuery('.lee-track-top-promo:eq(1) a').contents().unwrap();
    }
    else
    {
        jQuery('.gannett-not-logged-in').show();
    }
});
</script>
<script type="text/javascript" src="https://user.tucson.com/PTUC-GUP/user/?callback=gSP_local_callback&_ts=1594679537"></script>

