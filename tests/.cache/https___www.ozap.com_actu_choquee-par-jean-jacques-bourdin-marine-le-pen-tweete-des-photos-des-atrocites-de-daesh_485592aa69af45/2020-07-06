    var WB = WB || {};
    WB.mediametrieTracker = {
        callbackQueue: [],
        measuringSystemIsLoading: false,
        measuringSystemIsLoaded: false,
        eventsInitialised: false,
        contentTag: null,

        addToCallbackQueue: function (callback) {
            console.log('adding to callbackQueue');
            console.log(callback);

            this.callbackQueue.push(callback);
        },

        runCallbackQueue: function () {
            console.log('calling all callbackQueue items');

            for (var i = 0; i < this.callbackQueue.length; i++) {
                this.callbackQueue[i]();
            }
        },

        onLoaded: function () {
            this.measuringSystemLoaded = true;
            this.measuringSystemIsLoading = false;
            this.runCallbackQueue();
        },

        confStreamingAnalytics: function (nameUrl, videoID, durationInSec, callback) {
            var config = {
                serial: '243043218422',
                measure: 'streaming',
                levels: {
                    level_3: nameUrl,
                    level_4: window.location.hostname
                },
                newLevels: {
                    newLevel_1: "article",
                    newLevel_2: videoID,
                    newLevel_3: "485592",
                    newLevel_4: "Choquée par Jean-Jacques Bourdin, Marine Le Pen tweete des photos des atrocités de Daesh",
                    newLevel_5: "Radio|Internet|Video",
                    newLevel_6: "Marine Le Pen",
                    newLevel_7: window.top.location.href
                },
                implementation: 'automatic',
                streaming: {
                    diffusion: 'replay',
                    callbackPosition: callback,
                    playerName: 'JWPlayer',
                    streamName: nameUrl || 'default stream name',
                    streamDuration: durationInSec || 10,
                }
            };

            var version = jwplayer && jwplayer().version;
            if (version) {
                config.streaming.playerVersion = version;
            }

            return config;
        },

        /**
         * Main function for tracking jw players
         */
        track: function (player, data) {
            var self = this;

            if (!self.contentTag) {
                var config = self.getConfig(player, data);
                var contentTag = new eStatTag(config);

                if(!this.eventsInitialised){

                    player.on('complete', function(e){
                        contentTag.notifyPlayer('stop');
                    });

                    player.on('playlistItem', function(e){
                        contentTag.notifyPlayer('stop');
                    });

                    player.on('play', function(e){
                        contentTag.notifyPlayer('play');
                    });

                    player.on('pause', function(e){
                        contentTag.notifyPlayer('pause');
                    });

                    this.eventsInitialised = true;
                }

                self.contentTag = contentTag;
            } else {
                self.contentTag.set(self.getConfig(player, data));
            }
        },

        getConfig: function (player, data) {
            return this.confStreamingAnalytics(data.name, data.videoID, player.getDuration(), function () {
                return Math.floor(player.getPosition());
            });
        }
    };

