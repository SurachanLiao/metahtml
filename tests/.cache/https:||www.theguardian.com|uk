<section id="our-iranian-lockdown" class="fc-container fc-container--lazy-load fc-container--thrasher fc-container--will-have-toggle flashing-image js-container--lazy-load js-container--toggle " data-link-name="container-23 | our-iranian-lockdown" data-id="31c3b495-7e80-4df7-855f-e7e9f983286c" data-component="our-iranian-lockdown" aria-expanded="true">
<div class="fc-container__inner">
<header class="fc-container__header
                js-container__header
                ">
<div class="fc-container__header__title">
<h2 tabindex="0">Our Iranian Lockdown</h2>
</div>
</header>
<div class="fc-container--rolled-up-hide fc-container__body" data-title="Our Iranian Lockdown" data-id="31c3b495-7e80-4df7-855f-e7e9f983286c">
<div class="fc-slice-wrapper">
