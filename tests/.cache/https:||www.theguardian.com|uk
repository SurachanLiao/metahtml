<rect x="16" y="61" width="14" height="14" fill="#ffffff"/>
<rect x="61" y="1" width="14" height="14" fill="#ffffff"/>
<rect x="91" y="46" width="14" height="14" fill="#ffffff"/>
<rect x="1" y="61" width="14" height="14" fill="#ffffff"/>
<rect x="61" y="31" width="14" height="14" fill="#ffffff"/>
