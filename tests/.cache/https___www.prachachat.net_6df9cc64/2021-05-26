        ,
                onSliderLoaded : ud_on_slider_loaded,
                onSliderResize : (tdDetect.isAndroid)?ud_update_slider:ud_resize_normal_slide
    });
});
    

jQuery(document).ready(function() {
    
    jQuery("#td_uid_93_60aee3da186a8").data("ud_image_size", {width: 728, height: 520} );
    
    jQuery("#td_uid_93_60aee3da186a8").iosSlider({
        snapToChildren: true,
        desktopClickDrag: true,
        keyboardControls: false,
