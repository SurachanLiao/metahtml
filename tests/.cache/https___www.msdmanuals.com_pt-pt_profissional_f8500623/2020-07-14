<script type="text/javascript">
    $(".quiz__submit").click(function () {
        var value = $('input[type="radio"]:checked').val();
        var url = 'https://pt.msdmanualquiz.com/gcn/startquiz';
        if (value != undefined) {
                var answerChoice = value.replace(value.substr(0, value.indexOf("&quiz_id")), "");
                var valueBaseUrl = getBaseUrl(value);
                value = MManual.quizzes.getCorrectQuizzesUrlWithLanguages(value, valueBaseUrl) + answerChoice;
            }
            var startLink = 'https://pt.msdmanualquiz.com/gcn/startquizdirect/850?base_url=https%3A%2F%2Fwww.msdmanuals.com%2Fpt-pt&language=pt-PT';
            var quizzesBaseUrl = getBaseUrl(startLink);
            startLink = MManual.quizzes.getCorrectQuizzesUrlWithLanguages(startLink, quizzesBaseUrl);

        var fullLink = (value == undefined) ? startLink : url + value;

        var isHttps = window.location.protocol.indexOf("https:") > -1;
        if (isHttps) {
            fullLink = fullLink.replace("http:", "https:");
        }

        MManual.MultimediaPopupViewModel.showPopup({
            item: ko.mapping.fromJS({
                Title:'Vis&#227;o turva',
                MediaType : 'quiz',
                MediaNameTranslated : 'Quiz',
                ThumbnailUrl : '/Content/Images/Redesign/quizzes/quizzes_lightbulb_blue.png',
                IframeSrc: fullLink,
                UniqueId: '850'
            })
        });

    });
 function getBaseUrl(inputUrl) {
            return inputUrl.substr(0, inputUrl.indexOf("base_url")) + "base_url=" + window.location.origin;
        }

</script>


