multiCarrousel("multiCarouselMasvisto", 1);
multiCarrousel("multiCarouselGoles", 1);
multiCarrousel("multiCarouselMedia", 1);
multiCarrousel("multiCarouselUltimo", 2);
multiCarrousel("multiCarouselGeneric", 2);
multiCarrousel("multiCarouselGeneric1", 2);
multiCarrousel("multiCarouselGeneric2", 2);
function multiCarrousel(elementId, initValue) {
$('#' + elementId + '[data-type="multi"] .item').each(function() {
var next = $(this).next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
for (var i = 0; i < initValue; i++) {
next = next.next();
if (!next.length) {
next = $(this).siblings(':first');
}
next.children(':first-child').clone().appendTo($(this));
}
});
}
</script>
