});
$('.dropdown-toggle').dropdown()
// ===== Scroll to Top ====
$(window).scroll(function() {
if ($(this).scrollTop() >= 50) { // If page is scrolled more than 50px
$('.toTop').fadeIn(200); // Fade in the arrow
} else {
$('.toTop').fadeOut(200); // Else fade out the arrow
}
});
