$(document).ready(function () {
$("#verMasGallery2417644").click(function () {
loadNoticiasContent2417644('entrevistas');
});
var totalItem2417644 = 3;
function loadNoticiasContent2417644(secName) {
if (secName != '') {
var url = '/ajaxConfig/?service=ajax&item=noticias&imageVersion=BASE_LANDSCAPE_SMALL&part=noticias&contentType=news,videoReference,youtubeVideo,gallery,liveVideo&startIndex=' + totalItem2417644+'&sectionUniqueName=' + secName+"&homeSectionOnly=false&maximumLenghtOfTitle=300&numberOfItemsPerPage=3";
cSection = secName;
$.ajax({
url:url,
dataType:'json',
beforeSend:function(){
// $('#verMasGallery2417644').html('');
$('#verMasGallery2417644').html('Cargando...');
$("#verMasGallery2417644").prop('disabled', true);
$('#verMasGallery2417644').css( 'cursor', 'default' );
},
success:function (data) {
var totalNumFound = data.totalNumFound;
if (totalNumFound > data.articles.length + totalItem2417644) {
$('#verMasGallery2417644').show();
}
else {
$('#verMasGallery2417644').hide();
}
totalItem2417644 = totalItem2417644 + data.articles.length;
var htmlInt = '';
for (var i = 0; i < data.articles.length; i++) {
var imgSrc= "http://estaticos.24horas.cl/skins/24horas/gfx/BannerTwitterSection102015.png";
if(data.articles[i].imageUrl!=''){
imgSrc = data.articles[i].imageUrl;
}
var classFigure="";
var htmlLibyType="";
if(data.articles[i].articleTypeName == 'videoReference' || data.articles[i].articleTypeName == 'youtubeVideo'){
classFigure="boxvideo";
htmlLibyType='<a href="'+ data.articles[i].url +'"><span class="dataction">Ver Video</span></a>';
}
if(data.articles[i].articleTypeName == 'gallery'){
classFigure="boxgaleria";
htmlLibyType='<a href="'+ data.articles[i].url +'"><small>'+ data.articles[i].pictureRelCount +'</small></a>';
} if(data.articles[i].articleTypeName == 'liveVideo'){
classFigure="boxenvivo";
htmlLibyType='<a href="'+ data.articles[i].url +'"><span class="vivo">Vivo</span></a>';
} htmlInt +='<figure class="box '+classFigure+'">'
+'<picture>'
+'<a href="' + data.articles[i].url + '">'
+'<img alt="'+data.articles[i].title+'" src="' + imgSrc + '" class="img-responsive">'
+'<div class="data">'
+ htmlLibyType
+'</div>'
+'</picture>'
+'</a>'
+'<figcaption>'
+'<h2><a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a></h2>'
+'<p>'+data.articles[i].leadtext+'</p>'
+'</figcaption>'
+'</figure>';
}
var htmlDiv = '<div class="carouselList">'+htmlInt+'</div>';
$("#articlesPaginationGallery2417644").append(htmlDiv);
$('#verMasGallery2417644').html('Ver más');
$("#verMasGallery2417644").prop('disabled', false);
$('#verMasGallery2417644').css( 'cursor', 'pointer' );
},
error: function (jqXHR, exception) {
var msg = '';
if (jqXHR.status === 0) {
msg = 'Not connect.\n Verify Network.';
} else if (jqXHR.status == 404) {
msg = 'Requested page not found. [404]';
} else if (jqXHR.status == 500) {
msg = 'Internal Server Error [500].';
} else if (exception === 'parsererror') {
msg = 'Requested JSON parse failed.';
} else if (exception === 'timeout') {
msg = 'Time out error.';
} else if (exception === 'abort') {
msg = 'Ajax request aborted.';
} else {
msg = 'Uncaught Error.\n' + jqXHR.responseText;
}
},
}
);
}
}
});
</script>
