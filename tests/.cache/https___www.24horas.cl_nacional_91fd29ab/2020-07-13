<script type="text/javascript">
$(document).ready(function () {
$("#verMasGallery2417706").click(function () {
loadNoticiasContent2417706('nacional');
});
var totalItem2417706 = (6);
function loadNoticiasContent2417706(secName) {
if (secName != '') {
var url = '/ajaxConfig/?service=ajax&item=noticias&imageVersion=BASE_LANDSCAPE_SMALL&part=noticias&contentType=news,videoReference,youtubeVideo,gallery,liveVideo&startIndex=' + totalItem2417706+'&sectionUniqueName=' + secName+"&homeSectionOnly=false&maximumLenghtOfTitle=300&numberOfItemsPerPage=6";
cSection = secName;
$.ajax({
url:url,
dataType:'json',
beforeSend:function(){
// $('#verMasGallery2417706').html('');
$('#verMasGallery2417706').html('Cargando...');
$("#verMasGallery2417706").prop('disabled', true);
$('#verMasGallery2417706').css( 'cursor', 'default' );
},
success:function (data) {
var totalNumFound = data.totalNumFound;
if (totalNumFound > data.articles.length + totalItem2417706) {
$('#verMasGallery2417706').show();
}
else {
$('#verMasGallery2417706').hide();
}
totalItem2417706 = totalItem2417706 + data.articles.length;
for (var i = 0; i < data.articles.length; i++) {
var html = "";
if(data.articles[i].articleTypeName=='youtubeVideo' || data.articles[i].articleTypeName=='videoReference'){
html = '<figure class="box boxvideo">'
+'<picture>'
+'<a href="' + data.articles[i].url + '">'
+'<img src="' + data.articles[i].imageUrl + '" title="'+data.articles[i].title+'" alt="'+data.articles[i].title+'" class="img-responsive">'
+'</a>'
+'<div>'
+'<a href="' + data.articles[i].url + '"><span class="dataction">Ver Video</span>'
+'</a>'
+'</div>'
+'</picture>'
+'<figcaption>'
+'<h2><a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a></h2>'
+'</figcaption>'
+'</figure>' }else if(data.articles[i].articleTypeName=='gallery' ){
html = '<figure class="box boxgaleria">'
+'<picture>'
+'<a href="' + data.articles[i].url + '">'
+'<img src="' + data.articles[i].imageUrl + '" alt="'+data.articles[i].title+'" class="img-responsive">'
+'</a>'
+'<div>'
+'<a href="' + data.articles[i].url + '">'
+'<small>'+ data.articles[i].pictureRelCount +'</small>'
+'</a>'
+'</div>'
+'</picture>'
+'<figcaption>'
+'<h2><a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a></h2>'
+'</figcaption>'
+'</figure>'
}else if(data.articles[i].articleTypeName=='news' ){
html = '<figure class="box">'
+'<picture>'
+'<a href="' + data.articles[i].url + '">'
+'<img src="' + data.articles[i].imageUrl + '" alt="'+data.articles[i].title+'" class="img-responsive">'
+'</a>'
+'</picture>'
+'<figcaption>'
+'<h2><a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a></h2>'
+'</figcaption>'
+'</figure>'
}else if(data.articles[i].articleTypeName=='liveVideo' ){
html = '<figure class="box boxenvivo">'
+'<picture>'
+'<a href="' + data.articles[i].url + '">'
+'<img src="' + data.articles[i].imageUrl + '" alt="'+data.articles[i].title+'" class="img-responsive"></a>'
+'</picture>'
+'<figcaption>'
+'<h2>'
+'<a href="' + data.articles[i].url + '">'+data.articles[i].title+'</a>'
+'</h2>'
+'</figcaption>'
+'</figure>' }
$("#articlesPaginationGallery2417706").append(html);
}
$('#verMasGallery2417706').html('Ver más');
$("#verMasGallery2417706").prop('disabled', false);
$('#verMasGallery2417706').css( 'cursor', 'pointer' );
},
error: function (jqXHR, exception) {
var msg = '';
if (jqXHR.status === 0) {
msg = 'Not connect.\n Verify Network.';
} else if (jqXHR.status == 404) {
msg = 'Requested page not found. [404]';
} else if (jqXHR.status == 500) {
msg = 'Internal Server Error [500].';
} else if (exception === 'parsererror') {
msg = 'Requested JSON parse failed.';
} else if (exception === 'timeout') {
msg = 'Time out error.';
} else if (exception === 'abort') {
msg = 'Ajax request aborted.';
} else {
msg = 'Uncaught Error.\n' + jqXHR.responseText;
}
},
}
);
}
}
});
</script>
