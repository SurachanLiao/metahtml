                                    var text_view	= 'View';
                                    var text_views	= 'Views';

                                    $(document).ready(function () {
                                        $.ajax({
                                            type: 'GET',
                                            url: 'https://www.yumpu.com/fr/document/views/25736861',
                                            success: function (data) {
                                                data = parseInt(data);
                                                if (data === 0 || data > 1)
                                                    $("#yp-document-view-views").html("<span class='yp-strong-black-font'>" + data + "</span>" + ' Views');
                                                else if (data === 1)
                                                    $("#yp-document-view-views").html("<span class='yp-strong-black-font'>" + data + "</span>" + ' View');
                                                else
                                                    $("#yp-document-view-views").html('0 Views');
                                            }
                                        });
                                    });
                                </script>
                                <div id="yp-document-create" class="row row-height">
                                    <span class='yp-strong-black-font'>6</span> years ago                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row row-padd">
                                                    <ul id="yp-mag-tag-list" class="list-inline">
