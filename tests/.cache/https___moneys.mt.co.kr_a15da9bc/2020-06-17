    <script type="text/javascript" src="https://menu.mt.co.kr/js/jquery-1.12.3.min.js"></script>
    <script type="text/javascript" src="https://menu.mt.co.kr/common/js/slick.js"></script>
    <script type="text/javascript">
        var didScroll				// 스크롤시에 사용자가 스크롤했다는 것을 알림
        var delta = 237	// 동작의 구현이 시작되는 위치
        var lastScrollTop = 0
        var navbarHeight = $('#header').height()

        $(document).ready(function () {
            gnbNavigation()	// 메뉴

                        // 메인 슬라이드
            //$('.mtop_thum .slider').slick();			// TOP영역 슬라이드 -> index.html에서 실행되도록 수정
            $('.mthum_type1 .bthum .slider').slick()	// 682px 슬라이드
            $('.mthum_type1 .sthum .slider').slick()	// 322px 슬라이드
            $('.notice .slider').slick()				// 공지사항 슬라이드

            // 랭킹뉴스, 카테고리리스트
            setTimeout(function () {
                showRankingList('0')
                showCateList('0')
            }, 100)
                    })

        // 스크롤
        $(window).scroll(function (event) {
            didScroll = true
        })

        // 검색 닫기
        $('html').click(function (e) {
            if ($('.input_bx').css('display') == 'block') {
                if (!$('.search').has(e.target).length) {
                    $('.input_bx').css('display', 'none')
                }
            }
        })

        // 검색 열기
        function clickSearchBtn(e) {
            $('.input_bx').css('display', 'block')
            $('#kwd').focus()
        }

        function chkKwdSearch() {
            if ($.trim($('#kwd').val()) == '') {
                alert('검색어를 입력해 주십시오.')
            } else {
                document.searchForm.submit()
            }
        }

        // 메뉴
        function gnbNavigation() {
            // 상위 메뉴 하위메뉴 선택
            $('.gbundle').mouseover(function () {
                $(this).children('.glink').addClass('on')
            })
            $('.gbundle').mouseout(function () {
                $(this).children('.glink').removeClass('on')
            })
        }


                // 메인 > 랭킹뉴스 영역 슬라이드 초기화
        function slickRankingArea(no) {
            $('#rankArea' + no + ' .group').slick({
                infinite: true,
                speed: 500,
                slidesToShow: 3,
                slidesToScroll: 2,
                variableWidth: true
            })
        }

        // 메인 > 카테고리 영역 슬라이드 초기화
        function slickCateArea(no) {
            if ($('#cateArea' + no + ' .ngroup').hasClass('slick-initialized')) {
                $('#cateArea' + no + ' .ngroup').slick('unslick')        //슬릭해제
            }
            $('#cateArea' + no + ' .ngroup').slick()
        }

        // 메인 > 칼럼리스트
        function showColumnList(arg) {
            for (var i = 0; i < 9; i++) {
                $('#mcolumn .gisa_area' + i).hide()
                $('#mcolumn' + i).attr('class', 'bundle')
            }
            $('#mcolumn .gisa_area' + arg).show()
            $('#mcolumn' + arg).attr('class', 'bundle on')
        }

        // 메인 > 랭킹뉴스
        function showRankingList(arg) {
            $('div.thumArea[id^="rankArea"]').hide()
            $('li.bundle[id^="rankTab"]').removeClass('on')

            $('#rankArea' + arg).show()
            $('#rankTab' + arg).addClass('on')

            slickRankingArea(arg)
        }

        // 메인 > 카테고리별뉴스
        function showCateList(arg) {
            $('div.newsarea[id^="cateArea"]').hide()
            $('li[id^="cateTab"]').removeClass('on')

            $('#cateArea' + arg).show()
            $('#cateTab' + arg).addClass('on')

            slickCateArea(arg)
        }
        
        // 스크롤 다운하면 숨김, 스크롤 업하면  보임
        function hasScrolled() {
            var st = $(this).scrollTop()

            if (Math.abs(lastScrollTop - st) <= delta)
                return

            if (st > lastScrollTop && st > navbarHeight) { // Scroll Down
                $('#header .gnb_area').removeClass('nav_down').addClass('nav_up')
            } else { // Scroll Up
                if (st + $(window).height() < $(document).height()) {
                    $('#header .gnb_area').removeClass('nav_up').addClass('nav_down')
                }
            }
            if (st <= 237) {
                $('#header .gnb_area').removeClass('nav_down').addClass('nav_up')
            }

            lastScrollTop = st
        }

        // 메뉴 스크롤
        setInterval(function () {
            if (didScroll) {
                hasScrolled()  //스크롤 다운하면 숨김, 스크롤 업하면  보임
                didScroll = false
            }
        }, -250)
    </script>

    <!-- taboola광고 헤더코드  ask 2018.12.20 -->
    <script type="text/javascript">
        window._taboola = window._taboola || []
        _taboola.push({article: 'auto'})
        !function (e, f, u, i) {
            if (!document.getElementById(i)) {
                e.async = 1
                e.src = u
                e.id = i
                f.parentNode.insertBefore(e, f)
            }
        }(document.createElement('script'),
            document.getElementsByTagName('script')[0],
            '//cdn.taboola.com/libtrc/mt-moneys-desktop/loader.js',
            'tb_loader_script')
        if (window.performance && typeof window.performance.mark == 'function') {
            window.performance.mark('tbl_ic')
        }
    </script>

    
