        var iframe = document.getElementById('ifComment');
        if (isNaN(event.data)) return;
        var ifHeight = parseInt(event.data);
        console.log(ifHeight + 10);
        if (isNaN(ifHeight)) ifHeight = 240;
        iframe.height = ifHeight;
    }

    if (window.addEventListener) {
        window.addEventListener('message', resizeIframe, false);
    } else if (window.attachEvent) {
        window.attachEvent('onmessage', resizeIframe);
