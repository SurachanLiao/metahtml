(function(){
  var readCookie = function(cookieName) {var b = document.cookie.match('(^|;)\\s*' + cookieName + '\\s*=\\s*([^;]+)');return b ? b.pop() : ''; }
  window.geoCC=readCookie('geoCC'); var c = readCookie('euconsent');
  window.gdprApplies = (geoCC && /be|bg|cz|dk|de|ee|ie|el|es|fr|hr|it|cy|lv|lt|lu|hu|mt|nl|at|pl|pt|ro|si|sk|fi|no|se|gb|uk|unk/i.test(geoCC)) ? true: false;
  if (!gdprApplies || (c && c.indexOf('__') != -1)) { window.GDPR = 0; } /* consent */
  else { window.GDPR = 1; } /* noconsent */
  if (!gdprApplies || window.location.hostname == 'de.ign.com') { return; }
  window.googletag = window.googletag || {'cmd': []}; window.googletag.cmd.push(function() { window.googletag.pubads().setRequestNonPersonalizedAds(window.GDPR); console.log("NPA="+window.GDPR);  });
  (window.adsbygoogle=window.adsbygoogle||[]).requestNonPersonalizedAds=window.GDPR;
})();
</script>
  
  
  
