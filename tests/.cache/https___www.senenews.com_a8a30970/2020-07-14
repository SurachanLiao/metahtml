function fixHeaderNew(){
var $pullleft=$('.pull-left-mobile');
var $abonnezvousmontant=$('.abonnezvousmontant');
var $headbandlogo=$('#nwh');
var $headlogo=$('#nwh img');
var $menuPar=$('.menuPar');
var $livebox=$('.live-box');
var $logotext=$('.logotext');
if($(window).scrollTop() > 131||($(window).width() <=768&&$(window).scrollTop() > 50)){
$logotext.css({'font-size': '22px', 'margin': '14px 0'});
$menuPar.css({'display': 'none'});
if($(window).width() > 768){
$pullleft.css({'top': '-3px'});
$abonnezvousmontant.css({'display': 'none'});
$headbandlogo.css({'position': 'fixed','top': '0px','width': '100%','z-index': '100000',height:'50px'});
$livebox.css({'display': 'block'});
$livebox.css({'opacity': '1'});
}else{
$logotext.css({'text-align': 'left', 'margin-left': '54px'});
var $hamburger=$('#hamburger');
if($hamburger.hasClass('hamburger-activated')){
$hamburger.removeClass("hamburger-activated");
}
$headbandlogo.css({'position':'fixed','top': '0px','width':'100%'});
if($(window).scrollTop() > 200){
$livebox.css({'display': 'none'});
$livebox.css({'opacity': '0'});
}}
}else{
if($(window).width() > 768){
$pullleft.css({'top': '23px'});
$abonnezvousmontant.css({'display': 'block'});
$logotext.css({'font-size': '60px', 'margin': '30px 0', 'text-align': 'center', 'margin-left': 'unset'});
$headbandlogo.css({'position': 'relative','top': 'auto',height:'auto'});
$menuPar.css({'display': 'block'});
$livebox.css({'display': 'block'});
$livebox.css({'opacity': '1'});
}else{
$pullleft.css({'top': '-3px'});
$abonnezvousmontant.css({'display': 'none'});
$logotext.css({'font-size': '22px', 'margin': '14px 0', 'text-align': 'left', 'margin-left': '54px'});
$headbandlogo.css({'position': 'relative','top': 'auto'});
$menuPar.css({'display': 'none'});
$livebox.css({"display": "block"});$livebox.css({'opacity': '1'});
}}
}
$(window).scroll(fixHeaderNew);
fixHeaderNew();
});</script>
