var li_width = [195, 196, 196, 198, 197, 196];
var luxury_car_html = '';
luxury_car_html += '<div class="luxury_car">';
luxury_car_html += '<ul id="list_ul">';
for(var i = 0; i < luxury_car_data.length; i++) {
luxury_car_html += '<li><a href="" target="_blank" ><span class="logo"><img src="' + luxury_car_data[i][1] + '" ></span><span class="txt">' + luxury_car_data[i][0] + '</span></a></li>';
}
luxury_car_html += '</ul>';
luxury_car_html += '<div id="car_bar">'
for(var i = 0; i < luxury_car_data.length; i++) {

if(luxury_car_data[i][1] == "") {
luxury_car_html += '<div class="car_bar"><div class="car_wrap"><a target="_blank" href="' + luxury_car_data[i][4] + '"><img src="' + luxury_car_data[i][2] + '" width="1000" height="50"/></a></div></div>';
} else {

if(isLogin == 0 || isLogin == 'undefined') {
luxury_car_html += '<div class="car_bar"><div class="car_bar_l"><dl><dt><a href="' + luxury_car_data[i][6] + '" target="_blank"><img src="' + luxury_car_data[i][2] + '" /></a></dt></dl></div><div class="car_bar_r"><div class="car_wrap"><a target="_blank" href="' + luxury_car_data[i][4] + '"><img src="' + luxury_car_data[i][3] + '" width="1000" height="50"/></a></div></div></div>';
} else {
luxury_car_html += '<div class="car_bar"><div class="car_bar_l"><dl><dt><a href="' + luxury_car_data[i][6] + '" target="_blank"><img src="' + luxury_car_data[i][2] + '" /></a></dt><dd><p><a href="' + luxury_car_data[i][4] + '" target="_blank"><img src="' + luxury_car_data[i][3] + '" /></a></p><p>' +
(function() {
var uidArr = [];
var isGz = false;
for(var k = 0; k < gz_uid.length; k++) {
uidArr[k] = gz_uid[k].id;
}
for(var k = 0; k < uidArr.length; k++) {
if(luxury_car_data[i][3] == uidArr[k]) {
isGz = true;
break;
}
}
if(isGz) {
return '<img src="" />';
} else {
return '<a href="javascript:;" target="_blank" onclick="luxury_car_gz(' + luxury_car_data[i][5] + ',\'' + luxury_car_data[i][4] + '\')" id="luxury_gz_' + luxury_car_data[i][5] + '"><img src="" /></a>';
}

})() +
'</p></dd></dl></div><div class="car_bar_r"><div class="car_wrap"><a target="_blank" href=""><img src="" width="1000" height="50"/></a></div></div></div>';
}
}
}
luxury_car_html += '</div></div>';
document.getElementById("luxury_car").innerHTML = luxury_car_html;
var aLi = document.getElementById("list_ul").getElementsByTagName("li");
var oDiv = document.getElementById("car_bar");
var aCarBar = getByClass(oDiv, "car_bar");
var aCarWrap = getByClass(oDiv, "car_wrap");
var aImg = [];
for(var i = 0; i < aCarWrap.length; i++) {
aImg.push(aCarWrap[i].getElementsByTagName('img')[0]);
aLi[i].style.width = li_width[i] + 'px';
aLi[i].getElementsByTagName("a")[0].style.width = li_width[i] + 'px';
aLi[i].getElementsByTagName("a")[0].getElementsByTagName("span")[0].style.width = li_width[i] + 'px';
aLi[i].getElementsByTagName("a")[0].getElementsByTagName("span")[1].style.width = li_width[i] + 'px';
}

for(var i = 0; i < aLi.length; i++) {

aLi[i].index = i;

aLi[i].onmouseover = function() {
for(i = 0; i < aLi.length; i++) {
aLi[i].className = '';
aCarBar[i].style.display = 'none';
aImg[i].src = "";
aLi[i].getElementsByTagName("a")[0].href = luxury_car_data[i][4];
aCarWrap[i].getElementsByTagName("a")[0].href = luxury_car_data[i][4];
}
this.className = 'show';
aCarBar[this.index].style.display = 'block';
if(aImg[this.index].src != "http://d5.sina.com.cn/shh/lechan/20111227sina_car/images/pixal.png") {
aImg[this.index].src = luxury_car_data[this.index][3];
}

}
aLi[i].onmouseout = function() {
for(i = 0; i < aLi.length; i++) {
if(this.className = 'show') {
this.className = '';
aCarBar[this.index].style.display = 'none';
}
}
}
}
for(var i = 0; i < aCarBar.length; i++) {
aCarBar[i].index = i;
aCarBar[i].onmouseover = function() {
for(i = 0; i < aCarBar.length; i++) {
aCarBar[i].style.display = '';
aImg[i].src = "";
}
this.style.display = 'block';
aLi[this.index].className = 'show';
if(aImg[this.index].src != "http://d5.sina.com.cn/shh/lechan/20111227sina_car/images/pixal.png") {
aImg[this.index].src = luxury_car_data[this.index][3];
}

}
aCarBar[i].onmouseout = function() {
for(i = 0; i < aCarBar.length; i++) {
if(this.style.display = 'block') {
aLi[this.index].className = '';
this.style.display = 'none';
//aImg[i].src = "";
}
}
}
}

}
var getByClass = function(oParent, sClass) {
var parent = oParent || document;
var re = new RegExp('\\b' + sClass + '\\b');
var aEles = parent.getElementsByTagName('*');
var arr = [];
for(var i = 0; i < aEles.length; i++) {
if(re.test(aEles[i].className)) {
arr.push(aEles[i]);
}
}
return arr;
}

var getJson = function(url) {
var oScript = document.createElement('script');
oScript.type = 'text/javascript';
oScript.src = url;
document.getElementsByTagName('head')[0].appendChild(oScript);

}
var luxury_car_info = function(json) {
var isLogin = json.succ;
var gz_uid = json.result;
mainFunction(isLogin, gz_uid);
}

var luxury_car_gz = function(uid, monitor) {
var url = 'http://all.vic.sina.com.cn/201302auto/follow.php?uid=' + uid;
getJson(url);
var oImage = new Image();
oImage.src = monitor + '&_=' + new Date().getTime();
}

var luxury_car_follow = function(json) {
if(json.succ == 1) {
document.getElementById("luxury_gz_" + json.uid).parentNode.innerHTML = '<img src="//img.antpedia.com/images/ep/d2.sina.com.cn/468ac514cdabc2fd2cbf27c0acd3196e.png" />';
}
}
