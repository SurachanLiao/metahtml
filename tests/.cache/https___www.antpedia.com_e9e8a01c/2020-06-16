<script type="text/javascript">
//antopç¹èµåè¸©è¸©çåè½
function dianzan(type, info_id, obj) {
if(type == 1 || type == 2) {
$.ajax({
type: "post",
url: '/antopdeclare_action.php',
dataType: "json",
data: 'action=vote&vote=' + type + '&info_id=' + info_id,
success: function(m) {
if(m.type == 'ok') {
alert('æç¥¨æåï¼');
if(type == 1) { //å¦ææ¯ç¹èµçè¯ï¼åå¢å ä¸ç¥¨
var haspiao = parseInt($(obj).find("span").html());
haspiao = haspiao + 1;
$(obj).find("span").html(haspiao);
}
} else {
alert(m.msg);
}
}
});
} else {
alert("è¯·åæ³æä½ï¼");
}
}
</script>
