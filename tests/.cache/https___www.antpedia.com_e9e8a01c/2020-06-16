<script type="text/javascript">
window.onload = function() {
// é¦é¡µå·¦ä¾§Minisiteæ¨¡åä¸é¢å¹¿åä½è½®æ­Star
function $(param) {
if (arguments[1] == true) {
return document.querySelectorAll(param);
} else {
return document.querySelector(param);
}
}
var bannerBox = $(".bannerBox");
var bannerBox1 = $(".bannerBox-1 ul li", true);
var bannerBox2 = $(".bannerBox-2 ul");
var length = bannerBox1.length;

var str = "";
for (var i = 0; i < length; i++) {
if (i == 0) {
str += "<li class='on'>" + (i + 1) + "</li>";
} else {
str += "<li>" + (i + 1) + "</li>";
}
}
bannerBox2.innerHTML = str;

var current = 0;

var timer;
timer = setInterval(go, 3000);

function go() {
for (var j = 0; j < length; j++) {
bannerBox1[j].style.display = "none";
bannerBox2.children[j].className = "";
}
if (length == current) {
current = 0;
}
bannerBox1[current].style.display = "block";
bannerBox2.children[current].className = "on";
current++;
}

for (var k = 0; k < length; k++) {
bannerBox1[k].onmouseover = function() {
clearInterval(timer);
}
bannerBox1[k].onmouseout = function() {
timer = setInterval(go, 3000);
}
}

for (var u = 0; u < length; u++) {
bannerBox2.children[u].index = u;
bannerBox2.children[u].onmouseover = function() {
clearInterval(timer);
for (var j = 0; j < length; j++) {
bannerBox1[j].style.display = "none";
bannerBox2.children[j].className = "";
}
this.className = "on";
bannerBox1[this.index].style.display = "block";
current = this.index + 1;
}
bannerBox2.children[u].onmouseout = function() {
timer = setInterval(go, 3000);
}
}

function back() {
for (var j = 0; j < length; j++) {
bannerBox1[j].style.display = "none";
bannerBox2.children[j].className = "";
}
if (current == 0) {
current = length;
}
bannerBox1[current - 1].style.display = "block";
bannerBox2.children[current - 1].className = "on";
current--;
}
// é¦é¡µå·¦ä¾§Minisiteæ¨¡åä¸é¢å¹¿åä½è½®æ­End


// é¦é¡µä¸­é´é¡¶é¨å¹¿åä½è½®æ­ç¹æStar
function $(param01) {
if (arguments[1] == true) {
return document.querySelectorAll(param01);
} else {
return document.querySelector(param01);
}
}
var banner01Box = $(".banner01Box");
var banner01Box1 = $(".banner01Box-1 ul li", true);
var banner01Box2 = $(".banner01Box-2 ul");
var length01 = banner01Box1.length;

var str01 = "";
for (var ii = 0; ii < length01; ii++) {
if (ii == 0) {
str01 += "<li class='on'>" + (ii + 1) + "</li>";
} else {
str01 += "<li>" + (ii + 1) + "</li>";
}
}
banner01Box2.innerHTML = str01;

var current01 = 0;

var timer01;
timer01 = setInterval(goo, 3000);

function goo() {
for (var jj = 0; jj < length01; jj++) {
banner01Box1[jj].style.display = "none";
banner01Box2.children[jj].className = "";
}
if (length01 == current01) {
current01 = 0;
}
banner01Box1[current01].style.display = "block";
banner01Box2.children[current01].className = "on";
current01++;
}

for (var kk = 0; kk < length01; kk++) {
banner01Box1[kk].onmouseover = function() {
clearInterval(timer01);
}
banner01Box1[kk].onmouseout = function() {
timer01 = setInterval(goo, 3000);
}
}

for (var uu = 0; uu < length01; uu++) {
banner01Box2.children[uu].index = uu;
banner01Box2.children[uu].onmouseover = function() {
clearInterval(timer01);
for (var jj = 0; jj < length01; jj++) {
banner01Box1[jj].style.display = "none";
banner01Box2.children[jj].className = "";
}
this.className = "on";
banner01Box1[this.index].style.display = "block";
current01 = this.index + 1;
}
banner01Box2.children[uu].onmouseout = function() {
timer01 = setInterval(goo, 3000);
}
}

function back01() {
for (var jj = 0; jj < length01; jj++) {
banner01Box1[jj].style.display = "none";
banner01Box2.children[jj].className = "";
}
if (current01 == 0) {
current01 = length01;
}
banner01Box1[current01 - 1].style.display = "block";
banner01Box2.children[current01 - 1].className = "on";
current01--;
}
// é¦é¡µä¸­é´é¡¶é¨å¹¿åä½è½®æ­ç¹æEnd
}
</script>


