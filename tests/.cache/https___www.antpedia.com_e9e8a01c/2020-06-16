//æç´¢ä¸æèæ³åè½
var currid = 0;/*å½åå¤äºçèæ³ç»æä½ç½® ID , 0å·è¾å¥ä¸ºè¾å¥æ¡ ,1~N ä¸ºè¾å¥æ¡ä¸å¯éæ©çé¾æ¥*/
var id_range = 0;/*ä¸å±æå¤å°ä¸ªèæ³ç»æ*/
var t=1;

function setsearchtype(k){
t = k;
}
function skey_up(){
var newid = currid -1;
if(newid ==0){
newid = id_range;
}
$("a#skwlink"+ newid).css('background-color','#ccc');
$("a#skwlink"+ currid).css('background-color','#fff');
$("#key").val($("a#skwlink"+newid).text());
currid = newid;	

}

function skey_down(){
var newid = currid+1;
if(newid > id_range) {
newid = 1;
}
$("a#skwlink"+ newid).css('background-color','#ccc');
$("a#skwlink"+ currid).css('background-color','#fff');
$("#key").val($("a#skwlink"+newid).text());
currid = newid;	
}

function cleanmoredetail(){
$("div#moredetail").html('');
currid=0;
$("div#moredetail").css("display","none");
}

$(document).ready(function(){
$("#key").blur(function(){setTimeout(cleanmoredetail,500)});
$("#key").keyup(function(event){
var key_code = event.keyCode;
if(key_code == 13){	
$("#searchbtn").click();
return ;
}
if(key_code == 38){/*ä¸*/
skey_up();

return;
}
if(key_code == 40 ){/*ä¸ */
skey_down();
return;
}
$.ajax({
url: "//s.antpedia.com/keyword_for_other.php?t="+t+"&q="+$("#key").val(),
data:"{}",
type: "GET",
dataType: "jsonp",
jsonpCallback:"faces",
cache: true,
success: function(json){
if(json.length >3){
$("#moredetail").html('');
currid = 0;
for(i = 0;i<json.length;i++){
$("#moredetail").append('<a class="skwclass" id="skwlink'+(i+1)+'" href="//s.antpedia.com/'+t+'/'+json[i]+'" target=_blank>'+json[i]+'</a>');
}
$("#moredetail").css("display","");
}else{
$("#moredetail").html('');
$("#moredetail").css("display","none");
}
id_range = json.length;	
   },
error: function(e){
}
});
})
});
</script>
<script type="text/javascript">
/*ä¸é¢æ¯å¯¹èå¹¿åçjsä»£ç */
$(document).ready(function() {
var duilian = $("div.duilian");
var duilian_close = $("a.duilian_close");

var window_w = $(window).width();
if(window_w > 1000) {
duilian.show();
}
$(window).scroll(function() {
var scrollTop = $(window).scrollTop();
duilian.stop().animate({
top: scrollTop + 200
});
});
duilian_close.click(function() {
$(this).parent().hide();
return false;
});
});

