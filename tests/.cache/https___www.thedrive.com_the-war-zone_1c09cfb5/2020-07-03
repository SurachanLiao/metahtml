s(onlyVisible) {var adunits = [];var allSlots = [];Object.keys(feAdSlotsConfig).forEach(function(adSlotKey) {var slotObj = feAdSlotsConfig[adSlotKey];if (!slotObj || !slotObj.definedDivIds || slotObj.definedDivIds.length < 1) {console.log("No slot configuration found for " + adSlotKey);return;}var visibleDivs = [];if (onlyVisible) {var foundVisible = false;console.log("Checking visibility on " + adSlotKey, slotObj);slotObj.definedDivIds.forEach(function(definedDivId) {var totalVisibleTime = slotObj.viewability[definedDivId].totalViewableTime;if ( slotObj.viewability[definedDivId].viewableSince != null )totalVisibleTime += ((new Date()).getTime() - slotObj.viewability[definedDivId].viewableSince) / 1000;console.log("Total Visible Time For " + adSlotKey + " on " + definedDivId + ": " + totalVisibleTime);if (totalVisibleTime >= 30) {console.log("Adding " + adSlotKey + " on " + definedDivId + " to refresh queue");foundVisible = true;visibleDivs.push(definedDivId);/* Reset the counter */feAdSlotsConfig[adSlotKey].viewability[definedDivId] = {totalViewableTime: 0, viewableSince: null};}});if (!foundVisible) {return;}} else {slotObj.definedDivIds.forEach(function(definedDivId) {visibleDivs.push(definedDivId);});}var bids = buildBids(slotObj, adSlotKey);visibleDivs.forEach(function(visibleDivId) {var slot = slotObj.slots[visibleDivId];if ( slot ) {allSlots.push(slot);var adunit = {code: visibleDivId,mediaTypes: {banner: {sizes: slotObj.sizes}},bids: bids};adunits.push(adunit);}});});return {adunits: adunits, slots: allSlots};}function buildBidConfigForDiv(adSlotId, divId) {var slotObj = feAdSlotsConfig[adSlotId];if ( !slotObj ) {console.log("No ad slot found for " + adSlotId);return;}if ( !slotObj.slots[divId] ) {console.log("No matching div " + divId + " for ad slot " + adSlotId);return;}return {adunits: [{code: divId,mediaTypes: {banner: {sizes: slotObj.sizes}},bids: buildBids(slotObj, adSlotId)}],slots: [slotObj.slots[divId]]}}function prebidCallback(slots, isFirst) {googletag.cmd.push(function() {var refreshingFooter = false;pbjs.setConfig({priceGranularity: "high"});pbjs.setTargetingForGPTAsync();if ( ENABLE_AMAZON ) { apstag.setDisplayBids(); }feAdConfig.log('PBJS Init Ad Server - Refresh');console.log("Loading " + slots.length + " slots", slots);slots.forEach(function (slot) {if ( adSlotTargets.aid ) {slot.setTargeting("article", adSlotTargets.aid );} else if ( adSlotTargets.ch ) {slot.setTargeting("article", adSlotTargets.ch );}if ((segment && segment.contentCmsCategory && segment.contentCmsCategory === 'The War Zone') ||window.location.pathname.match(/^\/the-war-zone/)) {slot.setTargeting("site_section", "the-war-zone" );} else {slot.setTargeting("site_section", "other" );}var contentKeyword = "other";if ( segment && segment.contentCmsTerms ) {if (segment.contentCmsTerms.includes("fighter jets") ||segment.contentCmsTerms.includes("b-21") ||segment.contentCmsTerms.includes("f-35") ||segment.contentCmsTerms.includes("b61") ||segment.contentCmsTerms.includes("b-1b") ||segment.contentCmsTerms.includes("f-15") ||segment.contentCmsTerms.includes("f-15e") ||segment.contentCmsTerms.includes("mig-29") ||segment.contentCmsTerms.includes("mig 29") ||segment.contentCmsTerms.includes("cessna") ||segment.contentCmsTerms.includes("usaf") ||segment.contentCmsTerms.includes("airplanes") ||segment.contentCmsTerms.includes("bombers")) {contentKeyword = "airplane";}}slot.setTargeting("content_keyword", contentKeyword);if (isFirst) {slot.setTargeting("refresh", "false");} else {slot.setTargeting("refresh", "true");}if ( slot.getAdUnitPath() === "/21817035599/Desktop_Tablet_Sticky_Footer"|| slot.getAdUnitPath() === '/21817035599/Mobile_Sticky_Footer') {refreshingFooter = slot.getAdUnitPath();}});googletag.pubads().refresh(slots);if (refreshingFooter) {setTimeout(function() { refreshAd(refreshingFooter, feAdSlotsConfig[refreshingFooter].divId); },AD_REFRESH_TIME_SECONDS * 1000);}});}function refreshAd(adSlotId, divId) {if ( noRefreshAdUnits && noRefreshAdUnits.includes(divId) ) {console.log("Skipping refresh on ad unit with Direct campaign");return;}console.log("Refreshing a single ad: " + adSlotId + ", " + divId);if ( adSlotId.match("Desktop_Tablet_Sticky_Footer") && document.visibilityState && document.visibilityState !== 'visible' ) {console.log("Delaying sticky footer refresh until visible again");setTimeout(function() { refreshAd(adSlotId, divId); }, 5000);} else {pbjs.que.push(function () {var unitsAndDivs = buildBidConfigForDiv(adSlotId, divId);if (ENABLE_AMAZON) {apstag.fetchBids({slots: [{slotID: divId, sizes: feAdSlotsConfig[adSlotId].sizes, slotName: adSlotId}],timeout: PREBID_TIMEOUT});}pbjs.requestBids({adUnits: unitsAndDivs['adunits'],bidsBackHandler: function () {prebidCallback(unitsAndDivs['slots']);},timeout: PREBID_TIMEOUT});});}}</script></head><body class="main layoutName-channel the-war-zone-channel  contributor-channel "><header class="page-header" id="nav"><a title="The Drive" class="page-logo" href="/" data-analytics="Header Page Logo"></a><nav class="page-header-nav"><ul><li><a class="page-header-nav-pinned the-war-zone-nav-item--dark" href="/the-war-zone" data-analytics="Header The War Zone">The War Zone</a></li><li><a class="page-header-nav-pinned motorcycles-nav-item--dark" href="/motorcycles" data-analytics="Header Motorcycles">Motorcycles</a></li><li><a class="page-header-nav-pinned reviews-nav-item--dark" href="/reviews" data-analytics="Header Reviews">Reviews</a></li><li><a class="page-header-nav-pinned car-warranty-nav-item--dark" href="/car-warranty" data-analytics="Header Car Warranties">Car Warranties</a></li><!--<li><a class="page-header-nav-pinned shop-nav-item" href="/shop" data-analytics="Header Shop">Shop</a></li>--><div class="navCategories"></div><li class="page-header-nav-dropdown"><a id="mobile-header-search-button" class="mobile-page-header-search-button" data-analytics="Header Search" href="/search"><i class="icon icon-search"></i></a><a class="page-header-nav-mo