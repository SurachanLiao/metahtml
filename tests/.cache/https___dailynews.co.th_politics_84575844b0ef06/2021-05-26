<script>
	$(window).load(function(){
		var testLink= $(location).attr('href'); 
		console.log(testLink);

		var removed = false;
		var checkAd = setInterval(delAd, 100);
		function delAd() {
			if(testLink == 'https://www.dailynews.co.th/women/809339' || testLink == 'https://www.dailynews.co.th/women/809339/'){
				//console.log('pass');
				//$('.AdAsia_mid').size()
				if(!removed || $('.AdAsia_mid').size() > 0){
					$('.AdAsia_mid').remove();
					removed = true;
				}else{
					clearInterval(checkAd);
				}
			}	
		}

	});
</script>


