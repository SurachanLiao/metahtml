<script>
$(function(){

	$("#news-article iframe").css("max-width","100%");

  var $slideObj = $('#article-slide').children('.owl-carousel');
  var slideNum = $slideObj.children().length;

  var $asidVId = $('#aside-vdo').children('.content-box').children('.owl-carousel');
  var asidVIdNum = $asidVId.children().length;

  if(slideNum > 1 ){

    $slideObj.owlCarousel({
      autoplay: true,
      loop:true,
      items: 1,
      nav:true,
      video:true,
      navText: ['<i class="fa fa-chevron-left"></i>','<i class="fa fa-chevron-right"></i>'],
      onPlayVideo: function(){
        $slideObj.trigger('stop.owl.autoplay');
      },
      onStopVideo: function(){
        $slideObj.trigger('play.owl.autoplay');
      },
    });

  }else{

    $slideObj.addClass('single');
    $slideObj.owlCarousel({
      autoplay: false,
      loop: false,
      items: 1,
      nav: false,
      dots: false,
      video: true
    });

  }//end if

  if(asidVIdNum > 1 ){

    $asidVId.owlCarousel({
      loop:true,
      items: 1,
      nav: true,
      dots: false,
      autoplay: true,
      video: true,
      navText: ['<i class="fa fa-chevron-left"></i>','<i class="fa fa-chevron-right"></i>'],
      onPlayVideo: function(){
        $asidVId.trigger('stop.owl.autoplay');
      },
      onStopVideo: function(){
        $asidVId.trigger('play.owl.autoplay');
      },
    });

  }else{

    $asidVId.owlCarousel({
      loop:false,
      items: 1,
      nav: false,
      dots: false,
      autoplay: false,
      video: true,
      navText: ['<i class="fa fa-chevron-left"></i>','<i class="fa fa-chevron-right"></i>']
    });

  }

  //auto adjust video height for owl carousel (if any)
  //owlCarousel_autoAdjustVideoHeight();

});
 
function ajax_save(url) 
{
	var xhttp = new XMLHttpRequest();
	xhttp.onreadystatechange = function() 
	{
		if (xhttp.readyState == 4 && xhttp.status == 200) 
		{
			//alert(xhttp.responseText);
		}
	};
	xhttp.open("GET",url, true);
	xhttp.send();
}

var main_share_fb_count = 0;
$.getJSON('https://graph.facebook.com?id=https://www.dailynews.co.th/politics/845758', function(data){  
	 console.log(data.share.share_count);
	 main_share_fb_count = data.share.share_count;
}); 

function share_fb_lb()
{ 
	window.open('https://www.facebook.com/sharer/sharer.php?u=https://www.dailynews.co.th/politics/845758','share_fb_open','resizable=yes, scrollbars=no, titlebar=no, width=700, height=300, top=0, left=0');	

	ajax_save("/page/save_share/845758/1/0/politics/"+main_share_fb_count);
} 
function share_tw_lb()
{ 
	window.open('https://twitter.com/intent/tweet?counturl=https://www.dailynews.co.th/politics/845758;text=แห่แชร์ว่อน!หนุ่มโพสต์รูปแป้งในมือ ต่อหน้า\'ธรรมนัส\' ;url=https://www.dailynews.co.th/politics/845758;via=dailynewstwit','share_tw_open','resizable=yes, scrollbars=no, titlebar=no, width=700, height=443, top=0, left=0');	

	ajax_save("/page/save_share/845758/2/0/politics");
} 
function share_gl_lb()
{
	window.open('https://plus.google.com/share?url=https://www.dailynews.co.th/politics/845758','share_gl_open','resizable=yes, scrollbars=no, titlebar=no, width=530, height=460, top=0, left=0');	

	ajax_save("/page/save_share/845758/3/0/politics");
}

var xhttp_view = new XMLHttpRequest();
xhttp_view.onreadystatechange = function() 
{
	if (xhttp_view.readyState == 4 && xhttp_view.status == 200) 
	{
		//alert(xhttp_view.responseText); 
		var res = xhttp_view.responseText.split("."); 
		
					document.getElementById('span_reader_ajax').innerHTML = "จำนวนคนอ่าน " + res[0] + " คน";
			 
	}
};
xhttp_view.open("GET","/page/load_view/845758/", true);
xhttp_view.send();

//ajax_save("/page/save_view/845758/");  
