        <div class="checkbox ">
            <label>
                <input type="checkbox" name="filters" value="2021" >
                Talous- ja rahoitusasioiden neuvosto
            </label>
        </div>
        <div class="checkbox ">
            <label>
                <input type="checkbox" name="filters" value="2026" >
                Ty&#246;llisyys-, sosiaalipolitiikka-, terveys- ja kuluttaja-asioiden neuvosto
            </label>
        </div>
        <div class="checkbox ">
            <label>
                <input type="checkbox" name="filters" value="2025" >
                Ulkoasiainneuvosto
            </label>
        </div>
