    <script>
      var googletag = googletag || {};
      googletag.cmd = googletag.cmd || [];

      googletag.cmd.push(function() {
        googletag.pubads().disableInitialLoad();
      });

      //Funcion setConfig es equivalente a la funcion sizeMapping
      pbjs.que.push(function() {
        pbjs.addAdUnits(adUnits);
        pbjs.setConfig({
          sizeConfig: [{
            'mediaQuery': '(min-width: 1024px)',
            'sizesSupported': desktopSizes,
            'labels': ['desktop']
          }, {
            'mediaQuery': '(min-width: 768px) and (max-width: 1023px)',
            'sizesSupported': landscapeTablet,
            'labels': ['portrait']
          }, {
            'mediaQuery': '(min-width: 615px) and (max-width: 767px)',
            'sizesSupported': portraitTablet,
            'labels': ['landscape']
          }, {
            'mediaQuery': '(min-width: 320px) and (max-width: 614px)',
            'sizesSupported': phoneSizes,
            'labels': ['phone']
          }],
          priceGranularity: "dense"
        });

        pbjs.requestBids({
          bidsBackHandler: initAdserver,
          timeout: PREBID_TIMEOUT
        });
      });

      function initAdserver() {
        if (pbjs.initAdserverSet) return;
        pbjs.initAdserverSet = true;
        googletag.cmd.push(function() {
          pbjs.que.push(function() {
            pbjs.setTargetingForGPTAsync();
            googletag.pubads().refresh();
          });
        });
      }
      // in case PBJS doesn't load
      setTimeout(function() {
        initAdserver();
      }, FAILSAFE_TIMEOUT);
    </script>
