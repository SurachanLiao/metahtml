<section class="author soft-line clear-float update-top woauthor" itemprop="author" itemscope itemtype="https://schema.org/Person">
<meta itemprop="name" content="BT.com"/>
<div class="figure-txt">
<!-- setting like constraint value as BTFeedAuthor -->
<!-- getting site id -->
<!-- To get only BTFeedAuthor assets setting like constraint -->
<div class="image">
<img src="/images/press-association-136403946873306901-170613163228.jpg" alt="Press Association" title="Press Association" style='width:28px;height:28px'/>
</div>
<span class="small-gry-txt">Last updated: 14 October 2014, 12:07 BST</span>
</div>
<!-- print -->
<div class="print new-art-print">
<a href="#" onClick="window.print();return false;">Print this story</a>
</div>
