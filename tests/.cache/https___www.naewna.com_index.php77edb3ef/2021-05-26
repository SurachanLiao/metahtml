							FROM 	cms_crimeslide h
							INNER JOIN cms_news n ON h.news_id = n.news_id
							WHERE n.status='enable' && n.category='local' && subcategory = 'crime'
							&& n.thumb<>'' && (NOW() BETWEEN n.startdate AND n.stopdate)  && subcategory='crime' AND n.news_id NOT IN(575865,575860,575855,575854)
							ORDER BY ISNULL(h.orderid), h.orderid, n.news_id DESC
							LIMIT 0, 7-->				
