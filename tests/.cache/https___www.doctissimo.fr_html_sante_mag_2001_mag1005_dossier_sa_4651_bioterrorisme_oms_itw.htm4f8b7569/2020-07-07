        ||tc_vars.cm_rubrique==='EveilSens' // 31/12/2020
        ||tc_vars.level3==='change-de-bebe' // 31/12/2020
    );

    var trafficing_exclu_full = (
        tc_vars.level3==='charte-donnees-personnelles-cookies'
        ||tc_vars.level3==='charte-donnees-personnelles-et-cookies-forum'
