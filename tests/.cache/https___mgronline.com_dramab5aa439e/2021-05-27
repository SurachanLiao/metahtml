    <script language="javascript" type="text/javascript">
        (function() {
            var detectInterval = null;
            detectInterval = setInterval(function() {
                var check_ = false;
                /* if (_socialSDKStatusFbAsyncInit && _jwplayerJssdkLoaded && _socialSDKStatusYouTubeAsyncInit) {
                    check_ = true;
                } */
                if (typeof $ === 'function') {
                    if (typeof $.browser === 'object') { check_ = true; }
                }
                if (check_ && (typeof Factory === 'function')
                    && (typeof IEWindowResize === 'function') && (typeof DeviceSizeView === 'function')
                    && (typeof EllipsisText === 'function')) {
                    clearInterval(detectInterval);
                    detectInterval = null;

                    $(document).ready(function() {
                        var properties_ = {  
                            resize_: {
                                timer_: null,
                                delay_: 250,
                                size_: {
                                    width_: 0,
                                    height_: 0
                                }
                            }
                        };
                        var factoryUtil = new Factory();
                        
                        var gbFSData = factoryUtil.globalFontStyles(); // 14-02-2563 12:09 à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font
                        
                        function initial() {
                            // console.log('----------------____');
                            var browserLib = $.browser;
                            var deviceSizeViewLib = new DeviceSizeView({item_: $('div#p-device-size-view')});
                            var ellipsisTextLib = new EllipsisText({deviceSizeViewLib: deviceSizeViewLib});
                            var dramaListsItem = $('ul.drama-lists');

                            var ieWindowResizeLib = null;
                            function resizeTimer(_argument) {
                                var resize_ = properties_.resize_;
                                if (resize_.timer_ != null) {
                                    clearTimeout(resize_.timer_);
                                    resize_.timer_ = null;
                                }
                                if (_argument != undefined) {
                                    if (_argument.start_) {
                                        resize_.timer_ = setTimeout(function() {
                                            resize_.timer_ = null;
                                            if (_argument.beforeResize) { _resize(); }
                                            else { resize(); }
                                        }, resize_.delay_);
                                    }
                                }
                            }

                            function _resize() {
                                resizeTimer();
                                var resize_ = properties_.resize_;
                                var window_ = $(window);
                                var windowSize = {width_: window_.width(), height_: window_.height()};
                                // console.log('Home resize ' + $(window).width());
                                if ((resize_.size_.width_ != windowSize.width_) || (resize_.size_.height_ != windowSize.height_)) {
                                    if (resize_.size_.width_ != windowSize.width_) {
                                        deviceSizeViewLib.execute();
                                        // var deviceView = deviceSizeViewLib.view();
                                    }
                                    resizeTimer({start_: true});
                                }
                            }

                            $(window).on('resize orientationchange', function() {
                                if (ieWindowResizeLib != null) {
                                    ieWindowResizeLib.stop();
                                    ieWindowResizeLib = null;
                                }
                                resizeTimer({start_: true, beforeResize: true});
                            });

                            if (browserLib.name == 'msie') {
                                ieWindowResizeLib = new IEWindowResize({callBack: _resize});
                                ieWindowResizeLib.start();
                            }

                            function resize() {
                                resizeTimer();
                                var resize_ = properties_.resize_;
                                var window_ = $(window);
                                var windowSize = {width_: window_.width(), height_: window_.height()};
                                if ((resize_.size_.width_ != windowSize.width_) || (resize_.size_.height_ != windowSize.height_)) {
                                    if (resize_.size_.width_ != windowSize.width_) {
                                        /* à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font 14-02-2563 12:09 */
                                        deviceSizeViewLib.execute();
                                        var deviceView2 = deviceSizeViewLib.view2();
                                        var line = 2;
                                        if (gbFSData.type == 0) {
                                            if (deviceView2 != 'xs') { line = 3; }
                                        }
                                        /* à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font 14-02-2563 12:09 */
                                        ellipsisTextLib.execute({
                                            item_: dramaListsItem,
                                            // line: 2
                                            // line: 3 // à¸à¸£à¸±à¸à¸à¸à¸²à¸à¸ à¸²à¸à¹à¸à¸ List 28-01-2563 12:30
                                            line: line // à¸à¸£à¸±à¸à¸£à¸¹à¸à¹à¸à¸ font 14-02-2563 12:09
                                            // callBack: function(data_) {
                                                // console.log(data_);
                                            // }
                                        });
                                        resize_.size_.width_ = windowSize.width_;
                                    }
                                    if (resize_.size_.height_ != windowSize.height_) {
                                        resize_.size_.height_ = windowSize.height_;
                                        // console.log(windowSize.height_);
                                    }
                                }
                            }
                            resize();
                        } // end function initial

                        factoryUtil.readyInitial({
                            item_: $('div.image-guide-ready'),
                            callBack: function() { initial(); }
                        });
                    });
                }
            }, 250);
        })();
    </script>
