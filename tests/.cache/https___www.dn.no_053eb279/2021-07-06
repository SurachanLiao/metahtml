                <script>
                  googletag.cmd.push(function () {
                    var contentId = "googlead-sticky-right-2";
                    var slotUrl = "/21646926696/dn.no/sideboard_frontpage_right_2";
                    var slot = slots[contentId];
                    var position = '';
                    window.adPos++;
            
                    if (slotUrl !== '')
                      var position = '' + '_' + slotUrl.substring(slotUrl.lastIndexOf("/") + 1) + '_' + window.adPos;
            
                      googletag.cmd.push(function () {
                      slot.setTargeting('dn_position', position);
                      googletag.display(contentId);
                      googletag.pubads().refresh([slot]);
                    });
                  });
                </script>
              </div>
          </div>
