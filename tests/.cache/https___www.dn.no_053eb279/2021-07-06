Grid%22%3A%22%22%2C%22showOnTablet%22%3Atrue%2C%22showOnMobile%22%3Atrue%2C%22showOnDesktop%22%3Atrue%2C%22async%22%3Atrue%2C%22pixelDistance%22%3A200%7D%2C%22googlead-sem-johnsen-profier-frontbanner-2%22%3A%7B%22title%22%3A%226%3A%20ANNONSE-Sem%20%26%20Johnsen%20%2B%20Profier%20Frontbanner%202%22%2C%22id%22%3A%22googlead-sem-johnsen-profier-frontbanner-2%22%2C%22type%22%3A%22sphynx-ad%22%2C%22slotUrl%22%3A%22%2F21646926696%2Fdn.no%2Ffrontbanner_eiendom%22%2C%22size%22%3A%22%5B%5B2%2C%202%5D%2C%20%5B300%2C%20250%5D%2C%20%5B320%2C%20250%5D%2C%20%5B320%2C265%5D%2C%20%5B320%2C%20320%5D%2C%20%5B320%2C%20400%5D%2C%20%5B320%2C%20480%5D%2C%20%5B336%2C%20280%5D%2C%20%5B580%2C%20400%5D%2C%20%5B580%2C%20500%5D%2C%20%5B728%2C%2090%5D%2C%20%5B980%2C%20299%5D%2C%20%5B980%2C%20300%5D%2C%20%5B980%2C600%5D%5D%22%2C%22sizeMapping%22%3A%5B%7B%22slotSize%22%3A%22%5B%5B2%2C%202%5D%2C%20%5B970%2C%20250%5D%2C%20%5B980%2C%20299%5D%2C%20%5B980%2C%20300%5D%2C%20%5B980%2C%20600%5D%5D%22%2C%22browserSize%22%3A%22%5B900%2C%200%5D%22%7D%2C%7B%22slotSize%22%3A%22%5B%5B2%2C%202%5D%2C%20%5B580%2C%20400%5D%2C%20%5B580%2C%20500%5D%2C%20%5B728%2C%2090%5D%2C%20%5B980%2C%20299%5D%5D%22%2C%22browserSize%22%3A%22%5B600%2C%200%5D%22%7D%2C%7B%22slotSize%22%3A%22%5B%5B2%2C%202%5D%2C%20%5B300%2C%20250%5D%2C%20%5B320%2C%20250%5D%2C%20%5B320%2C265%5D%2C%20%5B320%2C%20320%5D%2C%20%5B320%2C%20400%5D%2C%20%5B320%2C%20480%5D%2C%20%5B336%2C%20280%5D%5D%22%2C%22browserSize%22%3A%22%5B320%2C%200%5D%22%7D%2C%7B%22slotSize%22%3A%22%5B%5B2%2C%202%5D%2C%20%5B300%2C%20250%5D%2C%20%5B320%2C%20250%5D%2C%20%5B320%2C265%5D%2C%20%5B320%2C%20320%5D%2C%20%5B320%2C%20400%5D%2C%20%5B320%2C%20480%5D%2C%20%5B336%2C%20280%5D%5D%22%2C%22browserSize%22%3A%22%5B0%2C0%5D%22%7D%5D%2C%22placeholder%22%3A%22lantern_placeholder_ad%22%2C%22disabled%22%3Afalse%2C%22desktopSize%22%3A%22100%25xauto%22%2C%22tabletSize%22%3A%22100%25xauto%22%2C%22mobileSize%22%3A%22300x250%22%2C%22desktopGrid%22%3A%22full%22%2C%22tabletGrid%22%3A%22full%22%2C%22showOnTablet%22%3Atrue%2C%22showOnMobile%22%3Atrue%2C%22showOnDesktop%22%3Atrue%2C%22async%22%3Atrue%2C%22pixelDistance%22%3A200%7D%7D";
      var ads = {};
      var slots = {};
      window.adPos = 0;
      var cookieNotificationStatus = window.nhst.cookieMessageShown && !window.nhst.shouldCheckIplogin ? 'Lukk' : '';
    
      var isValid = function (placeholder) {
        if (!placeholder.disabled && placeholder.slotUrl !== ''
          && typeof placeholder.size !== '')
          return true;
        return false;
      };
    
      var parseVal = function (val) {
        var value = val;
        try {
          value = JSON.parse(val);
        } catch (e) {
        }
        return value;
      };
    
      if (typeof placeholders !== 'undefined') {
    
        try {
          ads = JSON.parse(decodeURIComponent(placeholders));
        } catch (e) {
        }
    
        //if(!(Object.keys(ads).length === 0 && ads.constructor === Object)) {
        googletag.cmd.push(function () {
          $.each(ads, function (i, value) {
    
            if (value.type === 'sphynx-ad' && isValid(value)) {
    
              var slot = googletag.defineSlot(value.slotUrl, parseVal(value.size), value.id)
                .addService(googletag.pubads());
    
              if (value.sizeMapping.length > 0 && value.sizeMapping[0] !== null) {
                var mappingBuilder = googletag.sizeMapping();
                value.sizeMapping.forEach(function (element) {
                  mappingBuilder.addSize(parseVal(element.browserSize), parseVal(element.slotSize));
                });
                slot.defineSizeMapping(mappingBuilder.build());
              }
              slots[value.id] = slot;
            }
          });
    
          googletag.pubads().setTargeting('mode', 'prod');
          googletag.pubads().setTargeting('dn_loginstatus', loginstatus);
          googletag.pubads().setTargeting('dn_section', ["forsiden"])
          googletag.pubads().setTargeting("dn_topic", [])
          googletag.pubads().setTargeting("dn_keyword", []);
          googletag.pubads().setTargeting("dn_cookieinfo", cookieNotificationStatus);
          if (typeof AamGpt !== 'undefined') {
              if (typeof AamGpt.getCookie("gpt_aamsegments") != "undefined") {
              googletag.pubads().setTargeting(AamGpt.getKey("gpt_aamsegments"), AamGpt.getValues("gpt_aamsegments"));
            }
          }
          googletag.pubads().disableInitialLoad();
          googletag.pubads().setCentering(true);
          googletag.pubads().collapseEmptyDivs();
          googletag.pubads().enableLazyLoad({
            fetchMarginPercent: 150,
            renderMarginPercent: 75,
            mobileScaling: 2.0
          });
          googletag.enableServices();
    
          googletag.pubads().addEventListener('slotRenderEnded', function (event) {
            var labelToHideAdContainerIds = ["googlead-inline-artikkelbanner-first",
              "googlead-toppbanner-mobile"];
            if (labelToHideAdContainerIds.includes(event.slot.getSlotElementId())) {
              if (!event.isEmpty) {
                $('#' + event.slot.getSlotElementId()).siblings('label.inline-ad__label').removeClass('is-hidden');
                $('#' + event.slot.getSlotElementId()).closest('.mobile-top-ad').removeClass('is-hidden');
              }
            }
    
            try {
              var parallaxLen = $('#' + event.slot.getSlotElementId()).parent().find('.flyingCarpetHolder').length;
              if (parallaxLen > 0) {
                var containerName = '.flyingCarpetHolder';
                var myElement = $('#' + event.slot.getSlotElementId()).parent().find(containerName);
                var elementWatcher = scrollMonitor.create(myElement, 200);
                var target = $(".dnno-container__ad-sticky-content");
                elementWatcher.enterViewport(function() {
                  target.addClass("dnno-container__ad-sticky-content-hide");
                });
                elementWatcher.exitViewport(function() {
                  target.removeClass("dnno-container__ad-sticky-content-hide");
                });
              }
            } catch (err) {
              console.error('Unable to set up watcher:::', err);
            }
          });
        });
        //}
      }
    
    </script>


    <!--Page Auto-refresh Analytics Script-->
    <script type="text/javascript">
        if(typeof localStorage !== 'undefined') {
          var refreshScriptFlagCONST = 'refreshScriptFlag';
          if (localStorage.getItem(refreshScriptFlagCONST) === 'true') {
            var lanternTracking = {
              'frontRefreshAfterInactiveTab': 1,
            };
          } else {
            var lanternTracking = {
              'frontRefreshAfterInactiveTab': 0,
            };
          }
          localStorage.setItem(refreshScriptFlagCONST, false);
        }
    </script>
