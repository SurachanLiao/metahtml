                                    <div id="lantern-tracker__main-menu--../flyoutId--link--10518" class="flyout-content__section-item">
                                      <section class="section-contents">
                                        <header class="section-contents__header">
                                          <h2 class="section-title" id="dnaksjer-section-contents">
                                            <a href="https://www.dn.no/aksjer" class="section-title__link">
                                            DN Aksjer
                                            </a>
                                          </h2>  </header>
