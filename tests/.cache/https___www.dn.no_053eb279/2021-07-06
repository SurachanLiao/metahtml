    <script>
    
      googletag.cmd.push(function () {
        let topAdSlots = [];
        if(slots["googlead-toppbanner"]) {
          topAdSlots.push(slots["googlead-toppbanner"]);
          googletag.display("googlead-toppbanner");
        }
        if(slots["googlead-sticky-left-1"]) {
          topAdSlots.push(slots["googlead-sticky-left-1"]);
          googletag.display("googlead-sticky-left-1");
        }
        if(slots["googlead-sticky-right-1"]) {
          topAdSlots.push(slots["googlead-sticky-right-1"]);
          googletag.display("googlead-sticky-right-1");
        }
        googletag.pubads().refresh(topAdSlots);
      });
    </script>
  <!--ip login pop-up starts-->
<script type="text/javascript">
  $(document).ready(function () {
    if(window.nhst.shouldCheckIplogin) {
      $.ajax({
        type: "GET",
        dataType: "json",
        url: "https://" + location.host + "/bli-abonnent/api/v1/ip-login/lightbox",
        success: function (result) {
          //If IP Login user
          if (result != null && result.agreement_full == false) {
            //If lightbox is not shown already
            if (typeof localStorage !== "undefined") {
              var $iploginIframe = $("#iplogin-modal .iplogin-modal-iframe");
                window.addEventListener('message', function (event) {
                  var data = event.data;
                  if ((event.origin.indexOf('.dn.no') > -1) && data['iframe-height']) {
                    $iploginIframe.css({'height': data['iframe-height'] + 'px', 'max-height': $(window).height()-200 + 'px'});
                  }
                });

                $iploginIframe.attr("src", result.url);
                $('html').addClass('is-modal-open');
                $('.settings-modal').toggleClass('settings-modal-open');
            }
          }
        },
        complete: function () {
          // We performed check and we are setting check time in the local storage
          // as a timestamp
          localStorage.setItem("ip-login-modal-last-check", new Date().getTime());
        }
      });
    }
  });
</script>

<div id="iplogin-modal" class="settings-modal settings-modal__ip-login js-settings-modal" aria-modal="true" role="dialog">
