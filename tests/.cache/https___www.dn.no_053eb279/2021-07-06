      (function() {
          var vpwIdCookieValue = window.readCookie('VPW_ID');
          var loginInfo = window.readCookie('DN_LoginInfo');
          var isLoggedIn = (vpwIdCookieValue != null && loginInfo != null);
  
          if (isLoggedIn) {
              document.write("<meta property=\"lp:paywall_user\" content=\"1\"/>");
          }else{
              document.write("<meta property=\"lp:paywall_user\" content=\"0\"/>");
          }
      })();
