   <script>
   
   function handleDropdownMenu(elem) {
   	var top;
   	var left;
	if (elem != null) {
   		top=getPos(elem, '#document', 'bottom'); 
		left = getPos(elem, '#document', 'left') - getPos(document.getElementById('body_div'),'#document','left');
		
		var target = document.getElementById('headerMenu')
		if (target != null) {
			target.style.top = top + "px";
			target.style.left = left + "px";
			target.style.display = "block";
		}
	}
   }
   
   function getPos(elem, myroot, pos) {
	var myroot = myroot || "HTML";
	if (elem == null) return false;
		var rootT=elem.offsetTop 
	    	var rootL=elem.offsetLeft 
	        var rootB=elem.offsetHeight
	    	var rootR=elem.offsetWidth;
	    	var elemP = elem.parentNode;
	    	var mynode = elemP.id;
	    	
	    	while (mynode != myroot) {
	        	rootT += elemP.offsetTop;
	        	rootL += elemP.offsetLeft;
	        	
	        	elemP = elemP.parentNode;
	        	mynode = elemP.nodeName;
	    	}
	    		
	    	rootB += rootT;
	    	rootR += rootL;
	    		
	    	switch(pos)
		{
			case "top":
  				return rootT;
  				break;
			case "left":
  				return rootL;
  				break;
  			case "bottom":
  				return rootB;
  				break;
  			case "right":
  				return rootR;
  				break;
			default:
				return("error");
		}
	}

   function getPosById(elem, pID, pos) {
	if (elem == null) return false;
		var rootT=elem.offsetTop 
	    	var rootL=elem.offsetLeft 
	        var rootB=elem.offsetHeight
	    	var rootR=elem.offsetWidth;
	    	var elemP = elem.parentNode;
	    	var mynode = elemP.id;
	    	
	    	while (mynode != pID) {
	        	rootT += elemP.offsetTop;
	        	rootL += elemP.offsetLeft;
	        	//alert(elemP.id + "::" + rootT);
	        	elemP = elemP.parentNode;
	        	mynode = elemP.id;
			
	    	}
	    		
	    	rootB += rootT;
	    	rootR += rootL;
	    		
	    	switch(pos)
		{
			case "top":
  				return rootT;
  				break;
			case "left":
  				return rootL;
  				break;
  			case "bottom":
  				return rootB;
  				break;
  			case "right":
  				return rootR;
  				break;
			default:
				return("error");
		}
	}
</script>
   
   				

