	<script language="javascript">
		// functions for toggling content display
		//var totDiv = 0;
		
		// Show a single section
		function showSection(divId) {
			if ( document.getElementById("NA"+divId) ) { 
					document.getElementById("NA"+divId).style.display = "block";
					document.getElementById("dulcinea_section_title_"+divId).style.background =
						"url(/docroot/dulcinea/jsp/home/newsArticleMinus.gif) center left no-repeat";
			}
		}
		
		// Hide a single section
		function hideSection(divId) {
			if ( document.getElementById("NA"+divId) ) { 
					document.getElementById("NA"+divId).style.display = "none";
					document.getElementById("dulcinea_section_title_"+divId).style.background =
						"url(/docroot/dulcinea/jsp/home/newsArticlePlus.gif) center left no-repeat";
			}
		}
		
		// Toggles a single section
		function toggleSection(divId) {
			if ( document.getElementById("NA"+divId) )
				if ( document.getElementById("NA"+divId).style.display == "block" || document.getElementById("NA"+divId).style.display == "")
					hideSection(divId);
				else 
					showSection(divId);
		}
		
		// Hides all but a single section
		function hideAllBut(divId) {
		   for (i=0; i<totDiv; i++) {
				if ( document.getElementById("NA"+i) ) {
					if (i == divId)
						showSection(i);
					else
						hideSection(i);
				}
			}
		}
		
		// Hides all sections
		function hideAll() {
			for (i=0; i<totDiv; i++)
				if ( document.getElementById("NA"+i) )
					hideSection(i);	
		}
		
		// Shows all sections
		function showAll() { 
			for (i=0; i<totDiv; i++)
				if ( document.getElementById("NA"+i) )
					showSection(i);	
		}
		
		
		if ( navigator.userAgent.indexOf('Safari') != -1 ){
			document.getElementById("resize_for_safari_paragraph").style.width="213px";//This resize is for the what you will find panel
		}
	
	</script>
	<!-- END JAVASCRIPT FUNCTIONS -->
