					    for (var i = 0; i < window.ecsegmArr.length; i++) {
					        googletag.pubads().setTargeting(window.ecsegmArr[i].key, window.ecsegmArr[i].value);
					    }
					}

					googletag.pubads().enableLazyLoad({
						fetchMarginPercent: 100,  // Fetch slots within 5 viewports.
						renderMarginPercent: 100,  // Render slots within 2 viewports.
						mobileScaling: 1.0  // Double the above values on mobile.
					});


					//googletag.pubads().enableSingleRequest();

					var AmazonAds = 'N';
                    if (AmazonAds === "Y") {
                        var Include = true;
                        for (i = 0; i < gptAdSlots.length; i++) {
                            var Slot = {};
                            var sizestemp = gptAdSlots[i].getSizes();
                            var sizes = [];
                            for (k = 0; k < sizestemp.length; k++) {
                                var L = sizestemp[k].l;
                                var H = sizestemp[k].j;
                                var item = [];
                                Include = L > 1;
                                item.push(L);
                                item.push(H);
                                sizes.push(item);
                            }
                            if (Include)
                            {
                                Slot.slotID = gptAdSlots[i].getSlotElementId()
                                Slot.slotName = gptAdSlots[i].getAdUnitPath()
                                Slot.sizes = sizes;
                                Amazon.push(Slot);
                            }
                        }
                        googletag.pubads().disableInitialLoad();
                    }

                    googletag.enableServices();
                });
            </script>



