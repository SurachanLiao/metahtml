<script type="text/javascript">
    logoClick = function(e){
        if(""!=""){
         var pass_data= {hitType: "event",action: ""};
         sendGAPageview(pass_data);
        }
     };
    $(document).ready(function() {
        _auth = WAuth.getUserSync();
        if(_auth != null){
            _imgshow = $("#imageHeaderLogoShow").val();
            if(_imgshow == ""){
                $(".widget-logo").css("visibility","visible");
            }else{
                if(_auth['subscriptionStatus'] != "subscribed"){
                    $(".widget-logo").css("visibility","visible");
                }
            }
        }else{
            $(".widget-logo").css("visibility","visible");
        }
        $("#logoImageHeaderLink").bind('click',function(e){
             e.stopPropagation();
             e.preventDefault();
             logoClick();
             setTimeout(function(){
                if("" == "_self"){
                   window.location = "";
                }else{
                    window.open("","_blank");
                }
             }, 200);
        });
        $(".links-subhome .listado-items").simplyScroll({
            auto: false,
            speed: 30
        });
        $('.links-subhome .simply-scroll-btn-left').addClass('fa fa-angle-left');
        $('.links-subhome .simply-scroll-btn-right').addClass('fa fa-angle-right');
        var ulWidth = 0;
        var sliderWidth = $('.links-subhome .simply-scroll-clip').outerWidth();
        $(".links-subhome .listado-items li").each(function() {
            ulWidth = ulWidth + $(this).outerWidth();
        });
        if (ulWidth < sliderWidth) {
            $('.links-subhome .simply-scroll-btn').hide();
            $('.links-subhome .simply-scroll-clip').css({'width':'100%'});
        }
        if($(window).outerHeight(true) < 560)
            $('.dropdown-menu').css({'height':$(window).outerHeight(true)});
        $("header").css("padding-top","0");
        $('.container-submenu').add('.dropdown-menu').on('click', function(e){
            e.stopImmediatePropagation();
        });
        $('.dropdown-menu .close').dropdown('toggle');
        $('.navbar-nav li.dropdown').on('shown.bs.dropdown', function(){
            $('header .menu-overlay').show();
            $('html').css({'overflow':'hidden'});
        });
        $('.navbar-nav li.dropdown').on('hidden.bs.dropdown', function(){
            $('header .menu-overlay').hide();
            $('html').css({'overflow':'initial'});
        });
        //Desplegable Pase
        var toggle = false;
        $(".pase-module .pase_LoggedIn").click(function(){
            if(!toggle) {
                $(".pase-module .opciones.subnav").slideDown(300);
                toggle = true;
            } else {
                $(".pase-module .opciones.subnav").slideUp(300);
                toggle = false;
            }
        });
        //Fin deplegable Pase
        //Menú Desplegable
        $('.toggle-icon').on('click', function(e){
            e.stopImmediatePropagation();
            var thisElm = this;
            var elmsDesplegados = $('.subsections-col:visible');
            if($(thisElm).hasClass('open')){
                $(thisElm).removeClass('open');
                $(thisElm).closest('li').find('ul').slideUp(300);
                return false;
            }
            if(elmsDesplegados.length > 0) {
                $('.toggle-icon').removeClass('open');
                elmsDesplegados.slideUp(200, function(){
                    desplegarMenu(thisElm);    
                });
            } else {
                desplegarMenu(thisElm);
            }
        });
        function desplegarMenu(elm){
            var classSection = $(elm).attr('class').split(" ")[1];
            $(".subsections-col."+classSection).slideDown(300);
            $(elm).addClass('open');
        }
        //Fin desplegable
        var hasNotUlChild= $('.dropdown-menu .columna .section-name').not(':has("ul")');
        hasNotUlChild.find('.toggle-icon').hide();
         $('.dropdown-menu .columna').last().addClass('last');
         checkSticky();
    });
    function checkSticky() {
        if ($(window).outerWidth()<1024) {
            $("header").css("padding-top","45px");
         } else {
             $("header").css("padding-top","0");
         }
    }
    $(window).resize(function(){
       checkSticky();
    });
    $("#buscarForm").submit(function(e) {
        e.preventDefault();
        var val = $("#search_text").val();
        if (val != "") {
            window.location.assign("/buscador/?q=" + encodeURIComponent(val));
        }
    });
	if($(".subhome-name").length){
    	$("#sn"+$(".subhome-name").html().toLowerCase().replace(" ","-")).remove();
	}        
</script>
</header>
