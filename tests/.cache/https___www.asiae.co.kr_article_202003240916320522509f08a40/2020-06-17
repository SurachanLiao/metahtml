function goKakao()
{
	/*var kakaoUrl="https://story.kakao.com/share?url=https%3A%2F%2Fwww.asiae.co.kr%2Farticle%2F2020032409163205225";
	var popupOptions="width=600, height=460, resizable=no, scrollbars=yes, status=no";
	window.open(kakaoUrl,"kakaoPop",popupOptions);*/

		var strImg = "https://cwcontent.asiae.co.kr/asiaresize/20/2020032409090737792_1585008548.jpg";
		var Pageurl = "https://www.asiae.co.kr/article/2020032409163205225";
		if(strImg!='')
		{
			Kakao.Link.sendDefault({
				objectType: "feed",
				content: {
					title: "[포토]코로나19에 항공사 첫 셧다운 ",
					imageUrl: strImg,
					link: {
					webUrl: Pageurl,
					mobileWebUrl: Pageurl
					}
				}
			});
		}
		else
		{
			Kakao.Link.sendDefault({
				objectType: "feed",
				content: {
					title: "[포토]코로나19에 항공사 첫 셧다운 ",
					link: {
					webUrl: Pageurl,
					mobileWebUrl: Pageurl
					}
				}
			});

		}
		Kakao.Link.cleanup();
}

