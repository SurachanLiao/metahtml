<script type="text/javascript">
	function photoView( num )
	{
		$('.RightPhotoModule > li').each(function() {
			$(this).removeClass('over');
		});
		$('.RightPhotoModuleList > li').each(function() {
			$(this).removeClass('on');
		});

		var nNum = ( Number(num)+1 ) /2 ;
		nNum = Math.ceil(nNum);

		$('.RightPhotoModule li:eq('+num+')').addClass('over');

		$('.RightPhotoModuleList').attr('class', 'pop_big pop_big0'+nNum+' RightPhotoModuleList');
		$('.RightPhotoModuleList li:eq('+num+')').addClass('on');
	}
</script>

