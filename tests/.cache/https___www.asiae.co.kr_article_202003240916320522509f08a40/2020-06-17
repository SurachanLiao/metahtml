<script type="text/javascript">
if( getCookie( 'user_id' ) != '' && getCookie( 'islogin' ) == '1' )
{
	$('.btn_frind').attr('href', '//www.asiae.co.kr/mypage/');
	$('.btn_frind').attr('onclick', 'javascript:;');
	$('.btn_frind').attr('title', '마이페이지');
	$('.btn_logout').show();
}

/*
var sokboRolling = $('#sokboPlayer').jrolling({
	'items': '#sokboPlayer li',
	'width': '240px',
	'height': '33px',
	'move': 'up',
	'auto': true,
	'delay': 4000,
});
sokboRolling.target.on();
*/

jQuery(function($) {
	$('#sokboPlayer').ulslide({
		 axis: 'y'				// x, y
		,affect: 'slide'		// slide, fade
		,slidetag: 'li'
		,duration: 500
		,width: 400
		,height: 34
		,autoslide: 4000
		,mouseover: true
	});
});

jQuery(function($) {
	$('#search1').on('focus', function(){
		if($('#search1').val()=='뉴스검색'){
			$('#search1').val('');
		}
	});

	$('#search1').on('blur', function(){
		$('#query_result1').hide();
		if($('#search1').val()==''){
			$('#search1').val('뉴스검색');
			$('.header_sear').hide();
			$('.btn_searchon').attr('class', 'btn_search');
		}
	});
});

function checkSearchSubmit(obj)
{
	if( obj.sn.value && obj.sn.value != '뉴스검색' )
	{
		return true;
	}
	else
	{
		alert('검색어를 입력하세요.');
		return false;
	}
}

/*
$( function() {
	$('#search1').autocomplete({
		source: function( request, response ) {
			$.ajax( {
				url: '//www.asiae.co.kr/news/include/frame/autocomplete.php',
				type: 'post',
				dataType: 'json',
				data: {
					term: request.term
				},
				success: function( getData ) {
					response(getData);
				}
			});
		}
		,minLength: 2
		,focus: function( event, ui ) {
			$( "#search1" ).val( ui.item[1] );
			return false;
		}
		,select: function( event, ui ) {
			$( "#search1" ).val( ui.item[1] );
			return false;
		}
	})
	.autocomplete( 'instance' )._renderItem = function( ul, item ) {
		return $( '<li>' )
		.append( '<div>'+item[1]+'('+item[0]+')</div>' )
		.appendTo( ul );
	};
});
*/

adURL = "https://a1.asiae.co.kr/RealMedia/ads/adstream_mjx.ads/www.asiae.co.kr/society@x90,Right,Top1,Right1";
adURL = adURL.replace(/@/, "/1" + ((String)(Math.random())).substring (2, 11) + "@");
document.write ('<SCR' + 'IPT LANG' + 'UAGE="JAVAS' + 'CRIPT1.1" SRC="' + adURL + '"> </SCR' + 'IPT>');
</script>

<script>
function saveRefCookie(ref) {
	var spath = '/';
	var domain ='.asiae.co.kr';
	var name='aceRef';
	document.cookie = name+'='+escape(ref)+';'+';expires=0;path='+spath+';domain='+domain;
}
var curPage = document.location.href;
curPage   = curPage.slice(7);
var arr   = curPage.split("/");
curPage  = arr[0];

if(document.referrer) {
	SetCookie("aceRef", document.referrer, 0, "/", ".asiae.co.kr" );
}
else {
	SetCookie("aceRef", "", 0, "/", ".asiae.co.kr" );
}
</script>

