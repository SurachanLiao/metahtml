<script type="text/javascript">

function ctab(num,fs,ls){

	for(var i=fs; i<=ls; i++)
	{
		if(num==i){
			
		   document.getElementById('block_'+i).style.display = 'block';
		   document.getElementById('ctab'+i).className = "b-12";		   
		}
		else{
			document.getElementById('block_'+i).style.display = 'none';
			document.getElementById('ctab'+i).className = "bl-11-L";
		}
		
	}

if(num==10)
	{
		   document.getElementById('block_10').style.display = 'block';
		   document.getElementById('ctab10').className = "b-12";
		   document.getElementById('block_9').style.display = 'none';
			document.getElementById('ctab9').className = "bl-11-L";

	}

for(var i=1; i<=5; i++)
	{
         if(i!=fs && i!=ls)
		{
			document.getElementById('ctab'+i).className = "bl-11-L";

		}
    }

 if(fs==1 && ls==2)
	{
 if(document.getElementById("main_1").className=='plus')
		{
	     updown_stk(1);
		}
	}
	else if(fs==3 && ls==4)
	{
		if(document.getElementById("main_2").className=='plus')
		{
			
		updown_stk(2);
		
		}
	}
	else if(fs==5 && ls==6)
	{ 
		if(document.getElementById("main_3").className=='plus')
		{
		updown_stk(3);
		}
	}
   


}

function updown_stk(udtab)
{	
	var tabcnt;
	for (tabcnt=1; tabcnt<4; tabcnt++)
	{
		if(udtab==tabcnt)
		{
			if(document.getElementById("main_"+tabcnt).className=='plus')
			{
				document.getElementById("main_"+tabcnt).className='minus';	
				document.getElementById("maintg_"+tabcnt).className='PT5';
				$("#maintg_"+tabcnt).slideUp(1);
				$("#maintg_"+tabcnt).slideDown(500);
                if(tabcnt==1 && document.getElementById('ctab2').className =='bl-11-L')
				{
				document.getElementById('ctab1').className = "b-12";
				}
				else if(tabcnt==2 && document.getElementById('ctab4').className =='bl-11-L')
				{
					document.getElementById('ctab3').className = "b-12";
				}
				else if(tabcnt==3 && document.getElementById('ctab6').className =='bl-11-L')
				{
					document.getElementById('ctab5').className = "b-12";
				}
				
			}
			else
			{
				document.getElementById("main_"+tabcnt).className='plus';
				$("#maintg_"+tabcnt).slideUp(500);
				document.getElementById("maintg_"+tabcnt).className='dnone';
			}
		}
		else
		{
			$("#maintg_"+tabcnt).slideUp(500);
			
			document.getElementById("maintg_"+tabcnt).className = 'dnone';
			document.getElementById("main_"+tabcnt).className = 'plus';


			if(tabcnt==1)
				{
				document.getElementById('ctab1').className = "bl-11-L";
				document.getElementById('ctab2').className = "bl-11-L";
				}
				else if(tabcnt==2)
				{
					document.getElementById('ctab3').className = "bl-11-L";
					document.getElementById('ctab4').className = "bl-11-L";
				}
				else if(tabcnt==3)
				{
					document.getElementById('ctab5').className = "bl-11-L";
					document.getElementById('ctab6').className = "bl-11-L";
				}
				
		}
	}
}

</script>


