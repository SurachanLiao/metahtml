			function changerhstab(tabname) {
				if(tabname == 'todaynews') {
					document.getElementById('todaynews_head').className='act';
					document.getElementById('today_news').style.display='block'; 
					document.getElementById('topnews_head').className='nact';
					document.getElementById('top_news').style.display='none'; 
					document.getElementById('relatednews_head').className='nact';
					document.getElementById('related_news').style.display='none'; 
					
				}
				if(tabname == 'relatednews') {
					document.getElementById('todaynews_head').className='nact';
					document.getElementById('today_news').style.display='none'; 
					document.getElementById('topnews_head').className='nact';
					document.getElementById('top_news').style.display='none'; 
					document.getElementById('relatednews_head').className='act';
					document.getElementById('related_news').style.display='block'; 
					
				}
				if(tabname == 'topnews') {
					document.getElementById('todaynews_head').className='nact';
					document.getElementById('today_news').style.display='none'; 
					document.getElementById('relatednews_head').className='nact';
					document.getElementById('related_news').style.display='none'; 
					document.getElementById('topnews_head').className='act';
					document.getElementById('top_news').style.display='block'; 
					
				}
			}	
