		// Scroll annuaire sécurité
		$('#scroll-annuaire-magazine').click(function(){
			var position = $('#annuaire-magazine').position();
			tdUtil.scrollToPosition(position.top + 300, 1200);
			return false;
		});


		// Menu css
		if ($('.sf-menu .group:nth-child(2) > li:nth-child(2)').hasClass('current-menu-item')) {
				$('.sf-menu .group:nth-child(2) > li:first-child > a').css('background-color', '#cc0001')
		}
		$('.sf-menu .group:nth-child(2) > li:nth-child(2):not(.current-menu-item)').hover( 
			function() {
				$('.sf-menu .group:nth-child(2) > li:first-child > a').css('background-color', '#cc0001')
			},
			function() {
				$('.sf-menu .group:nth-child(2) > li:first-child > a').css('background-color', '')
			}
		)
		$('.sf-menu .group:nth-child(2) > li:first-child > a').on('click', function() {
			if ($('.sf-menu .group:nth-child(2) > li:first-child > a:active, .sf-menu .group:nth-child(2) > li:first-child > a:focus').length > 0) {
				$('.sf-menu .group:nth-child(2) > li:nth-child(2)').css('background-color', '#cc0001')
			}
		})
		$('.sf-menu .group:nth-child(2) > li:nth-child(2) > a').on('click', function(e) {
			if ($('.sf-menu .group:nth-child(2) > li:nth-child(2) > a:active, .sf-menu .group:nth-child(2) > li:nth-child(2) > a:focus').length > 0) {
				$('.sf-menu .group:nth-child(2) > li:nth-child(2)').css('background-color', '#cc0001')
				$('.sf-menu .group:nth-child(2) > li:nth-child(2) > a').each(function() {
