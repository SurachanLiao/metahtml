                    </div>
                    <br class="floatbox__clear">
                </div>
                <!-- /ko -->
                <!-- ko if: $data.activeGroup() === "Biodigitals" -->
                <div class="hero__content--active hero__content">
                    <div class="floatbox">
                    </div>
                    <br class="floatbox__clear">
                </div>
