	<script>
        function CollectionItemClasses() {
            if ('undefined' !== typeof showcase) {
                Array.prototype.slice.call(document.getElementsByClassName('collections__showcase'))
                    .forEach(function (collection) {
                        showcase.setLayoutMethodFixedClass(collection);
                    });
            }
        }

        window.addEventListener('resize', CollectionItemClasses)
        document.addEventListener('DOMContentLoaded', CollectionItemClasses);
	</script>

