            if(typeof(promoEl.attr("data-service-id"))!='undefined'){ ajaxUrl += '&service_id=' + promoEl.data('service-id'); }

            $.ajax({
                url:ajaxUrl,
                dataType:"json",
                success:function(data,textStatus,jqXHR){
                    __tnt.log("Promo JSON - success");
                    var impressionsData = [],
                        i = 1;
                    
                    if(data.length){
                    $.each(data,function(){
                        var promo = this,
                            template = promoEl.find('template.template-promo-service'),
                            wrapper = promoEl.find('.output'),
                            buttonPurchase = "Get Started",
                            customZipcodes = promo.custom.zipcodes,
                            customClassName = "",
                            servicePanelClasses = "promotion-service subscription-service panel panel-default ";

                        if(promo.custom.button_text){ buttonPurchase = promo.custom.button_text }
                        if(promo.custom.custom_class){ customClassName += promo.custom.custom_class }
                        if(customZipcodes){ customClassName += ' delivered' }
                        
                        // Array of Items to modify in the template
                        var elements = [{
                            'element' : '.subscription-service.panel',
                            'attr' : [{
                               'attr' : 'class',
                               'value' : servicePanelClasses + customClassName
                            }]
                        },{
                            'element' : 'h3',
                            'html' : promo.name
                        },{
                            'element' : '.service-note',
                            'html': promo.custom.note
