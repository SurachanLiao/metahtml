<script>
    jQuery(document).ready(function($){
        // GTM Tracking Code
        $('.subscription-services').on('tncms-track-services-displayed',function(){
            if(window.dataLayer){
                var i = 1;
                var impressionsData = [];
                $('.subscription-service').each(function(){
                if($(this).is(':visible')){
                    if(typeof($(this).data('impression'))=='undefined'){
                        var service = __tnt.services[$(this).data('service-id')];

                        $(this).find('a[data-subscription-service]').attr('data-subscription-position',i);

                        impressionsData.push({
                            'id': service.id,
                            'name': service.name,
                            'price':'',
                            'brand':"The Coos Bay World",
                            'category':service.category,
                            'list':service.list,
                            'position':i,
                            'variant':''
                        });

                        $(this).attr('data-impression',true);
                        i++;
                    }

                    if(!$(this).hasClass('event-handled')){
                        $(this).find('a[data-subscription-service]').on('click',function(){
                            var serviceId = $(this).attr('data-subscription-service');
                            var service = __tnt.services[serviceId];
                            var serviceUrl = $(this).attr('href');
                            var zipcodes = $(this).data('subscription-service-zipcodes');
                            var position = Number($(this).attr('data-subscription-position'));

                            dataLayer.push({
                                'ecommerce':{
                                    'click':{
                                        'actionField': {'list':''},
                                        'products':[{
                                            'name': service.name,
                                            'id': serviceId,
                                            'price':'',
                                            'brand': "The Coos Bay World",
                                            'category': service.category,
                                            'quantity':1,
                                            'position':position,
                                            'variant':''
                                        }]
                                    }
                                },
                                'event':'tncms.event.trigger',
                                'tncms': {
                                    'event': {
                                        'trigger': {
                                            'action': 'productClick',
                                            'category': 'subscription',
                                            'label':'user clicked on a service'
                                        }
                                    }
                                },
                                'eventCallback': function(){
                                    document.location = serviceUrl;
                                }
                            });
                    
                            return false;
                        });
                        $(this).addClass('event-handled');
                   }
                    
                }
                });
                
                if(impressionsData.length){
                    dataLayer.push({
                        'ecommerce':{
                            'impressions': impressionsData
                        },
                        'event':'tncms.event.trigger',
                        'tncms': {
                            'event': {
                                'trigger': {
                                    'action': 'productImpressionsFinished',
                                    'category': 'subscription',
                                    'label':'services displayed to user'
                                }
                            }
                        }
                    });
                }
            }
        }); // End GTM

        $('.subscription-service .collapse-rates').on('show.bs.collapse', function(){
            $(this).siblings('.view-rates-container').find('.view-rates-text').text('Hide all rates');
            $(this).siblings('.view-rates-container').find('[data-fa-i2svg]').attr('data-icon', 'caret-up');
        });
        $('.subscription-service .collapse-rates').on('hide.bs.collapse', function(){
            $(this).siblings('.view-rates-container').find('.view-rates-text').text('View all rates');
            $(this).siblings('.view-rates-container').find('[data-fa-i2svg]').attr('data-icon', 'caret-down');
        });
        $.each($('.subscription-services-filter').find('a'), function(){
            $(this).on('click',function(){
                $('.subscription-services-filter').find('a').removeClass('btn-primary').addClass('btn-default');
                $(this).addClass('btn-primary');

                if($(this).attr('data-subscription-filter')){
                    var filtered = $(this).attr('data-subscription-filter');
                    $.each($('.subscription-services').find('.subscription-service'),function(){
                        if($(this).hasClass(filtered)){ $(this).removeClass('hidden').show(); } else { $(this).hide(); }
                    });
                } else {
                    $('.subscription-services').find('.subscription-service').removeClass('hidden').show();
                }
                $('.subscription-services').trigger('tncms-track-services-displayed');

                return false;
            });
        });

        $('.subscription-services').trigger('tncms-track-services-displayed');
    });
</script>

