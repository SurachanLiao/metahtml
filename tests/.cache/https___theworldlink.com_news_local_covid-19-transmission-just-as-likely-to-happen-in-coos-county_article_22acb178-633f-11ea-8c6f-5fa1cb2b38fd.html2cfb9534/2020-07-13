__tnt.subscription.promos.fetch = function(promoEl, promoVal, blankSearch, callback){
    if(promoVal!="" && promoVal!=null || blankSearch){
// reset form
            promoEl.find('.promo-loading').show();
            promoEl.find('.output').html("");
            promoEl.find('.service-promo-status-message').html('');
            promoEl.find('.promo-placeholder').hide();
            
