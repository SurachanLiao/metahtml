  function checarScroll() {
     $(".limiter").attr("style","position:relative;z-index:1");
    $(window).scroll(function() {           
        alto=$(this).scrollTop();
        if($(this).scrollTop() <= 320) {

            if($(".container_publateral").is(":visible")){
                $(".container_publateral").attr("style","top:100px;position:fixed;z-index:999;left:-10px; ");
            }
        } else{
            if($(".container_publateral").is(":visible")){
                $(".container_publateral").attr("style","top:10px;position:fixed;z-index:999;left:-10px; ");
            }
        }
 });
    }
