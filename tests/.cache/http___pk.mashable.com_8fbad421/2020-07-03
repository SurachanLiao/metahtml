    window.ICMS = window.ICMS || { cmd: [], libs: ['/cookienotice.js?349'], css: [], gdprloader: [] };
    ICMS.lang = 'en'; ICMS.siteID = 'mashable_pk'; ICMS.siteName = 'Mashable Pakistan';
    ICMS.baseUri = ''; ICMS.pageName = ''; ICMS.pageUrl = '';
    ICMS.isPublished = ''; ICMS.backendEditUrl = '';
