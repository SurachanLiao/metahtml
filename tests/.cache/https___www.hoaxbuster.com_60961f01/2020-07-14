		<script type="text/javascript">
$(document).ready(function(){
	var idOn = 0;
	var maxId = 0;
	$( "#navbar-articles li" ).each(function( index ) {
	  if(maxId<$(this).find("a").data('goto')) maxId = $(this).find("a").data('goto');
	});
	
	$('#navbar-articles a').on('click', function(){
		$('#slider_articles').slick('slickGoTo', $(this).data('goto'));
		$('#navbar-articles a').removeClass('on');
		$(this).addClass('on');
		idOn = $(this).data('goto');
		return false;
	});
	$('#navbar-articles li:first-child a').addClass('on');
	
	$('#slider_articles').slick({
		  slidesToShow: 1,
		  slidesToScroll: 1,
		  autoplay: false,
		  autoplaySpeed: 3000,
		  arrows: false,
		  dots: false,
		  swipe: false,
	});
	
	$('#slider').slick({
		  slidesToShow: 1,
		  slidesToScroll: 1,
		  autoplay: true,
		  autoplaySpeed: 3000,
		  arrows: false,
		  dots: true,
	      infinite: true,
	      dotsClass: 'hoax-dots'
	});
	
	$('#prev_menu_article').on('click', function(){
		$("#slider_articles").slick('slickPrev');
		if(idOn>0) idOn--; else idOn=maxId;
		$('#navbar-articles a').removeClass('on');
		$("#navbar-articles li:nth-child("+parseInt(idOn+1)+") a").addClass('on');
	});
	$('#next_menu_article').on('click', function(){
		$("#slider_articles").slick('slickNext');
		if(idOn<maxId) idOn++; else idOn=0;
		$('#navbar-articles a').removeClass('on');
		$("#navbar-articles li:nth-child("+parseInt(idOn+1)+") a").addClass('on');
	});

	var client = new XMLHttpRequest();
	client.open("POST", "/forum/hbx_topics_anywhere.php", true);
	client.onload = function () {
		console.log(this.responseText);
		var posts=JSON.parse(this.responseText);
		var displayedPosts="";

		$.each(posts, function(index, value){
			var avatar='img/Hoaxbuster_logo_forum.jpg';
			if(value.POST_AVATAR!="") avatar="uploads/"+value.POST_AVATAR;
			displayedPosts=displayedPosts+'<div class="col-sm-6 col-md-4"><div class="before_chat"><a href="'+value.POST_LINK+'"></a></div><div class="contenu_forum"><div class="row design_forum_contenu"><div class="col-4" style="padding-right: 12px;"><div class="img_forum"><img src="'+avatar+'" alt="">';
			displayedPosts=displayedPosts+'<div class="forum_utilisateur">Par '+value.POST_AUTHOR+'</div></div></div><div class="col-8">';
			displayedPosts=displayedPosts+'<div class="title_article"><a href="'+value.POST_LINK+'">'+value.TOPIC_TITLE+'</a></div>';
			displayedPosts=displayedPosts+'<div class="preview_chat"><a href="'+value.POST_LINK+'">'+value.POST_TEXT+'</a></div><div class="info_chat"><a href="'+value.POST_LINK+'">'+value.POST_DATE+' | PAR <span style="text-transform: uppercase;">'+value.TOPIC_AUTHOR+' | <i class="fas fa-comments"></i> '+value.TOPIC_NB_POSTS+'</a></div></div></div></div></div>';
		});		
		
		$('#zone_forum').html(displayedPosts);
	};
	client.onerror = function(){  }
	client.send();
});
</script>
		<script type="text/javascript">
		$(document).ready(function(){
			
			$("#rech_avance").on("click",function(){
				if($('.rech_avance').css('display') == 'none') {
					$('.rech_avance').css('display', 'flex');
					$("#rech_avance").text('- Recherche avancée');
				} else {
					$('.rech_avance').css('display', 'none');
					$("#rech_avance").text('+ Recherche avancée');
					$(".rech_avance select option").prop("selected", false);
				}
			});
			
						
			
			$('.partenaires').slick({
				  slidesToShow: 4,
				  slidesToScroll: 4,
				  autoplay: true,
				  autoplaySpeed: 2000,
				  arrows: false,
				  dots: false,
			      infinite: true,	
				  responsive: [
				    {
				      breakpoint: 992,
				      settings: {
				        slidesToShow: 3,
				        slidesToScroll: 3
				      }
				    },
				    {
					      breakpoint: 768,
					      settings: {
					        slidesToShow: 2,
					        slidesToScroll: 2
					      }
					    },
				    {
				      breakpoint: 576,
				      settings: {
				        slidesToShow: 1,
				        slidesToScroll: 1
				      }
				    }
				  ]
			});
		});
		</script>
    </body>
