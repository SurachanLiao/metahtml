		function addView()
		{
                        //console.log('idContent : 916907');
            if(typeof(Storage)!=="undefined" && typeof(localStorage['lastcount'+916907]) !== "undefined"){
                var lastcount = JSON.parse(localStorage['lastcount'+916907]);
                //js_log('Storage :');
                //js_log(' lastcount.id : '+lastcount.id);
                //js_log(' lastcount.date : '+lastcount.date);
                if(new Date(new Date() - new Date(1800000)) < new Date(lastcount.date) && lastcount.id == 916907){ // Plus que 12 heures
                    //js_log('View already counted !');
                    return false;
                }
            }

            //js_log('Req : Count the view...');
                        $.post(
                '/viewcounter/add',
                { id:916907 },
                function(data){
                                        if(data.saved == true){
                        //js_log('View saved !');
                        if(typeof(Storage) !== "undefined"){
                            localStorage['lastcount'+916907] = JSON.stringify({
                                "date": new Date(),
                                "id": data.id
                            });
                        }
                    }else{
                                                for(key in data.errors) {
                            console.log(data.errors[key]);
                        }
                    }
                }
            );
		}
