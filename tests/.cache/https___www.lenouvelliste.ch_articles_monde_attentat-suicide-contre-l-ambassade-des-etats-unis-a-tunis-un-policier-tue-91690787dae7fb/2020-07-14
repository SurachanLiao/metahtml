    function reloadContent()
    {
        $("#reloadContent").css("opacity", "0.6");
        $(".loading").show();

        $.ajax({
            type: "GET",
            url: "/_private/articles/partial/916907/275?reload=true",
            success: (function(data) {
                $("#reloadContent").css("opacity", "1");
                $("#reloadContent").html(data);
                $(".loading").hide();

                if (typeof(FB) != 'undefined' && typeof(FB.XFBML) != 'undefined') {
                    FB.XFBML.parse();
                }
            }),
            error: (function(err1,err2,err3) {
                $("#reloadContent").css("opacity", "1");
                $(".loading").hide();
            })
        });
    }

    jQuery(window).on('load',function($){
        if(jQuery("#slider-page-simple").length) {
            var sliderObject = jQuery("#slider-page-simple").data('royalSlider');

            var query = getQueryParams(document.location.search);
            var hash = query.image;
            if(hash != undefined && hash.length > 0){
                hash = parseInt(hash);
                var multiple = Math.ceil(hash/4)-2;
                hash = hash + multiple;
                sliderObject.goTo(hash);
            }

            sliderObject.ev.on('rsAfterSlideChange', function(event) {
                var index = sliderObject.currSlideId;
                index++;
                //var multiple = Math.ceil(index/5);
                //var indexShow = index-(multiple-1);
                //if(index % 5 != 0) {
                //document.location.hash = index;
                ChangeUrl('Photos-916907?image=' + index, window.location.pathname + "?image=" + index);
                jQuery("#share_google_plus").attr("href", "https://plus.google.com/share?url=" + window.location.href);
                jQuery("#share_twitter").attr("href", "https://twitter.com/home?status=" + window.location.href);
                jQuery("#share_facebook").attr("href", "https://www.facebook.com/sharer/sharer.php?u=" + window.location.href);
                jQuery(".share_google").attr("href", "https://plus.google.com/share?url=" + window.location.href);
                jQuery(".share_twitter").attr("href", "https://twitter.com/home?status=" + window.location.href);
                jQuery(".share_fb").attr("href", "https://www.facebook.com/sharer/sharer.php?u=" + window.location.href);
                //}
            });
        }
    });

    <!-- gallery bottom --><!-- fin gallery bottom -->

    function ChangeUrl(page, url) {
                if (typeof (history.pushState) != "undefined") {
            var obj = { Page: page, Url: url };
            history.pushState(obj, obj.Page, obj.Url);
        } else {
            //alert("Browser does not support HTML5.");
        }
    }

    function getQueryParams(qs) {
        qs = qs.split('+').join(' ');

        var params = {},
            tokens,
            re = /[?&]?([^=]+)=([^&]*)/g;

        while (tokens = re.exec(qs)) {
            params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
        }

        return params;
    }
