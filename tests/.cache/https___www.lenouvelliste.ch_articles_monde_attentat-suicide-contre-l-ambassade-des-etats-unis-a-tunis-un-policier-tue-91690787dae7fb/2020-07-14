    function goToSlide(index)
    {
        if(sliderObject == undefined) {
            var sliderObject = jQuery("#slider-page-simple").data('royalSlider');
        }
        sliderObject.goTo(index);
    }

    function diff_time(dt2, dt1)
    {
        var diff =(dt2.getTime() - dt1.getTime()) / 1000;
        diff /= 60;
        var minutes = Math.abs(Math.round(diff));

        if(minutes>59 && minutes<3600){

            var hours = Math.abs(Math.floor(minutes/60));
            if(hours==1){
                return "Actualisé il y a 1 heure";
            }
            else{
                var th = "heures";
                return "Actualisé il y a "+ hours + " " + th;
            }
        }
        else if(minutes>59 && minutes>=3600){
            return null;
        }
        else{
            var tm = "minutes";
            if(minutes<=1){tm="minute";}
            return "Actualisé il y a "+ minutes + " "+tm;
        }

        return minutes;
    }

    function reloadUpdated(){
        $(".article-refresh.datetime").each(function(){
            var timestamp = new Date($(this).data("timestamp") * 1000);
            var now = new Date();
            var diff = diff_time(now, timestamp);

            if(diff==null){
                var value = $(this).data("datetime");
                $(this).html(value);
            }
            else{
                $(this).html(diff);
            }
        });

        if($(".article-refresh.datetime").length>0){
            setInterval(reloadUpdated,60000);
        }
    }

    jQuery(document).ready(function($) {


        var navTab = $('.nav-tabs');

        navTab.find('li').click(function(){
