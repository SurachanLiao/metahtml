        <!-- DATALAYER A ENVOYER LORS DE LA PREMIERE ACTION DE SCROLL SUR LA PAGE / L'ECRAN. -->
        var scrolled = false;
        var scrollStep1 = false;
        var scrollStep2 = false;
        var scrollStep3 = false;
        var transactionOkSend = false;
        var timer;
        $(window).scroll(function(event) {
            var scrollTop = $(window).scrollTop();
            var docHeight = $(document).height();
            var winHeight = $(window).height();
            var scrollPercent = (scrollTop) / (docHeight - winHeight);
            var scrollPercentRounded = Math.round(scrollPercent*100);
            //console.log(scrollPercentRounded);
            if(!scrolled) {
                scrolled = true;
                dataLayer.push({
                    'event': 'addToCart',
                    'ecommerce': {
                        'add': {
                            'products': [{
                                'name': 'Attentat\u0020suicide\u0020contre\u0020l\u2019ambassade\u0020des\u0020Etats\u002DUnis\u0020\u00E0\u0020Tunis\u003A\u0020un\u0020policier\u0020tu\u00E9',       // Titre de l'article
                                'id': '916907', // ID de l'article
                                'price': '296', // nombre de mots de l'article
                                'brand': 'na', // provenance de l'article : ATS / Région IECG / Communiqué
                                'category': 'LeNouvelliste\/monde\/article', // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                'quantity': 1
                            }]
                        }
                    }
                });
            }

            <!-- DATALAYER A ENVOYER A 25% (step1) / 50% (step2) / 75% (step3) DE SCROLL. -->
            if((scrollPercentRounded >= 25 && scrollPercentRounded <= 49) && !scrollStep1) {
                scrollStep1 = true;
                dataLayer.push({
                    'event': 'checkout',
                    'ecommerce': {
                        'checkout': {
                            'actionField': {'step': 1}, // 1, 2 ou 3 en fonction du pourcentage de scroll
                            'products': [{
                                'name': 'Attentat\u0020suicide\u0020contre\u0020l\u2019ambassade\u0020des\u0020Etats\u002DUnis\u0020\u00E0\u0020Tunis\u003A\u0020un\u0020policier\u0020tu\u00E9',        // Titre de l'article
                                'id': '916907', // ID de l'article
                                'price': '296',  // nombre de mots de l'article
                                'brand': 'na', // provenance de l'article : ATS / Région IECG / Communiqué
                                'category': 'LeNouvelliste\/monde\/article', // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                'quantity': 1
                            }]
                        }
                    }
                });
            }
            if((scrollPercentRounded >= 50 && scrollPercentRounded <= 74) && !scrollStep2) {
                scrollStep2 = true;
                dataLayer.push({
                    'event': 'checkout',
                    'ecommerce': {
                        'checkout': {
                            'actionField': {'step': 2}, // 1, 2 ou 3 en fonction du pourcentage de scroll
                            'products': [{
                                'name': 'Attentat\u0020suicide\u0020contre\u0020l\u2019ambassade\u0020des\u0020Etats\u002DUnis\u0020\u00E0\u0020Tunis\u003A\u0020un\u0020policier\u0020tu\u00E9',        // Titre de l'article
                                'id': '916907', // ID de l'article
                                'price': '296',  // nombre de mots de l'article
                                'brand': 'na', // provenance de l'article : ATS / Région IECG / Communiqué
                                'category': 'LeNouvelliste\/monde\/article', // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                'quantity': 1
                            }]
                        }
                    }
                });
            }
            if((scrollPercentRounded >= 75 && scrollPercentRounded <= 100) && !scrollStep3) {
                scrollStep3 = true;
                dataLayer.push({
                    'event': 'checkout',
                    'ecommerce': {
                        'checkout': {
                            'actionField': {'step': 3}, // 1, 2 ou 3 en fonction du pourcentage de scroll
                            'products': [{
                                'name': 'Attentat\u0020suicide\u0020contre\u0020l\u2019ambassade\u0020des\u0020Etats\u002DUnis\u0020\u00E0\u0020Tunis\u003A\u0020un\u0020policier\u0020tu\u00E9',        // Titre de l'article
                                'id': '916907', // ID de l'article
                                'price': '296',  // nombre de mots de l'article
                                'brand': 'na', // provenance de l'article : ATS / Région IECG / Communiqué
                                'category': 'LeNouvelliste\/monde\/article', // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                'quantity': 1
                            }]
                        }
                    }
                });
            }
            <!-- DATALAYER PERMETTANT DE SUIVRE UNE "TRANSACTION" LORSQU'UN ARTICLE EST LU A 90% DE SCROLL AVEC MINIMUM 20 SECONDES SUR LA PAGE / L'ECRAN. A ENVOYER A 90% DE SCROLL QUAND L'INTERNAUTE A PASSE AU MOINS 20 SECONDES DEPUIS LE CHARGEMENT DE LA PAGE / L'ECRAN. -->
            if((scrollPercentRounded >= 75 && scrollPercentRounded <= 100)) {
                if(timer == null && !transactionOkSend) {
                    timer = setTimeout(function () {
                        dataLayer.push({
                            'event': 'transactionOK',
                            'ecommerce': {
                                'purchase': {
                                    'actionField': {
                                        'id': guid(),   // générer un id aléatoire.
                                        'revenue': '80'    // price de l'article
                                    },
                                    'products': [{                            // List of productFieldObjects.
                                        'name': 'Attentat\u0020suicide\u0020contre\u0020l\u2019ambassade\u0020des\u0020Etats\u002DUnis\u0020\u00E0\u0020Tunis\u003A\u0020un\u0020policier\u0020tu\u00E9',       // Titre de l'article
                                        'id': '916907', // ID de l'article
                                        'price': '296',// nombre de mots de l'article
                                        'brand': 'na', // provenance de l'article : ATS / Région IECG / Communiqué
                                        'category': 'LeNouvelliste\/monde\/article', // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                        'quantity': 1
                                    }]
                                }
                            }
                        });
                        clearTimeout(timer);
                        timer = null;
                        transactionOkSend = true;
                    }, 20000);
                }
            }
            else{
                clearTimeout(timer);
                timer = null;
            }
        });
        
    });
    function guid() {
        function s4() {
            return Math.floor((1 + Math.random()) * 0x10000)
                .toString(16)
                .substring(1);
        }
        return s4() + s4() + '-' + s4() + '-' + s4() + '-' +
            s4() + '-' + s4() + s4() + s4();
    }
