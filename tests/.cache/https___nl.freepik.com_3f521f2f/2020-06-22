<aside id="header-filter" class="header-filter search-filter trigger" data-permanent="true">
    <div class="scrollable">
        <div class="row row--vertical-center row--space-between row--title pd-right-lv1">
            <h6 class="trigger__button button button--sm button--link-dark mg-none-i font-sm uppercase">Filters</h6>

            <button class="trigger__close button button--sm button--link-dark button--icon">
                <i class="icon--cross"></i>
            </button>
        </div>

        <div class="container-accordion">
            <div class="accordion mg-none accordion--ready" data-closable="true" data-filter="category">
                <div class="accordion__container">
                    <button class="accordion__link semibold nostyle block" data-trigger="filter-type">
                        <i class="icon icon--sm mg-right-lv1 icon--resources inline-block v-alignc"></i>
                        <span class="font-sm v-alignc">Categorie</span>
                    </button>
                    <div class="accordion__content pd-x-none">
                        <div id="filter-type" class="trigger">
                            <label for="type-vector" class="filter checkbox checkbox--xs" data-field-name="type">
                                <span class="checkbox__indicator"><i class="icon icon--check"></i></span>
