<style>
ul.meja { margin: 0; padding-left: 0px;font-family:Arial,Helvetica,sans-serif;font-size:11px;font-weight:bold;
} 
.meja li {list-style:none;display:inline;}
.meja a {padding:0px 0px;display:inline-block;width: 22%;background:#3333ff;color:#eee;text-decoration:none;text-align: center;}
.meja a.active {background:#66cc00; color:#000000;}
</style>


<script>
$('ul.meja').each(function(){
    // For each set of meja, we want to keep track of
    // which side is active and it's associated content
    var $active, $content, $links = $(this).find('a');

    // If the location.hash matches one of the links, use that as the active side.
    // If no match is found, use the first link as the initial active side.
    $active = $($links.filter('[href="'+location.hash+'"]')[0] || $links[0]);
    $active.addClass('active');
    $content = $($active.attr('href'));

    // Hide the remaining content
    $links.not($active).each(function () {
        $($(this).attr('href')).hide();
    });

    // Bind the click event handler
    $(this).on('click', 'a', function(e){
        // Make the old side inactive.
        $active.removeClass('active');
        $content.hide();

        // Update the variables with the new link and content
        $active = $(this);
        $content = $($(this).attr('href'));

        // Make the side active.
        $active.addClass('active');
        $content.show();

        // Prevent the anchor's default click action
        e.preventDefault();
    });
});
</script>
</div>
