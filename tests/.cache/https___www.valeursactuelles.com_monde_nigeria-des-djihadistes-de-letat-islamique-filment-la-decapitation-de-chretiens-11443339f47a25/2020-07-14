            <script type="text/javascript">
        window. onload = function(){
          var pub_article = document.querySelector('.node--type-article .field--type-text-with-summary');
          if (pub_article !== null) {
            var new_block_commentary = document.createElement('div');
            new_block_commentary.setAttribute("class", "view-row");
            new_block_commentary.setAttribute("style", "text-align:center;width:100%");

            var new_block_commentary_a = document.createElement('a');
            new_block_commentary_a.setAttribute("href", "#node-article-comment");
            new_block_commentary_a.setAttribute("class", "button button-comment");
                        new_block_commentary_a.innerHTML = Drupal.t('Lire les commentaires');
            new_block_commentary.appendChild(new_block_commentary_a);
            pub_article.appendChild(new_block_commentary);

            var new_block = document.createElement('div');
            new_block.setAttribute("id", "taboola-below-article-thumbnails");
            pub_article.appendChild(new_block);

            window._taboola = window._taboola || [];
            _taboola.push({
                mode: 'alternating-thumbnails-a',
                container: 'taboola-below-article-thumbnails',
                placement: 'Below Article Thumbnails',
                target_type: 'mix'
            });
          }
        };
      </script>
