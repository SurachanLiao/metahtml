        var articleImage= "/image/policy:1.49800238:1624456049/image.jpg?f=Wide%26w=1200%26%24p%24f%24w=834285a";
        var articleTags= "Konsument%2C+Livsmedel%2C+Livsmedelsverket";
        var articlePublishedTime= "2021-06-20 13:12:00";
        var d = document;
        var s = d.createElement('script');
        var scriptUrl = 'https://comment.ifragasatt.se/load?articleId=' + articleId + '&customerId=' + customerId + '&commentsToShow=' + commentsToShow +
                                '&alias=' + alias + '&cssUrl=' + cssUrl + '&adFrequency=' + adFrequency + '&articleUrl=' + articleUrl + '&articleTitle=' + articleTitle +
