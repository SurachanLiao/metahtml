        (d.head || d.body).appendChild(s);

        // Create an observer instance linked to the callback function
        var observer = new MutationObserver(
            function(mutationsList, observer) {
                for(let mutation of mutationsList) {
                    if (mutation.type === 'attributes' && mutation.target.tagName === 'IFRAME') {
                        const event = 'standardEvent';
                        const eventLabel = decodeURIComponent(articleTitle.replace(/\+/g, '%20'));
                        let foundLayer = false;

                        for (const layer of dataLayer) {
                            if (layer.event === event && layer.eventLabel === eventLabel) {
                                foundLayer = true;
                            }
                        }

                        if (!foundLayer) {
                            dataLayer.push({
                                'event': event, 
                                'eventCategory': 'Ifrågasatt', 
                                'eventAction': 'Expandera fönstret', 
                                'eventLabel': eventLabel
                            });
                        }
                    }
                }
            }
        );

        // Start observing the target node for configured mutations
        observer.observe(
            document.getElementById('ifragasatt-1.49800249'),
            { attributeFilter: ['style'], childList: true, subtree: true }
        );
    })();
