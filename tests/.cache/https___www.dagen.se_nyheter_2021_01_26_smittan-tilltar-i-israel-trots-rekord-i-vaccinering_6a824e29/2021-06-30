 domain=\"+domain :  \"\";\n            document.cookie = name+\"=\"+value+expires+domainStr+\"; path=/\";\n        },\n        readCookie: function(name){\n            var nameEQ = name + \"=\";\n            var ca = document.cookie.split(';');\n            for(var i=0;i < ca.length;i++) {\n                var c = ca[i];\n                while (c.charAt(0)==' ') c = c.substring(1,c.length);\n                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);\n            }\n            return null;\n        },\n        deleteCookie: function(name,domain){\n            this.createCookie(name,\"\",-1,domain);\n        }\n    };\n    \n<\/script>\n\n\n<style>\n.cookie-acceptance {\n  position: fixed;\n  left: 0px;\n  right: 0px;\n  bottom: 0px;\n  display: block;\n  width: 90%;\n  min-height: 100px;\n  margin-right: auto;\n  margin-left: auto;\n  padding: 10px 20px 10px;\n  border-radius: 2px 2px 0px 0px;\n  background-color: #e6e9ed;\n  line-height: 130%;\n  display:none;\n}\n\n.cookie-acceptance-wrapper {\n  display: block;\n  max-width: 1200px;\n  margin-right: auto;\n  margin-left: auto;\n}\n\n.cookie-acceptance-title {\n  max-width: 60%;\n  font-size: 1rem;\n}\n\n.cookie-acceptance-text {\n  margin-top: 0px;\n  margin-bottom: 10px;\n  font-size: 15px;\n}\n\n.cookie-acceptance-button {\n  background-color: #96CA4D;\n  font-size: 15px;\n}\n\n.cookie-acceptance-close {\n  position: absolute;\n  top: 0px;\n  right: 0px;\n  width: 20px;\n  margin-top: 30px;\n  margin-right: 30px;\n}\n\n.w-button {\n    display: inline-block;\n    padding: 9px 15px;\n    color: white;\n    text-decoration: none;\n    cursor: pointer;\n}\n</style>\n"},"displayProperties":{},"localEdits":{},"variants":{}}}]}]}]};Fusion.spa=false;Fusion.spaEnabled=false;</script><script type="text/javascript" src="/pf/resources/js/mentor-common.js?d=68" defer=""></script><script type="text/javascript" src="/pf/resources/js/tracking.js?d=68" defer=""></script><script type="text/javascri