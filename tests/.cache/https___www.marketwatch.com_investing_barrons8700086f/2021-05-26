        <script type="text/javascript">
            (function() {
                function loadTealiumScript() {
                    var script = document.createElement('script');
                    script.async = true;
                    script.src = '//tags.tiqcdn.com/utag/wsjdn/marketwatch/prod/utag.js';
                    var element = document.getElementsByTagName('script')[0];
                    element.parentElement.insertBefore(script, element);
                }

                if (window.djcmp && window.djcmp.gdprApplies('cookieOnly')) {
                    window.djcmp('onReady', function() {
                        loadTealiumScript();
                    });
                } 
                else {
                    loadTealiumScript();
                }
            })();
        </script>
        
