                                        observeFromUAC: !false,
                                        triggerPrebid: true,
                                        moatEnabled: true,
                                        collapseAdBeforeFetch: true,
                                        threshold: 0.01
                                    }
                                });
                            }();
                        </script>
                    </div>
                </div>
            </div>
            </div>
