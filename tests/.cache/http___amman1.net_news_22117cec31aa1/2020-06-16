<script src="/content/assets/dataTables/js/jquery.dataTables.min.js"></script>

<script src="/content/assets/Polyfill-Object-fit/js/polyfill.object-fit.core.js"></script>
<script src="/content/assets/Polyfill-Object-fit/js/polyfill.getMatchedCSSRules.js"></script>

<script src="/js/jquery.mCustomScrollbar.min.js"></script>
<script src="/js/jquery.newsticker.min.js"></script>
<script src="/js/fancy.js"></script>
<script src="/content/js/shared.js" type="text/javascript"></script>
<script>
                    
        jQuery.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': jQuery('meta[name="csrf-token"]').attr('content')
            }
        });
        var CSRF_TOKEN = jQuery('meta[name="csrf-token"]').attr('content');
           function send_comment(){
               console.log(CSRF_TOKEN);
               var messageafter =' تم ارسال التعليق بنجاح سيتم النشر بعد المراجعة';
            $.post('/sendcomment', {
                'body' : $('#comment').val(),
                'article_id': $('#article_id').val(),
                '_token': CSRF_TOKEN,
                'email': $('#email').val(),
                'commenter_name':  $('#commenter_name').val(),
               
            }).done(function (result) {
                $('.toremove-beforeajax').remove(); 
              
                $('#ajax-message').html(
                    '<div class="alert alert-success"><i class="fa fa-check-circle fa-lg"></i> ' +
                        messageafter + '</div>');   
              

            })
            .fail(function (xhr, status, error) {
                var  msgerror =  '' ;
                if($('#commenter_name').val() == ''){
                    msgerror += ' <b> الاسم لا يمكن ان يكون فارغا  </b> <br/>' 
                }
                if($('#comment').val() == ''){
                    msgerror += ' <b>  التعليق لا يمكن ان يكون فارغا </b> <br/>'
                }
                if($('#email').val() == ''){
                    msgerror += ' <b> البريد الالكتروني لا يمكن ان يكون فارغا </b> <br/>'
                }
                
             
                $('.toremove-beforeajax').remove(); 
                $('#ajax-message').html(
                    '<div class="alert alert-danger"><i class="fa fa-wrong-circle fa-lg"></i> ' +
                        msgerror + '</div>');
            });
            return false;
        }
        </script>
</body>
