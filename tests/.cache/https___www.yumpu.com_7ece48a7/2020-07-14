    var upload_file_name = '';
    var upload_file_size = 0;
    var upload_hash = create_upload_hash();
    var last_upload_progress = 0;
    var user_logged_in = false;

    $(function() {
		try {
			Tracking.ypeventtrack('all', 'ymp_visit_homepage');
		} catch (e) {}
		
        if($.support.leadingWhitespace)
        {
            $('.yp-file-progress').knob({
                'min': 0,
                'max': 100,
                'value': 0,
                'readOnly': true,
                'width': 200,
                'height': 200,
                'fgColor': '#0082B2',
                'inputColor': '#0082B2',
                'dynamicDraw': true,
                'thickness': 0.1,
                'tickColorizeValues': true,
                'skin': 'tron',
                'draw' : function () {
                    $(this.i).val(this.cv + '%')
                }
            });
        }
        else
        {
            $('.yp-file-progress-wrapper').addClass('hidden');
            $('.yp-upload-progress-ie8').removeClass('hidden');
        }

        var uploader = new plupload.Uploader({
            runtimes: 'html5,flash,html4',
            url : 'https://yumpu-uploads.s3.dualstack.eu-west-1.amazonaws.com/',
            multipart: true,
            multipart_params: {
                'key': '${filename}',
                'Filename': '${filename}',
                'acl': 'public-read',
                'Content-Type': 'application/pdf',
                'AWSAccessKeyId' : 'AKIAJSXP3ZUA4KBMEINA',
                'policy': '',
                'signature': '3YvURLPAVlEmuHi+8RQFWFs6520='
            },
            file_data_name: 'file',
            browse_button: 'yp-upload-box-step-select-file',
            container: 'yp-upload-container',
            drop_element: 'yp-body',
            max_file_size: '600mb',
            unique_names: true,
            max_file_count: 1,
            multi_selection: false,
            filters : {
                max_file_size: '600mb',
                mime_types: [
                    {title: "PDF file", extensions: "pdf"}
                ]
            },
            flash_swf_url : 'https://assets.yumpu.com/release/w9AjCVk60i/v4/js/plugins/plupload-2.1.1/Moxie.swf',
    		silverlight_xap_url : 'https://assets.yumpu.com/release/w9AjCVk60i/v4/js/plugins/plupload-2.1.1/Moxie.xap',
            init: {
                Init: function(up, params) {

                },

                PostInit: function() {

                    $("#yp-body").on("dragover", function(e) {
                        if(!upload_running)
                        {
                            clearTimeout(tid);
                            e.stopPropagation();
                            e.preventDefault();
                            $('#yp-sp-drop-overlay-container').show();
                            $('#yp-sp-drop-overlay').show();
                        }
                    });

                    $('#yp-body').on("dragleave", function(e) {
                        if(!upload_running)
                        {
                            e.stopPropagation();
                            tid = setTimeout(function(){
                                e.stopPropagation();
                                $('#yp-sp-drop-overlay-container').hide();
                                $('#yp-sp-drop-overlay').hide();
                            }, 0);
                        }
                    });

                    $('#yp-body').on("drop", function(e) {
                        if(!upload_running)
                        {
                            $('#yp-sp-drop-overlay-container').hide();
                            $('#yp-sp-drop-overlay').hide();
                        }
                    });
                },
                FilesAdded: function(up, files) {
                    if(!upload_running)
                    {
                        plupload.each(files, function(file) {
                            if(up.files.length > 1)
                            {
                                up.removeFile(file);
                            }

                            if(up.files.length == 1)
                            {
                                $(window).on('beforeunload', function () {
                                    return '';
                                });

                                $('#yp-upload-box-step-select-file').addClass('hidden');
                                $('#yp-upload-box-step-upload-file').removeClass('hidden');
                                $('#yp-upload-file-total-size').html(plupload.formatSize(file.size));
                                $('#yp-upload-file-upload-progress-ie8').html('0%');

                                upload_file_name = file.name;
                                upload_file_size = plupload.formatSize(file.size);

                                up.disableBrowse(true);
                                upload_running = true;
                                uploader.start();
                            }
                        });
                    }
                },
                UploadProgress: function(up, file) {
                    $({value: last_upload_progress}).animate({value: file.percent}, {
                        duration: 1000,
                        easing:'swing',
                        step: function()
                        {
                            $('#yp-upload-file-upload-progress').val(Math.ceil(this.value)).trigger('change');
                        }
                    });

                    $('#yp-upload-file-upload-progress-ie8').html(file.percent + '%');
                    $('#yp-upload-file-uploaded-size').html(plupload.formatSize(file.loaded));

                    if(file.percent == 100)
                    {
                        $('#yp-upload-file-upload-progress-ie8').html('');
                        $('#yp-upload-file-upload-progress-ie8').css('background-position', '0 200px');
                    }

                    last_upload_progress = file.percent;
                },
                FileUploaded: function(up, file, json_response){
                    $(window).unbind('beforeunload');

                    if(json_response.status)
                    {
                        if(json_response.status != 204)
                        {
                            Messenger().post({
                                message: 'There was an error while uploading.',
                                type: 'error',
                                showCloseButton: true
                            });

                            $('#yp-upload-file-upload-progress').val(0).trigger('change');
                            $('#yp-upload-file-upload-progress-ie8').html('0%');

                            uploader.splice();
                            uploader.refresh();
                        }
                        else
                        {
                            log_upload(upload_hash, file.id, file.name);
                            uploader.destroy();

                            //HD Tracking
                            							try {
								Tracking.ypeventtrack('all', 'ymp_action_upload_completed_homepage');
								Tracking.ypeventtrack('all', 'ymp_action_upload_completed_all');
							} catch (e) {}
                         }
                    }
                },
                BeforeUpload: function(uploader, file) {
                    uploader.settings.multipart_params.key = upload_hash + '.pdf';
                },
                Error: function(up, err) {
                    Messenger().post({
                        message: 'Error: ' + err.code + ', Message: ' + err.message + (err.file ? ', File: ' + err.file.name : ''),
                        type: 'error',
                        showCloseButton: true
                    });

                    up.refresh();
                }
            }
        });

                    uploader.init();
            
        $('#yp-btn-upload-login').on('click', function(){
            $('#yp-upload-box-step-login').addClass('hidden');
            $('#yp-upload-box-step-login-email').removeClass('hidden');
        });

        $('#yp-btn-upload-register').on('click', function(){
            $('#yp-upload-box-step-login').addClass('hidden');
            $('#yp-upload-box-step-register').removeClass('hidden');
        });

        $('#yp-btn-upload-login').on('click', function(){
            $('#yp-upload-box-step-login').removeClass('hidden');
            $('#yp-upload-box-step-register').addClass('hidden');
        });

        $('#yp-btn-upload-login-email-fp').on('click', function(){
            $('#yp-upload-box-step-login').addClass('hidden');
            $('#yp-upload-box-step-login-fp').removeClass('hidden');
        });

        $('#yp-btn-upload-login-fp').on('click', function(){
            user_forgot_password();
        });

        $("#yp-input-upload-login-fp-email").on('keyup', function (e) {
            if(e.keyCode == 13)
            {
                user_forgot_password();
            }
        });

        $("#yp-input-upload-login-fp-cc").on('keyup', function (e) {
            if(e.keyCode == 13)
            {
                user_forgot_password();
            }
        });

        $('#yp-btn-upload-login-email').on('click', function(){
            user_login();
        });

        $("#yp-input-upload-login-email").on('keyup', function (e) {
            if(e.keyCode == 13)
            {
                user_login();
            }
        });

        $("#yp-input-upload-login-password").on('keyup', function (e) {
            if(e.keyCode == 13)
            {
                user_login();
            }
        });

        $('#yp-btn-upload-register-email').on('click', function(){
            create_user();
        });

        $('#yp-upload-mag-email-form').on('submit', function(e){
            e.preventDefault();
        });

        $('#yp-input-upload-register-email').on('keyup', function (e) {
            if(e.keyCode == 13)
            {
                create_user();
            }
        });

        $('.yp-lnk-upload-fbc').attr('href', 'https://www.yumpu.com/user/connect/facebook/upload/en' + '/' + upload_hash);
        $('.yp-lnk-upload-gc').attr('href', 'https://www.yumpu.com/user/connect/google/upload/en' + '/' + upload_hash);
        $('.yp-lnk-upload-tc').attr('href', 'https://www.yumpu.com/user/connect/twitter/upload/en' + '/' + upload_hash);
    });

    function analyse_upload() {
        $('#yp-upload-file-analyse-progress').val(0).trigger('change');
        $('#yp-upload-file-analyse-progress-ie8').html('0%');

        analyse_upload_interval = window.setInterval(function () {
            if (!analyse_upload_running && upload_id) {
                analyse_upload_running = true;

                $.ajax({
                    type: 'GET',
                    cache: false,
                    url: 'https://www.yumpu.com/en/document/progress' + '/' + upload_id
                }).done
                (
                    function (analyse_upload_progress_data) {
                        analyse_upload_running = false;

                        json_analyse_upload_progress_data = $.parseJSON(analyse_upload_progress_data);
                        if(json_analyse_upload_progress_data.details)
                        {
                            if(json_analyse_upload_progress_data.details.split && json_analyse_upload_progress_data.details.split.state == 'processing' && json_analyse_upload_progress_data.details.split.progress >= 0 && json_analyse_upload_progress_data.details.split.progress <= 100 && last_analyse_progress < 100)
                            {
                                rendering_progress_started = true;

                                $({value: last_analyse_progress}).animate({value: json_analyse_upload_progress_data.details.split.progress}, {
                                    duration: 1000,
                                    easing:'swing',
                                    step: function()
                                    {
                                        $('#yp-upload-file-analyse-progress').val(Math.ceil(this.value)).trigger('change');
                                    }
                                });

                                $('#yp-upload-file-analyse-progress-ie8').html(json_analyse_upload_progress_data.details.split.progress + '%');
                                last_analyse_progress = json_analyse_upload_progress_data.details.split.progress;
                            }
                            else if(json_analyse_upload_progress_data.details.text_extract && json_analyse_upload_progress_data.details.text_extract.state == 'processing' && json_analyse_upload_progress_data.details.text_extract.progress >= 0 && json_analyse_upload_progress_data.details.text_extract.progress <= 100 && last_rendering_progress < 100)
                            {
                                text_extraction_progress_started = true;

                                if(rendering_progress_started)
                                {
                                    rendering_progress_started = false;

                                    $('#yp-upload-file-analyse-progress').addClass('hidden');
                                    $('#yp-upload-file-analyse-progress').parent().prepend($('#yp-upload-file-analyse-progress-check').removeClass('hidden'));

                                    $('#yp-upload-file-analyse-progress-ie8').html('');
                                    $('#yp-upload-file-analyse-progress-ie8').css('background-position', '0 200px');

                                    $('#yp-upload-box-step-analyse-file .yp-upload-box-headline h2').html('Done');

                                    $({value: last_analyse_progress}).animate({value: '100'}, {
                                        duration: 1000,
                                        easing:'swing',
                                        step: function()
                                        {
                                            $('#yp-upload-file-analyse-progress').val(Math.ceil(this.value)).trigger('change');
                                        }
                                    });

                                    window.setTimeout(function(){
                                        $('#yp-upload-box-step-analyse-file').addClass('hidden');
                                        $('#yp-upload-box-step-rendering-file').removeClass('hidden');
                                    }, 2500);
                                }

                                $({value: last_rendering_progress}).animate({value: json_analyse_upload_progress_data.details.text_extract.progress}, {
                                    duration: 1000,
                                    easing:'swing',
                                    step: function()
                                    {
                                        $('#yp-upload-file-rendering-progress').val(Math.ceil(this.value)).trigger('change');
                                    }
                                });

                                $('#yp-upload-file-rendering-progress-ie8').html(json_analyse_upload_progress_data.details.text_extract.state + '%');
                                last_rendering_progress = json_analyse_upload_progress_data.details.text_extract.progress;
                            }
                            else if(json_analyse_upload_progress_data.overview.step && json_analyse_upload_progress_data.overview.step == 'magazine_created')
                            {
                                if(text_extraction_progress_started)
                                {
                                    text_extraction_progress_started = false;

                                    $('#yp-upload-file-rendering-progress').addClass('hidden');
                                    $('#yp-upload-file-rendering-progress').parent().prepend($('#yp-upload-file-rendering-progress-check').removeClass('hidden'));

                                    $('#yp-upload-file-rendering-progress-ie8').html('');
                                    $('#yp-upload-file-rendering-progress-ie8').css('background-position', '0 200px');

                                    $('#yp-upload-box-step-rendering-file .yp-upload-box-headline h2').html('Done');

                                    $({value: last_rendering_progress}).animate({value: '100'}, {
                                        duration: 1000,
                                        easing:'swing',
                                        step: function()
                                        {
                                            $('#yp-upload-file-rendering-progress').val(Math.ceil(this.value)).trigger('change');
                                        }
                                    });
                                }

                                window.clearInterval(analyse_upload_interval);
                                window.setTimeout(function(){
                                    if(json_analyse_upload_progress_data.overview.redirect)
                                    {
                                        window.location.href = json_analyse_upload_progress_data.overview.redirect;
                                    }
                                    else
                                    {
                                        window.location.href = 'https://www.yumpu.com/en/account/documentmanager';
                                    }
                                }, 2500);
                            }
                            else if(json_analyse_upload_progress_data.overview.step && json_analyse_upload_progress_data.overview.step == 'rendering_failed')
                            {
                                Messenger().post({
                                    message: json_analyse_upload_progress_data.message,
                                    type: 'error',
                                    showCloseButton: true
                                });

                                window.clearInterval(analyse_upload_interval);

                                $('#yp-upload-file-analyse-progress').val(0).trigger('change');
                                $('#yp-upload-file-analyse-progress-ie8').html('0%');

                                $('#yp-upload-file-rendering-progress').val(0).trigger('change');
                                $('#yp-upload-file-rendering-progress-ie8').html('0%');
                            }
                        }
                        else
                        {
                            $('#yp-upload-file-analyse-progress').val(0).trigger('change');
                            $('#yp-upload-file-analyse-progress-ie8').html('0%');

                            $('#yp-upload-file-rendering-progress').val(0).trigger('change');
                            $('#yp-upload-file-rendering-progress-ie8').html('0%');
                        }
                    }
                );
            }
        }, 2500);
    }

    function create_upload_hash()
    {
        var random_hash = '';
        var pool = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';

        for( var i=0; i < 70; i++ )
            random_hash += pool.charAt(Math.floor(Math.random() * pool.length));

        return random_hash;
    }

    function log_upload(upload_hash, file_id, file_name) {
        $.ajax({
            type: 'POST',
            data: {
                file_id: file_id,
                file_name: file_name
            },
            url: 'https://www.yumpu.com/en/upload/log' + '/' + upload_hash
        }).done(function (msg) {
            json_data = $.parseJSON(msg);
            if(json_data)
            {
                if(json_data.ok)
                {
                    $('#yp-upload-file-upload-progress-info').html('Done');

                    $('#yp-upload-file-upload-progress').addClass('hidden');
                    $('#yp-upload-file-upload-progress').parent().prepend($('#yp-upload-file-upload-progress-check').removeClass('hidden'));

                    if(user_logged_in)
                    {
                        window.setTimeout(function(){
                            $('#yp-upload-box-step-upload-file').addClass('hidden');
                            $('#yp-upload-box-step-analyse-file').removeClass('hidden');
                        }, 2500);

                        create_upload(true);
                    }
                    else
                    {
                        $('#yp-upload-box-step-upload-file').addClass('hidden');
                        $('#yp-upload-box-step-register').removeClass('hidden');
                    }
                }
                else
                {
                    Messenger().post({
                        message: 'Message: ' + json_data.message,
                        type: 'error',
                        showCloseButton: true
                    });
                }
            }
        });
    }

    function create_upload(show_analyse_upload_progress) {
        $.ajax({
            type: 'GET',
            cache: false,
            url: 'https://www.yumpu.com/en/upload/create' + '/' + upload_hash
        }).done(function (msg) {
            json_data = $.parseJSON(msg);
            if (json_data) {
                if (json_data.ok) {
                    upload_id = json_data.upload_id;
                    if(show_analyse_upload_progress)
                    {
                        analyse_upload();
                    }
                }
                else {
                    Messenger().post({
                        message: 'Message: ' + json_data.message,
                        type: 'error',
                        showCloseButton: true
                    });
                }
            }
        });
    }

    function create_user() {
        $.ajax({
            type: 'POST',
            url: '/en/user/create',
            data: $('#yp-upload-mag-email-form').serialize(),
        }).done(function (msg) {
            json = $.parseJSON(msg);
            if(json)
            {
                if(json.ok)
                {
                    create_upload(false);
                    window.location.replace(json.redirect);
                    $('#yp-upload-box-step-register').addClass('hidden');
                    $('#yp-upload-box-step-ci').removeClass('hidden');
                }
                else
                {
                    try {
                        grecaptcha.reset();
                    } catch (e) {}
                    Messenger().post({
                        message: json.message,
                        type: 'error',
                        showCloseButton: true
                    });
                }
            }
        });

        return false;
    }

    function user_login() {
        $.ajax({
            type: 'POST',
            cache: false,
            url: '/en/user/login/verify',
            data: {
                'yp-input-user-login-email-username': $('#yp-input-upload-login-email').val(),
                'yp-input-user-login-password': $('#yp-input-upload-login-password').val()
            }
        }).done(function (msg) {
            json = $.parseJSON(msg);
            if(json)
            {
                if(json.success)
                {
                    user_logged_in = true;
                    create_upload(true);
					try {
                    	Tracking.ypeventtrack('all', 'DocumentUpload');
					} catch (e) {}

                    //Google Tag Manager
                    try {
                        dataLayer.push({'event': 'DocumentUpload'});
                    } catch (e) {}

                    $('#yp-upload-box-step-login').addClass('hidden');
                    $('#yp-upload-box-step-analyse-file').removeClass('hidden');
                }
                else
                {
                    Messenger().post({
                        message: json.message,
                        type: 'error',
                        showCloseButton: true
                    });

                    $('#yp-input-upload-register-email').val($('#yp-input-upload-login-email').val());
                    $('#yp-btn-upload-login').click();

                }
            }
        });

        return false;
    }

    function user_forgot_password() {

        $.ajax({
            type: 'POST',
            cache: false,
            url: '/en/user/forgot_password/verify',
            data: {
                'yp-input-user-fp-email': $('#yp-input-upload-login-fp-email').val(),
                'g-recaptcha-response': $('#g-recaptcha-response').val(),
                'upload_hash': upload_hash
            }
        }).done(function (msg) {
            json = $.parseJSON(msg);
            if(json)
            {
                if(json.success)
                {
                    $('#yp-upload-box-step-login-fp').addClass('hidden');
                    $('#yp-upload-box-step-ci-fp').removeClass('hidden');
                }
                else
                {
                    Messenger().post({
                        message: json.message,
                        type: 'error',
                        showCloseButton: true
                    });

                }
            }
        });

        return false;
    }
</script>

