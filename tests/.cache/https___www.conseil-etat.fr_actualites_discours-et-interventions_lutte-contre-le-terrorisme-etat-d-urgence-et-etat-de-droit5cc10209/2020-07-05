            .starter-template {
              padding: 3rem 1.5rem;
              text-align: center;
            }
            .query .card-block {
                padding: 1.25rem;
            }
            .ui-autocomplete {
                z-index: 1050 !important;
                cursor: pointer;
                background-color: white;
            }
            .ui-menu-item:hover {
                color:#005092;
