$(document).ready(function() {
var $status = $('.pagingInfo-6SHC1');
$('.hc6-slider1').on('init reInit afterChange', function(event, slick, currentSlide, nextSlide) {
//currentSlide is undefined on init -- set it to 0 in this case (currentSlide is 0 based)
var i = (currentSlide ? currentSlide : 0) + 1;
$status.text(i + '/' + slick.slideCount);
});
$('.hc6-slider1').slick({
infinite: true,
centerMode: true,
centerPadding: '52.5px',
slidesToShow: 1,
slidesToScroll: 1,
focusOnSelect: true,
variableWidth: true,
autoplay: false,
autoplaySpeed: 2000,
prevArrow: '<div class="pc-prev"></div>',
nextArrow: '<div class="pc-next"></div>',
