<script type="1de23fd819e1daf0f37283d9-text/javascript">
$(document).ready(function() {
var $status = $('.pagingInfo-6SHC2');
$('.hc6-slider2').on('init reInit afterChange', function(event, slick, currentSlide, nextSlide) {
//currentSlide is undefined on init -- set it to 0 in this case (currentSlide is 0 based)
var i = (currentSlide ? currentSlide : 0) + 1;
$status.text(i + '/' + slick.slideCount);
});
$('.hc6-slider2').slick({
infinite: true,
centerMode: true,
centerPadding: '52.5px',
slidesToShow: 1,
slidesToScroll: 1,
focusOnSelect: true,
variableWidth: true,
autoplay: false,
autoplaySpeed: 2000,
prevArrow: '<div class="pc-prev"></div>',
nextArrow: '<div class="pc-next"></div>',
responsive: [{
breakpoint: 767,
settings: {
slidesToShow: 2,
centerMode: false,
centerPadding: '0px'
}
}]
});
});
</script>
