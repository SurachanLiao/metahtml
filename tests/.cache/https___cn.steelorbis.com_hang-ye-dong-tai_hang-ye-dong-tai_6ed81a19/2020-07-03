	                   options.each(function() {
	                       if ($(this).attr('label') !== undefined) {
	                           labels.push($(this).attr('label'));
	                       }
	                       else {
	                           labels.push($(this).html());
	                       }
	                   });
	                return '产品组: ' + labels;
	           } else if ($('option', $(select)).length == options.length) {
	        	   return '产品组: 全部';
	           } else {
	        	   return '产品组: ' + options.length + '选中';
	           }
