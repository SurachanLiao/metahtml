                <script type="text/javascript">

            function abrirVentana_2(url) {
                // descomentar esto y quitar href de imagen
                window.open(url, "nuevo", "directories=no, location=no, menubar=no, scrollbars=yes, statusbar=no, tittlebar=no, width=400, height=400");
            }

            $(document).on('click', '#bar-hide-button', function(){
                if( $('#bar-hide-button').hasClass('normal') ){
                    $('.final_bar').removeClass('before_remove_transition');
                    $('#bar-hide-button').removeClass('normal');
                    $('#bar-hide-button').addClass('abajo');
                    $('.final_bar').removeClass('transition_up_class');
                    setTimeout(function(){
                        $('.final_bar').addClass('transition_down_class');
                    },100);
                }else{
                    $('#bar-hide-button').removeClass('abajo');
                    $('#bar-hide-button').addClass('normal');
                    $('.final_bar').addClass('before_remove_transition');
                    $('.final_bar').removeClass('transition_down_class');
                    setTimeout(function(){
                        $('.final_bar').addClass('transition_up_class');

                    },100);
                }
            })

            $(document).on('click', '#boton_reproducir_bar', function(){
                if( document.getElementById("stream_bar").paused ){
                    document.getElementById("stream_bar_senal_2").pause();
                    document.getElementById("stream_bar").play();
                    $('#boton_reproducir_bar').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/pause.png');
                    $('#boton_reproducir_bar_senal_2').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/play.png');
                }else{
                    document.getElementById("stream_bar").pause();
                    $('#boton_reproducir_bar').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/play.png');
                }
            });

            $(document).on('click', '#boton_reproducir_bar_senal_2', function(){
                if( document.getElementById("stream_bar_senal_2").paused ){
                    document.getElementById("stream_bar").pause();
                    document.getElementById("stream_bar_senal_2").play();
                    $('#boton_reproducir_bar_senal_2').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/pause.png');
                    $('#boton_reproducir_bar').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/play.png');
                }else{
                    document.getElementById("stream_bar_senal_2").pause();
                    $('#boton_reproducir_bar_senal_2').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2017/08/play.png');
                }
            });
            var loadScript = function (src, callbackfn) {
                var newScript = document.createElement("script");
                newScript.type = "text/javascript";
                newScript.setAttribute("async", "true");
                newScript.setAttribute("src", src);
                newScript.setAttribute("data-dojo-config", "usePlainJson: true, isDebug: false");

                if (newScript.readyState) {
                    newScript.onreadystatechange = function () {
                        if (/loaded|complete/.test(newScript.readyState))
                        callbackfn();
                    }
                    } else {
                    newScript.addEventListener("load", callbackfn, false);
                }

                document.documentElement.firstChild.appendChild(newScript);
            }
            function setCookie(cname, cvalue, exdays) {
                var d = new Date();
                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                var expires = "expires=" + d.toUTCString();
                document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
            }

            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }
            var ce = getCookie('EvilCount');
            if (ce != "" && ce != null) {
                ce = parseInt(ce);
                } else {
                ce = 0;
            }
            var ceOcultar = getCookie('MCEvilPopupClosed');
            ce++;
            if (ce >= 3) {
                setCookie('MCEvilPopupClosed', 'yes');
                setCookie('EvilCount', ce, 10);
                } else {
                setCookie('MCEvilPopupClosed', '', 0);
                setCookie('EvilCount', ce, 10);

            }

        </script>
        <style>
            .mc-banner, .mc-modal{
            top:30% !important;
            }
        </style>
        
    </section>


</div>
