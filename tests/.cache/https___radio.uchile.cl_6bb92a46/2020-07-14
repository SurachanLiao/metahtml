    $(document).on('click', '#radio_new_logo1', function(){
        if( document.getElementById("stream_bar").paused ){
            document.getElementById("stream_bar_senal_2").pause();
            document.getElementById("stream_bar").play();
            $('#radio_new_logo').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2019/11/PlayingRuch.gif');
            $('#text_active_senal_2').attr('src', 'https://radio.uchile.cl/wp-includes/images/radioS2.png');
            document.cookie = 'play1=1';
            document.cookie = 'play2=0';
        }else{
            document.getElementById("stream_bar").pause();
            $('#radio_new_logo').attr('src', 'https://radio.uchile.cl/wp-includes/images/LogoplayFM.png');
            document.cookie = 'play1=0';
        }
    });

    $(document).on('click', '#text_active_senal_21', function(){
        if( document.getElementById("stream_bar_senal_2").paused ){
            document.getElementById("stream_bar").pause();
            document.getElementById("stream_bar_senal_2").play();
            $('#text_active_senal_2').attr('src', 'https://radio.uchile.cl/wp-content/uploads/2019/11/PlayingRuch.gif');
            $('#radio_new_logo').attr('src', 'https://radio.uchile.cl/wp-includes/images/LogoplayFM.png');
            document.cookie = 'play2=1';
            document.cookie = 'play1=0';
        }else{
            document.getElementById("stream_bar_senal_2").pause();
            $('#text_active_senal_2').attr('src', 'https://radio.uchile.cl/wp-includes/images/radioS2.png');
            document.cookie = 'play2=0';
        }
    });
</script>
