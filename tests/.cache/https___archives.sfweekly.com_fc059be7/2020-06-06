          <script>
            if (jQuery) {
              (function($) {

                // Add hover behavior for overlays.
                $('#NavigationandSearch .collapsed').hide();
                $('#NavigationandSearch .expandable').hover(
                  function() {
                    var expandable = $(this);
                    var launchtimer = window.setTimeout(function() {
                      expandable.addClass('expanded');
                      expandable.find('.expanding.collapsed').each(function() {
                        $(this).removeClass('collapsed');
                        $(this).show();
                      });
                    }, 250);
                    expandable.data('launchtimer', launchtimer);
                  },
                  function() {
                    // Don't unhover if entering an image editor button.
                    if (!$('.imgeditor_button').is(':visible')) {
                      var expandable = $(this);
                      var launchtimer = expandable.data('launchtimer');
                      if (launchtimer != null) {
                        window.clearTimeout(launchtimer);
                      }
                      expandable.find('.expanding').each(function() {
                        $(this).hide();
                        $(this).addClass('collapsed');
                      });
                      expandable.removeClass('expanded');
                    }
                  }
                );
              })(jQuery);
            }
          </script>
        
      
  
