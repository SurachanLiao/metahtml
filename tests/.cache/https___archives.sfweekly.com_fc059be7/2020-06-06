        <script>
          (function ($) {
            function togglePanel(container, index) {
              // Update narrowByDate in form.
              $(container).find("input[name='narrowByDate']").val($(container).children('.tabs').find('li[value="' + index + '"]>a').attr("data-date"));

              // Update selected tab.
              $(container).children('.tabs').children('li').removeClass('selected');
              $(container).children('.tabs').children('li[value="' + index + '"]').addClass('selected');

              // Update selected panel.
              var panel = $(container).children('.panels').children('li[value="' + index + '"]');
              $(container).children('.panels').children('li').hide();
              panel.show();

              // Load newly-selected items, if needed.
              if (!panel.hasClass('loaded')) {
                panel.addClass('loading');
                var loadURL = panel.attr('data-load');
                if (loadURL) {
                  // Assumes we load contents from a disposable container.
                  panel.load(loadURL, function () {
                    panel.removeClass('loading').addClass('loaded');
                  });
                }
              }
            }

            // Populate selectors via AJAX.
            $("#TheaterSearchPanel select[data-load]").each(function () {
              var container = $(this);
              if (!container.hasClass('loaded')) {
                container.addClass('loading');
                var loadURL = container.attr('data-load');
                if (loadURL) {
                  // Assumes we load contents from a disposable container.
                  $(this).load(loadURL, function () {
                    container.children('select').children().unwrap();
                    container.removeClass('loading').addClass('loaded');
                  });
                }
              }
            });

            togglePanel($('#TheaterSearchPanel>.componentBody'), 1);

            $('#TheaterSearchPanel>.componentBody>.tabs>li').click(function(v) {
              var index = $(this).attr('value');
              if (index) {
                togglePanel($(this).closest('.componentBody'), index);
              }
              v.preventDefault();
            });
          })(jQuery);
        </script>
      
    </div><!-- #TheaterSearchPanel last updated 2020-06-06 08:00:00 -->
  


