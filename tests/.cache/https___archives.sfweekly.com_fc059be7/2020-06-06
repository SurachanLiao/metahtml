      <script>
        (function ($) {
          // If selectors aren't there, pull them in.
          $("#DiningSearchPanel select[data-load]").each(function () {
            var container = $(this);
            if (!container.hasClass('loaded')) {
              var loadURL = container.attr('data-load');
              if (loadURL) {
                // Assumes we load contents from a disposable container.
                $(this).load(loadURL, function () {
                  container.children('select').children().unwrap();
                  container.addClass('loaded');
                });
              }
            }
          });

          // Load contents, if needed.
          $("#DiningSearchPanel div[data-load]").each(function () {
            var container = $(this);
            if (!container.hasClass('loaded')) {
              var loadURL = container.attr('data-load');
              if (loadURL) {
                // Assumes we load contents from a disposable container.
                $(this).load(loadURL, function () {
                  container.addClass('loaded');
                });
              }
            }
          });

        })(jQuery);
      </script>
    </div><!-- #DiningSearchPanel last updated 2020-06-06 08:00:00 -->
  


