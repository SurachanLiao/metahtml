$(function(){
// ------------------------------------------------------------------------------------
// test etat menu mobile depuis cookie
var stickystate = 'open';
if(existsCookie('stickycookie')){
stickystate = readCookie('stickycookie');
if (stickystate == 'close') {
$('.menu_special').accordion({
header: '.menu_special_header',
heightStyle: "content",
collapsible: true,
active: false
});
}
else {
$('.menu_special').accordion({
header: '.menu_special_header',
heightStyle: "content",
collapsible: true
});
}
}
else{
createCookie('stickycookie',stickystate,30);
$('.menu_special').accordion({
header: '.menu_special_header',
heightStyle: "content",
collapsible: true,
active: false
});
}
// ------------------------------------------------------------------------------------
// ------------------------------------------------------------------------------------
// Menu mobile hide
$( ".menu_special_header" ).click(function() {
// ecriture etat dans cookie
if($( ".menu_special_header" ).hasClass('ui-accordion-header-active')){
stickystate = 'open';
if (typeof ga != 'undefined') {
ga('send','event', 'Action','Menu Mobile','Open');
}
//console.log('Open');
//console.log($(window).width());
}
else {
stickystate = 'close';
if (typeof ga != 'undefined') {
ga('send', 'event', 'Action', 'Menu Mobile', 'Close');
}
//console.log('Close');
//console.log($(window).width());
}
createCookie('stickycookie',stickystate,30);
return false;
});
// Menu mobile hide
// ------------------------------------------------------------------------------------
}); </script> </section> <!--/ Center --> <div class="clear"></div> <!--/ Content --> <div class="clear"></div> <!-- Rassurance --> <aside id="rassurance" class="clearfix" role="complementary"> <div class="container"> <ul class="fl"> <li id="rassurance01"> <span class="link_footer first clearfix"> <span class="picto_contain picto_contain_04"> <span class="picto icon-delivery-2"></span> </span> <div class="clear"></div> <span class="push_title">Livraison au choix</span> <div class="clear"></div> <span class="text">
