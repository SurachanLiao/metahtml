$(document).on('click', '#search_init', function ()
{
var button = $('#search_button');
$('#search_query_top').val("");
button.addClass("disabled");
button.attr("disabled", "disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').removeClass("content-body-blur");
$('#page').removeClass("overlay");
$(this).hide();
$("#search_autocomplete > ul").empty();
return false;
});
$('#search_block_top').on('click', '.other_button', function ()
{
var tab = { };
tab.query = { };
tab.query.keyword = $('#search_query_top').val();
var search_query = JSON.stringify(tab);
window.location = "https://www.franceloisirs.com/other-search.php?search_query=" + search_query;
return false;
});
$(document).on('keyup', '#search_query_top', function()
{
var button = $('#search_button');
if($(this).val().length >=1)
{
button.removeClass("disabled");
button.removeAttr("disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').addClass("content-body-blur");
$('#page').addClass("overlay");
$('#search_init').show();
}
else
{
button.addClass("disabled");
button.attr("disabled", "disabled");
$('#menu_content, #menu_contentf, #columns, .bandeau_header').removeClass("content-body-blur");
$('#page').removeClass("overlay");
$('#search_init').hide();
}
});
$('#search_autocomplete').on('click', '.kxr_filter', function ()
{
var field = $(this).attr('kxr_field');
var value = $(this).attr('kxr_value');
var tab2 = { };
var tab ={ };
tab[value] = "1";
tab2[field] = tab;
tab2['keyword'] = $('#search_query_top').val();
window.location = "https://www.franceloisirs.com/recherche?search_query=" + JSON.stringify({ query: tab2 });
});
$('#search_autocomplete').on('click', '.kxr_filter_all', function ()
{
var tab2 = { };
tab2['keyword'] = $('#search_query_top').val();
window.location = "https://www.franceloisirs.com/recherche?search_query=" + JSON.stringify({ query: tab2 });
});
$('#search_autocomplete').on('click', '.kxr_suggest', function ()
{
var value = $(this).attr('kxr_value');
var tab = { };
tab['keyword'] = value;
window.location = "https://www.franceloisirs.com/recherche?search_query=" + JSON.stringify({ query: tab });
});
$("#search_query_top").autocomplete({
source: "https://www.franceloisirs.com/recherche?ajaxSearch=1",
minLength: 3,
delay: 600,
appendTo: '#search_autocomplete',
focus: function (event, ui)
{
$('ul.ui-autocomplete li').removeClass('selected');
var menu = $(this).data("uiAutocomplete").menu.element;
var focused = menu.find("li:has(a.ui-state-focus)");
focused.addClass('selected');
},
select: function (event, ui)
{
if ($('ul.ui-autocomplete li.selected a.button').is(':hover'))
{
return false;
}
if (ui.item && ui.item.cat_link)
{
document.location.href = ui.item.cat_link;
}
else if (ui.item && ui.item.product_link)
{
document.location.href = ui.item.product_link;
}
else
{
$('#searchbox').submit();
}
},
open: function (event, ui)
{
current_group = "";
current_universe = 0;
chaptercounter = 0;
},
close: function()
{
setTimeout(function() { $("#search_init").click(); }, 300);
},
}).data("ui-autocomplete")._renderItem = function (ul, item)
{
if (typeof(item.total) !== 'undefined')
{
if (item.other)
{
var ap = $("<li class='ui-autocomplete-group'></li>").data("item.autocomplete", item).append("<a href='#'> <strong>Resultats dans Autres <span class='search_query_top_result product_title'>  voir les  " + item.other + " résultats &rsaquo;</span></strong></a>");
}
if (item.total)
{
var ap = $("<li class='ui-autocomplete-group ui-title'></li>").data("item.autocomplete", item).append("<a href='#' class='kxr_filter_all product_title'> <strong>Tout <span class='search_query_top_result author_title'>  voir les  " + item.total + " résultats &rsaquo;</span></strong></a>");
}
if (ap)
{
return ap.appendTo(ul);
}
else
{
var a = "<a href='#' class='kxr_filter_all product_title'> <strong>Aucun Résultat</strong></a>";
var ap = $("<li class='ui-autocomplete-group ui-title'></li>").data("item.autocomplete", item).
append(a);
return ap.appendTo(ul);
}
}
// TODO mettre Le choix du club dans tous les cas ...
else if (item.group_universe>0 && item.group_universe != current_universe)
{
var universe_string = "";
if(typeof(current_universe)=="undefined" || current_universe==0 || item.group_universe==1)
{
var ap = $("<li class='ui-autocomplete-category uni_1'></li>").append( "<span>Le Choix du Club</span>");
ap.appendTo(ul);
if((typeof(current_universe)=="undefined" || current_universe==0 ) && item.group_universe==2)
{
var a = "<a href='#' class='kxr_filter_all product_title'> <strong>Aucun Résultat</strong></a>";
var ap = $("<li class='ui-autocomplete-group ui-title'></li>").data("item.autocomplete", item).
append(a);
ap.appendTo(ul);
}
}
if(item.group_universe==2) {
universe_string = "<span>Encore plus de choix</span>";
}
current_universe = item.group_universe;
if(universe_string) {
var ap = $("<li class='ui-autocomplete-category uni_"+item.group_universe+"'></li>").append(universe_string);
return ap.appendTo(ul);
}
var ap = $("<li class=''></li>");
return ap.appendTo(ul);
}
else if (typeof(item.feature_name) !== 'undefined')
{
//Auteurs, Séries, ...
var all_features = '';
var sep_feature = '&nbsp;:&nbsp;';
for (var i=0 ; i<item.list.length ; i++)
{
all_features += sep_feature + "<a href='" + item.list[i].link + "'>" + item.list[i].name + "</a>"
sep_feature = '&nbsp;&rsaquo;&nbsp;';
}
ap = $("<li class='ui-menu-item ui-title ui-serie' role='presentation'></li>").data("item.autocomplete", item).append(item.feature_name + all_features);
return ap.appendTo(ul);
}
else if (typeof(item.origin) !== 'undefined' && item.origin != 'ics' )
{
var ap = $("<li class='ui-menu-item ui-title ui-serie' role='presentation'></li>").data("item.autocomplete", item).append(item.origin+": <a href='" + item.link + "'>" + item.name + "&rsaquo;</a>");
return ap.appendTo(ul);
}
else if (typeof(item.chapreferency) !== 'undefined')
{
chaptercounter ++;
if (chaptercounter == 1) {
var ap = $("<li class='ui-autocomplete-group ui-title chapitre-title'><a href='#' class='kxr_filter_all'><span class='centered-height'>Voir les résultats chez notre partenaire Chapitre.com</span><span class='chpt_logo'><img src='//www.franceloisirs.com/themes/img/chapitre_logo.png'></span></a></li>\n\ <li class='ui-menu-item ui-product clearfix chapitre-group'>\n\ <a class='ui-corner-all centered-height' tabindex='-1' target='_blank' href='" + item.producturl + "?s'>" + highlight(item.title, search_query) + "<span class='search_query_top_result'>&rsaquo;</span></a>\n\<a class='button bp7 button_search' target='_blank' href='" + item.producturl + "?s'>\n\ <span class='text mobile-hidden'>voir la fiche sur chapitre.com</span>\n\ <span class='icon-shopping-cart only-mobile'></span>\n\ </a></li>");
}
else {
var ap = $("<li class='ui-menu-item ui-product clearfix chapitre-group'>\n\ <a class='ui-corner-all centered-height' tabindex='-1' target='_blank' href='" + item.producturl + "?s'>" + highlight(item.title, search_query) + "<span class='search_query_top_result'>&rsaquo;</span></a>\n\<a class='button bp7 button_search' target='_blank' href='" + item.producturl + "?s'>\n\ <span class='text mobile-hidden'>voir la fiche sur chapitre.com</span>\n\ <span class='icon-shopping-cart only-mobile'></span>\n\ </a></li>");
}
return ap.appendTo(ul);
}
else if (item.lib_cat === 'suggestions')
{
if (current_group !== item.group)
{
current_group = item.group;
var ap = $("<li class='ui-autocomplete-group'></li>").data("item.autocomplete", item).append("<a href='#'> <strong><span class='author_title' >Voici nos suggestions :</span></strong></a>");
ap.appendTo(ul);
}
var ap = $("<li class='ui-menu-item' role='presentation'></li>").data("item.autocomplete", item).append("<a href='#' class='kxr_suggest' kxr_value='" + item.name + "'>" + item.name + "</a>");
return ap.appendTo(ul);
}
else if (item.group_universe > -1)
{
var search_query = $('#search_query_top').val();
var ap = $("<li class='ui-autocomplete-group ui-title'></li>").data("item.autocomplete", item);
return ap.appendTo(ul);
}
else if (item.cat_id > 0)
{
var search_query = $('#search_query_top').val();
var ap = $("<li class='ui-autocomplete-group ui-title'></li>").data("item.autocomplete", item).append("<a href='#' class='kxr_filter product_title' kxr_field='" + item.field + "' kxr_value='" + item.group + "'> <strong>" + highlight(item.lib_cat, search_query) + " <span class='search_query_top_result author_title'>  voir les  " + item.groupCount + " résultats &rsaquo;</span></strong></a>");
return ap.appendTo(ul);
}
else
{
var search_query = $('#search_query_top').val();
var author = "";
if (typeof(item.auteur) !== 'undefined')
{
var urlBase = "//www.franceloisirs.com/auteur/";
for (var i = 0; i < item.auteur.length; i++)
{
author += '<span class="search_query_top_type author_title"><a href="' + urlBase + item.auteur[i].link_rewrite + '">' + highlight(item.auteur[i].value, search_query) + '</a></span>';
}
}
var tag = "";
if (item.cdc != "")
{
tag = item.cdc;
}
var url_panier = "https://www.franceloisirs.com/panier?content_only=1&qty=1&add=1&id_product=" + item.id + "&offer_id=" + item.offer_id;
var lien_tag = "<a href='" + item.product_link + "'><span class='tag " + item.css_class + "'>" + tag + "</span></a>";
if (tag == 'divers')
{
lien_tag = "<span class='tag tag_empty'></span>";
}
var libelle_ajouter = "Ajouter au panier";
if (item.delivery_status == 8)
{
libelle_ajouter = "Pré-commander";
}
var html_btn = "<a class='button bp7 button_title fancy js_search_autocomplete' href='" + url_panier + "'>" + libelle_ajouter + "</a>"
if (item.only_resa)
{
html_btn = "<a class='button bp7 button_title' href='" + item.product_link + "'>Réserver en boutique</a>"
}
if (item.nombre_prix > 1)
{
html_btn = "<a class='button bp7 button_title' href='" + item.product_link + "'>Voir le produit</a>"
}
if (item.active == 0)
{
html_btn = "<span class='nodispo button add_cart button_title'>Non disponible</span>";
}
var html_sugg = "<span class='shopping_cart_img_container'>\n\ <span class='centered-height'>\n\ <a href='" + addUrlParam(item.product_link, 's', '') + "'>\n\ <img class='shopping_cart_list_img' src='" + item.image_url + "' />\n\ </a>\n\ </span>\n\ </span>\n\ <span class='shopping_cart_info_container static'>\n\
" + lien_tag + "\n\ <span class='shopping_cart_list_title product_title'>\n\ <a class='ui-corner-all' tabindex='-1' href='" + addUrlParam(item.product_link, 's', '') + "'>" + highlight(item.name, search_query) + "</a>\n\ </span>\n\ <span class='shopping_cart_list_author author_title'>" + author + "</span>\n\ <span class='shopping_cart_list_author reference_title'>référence : " + highlight(item.reference, search_query) + "</span>\n\ </span>\n\n" + html_btn;
var ap = $("<li class='ui-menu-item ui-product clearfix' role='presentation'></li>").data("item.autocomplete", item).append(html_sugg);
return ap.appendTo(ul);
}
};
$("#body_container").live("focusin", function(event) {
$("#search_autocomplete ul").show();
});
$("body").on("popin-close", function(event, element) {
if($(element).hasClass("js_search_autocomplete")) {
$("#search_autocomplete ul").show();
}
});
});
function highlight(text, keywords)
{
if (keywords && text && keywords != "" && text != "")
{
var list = keywords.split(' ')
for (var key in list)
{
if (list[key].length > 2)
{
var regexp = new RegExp('(' + list[key] + ')', 'gi');
text = text.replace(regexp, "<b>$1</b>");
}
}
}
return text;
}
