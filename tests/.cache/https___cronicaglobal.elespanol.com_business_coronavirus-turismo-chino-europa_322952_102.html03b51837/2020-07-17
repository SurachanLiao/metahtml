            // IE support
            (function () {
                if (typeof window.CustomEvent === "function") {
                    return false;
                }

                function CustomEvent(event, params) {
                    params = params || {bubbles: false, cancelable: false, detail: undefined};
                    var evt = document.createEvent("CustomEvent");
                    evt.initCustomEvent (event, params.bubbles, params.cancelable, params.detail);
                    return evt;
                }

                CustomEvent.prototype = window.Event.prototype;
                window.CustomEvent    = CustomEvent;
            })();

            // Browser support version choice
            (function(w, d){
                var b = d.getElementsByTagName('body')[0];
                var s = d.createElement("script"); s.defer = true;
                var v = !("IntersectionObserver" in w) ? "8.7.1" : "10.5.2";
                s.src = "https://cdnjs.cloudflare.com/ajax/libs/vanilla-lazyload/" + v + "/lazyload.min.js";
                w.addEventListener('LazyLoad::Initialized', function (e) {
                    w.lazyLoad = e.detail.instance;
                }, false);
                w.lazyLoadOptions = {};
                b.appendChild(s);
            }(window, document));
        </script>
    

    <script>
      document.addEventListener("DOMContentLoaded", function(){
        window.paywall = new ComitiumPaywall();
        paywall.init("[data-swipe-navigation-current]");

        
            var fixContainers = function() {
              $('[data-index]').each(function(k,v){
                  var index      = $(this).data("index");
                  var isCurrent  = index == SwipeNavigation.currentIndex;
                  var ads        = $(this).find('div[id^="sas_"]');

                  if (!isCurrent) {
                      ads.each(function(k,v){
                          var id = $(this).attr("id");
                          if (/_bab/.test(id) === false) {
                              id = $(this).attr("id") + "_bab";
                          }
                          $(this).attr("id", id);
                      });
                  } else {
                      ads.each(function(k,v){
                          var id = $(this).attr("id").replace("_bab", "");
                          $(this).attr("id", id);
                      });
                  }
              });
            }

            
            if (typeof SwipeNavigation !== "undefined") {
              window.SwipeNavigation = new SwipeNavigation({
                treeFile: "https://cronicaglobal.elespanol.com/uploads/cs_statics/categories/cronica-global/2.json",
                currentId: "_322952",
                containerSelector: '.row-center',
                mobileWidth: 789,
                adsSelector: 'div[id^="sas_"]',
                onHandleCurrentIndex: function(newIndex) {
                  if (typeof window.twttr === 'undefined') {
                    window.twttr = (function (d,s,id) {

                        var t, js, fjs = d.getElementsByTagName(s)[0];
                        if (d.getElementById(id)) return; js=d.createElement(s); js.id=id;
                        js.src="https://platform.twitter.com/widgets.js";
                        fjs.parentNode.insertBefore(js, fjs);

                        return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f) } });

                    }(document, "script", "twitter-wjs"));
                  } else {
                    window.twttr.widgets.load();
                  }

                  $('[data-index]').each(function(k,v){
                      var index      = $(this).data("index");
                      var isCurrent  = index == newIndex;
                      var ads        = $(this).find('div[id^="sas_"]');

                      if (!isCurrent) {
                          ads.each(function(k,v){
                              var id = $(this).attr("id");
                              if (/_bab/.test(id) === false) {
                                  id = $(this).attr("id") + "_bab";
                              }
                              $(this).attr("id", id);
                          });
                      } else {
                          ads.each(function(k,v){
                              var id = $(this).attr("id").replace("_bab", "");
                              $(this).attr("id", id);
                          });
                      }
                  });

                },
                onLoadPrev: function(el, text) {
                    $(el).find('picture img, picture source').each(function(k,v){
                        var attr = $(this).is("img") ? "src" : "srcset";
                        var src = $(this).data(attr);

                        $(this).attr(attr, src);
                    });
                },
                onLoadNext: function(el, text) {
                    $(el).find('picture img, picture source').each(function(k,v){
                        var attr = $(this).is("img") ? "src" : "srcset";
                        var src = $(this).data(attr);

                        $(this).attr(attr, src);
                    });
                },
                onTransitionEnd: function(data) {
                    clearTimeout(window.swiperTimout);

                    window.swiperTimout = setTimeout(function(){
                        // Refresh paywall
                        paywall.init("[data-swipe-navigation-current]");

                        // Refresh ads
                        //googletag.pubads().refresh();

                        sas.refresh();

                        if ($('[data-swipe-navigation-current] .news_ext_body2').length > 0 && $("[data-swipe-navigation-current] [data-comitium-paywall]").length == 0) {
                             window.AdsParagraphs('[data-swipe-navigation-current] .news_ext_body2 > p')
                         }

                         if ($('[data-swipe-navigation-current] .opinion_ext_body').length > 0 && $("[data-swipe-navigation-current] [data-comitium-paywall]").length == 0) {
                             window.AdsParagraphs('[data-swipe-navigation-current] .opinion_ext_body > p')
                         }

                        // Update new lazy images
                        lazyLoad.update();

                        // Custom libraries
                        window.SocialAnalytics.update();
                        window.ScrollAnalytics.sendAndUpdate();

                        // Stats
                        if (typeof gtag !== "undefined") {
                            gtag('event', 'page_view', {
                                title: document.title,
                                location: location.href,
                                page: location.pathname
                            });
                        } else if (typeof ga !== "undefined") {
                            ga("send", {
                                hitType: "pageview",
                                title: document.title,
                                location: location.href,
                                page: location.pathname
                            })
                        }
                    }, 1000);
                  }
              });


            }
        
        window.SocialAnalytics = new SocialAnalytics({});
        window.ScrollAnalytics = new ScrollAnalytics({
            elements: ['[data-swipe-navigation-current] .row-center']
        });
      });
