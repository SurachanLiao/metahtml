<script>
    ;(function(){
        var currentArticle = document.currentScript.closest('article');
        // Hide and show depending on logged in status:
        currentArticle.querySelectorAll(headerScripts.isLoggedIn() ? '.js-show-not-logged-in' : '.js-show-logged-in').forEach(function(element) {
            element.style.display = 'none';
        });
        // Make list of subscription content expandable and collapsible:
        function expandInfoList() {
            var clickable = event.target.closest('.js-toggle-subscribe-content');
            if (clickable !== null) {
                clickable.nextElementSibling.style.display = clickable.nextElementSibling.style.display === 'block' ? 'none' : 'block';
            }
        }
        currentArticle.addEventListener('click', expandInfoList);
        // Trigger Klarna
        (function(fn){
            if (document.readyState != 'loading') fn();
            else document.addEventListener('DOMContentLoaded', fn);
        })(function(){
            //if (window.slm.klarna !== undefined && window.slm.klarna.firstInit === false && headerScripts.isLoggedIn()) {
            //    var packageCode = currentArticle.querySelector('.klarna-container').dataset.packageCode;
            //    siteScripts.klarna.initKlarnaBox('klarna-1-50414731', packageCode, currentArticle, 'recurring');
            //    window.slm.klarna.firstInit = true;
            //}
            if (window.slm.klarna !== undefined && window.slm.klarna.firstInit === false && headerScripts.isLoggedIn()) {
                var packageCode = currentArticle.querySelector('.klarna-container').dataset.packageCode;
                var articleIdDash = currentArticle.dataset.articleId.replace('.', '-');
                console.log('[MARVAN]', articleIdDash);
                // 'klarna-1-50414731'
                currentArticle.querySelector('.checkout-container > div').id = 'klarna-' + articleIdDash;
                console.log('[MARVAN]', currentArticle.querySelector('.checkout-container > div'));
                siteScripts.klarna.initKlarnaBox('klarna-' + articleIdDash, packageCode, currentArticle, 'recurring');
                window.slm.klarna.firstInit = true;
            }
        });
        // Get correct article link, we cannot trust Velocity
        var rootUrl = window.location.protocol + '//' + window.location.hostname + (window.location.port ? ':' + window.location.port : '');
        var articleUrl = '/sport/fotboll/ljungskile-sk-testade-duo-mot-ifk-g%C3%B6teborg-1.50414731';
        if (articleUrl.indexOf('http') === 0) {
            articleUrl = articleUrl.split(':');
            articleUrl[0] = 'https';
            articleUrl = articleUrl.join(':');
        } else if (articleUrl.length === 0) {
            // A completely broken URL, the articleLink variable was null, possibly a longread.
            articleUrl = rootUrl + currentArticle.dataset.url;
        } else {
            articleUrl = rootUrl + articleUrl;
        }
        // Hydrate registration link with a continueUrl value
        var registrationLink = currentArticle.querySelector('.js-registration-form');
        registrationLink.href += '&continueUrl=' + encodeURIComponent(articleUrl);
        // Add temporary click event to registration link
        registrationLink.addEventListener('click', function() {
            console.log('[MARVAN] Logging click event 1b');
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({
                'event': 'gaLockedArticleEvent',
                'step': '1b'
            });
        })
        // Hydrate abroad subscription link with JS-only information
        var abroadSubscriptionLink = currentArticle.querySelector('.js-subscribe-abroad');
        if ((googleAnalyticsString = headerScripts.getCookie('_ga')).length > 0) {
            abroadSubscriptionLink.href += '&_ga=' + googleAnalyticsString;
        }
        var returnUrl = articleUrl;
        if (false === headerScripts.getUserData()) {
            returnUrl += (returnUrl.indexOf('?') !== -1 ? '&' : '?') + 'loggain=artikel';
        } else {
            returnUrl = rootUrl + '/internal-api/user/refresh?continueUrl=' + encodeURIComponent(returnUrl);
        }
        abroadSubscriptionLink.href += '&returnUrl=' + encodeURIComponent(returnUrl);
        if (window.slm.premiumConfig.tuloTrafficSource.length > 0) {
            abroadSubscriptionLink.href += '&source=' + window.slm.premiumConfig.tuloTrafficSource;
        }
        if (false !== headerScripts.getUserData()) {
            abroadSubscriptionLink.href = rootUrl + '/internal-api/user/ticket?continueUrl=' + encodeURIComponent(abroadSubscriptionLink.href);
        }
    }());
</script>                                </div>
