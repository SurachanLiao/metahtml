                        {
                            jQuery(".list_tab li").removeClass("on");
                            jQuery(this).addClass("on");
                        },
                        mouseenter : function() 
                        {
                            jQuery(".list_tab li:first").removeClass("on");
                            jQuery(this).addClass("on");
                        },
                        mouseleave : function() 
                        {
                            jQuery(this).removeClass("on");
                            jQuery(".list_tab li:first").addClass("on");
