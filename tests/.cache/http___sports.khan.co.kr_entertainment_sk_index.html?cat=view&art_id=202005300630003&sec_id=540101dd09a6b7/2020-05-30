                <script type="text/javascript">
                var cate = '1';

                // 카테고리별 활성화(연예, 스포츠, 생활)
                if (typeof cate !== "undefined" && cate != "")
                {
                    var popularity = cate;
                }
                else 
                {
                    // 카테고리 랜덤 노출(연예, 스포츠, 생활 제외-만화뷰)
                    var popularity = Math.ceil(Math.random() * 3);
                }

                jQuery("#popularity_tabs > li:eq("+(popularity-1)+")").addClass("on");
                jQuery("#popularity_tab"+popularity).show();

                // 카테고리 마우스 오버 시 해당 컨텐츠 노출
                function popularity_tab(val) 
                {
                    var cont_val = parseInt(val) + parseInt(1);

                    jQuery("#popularity_tab1, #popularity_tab2, #popularity_tab3").hide();
                    jQuery("#popularity_tab"+cont_val).show();
                    jQuery("#popularity_tabs > li").removeClass("on");
                    jQuery("#popularity_tabs > li:eq("+val+")").addClass("on");
                }
                </script>                <!-- // 오늘의 인기뉴스 -->

