    <script type="text/javascript">
    jQuery(document).ready(function() 
    {
        var isDoing = false;

        // 본문 높이에 따라 우측 특정 영역 고정
        //var doFSR = 'N'; // n일 경우 작동 중지 (디버깅을 하기 위해 사용)
        if (typeof doFSR == 'undefined' || doFSR == '') { doFSR = 'Y'; }

        var artType = 'skhan';
        if (artType == "skhan_poll") { var topbar_limit = jQuery('.tit_head').offset().top; } // 탑바 노출 위치(좌표)
        else 
        {
            var topbar_limit = jQuery('#article_title').offset().top; // 탑바 노출 위치(좌표)
            var body_h = jQuery('div.art_body').height(); // 문서 높이

            var $fixEl = jQuery('#FSLimit2').next(); // 고정요소

            var ft = jQuery('#footer').offset().top; // 푸터영역 scrollTop
            var rt = jQuery('aside.aside_cont_right').offset().top; // 우측영역 최상단 scrollTop
            var rh = jQuery('aside.aside_cont_right').height(); // 우측영역 높이값
            var std = $fixEl.offset().top; // 스크롤 기준 scrollTop
        }

        jQuery(window).scroll(function() 
        {
            rh = jQuery('aside.aside_cont_right').height(); // 추가(우측 영역 높이값 오류)_180601_이진주
            var this_scrolltop = jQuery(this).scrollTop(); // 스크롤바의 수직 위치(좌표)

            var t = parseInt($('#topBarWrap').css('top').replace('px', ''));
            var docHeight = jQuery(document).height(); // HTML document(현재 문서 높이 브라우저 창)
            var windowHeight = jQuery(window).height(); // browser viewport(브라우저 현재 창 가시 지역 높이)
            var progress = (this_scrolltop / (docHeight - windowHeight)) * 100 + '%';

            if (isDoing == false)
            {
                if (this_scrolltop >= topbar_limit)
                {
                    if (t < 0) 
                    {
                        isDoing = true;
                        $('#topBarWrap').animate({'top' : '+=76'}, function() { isDoing = false; });
                    }
                }
                else
                {
                    if (t >= 0)
                    {
                        isDoing = true;
                        $('#topBarWrap').animate({'top' : '-=76'}, function() { isDoing = false; });
                    }
                }
            }
            jQuery('#read_progress').css('width', progress);

            if (artType != "skhan_poll")
            {
                var crt = jQuery('aside.aside_cont_right').offset().top; // 가변적 우측영역 최상단 scrollTop
                var cstd = $fixEl.offset().top; // 기준요소의 가변값
                var css = {'position':'', 'left':'', 'top':'', 'bottom':''}; // css 초기화

                // 스크롤 위치가 고정 요소를 넘어갈 경우
                if (std - 76 <= this_scrolltop)
                {
                    // left값 (60 : article_cont_left 패딩값)
                    var l = jQuery('article.wrap_news_body').offset().left + jQuery('article.article_cont_left').width() + 60;

                    // 우측영역 최하단이 푸터와 만날 경우
                    if (crt + rh > ft - 10)
                    {
                        var b = -ft + rt + rh;
                        css = {'position':'relative', 'left':'', 'top':'', 'bottom':b +'px'};

                        // 기준 요소가 화면에 노출될 경우
                        if (jQuery('aside.aside_cont_right').css('position') == 'relative' && this_scrolltop <= cstd)
                        {
                            var t = -std + rt + 76;
                            css = {'position':'fixed', 'left':l+'px', 'top': t +'px', 'bottom':''};
                        }
                    }
                    // 고정요소 고정
                    else
                    {
                        var t = -std + rt + 76;
                        css = {'position':'fixed', 'left':l+'px', 'top': t +'px', 'bottom':''};
                    }
                }
                else { css = {'position':'', 'left':'', 'top':'', 'bottom':''}; }
                jQuery('aside.aside_cont_right').css(css);
            }
        });
    });
    </script>
    <!-- //floating navigation bar -->
    <script type="text/javascript">
    // 로그인/로그아웃 시 노출 될 링크
    if (document.cookie.match(/khan_uid/i) || document.cookie.match(/khan_id/i)) 
    {
        jQuery(".logIn").hide();
        jQuery(".logOut").show();
    } 
    else 
    {
        jQuery(".logIn").show();
        jQuery(".logOut").hide();
    }

    // 로그아웃
    function logout()
    {
        window.location.href = "https://member.khan.co.kr/login/logout.html?url="+ encodeURIComponent(location.href);
    }

    // 기사 검색 바로가기
    jQuery("#search").focus(function() { jQuery(this).find(".tf_search").addClass("on"); });

    // 기사 검색 - 상단
    jQuery("#main_top_search_btn").on('click',function() { search_input_fn(); });

    jQuery("#main_top_search_input").keypress(function(e) 
    {
        if (e.which == 13) { search_input_fn(); }
    });

    function search_input_fn() 
    {
        var main_search_val = jQuery("#main_top_search_btn_yn").val();
        if (main_search_val == "0") 
        {
            jQuery("#main_top_search_input").css("display","inline-block");
            jQuery("#main_top_search_btn_yn").val("1");
            jQuery("#main_top_search_input").val("").focus();
        } 
        else 
        {
            if (jQuery("#main_top_search_input").val()) 
            {
                jQuery("#main_top_search_input").css("display","none");
                jQuery("#main_top_search_btn_yn").val("0");
                search.submit();
            } 
            else 
            {
                jQuery("#main_top_search_input").css("display","none").val("");
                jQuery("#main_top_search_btn_yn").val("0");
            }
        }
    }

    // side Navigation Bar
    //jQuery(".snb").focusin(function() { jQuery(this).addClass("on"); });

    jQuery(".snb .snbToggle").focusout(function() { jQuery(".snb").removeClass("on"); });

    jQuery(".snbToggle").click(function(e) 
    {
        if (jQuery(".snb").hasClass("on")) { jQuery(".snb").removeClass("on"); } 
        else { jQuery(".snb").addClass("on"); } 
    });

    jQuery(".snb").mouseleave(function() 
    {
        if (jQuery(".snb").hasClass("over")) 
        {
            jQuery(".list_snb > li").removeClass("on");
            jQuery(this).removeClass("over");
        }
    });

    jQuery(".link_snb").on("mouseover focusin", function() 
    { 
        jQuery(".list_snb > li").removeClass("on");
        if (jQuery(this).hasClass("null")) { jQuery(".snb").removeClass("over"); } 
        else 
        {
            jQuery(this).closest("li").addClass("on");
            jQuery(".snb").addClass("over");
        }
    });

    jQuery(document).ready(function(e) 
    {
        jQuery("#comment_down").on('click',function() { window.scrollTo(0,10000); });	
    });
    </script>

	


