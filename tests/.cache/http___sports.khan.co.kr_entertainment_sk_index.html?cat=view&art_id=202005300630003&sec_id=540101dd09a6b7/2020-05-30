        function A1MakeIframe(path) {
            var h = document;
            var c = h.createElement("iframe");
            c.src = path;
            c.style.margin = "0px";
            c.style.borderWidth = "0px";
            c.style.padding = "0px";
            c.scrolling = "no";
            c.frameBorder = "0";
            c.style.width = "0px";
            c.style.height = "0px";
            document.body.appendChild(c);
        }
        
