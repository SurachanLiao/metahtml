            <script type="text/javascript">
            var now = new Date();
            var y = now.getFullYear();
            var m = (now.getMonth()+1)>9 ? ""+(now.getMonth()+1) : "0"+(now.getMonth()+1);
            var d = now.getDate()>9 ? ""+now.getDate() : "0"+now.getDate();
            var today = y+m+d;

            jQuery(".poll_cont").each(function(index) 
            {
                var poll_sDate = jQuery(this).data("sdate");
                var poll_eDate = jQuery(this).data("edate");
                if (poll_sDate < today && poll_eDate < today) { jQuery(this).find(".tit_caption em").text("결과보기"); }
                else if (poll_sDate <= today && poll_eDate >= today) { jQuery(this).find(".tit_caption em").text("투표하기"); }
            });

            function movePoll(pos) 
            {
                var pan_width = jQuery("#poll_slide li:first").width();
                var sel_idx = jQuery("#pollSlide_paging button.btn_now.on").index();
                if (pos == "next")
                {
                    jQuery("#poll_slide").animate(
                    {
                        left : "-="+ pan_width,
                    }, 500, function() {
                        jQuery("#poll_slide").append(jQuery("#poll_slide li:lt(1)")).css("left", "0");
                    });

                    if (sel_idx >= 2) { var sel_index = 0; }
                    else { var sel_index = sel_idx + parseInt(1); }

                    jQuery("#pollSlide_paging button.btn_now").removeClass("on");
                    jQuery("#pollSlide_paging button.btn_now:eq("+sel_index+")").addClass("on");
                }
                else 
                {
                    var max = jQuery("#poll_slide li").length;
                    var gt = max - 2;
                    jQuery("#poll_slide").prepend(jQuery("#poll_slide li:gt("+gt+")")).css("left", "-"+ pan_width +"px");
                    jQuery("#poll_slide").animate(
                    {
                        left : "+="+ pan_width,
                    }, 500);

                    if (sel_idx == 0) { var sel_index = 2; }
                    else { var sel_index = sel_idx - parseInt(1); }

                    jQuery("#pollSlide_paging button.btn_now").removeClass("on");
                    jQuery("#pollSlide_paging button.btn_now:eq("+sel_index+")").addClass("on");
                }
            }
            </script>                <!-- // POLL -->

