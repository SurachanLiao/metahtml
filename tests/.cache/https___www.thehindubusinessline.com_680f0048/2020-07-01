<script type="cd96444e283479ed2f819a79-text/javascript">
require(['wfsite'],function(wfsite){
$(document).ready(function() { $(".covidCube").css("display", "none");
if(isCampaignLive("cubeDefault",1)){
$(".covidCube").css("display", "block");
setFrequency("cubeDefault",365,1);
}
else if(isCampaignLive("clickbtnCube",365)){
$(".covidCube").css("display", "block");
}
$(".closexCube").click(function(){
$(".covidCube").css("display", "none");
});
$(".closexCube").click(function(){
$(".covidCube").css("display", "none");
setFrequency("clickbtnCube",365,1);
});
});
});
$(document).ready(function(){
$("a.removeentrySubs").on("click", function(){
$("div.covidCube").addClass("bottomHeight");
});
//var footerSticky = $('.entrySubs:visible').length;
if($(".entrySubs").is(":hidden") == false){
$("div.covidCube").removeClass("bottomHeight");
}
});
</script>
