$(document).ready(function() {
$("a").filter(function() {
var aUrl = $(this).attr("href");
if (aUrl && aUrl.match(/article(\d+).ece$/)) {
$(this).attr("href", aUrl + "?homepage=true");
return true;
} else {
return false;
}
});
});
