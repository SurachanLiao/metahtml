});
$(document).ready(function() {
hidedd = true;
$(document).on("click",".sso-signinmobile",function(){	$(this).children(".dropdown-menu").toggle();
if (hidedd)$('.dropdown-menu.sso-signinuser').removeClass('dropdwnHide');
hidedd = false;
});	$('body').on('click', function () {
if (hidedd)$('.dropdown-menu.sso-signinuser').addClass('dropdwnHide');
hidedd = true;
});
});
var els = document.querySelectorAll("#main-menu>ul>li:nth-last-child(-n+2)");
[].forEach.call(els, function(els) {	els.style.display = "none";
});
document.getElementsByClassName("subsEntrymenu")[0].style.display = "block";
document.getElementsByClassName("submobileentry")[0].style.display = "block";
</script>
</ul>
</div>
</div>
</div>
</nav>
