<script>
document.addEventListener('DOMContentLoaded',function(){
	assignSharerToArticlePage();
	GreenAudioPlayer.init({
		selector: '.Audio',
		stopOthersOnPlay: true,
		enableKeystrokes: true
	});
	function isEmpty( el ){
		return !$.trim(el.html())
	}
	if (isEmpty($('.audio-wrapper'))) {
		$('.audio-wrapper').hide();
	}

	if($('.dot-article p.signatur').length>0){
		var name = $('.dot-article-sidecol span.user-firstname').text()+' '+$('.dot-article-sidecol span.user-lastname').text()
		if(name='DAG OG TID'){
			var newname = $('.dot-article-text p.signatur').text();
		$('.dot-article-sidecol span.user-lastname').remove();
		$('.dot-article-sidecol span.user-firstname').text(newname)
		}
	}

})

