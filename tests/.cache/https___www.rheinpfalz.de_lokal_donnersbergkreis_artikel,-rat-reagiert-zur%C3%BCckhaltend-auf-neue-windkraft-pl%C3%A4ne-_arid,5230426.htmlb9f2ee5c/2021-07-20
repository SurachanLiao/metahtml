		//------------OPT--OUT--------------------------

		function GA_OptOut(){
			var cookieName = '_ga_opt_out';
			var cookieValue = 'true';
			var myDate = new Date();
			myDate.setMonth(myDate.getMonth() + 120);
			document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate
					+ ";domain=.rheinpfalz.de;path=/";
		}

		function GTM_OptOut(){
			var cookieName = '_dcl_opt_out';
			var cookieValue = 'true';
			var myDate = new Date();
			myDate.setMonth(myDate.getMonth() + 120);
			document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate
					+ ";domain=.rheinpfalz.de;path=/";
		}

		//----------OPT-----IN--------------------

		function GA_OptIn(){
