					cmsSpecialOffer = 'offer_rhp_digital_probe_NMA_13000295,offer_rhp_print_probe_NMA_13000295,offer_rhp_digital_akti_6w_NMA_13000296,offer_rhp_print_akti_6w_NMA_13000296';
					// Set type of advertisement
					cmsSpecialLayout = 'rhp_native_c1_christmas';
					// Set landingpage
					cmsSpecialLink = 'https://www.rheinpfalz-abo.de/weihnachten/?utm_source=rheinpfalz.de&utm_medium=nativead&utm_campaign=NMA13000295-297&utm_content=weihnachten';
