		}

		// Init civey poll widget
		function initCiveyPoll(){
			// Get widget container by Id
			let parent_node = document.getElementById('civeyWidgetContainer');

			// Render iframe if civeyWidgetContainer exists on page
			if(parent_node !== null){
				// Check if poll widget is already rendered and prevent widget from multiple rendering
				let parent_node_value = parent_node.innerHTML;
				if(parent_node_value === '' || parent_node_value === '&nbsp;'){
					// Get poll Id from civeyWidgetContainer
					let pollId = parent_node.getAttribute('data-poll');

					// Init iframe creation with dynamic poll
					let s = document.createElement('iframe');
					s.loading = 'lazy';
					s.frameborder = 0;
					s.height = '500';
					s.width = '100%';
					s.src = 'https://widget.civey.com/'+pollId;

					// Append iframe to civeyWidgetContainer
					parent_node.appendChild(s);
				}
			}
		}

		// Init civey result widget
		function initCiveyResult(){
			// Get widget container by Id
			let parent_node = document.getElementById('civeyResultContainer');
			// Render iframe if civeyResultContainer exists on page
			if(parent_node !== null){
				// Check if result widget is already rendered and prevent widget from multiple rendering
				let parent_node_value = parent_node.innerHTML;
				if(parent_node_value === '' || parent_node_value === '&nbsp;'){
					// Get poll Id from civeyResultContainer
					let resultId = parent_node.getAttribute('data-result');

					// Init iframe creation with dynamic poll
					let s = document.createElement('iframe');
					s.loading = 'lazy';
					s.frameborder = 0;
					s.height = '500';
					s.width = '100%';
					s.src = 'https://app.civey.com/embed/results/'+resultId;

					// Append iframe to civeyWidgetContainer
					parent_node.appendChild(s);
				}
			}
		}

