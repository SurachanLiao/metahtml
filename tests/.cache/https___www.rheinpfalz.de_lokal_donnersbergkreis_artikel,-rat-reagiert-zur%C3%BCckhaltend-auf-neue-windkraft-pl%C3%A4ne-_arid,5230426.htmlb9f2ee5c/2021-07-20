		// Init happy contest widget
		function initHappyContest(){
			// Get widget container by Id
			let parent_node = document.getElementById('happyContestContainer');
			// Render script if happyContestContainer exists on page
			if(parent_node !== null){
				// Get contest Id from happyContestContainer
				let contestId = parent_node.getAttribute('data-contest');

				// Init script creation with dynamic contest
				let s = document.createElement('script');
				s.src = 'https://' + contestId + '.hc-apps.de/embed/script/' + contestId;

				// Append script to happyContestContainer
				parent_node.appendChild(s);
			}
		}

		// Init basic cmp state pushs for trakken
		function pushConsentState(consentAction,consentChoice){
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				'event':'cookieConsent',
				'action':consentAction,
				'choice':consentChoice
			});
		}

		// Get cookie for consent detection
		window.getCookie = function(name) {
			match = document.cookie.match(new RegExp(name + '=([^;]+)'));
			if (match) return match[1];
		};
		let consentRHP = getCookie('consentRHP');
		let consentSP = getCookie('_sp_enable_dfp_personalized_ads');

