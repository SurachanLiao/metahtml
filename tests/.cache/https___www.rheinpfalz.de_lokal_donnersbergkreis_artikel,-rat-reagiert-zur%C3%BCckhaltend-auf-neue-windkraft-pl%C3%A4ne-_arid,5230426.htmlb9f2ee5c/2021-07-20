		// RHP object usse the uevice detection 'mobile-detect.js'
		RHP.getC1Origin = function () {
			var md = new MobileDetect(window.navigator.userAgent);
			if ( md.mobile() || md.tablet() ) {
				console.log('mobile');
				return 'mobile';
			}
			//console.log('web');
			return 'web';
		};

		// Assign that C1 data (object) to as syntax in PHPTAL
		var c1ServiceId = 'rhp_rhp.de';
		var c1Site = 'rheinpfalz.de';
		var c1Origin = RHP.getC1Origin();
		var c1Content = decodeURIComponent('/lokal/donnersbergkreis_artikel,-rat-reagiert-zurückhaltend-auf-neue-windkraft-pläne-_arid,5230426.html');
