					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=28';
					break;
				case 'offer_rhp_print_geschenk_3':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=26';
					break;
				case 'offer_rhp_print_card_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=23';
					break;
				case 'offer_rhp_digital_card':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=5';
					break;
				case 'offer_rhp_digital_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=9';
					break;
				case 'offer_rhp_digital_card_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=14';
					break;
				case 'offer_rhp_upgrade_digital_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=37';
					break;
				case 'offer_rhp_premium':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=6';
					break;
				case 'offer_rhp_premium_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=10';
					break;
				case 'offer_rhp_premium_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=19';
					break;
					case 'offer_rhp_premium_upgrade_print':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=37';
					break;
				case 'offer_rhp_digital_6m_fck':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=15';
					break;
				case 'offer_rhp_tagesticket_premium':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=284';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=242';
					}
					break;
				case 'offer_rhp_print_akti_3m_NMA_13000279':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=146';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=125';
					}
					break;
				case 'offer_rhp_plus':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=226';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=202';
					}
					break;
				case 'offer_rhp_digital_akti_3m_NMA_13000279':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=148';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=126';
					}
					break;
				case 'offer_rhp_premium_akti_3m_NMA_13000213':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=149';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=127';
					}
					break;
				default:
					offerHost = creAboHost;
			}

			if (conversionType === 'RegPlusAboWall' || conversionType === 'RegPlusAboWallFree'){
				text = '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn rhp-btn-text rhp-blue-text rhp-bold toOffer" data-offer="'+ offer.offer_id.trim() + '"><h4>' + name + ' - ' + price.toFixed(2) + '&euro;<\/h4><\/a>';

				// Linkchange for paypal checkout
				if( offerLink !== '' ){
					text = '<a href="#" data-url="'+ offerHost + offerLink + '" class="btn rhp-btn-text rhp-blue-text rhp-bold toOffer" data-offer="'+ offer.offer_id.trim() + '"><h4>' + name + ' - ' + price.toFixed(2) + '&euro;<\/h4><\/a>';
				}

				template = jQuery('<div class="offer rhp-plus-abo-offer col-sm-12"><\/div>');
				template.html(text);
			}else{
				text = '<header><h4>' + name + '<span class="float-right w-auto text-right">' + price.toFixed(2) + '&euro;<\/span><\/h4><\/header>';
				text += '<p><small>' + description + '<\/small><\/p>';

				if (disrupter) {
					text += '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn btn-primary btn-success toOffer" data-offer="'+ offer.offer_id.trim() + '">Auswählen<\/a>';
				}else{
					text += '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn btn-light btn-success toOffer" data-offer="'+ offer.offer_id.trim() + '">Auswählen<\/a>';
				}

				text += '<hr class="sand">';

				template = jQuery('<div class="offer col-sm-12"><\/div>');
				template.html(text);

				if (disrupter) {
					var title;
					var color;
					if (disrupter === 'topoffer') {
						title = 'Unsere Empfehlung';
						color = 'red';
					} else if (disrupter === 'bestselling') {
						title = 'Meistverkauft';
						color = 'green';
					} else {
						title = '';
						color = 'blue';
					}
					var ribbon = jQuery('<span class="highlight ' + color + '">' + title + '<\/span>');
					template.prepend(ribbon);
				}
			}
			return template;
		}

		/**
		 * getAdvertOptin()
		 * ================
		 * API Call um aktuellen WerbeOptin-Status zurück zu liefern.
		 */
		function getAdvertOptin(){
			window.getCookie = function(name) {
				match = document.cookie.match(new RegExp(name + '=([^;]+)'));
				if (match) return match[1];
			};
			var cresid = getCookie('cresid');
			if(cresid){
				jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/get', function(response) {
					var status = response.indexOf('checked');
					if(status !== -1){
						jQuery('input#werbeOptin').prop('checked',true);
					}else{
						jQuery('input#werbeOptin').removeAttr('checked');
					}
				});
			}
		}

		/**
		 * setAdvertOptin()
		 * ================
		 * API-Call um WerbeOptin zu setzen.
		 */
		function setAdvertOptin(){
			window.getCookie = function(name) {
				match = document.cookie.match(new RegExp(name + '=([^;]+)'));
				if (match) return match[1];
			};
			var cresid = getCookie('cresid');
			if(cresid){
				jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/delete', function(response) {
					var status = response.indexOf('true');
					jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/advertising', function(response) {
						var status = response.indexOf('true');
						if(status !== -1){
							jQuery('.featherlight').hide();
						}
					});
				});
			}
		}

		/**
		 * startConversion
		 * ===========
		 * C1 Request Sender für Conversion Funnel Tracking
		 *
		 * @param conversionType
		 */
		function startConversion(conversionType){
			//cre_client.set_page_view();
			cre_client.set_service_id(c1ServiceId);
			cre_client.set_site(c1Site);
			cre_client.set_origin(c1Origin);
			cre_client.set_channel('1070');
			cre_client.set_doc_type('paywall');
			cre_client.set_content_id('/offer/view/'+conversionType);
			cre_client.request();
		}

		/**
		 * template_activation(data)
		 * ===================
		 * Aktiviert das Template, wenn die PayWall aktiv ist, oder das Template Initialer Login aktiv ist.
		 *
		 * @param data
		 */
		function template_activation(data,tracking_id) {
			console.log(data);
			console.log(data.template_set);
			console.log(data.template_id);
			var tracking_id = tracking_id;
			RHP.tracking_id = tracking_id;
			var isPaywallActive = (data.status === 'paywall' || data.status === 'always_paid');
			if(data.template_id !== undefined){
				if (isPaywallActive || data.template_id === 'rhp_initial_login') {
                    cre_templateclient.show({
						"container_id":"widgetContainer",
						"templateset_name":data.template_set,
						"template_name":data.template_id,
					});
				}
			}
			// Get action lanes from c1 backend and start snippet rendering
			if(data.action_groups != undefined){
				let snippetParams = data.action_groups.cms_snippet.parameters;
				snippetParams.forEach(function(snippetParam){
					// Check if snippet parameter is of type adserver
					if(snippetParam.indexOf('adserver_') < 0){
						startConversion('cmsSnippet');
						showCmsSpecials(snippetParam);
					}else{
						// Set user group information on ad zone
						snippetParam = snippetParam.replace('adserver_','');

						// Iterate through c1 rectangle places and set data attribute for targeting
						let c1Rectangles = document.querySelectorAll('ins.c1SnippetRectangle');
						c1Rectangles.forEach(function(c1Rectangle){
							c1Rectangle.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 rectangle places on article detail and set data attribute for targeting
						let c1RectanglesSixBlocks = document.querySelectorAll('ins.c1SnippetRectangleSixBlocks');
						c1RectanglesSixBlocks.forEach(function(c1RectangleSixBlocks){
							c1RectangleSixBlocks.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 billboard places and set data attribute for targeting
						let c1Billboards = document.querySelectorAll('ins.c1SnippetBillboard');
						c1Billboards.forEach(function(c1Billboard){
							c1Billboard.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 native places and set data attribute for targeting
						let c1Natives = document.querySelectorAll('ins.c1SnippetNative');
						c1Natives.forEach(function(c1Native){
							c1Native.setAttribute('data-revive-c1usergroup',snippetParam)
						});
					}

				});
			}
		}

		// DataLayer Push for c1 user groups
		function pushSapDataLayer(sapParameter){
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				'c1UserGroups':sapParameter
			});
		}

		/**
