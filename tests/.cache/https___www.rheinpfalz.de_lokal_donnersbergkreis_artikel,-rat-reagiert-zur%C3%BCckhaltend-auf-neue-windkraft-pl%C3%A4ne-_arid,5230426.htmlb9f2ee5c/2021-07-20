		// Call to C1 features
		downloadScript(creClientHost + "cre-1.0/tracking/feature.js", function () {
			if ( typeof (cre_client) != "undefined" ) {
				/**
				 * getServiceCookie(cookie)
				 * ===================
				 * Liest Cookies aus und validiert globale Session bei match gegen C1
				 *
				 * @param cookie
				 */
				window.getServiceCookie = function(cookie) {
					match = document.cookie.match(new RegExp(cookie + '=([^;]+)'));
					console.debug('JRX CRESID SERVICE: ' + match);
					matchCresId = document.cookie.match(new RegExp('cresid=([^;]+)'));
					console.debug('JRX CRESID GLOBAL: ' + matchCresId);

					if(!match && matchCresId != null){
						window.CeleraOne.JSSDK.validateGlobalSession(
								function(response){
									if(response.status === 'error'){
										console.log('Sie sind nicht eingeloggt!');
										console.debug('No global session.');
									}else{
										login_global();
										console.debug('Global session.');
									}
								});
					}else if(!matchCresId){
						console.log('Sie sind nicht eingeloggt!');
						console.debug('No cresid cookie.');
					}else{
						console.log(cookie+' ist vorhanden. Keine Aktion notwendig.');
						changeLoginView();
					}
				};

				/**
				 * changeLoginView()
				 * ===================
				 * Dynamische Anpassung des Anmelden-Buttons je nach Loginstatus
				 */
				function changeLoginView(){
					jQuery(document).ready(function(){
						jQuery("a.rhp-login").html('<i class="fal fa-user-check"><\/i>Mein Konto').addClass("customer").data('trakken','Mein Konto');
						jQuery("a.rhp-login").attr("href", creAboHost + 'kundenkonto');
					});
				}

				/**
				 * login_global()
				 * ===================
				 * Login Handler gegen C1, falls globale Session vorhanden ist.
				 */
				function login_global() {
					window.CeleraOne.JSSDK.loginWithGlobalSession(null,{
						onSuccess: function(){
							console.log('Sie wurden erfolgreich eingeloggt!');
							changeLoginView();
						},
						onError:function(){
							console.log('Sie wurden nicht eingeloggt!');
						}
					});
				}
				/**
				 * checkLogin()
				 * ===================
				 * Liefert hostabhängig C1 Servicecookie Benennung.
				 */
				window.checkLogin = function(){
					var host = window.location.hostname;
					var serviceCookie = '';
					switch(host){
						case 'epaper.rheinpfalz.de':
							serviceCookie = 'rhp_epaper_service';
							break;
						case 'epapertest.rheinpfalz.de':
							serviceCookie = 'rhp_epaper_service';
							break;
						case 'service.rheinpfalz.de':
							serviceCookie = 'rhp_service_service';
							break;
						case 'servicetest.rheinpfalz.de':
							serviceCookie = 'rhp_service_service';
							break;
						case 'ticket.rheinpfalz.de':
							serviceCookie = 'rhp_ticket_service';
							break;
						case 'shop.rheinpfalz.de':
							serviceCookie = 'rhp_shop_service';
							break;
						case 'shopstage.rheinpfalz.de':
							serviceCookie = 'rhp_shop_service';
							break;
						case 'trauer.rheinpfalz.de':
							serviceCookie = 'rhp_trauer_service';
							break;
						case 'app.rheinpfalz.de':
							serviceCookie = 'rhp_app-hybrid_service';
							break;
						case 'abo.rheinpfalz.de':
							serviceCookie = 'rhp_aboweb_service';
							break;
						case 'abotest.rheinpfalz.de':
							serviceCookie = 'rhp_aboweb_service';
							break;
						case 'adweborder.rheinpfalz.de':
							serviceCookie = 'rhp_adweb_service';
							break;
						case 'adweborder-k.rheinpfalz.de':
							serviceCookie = 'rhp_adweb_service';
							break;
						default:
							serviceCookie = 'rhp_rhp.de_service';
					}
					getServiceCookie(serviceCookie);
				};

				jQuery(document).ready(function(){
					checkLogin();
				});

				var cmpChr = ((c1Origin === c1Values.origin) ? ' === ' : ' !== ');

				cre_client.set_page_view();
				cre_client.set_service_id(c1ServiceId);
				cre_client.set_site(c1Site);
				cre_client.set_origin(c1Origin);
				cre_client.set_content_id(c1Content);
				cre_client.set_doc_type(c1DocType);
				cre_client.set_kicker(c1Kicker);
				cre_client.set_channel(c1Channel);
				cre_client.set_sub_channel(c1SubChannel);
				cre_client.set_sub_sub_channel(c1SubSubChannel);
				cre_client.set_cms_id(c1CmsId);
				cre_client.set_heading(c1Heading);
				cre_client.set_entitlement(c1Entitlement);
				cre_client.set_tag(c1Tag);
				cre_client.addListener(content_manager);
				var res = cre_client.request();
			} else {
				// push fallback for cre client
				window.dataLayer = window.dataLayer || [];
				dataLayer.push({
					event:'dataPushedFB',
				});
			}
		});

		/**
		 * offerClick()
		 * ============
		 * Ermöglicht Linkverfolgung nach Klick via data-url Attribut.
		 */
		function offerClick(data, offers, impressions){
			jQuery('.toOffer').click(function(){
				var offer = jQuery(this).data('offer');
				var url = jQuery(this).data('url');
				if (typeof(cre_client) != "undefined") {
					console.log('CONVERSION-SET begin');
					//cre_client.set_page_view();
					cre_client.set_service_id(c1ServiceId);
					cre_client.set_site(c1Site);
					cre_client.set_origin(c1Origin);
					cre_client.set_doc_type('paywall');
					cre_client.set_channel('1070');
					cre_client.set_cms_id('/offer/offerToOrder/submit');
					cre_client.set_content_id('/offer/offerToOrder/submit');
					cre_client.set_offer_ids(offer);
					cre_client.request();
					console.log('CONVERSION-SET end');
				}
				pushProductClick(data,impressions,offer);
				window.location.href = url;
			});
		}

		/**
		 * renderPlusArticle()
		 * ===================
		 * Blendet die Beschreibung des OfferPageWidgets für Plus Artikel ein und die Standardbeschreibung aus.
		 */
		function renderPlusArticle(){
			jQuery('.c1StandardDescription').hide();
			jQuery('.c1PlusArticle').show();
		}
		
