    <script type="text/javascript">
        document.getElementById('stampenCookieInformationContainer').focus();
        document.forms['stampenCookieForm'].onsubmit = acceptCookies;
        document.querySelector(".js-accept-cookies").addEventListener('click', acceptCookies);


        function acceptCookies(event) {
            event.preventDefault();
            var cookieExpire = new Date();
            cookieExpire.setDate(cookieExpire.getDate() + 30);
            var cookie = "stampen-cookies-hide-information=yes; expires=" + cookieExpire.toUTCString() + "; path=/;";
            document.cookie = cookie;
            var cookieContainer = document.getElementById('stampenCookieInformationContainer');
            cookieContainer.parentNode.removeChild(cookieContainer);

            toggleStickyNav();
            return false;
        }
    </script>
    <script type="text/javascript">
        if (document.cookie.indexOf('stampen-cookies-hide-information') === -1) {
            document.getElementById('stampenCookieInformationContainer').style.display = "block";
        }
    </script>
</div>
