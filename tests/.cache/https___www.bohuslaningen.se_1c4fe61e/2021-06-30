<!-- MARVAN FIX APPS -->
<script>
;(function(headerScripts){
    headerScripts.getUserData = function() {
        var encodedUserData = headerScripts.readCookie('p_userData');
        if (encodedUserData === false) {
            return false;
        }
        try {
            var userData = JSON.parse(decodeURIComponent(atob(encodedUserData)));
            userData.id = userData.paywayId;
            return userData;
        } catch (e) {
            console.warn(e);
            return false;
        }
    }
})(window.headerScripts);
</script>
<!-- END MARVAN FIX APPS -->

