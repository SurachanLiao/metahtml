        }

        function consentDate() {
            d = new Date();
            document.cookie = "consentDate=" + d;
        }

        var consentCookie = (document.cookie.match(/^(?:.*;)?\s*consentDate\s*=\s*([^;]+)(?:.*)?$/) || [, null])[1];
