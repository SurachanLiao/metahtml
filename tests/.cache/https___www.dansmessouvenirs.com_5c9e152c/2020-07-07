<script>
var boolPodcastToggle = true;
function MaskShowPodcast()
{
  if (boolPodcastToggle == false)
  {
document.getElementById("podcast-menu-access-bar").style.display = "block";
boolPodcastToggle = true;
  }
  else
  {
document.getElementById("podcast-menu-access-bar").style.display = "none";
boolPodcastToggle = false;
  }
}
</script>
<script>
var boolTaskbarToggle = false;
function MaskShowTaskbar()
{
  if (boolTaskbarToggle == false)
  {
document.querySelectorAll('div.row')[0].children[1].style.display = "none";
document.querySelectorAll('div.row')[0].children[0].style.maxWidth = "100%";
document.querySelectorAll('div.row')[0].children[0].style.flex = "0 0 100%";
window.dispatchEvent(new Event('resize'));
boolTaskbarToggle = true;
  }
  else
  {
document.querySelectorAll('div.row')[0].children[1].style.display = "block";
document.querySelectorAll('div.row')[0].children[1].style.maxWidth = "30%";
document.querySelectorAll('div.row')[0].children[1].style.flex = "0 0 30%";
document.querySelectorAll('div.row')[0].children[0].style.maxWidth = "70%";
document.querySelectorAll('div.row')[0].children[0].style.flex = "0 0 70%";
window.dispatchEvent(new Event('resize'));
boolTaskbarToggle = false;
  }
}
</script>
<script>
var boolRecentToggle = false;
function MaskShowRecent()
{
  if (boolRecentToggle == false)
  {
document.getElementById("RecentSection").style.opacity = "0";
boolRecentToggle = true;
  }
  else
  {
document.getElementById("RecentSection").style.opacity = "1";
boolRecentToggle = false;
  }
}
</script>

