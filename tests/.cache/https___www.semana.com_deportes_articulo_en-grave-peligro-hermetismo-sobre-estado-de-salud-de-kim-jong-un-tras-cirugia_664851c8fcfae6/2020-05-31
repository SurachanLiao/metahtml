            $("#modalemailConfirmation").removeClass("md-show");
            $("#modalLoginPrinted").removeClass("md-show");
            $("#modalRegisterPaywall").addClass("md-show");
        }
        $(document).ready(function () {
            flowPrintedEdition(false);
        });
        $(".md-").click(function () {
            $("#"+$(this).data("modalid")).removeClass("md-show");
        });
        $('#modalLoginPrinted').keypress(function (event) {
            if (event.which == 13) {
                GetUserFromLogin('userLoginPay', 'passwordLoginPay', 'MessageLoginUserPay');
            }
        });
