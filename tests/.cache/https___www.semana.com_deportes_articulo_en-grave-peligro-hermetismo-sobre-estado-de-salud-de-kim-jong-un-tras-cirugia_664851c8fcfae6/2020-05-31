        });

        if (isSubscriber) {
            $("#validRecomendados").removeAttr("style");
            $(".recomendadosInterna").removeAttr("style");
        } else {
            $('#validRecomendados').css('visibility', 'hidden');
            $('.recomendadosInterna').css('visibility', 'hidden');
        }
    });

    function updateInfo(element) {
        var title = $(element).find('.tittleArticuloOpinion').html();
        if (location.pathname != $(element).attr("item-url")) {
            window.history.pushState("", "", $(element).attr("item-url"));
            ga('send', 'pageview', $(element).attr("item-url"));
            $($('.whatsapp > iframe').contents().find('.wa_btn')).attr('href', 'whatsapp://send?text=' + encodeURIComponent(title) + encodeURIComponent(document.URL));
            $(".tittleArticuloOpinion.titleFixed").html(title)
            $("meta[property='ps:title']").attr('content', title);
            getPaywallPermision();
            verifySubscribersAndShowPaywallMeteredBanner();
            if (isSubscriber) {
                $("#validRecomendados").removeAttr("style");
                $(".recomendadosInterna").removeAttr("style");
            } else {
                $('#validRecomendados').css('visibility', 'hidden');
                $('.recomendadosInterna').css('visibility', 'hidden');
            }
        }

    }

    function insertPinochometro(elVerdad, elMentira, elMasCierto,elMasFalso,elIncierto) {
        $(elVerdad).prepend('<div class="titlePinochometro"><img src="/img/pinochometro/Pinocho-verde.png"><span>VERDAD</span><div class="pcmLogo"><span>PINOCHÓ</span><span class="gris">METRO</span></div></div>');
        $(elMentira).prepend('<div class="titlePinochometro"><img src="/img/pinochometro/Pinocho-rojo.png"><span>MENTIRA</span><div class="pcmLogo"><span>PINOCHÓ</span><span class="gris">METRO</span></div></div>');
        $(elMasCierto).prepend('<div class="titlePinochometro"><img src="/img/pinochometro/Pinocho-amarillo.png"><span>Más cierto que falso</span><div class="pcmLogo"><span>PINOCHÓ</span><span class="gris">METRO</span></div></div>');
        $(elMasFalso).prepend('<div class="titlePinochometro"><img src="/img/pinochometro/Pinocho-amarillo.png"><span>Más falso que verdadero</span><div class="pcmLogo"><span>PINOCHÓ</span><span class="gris">METRO</span></div></div>');
        $(elIncierto).prepend('<div class="titlePinochometro"><img src="/img/pinochometro/Pinocho-blanco.png"><span>No se puede verificar</span><div class="pcmLogo"><span>PINOCHÓ</span><span class="gris">METRO</span></div></div>');
    }
    function loadCommentsEditor() {
        $(document).foundation({
            tooltip: {
                selector: '.has-tip',
                additional_inheritable_classes: [],
                tooltip_class: '.tooltip',
                touch_close_text: 'tap to close',
                disable_for_touch: false,
                tip_template: function (selector, content) {
                    return '<span data-selector="' + selector + '" class="'
                        + Foundation.libs.tooltip.settings.tooltip_class.substring(1)
                        + '">' + content + '<br><div class="autorAnalisis">' + $('.has-tip[data-selector="' + selector + '"]').data('autor') + '</div><span class="nub"></span></span>';
                }
            }
        });
    }
    insertPinochometro($(".pcm-verdad"), $(".pcm-mentira"), $(".pcm-masCierto"), $(".pcm-masFalso"), $(".pcm-incierto"));   
    showPaywallFacebookMetered();
    
</script>

                
