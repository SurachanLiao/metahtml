			<script type="text/javascript">
			function mkresize(elem, max) {
				if (!elem.tagName || elem.tagName !== 'IMG' || !/^mr/.test(elem.name)) return;
				max = max || 100;
				var devicewidth = document.body.clientWidth - 20;
				if (max > devicewidth ) max = devicewidth ;
				elem.removeAttribute('width');
				elem.removeAttribute('height');
				if (elem.width >= elem.height) {
					if (elem.width > max) {
						elem.width = max;
						createWarning(elem,max);
					}
				} else {
					var ratio = elem.height / elem.width;
					if (elem.height > max * 2) {
						var nWidth = Math.round(max * 2 / ratio);
						if (nWidth > max) elem.width = nWidth = max;
						else elem.height = max * 2;
						createWarning(elem, nWidth);
					} else if (elem.width > max) {
						elem.width = max;
						createWarning(elem,max);
					}
				}
			}

			function createWarning (elem,current_width){
				if (current_width < 20) return;
				if (!!elem.previousSibling && /mkr_thead/.test(String(elem.previousSibling.className))) return;

				div = document.createElement('DIV');
				img = document.createElement('IMG');
				text = document.createTextNode('This image has been resized.Click to view original image');
	
				img.src = 'images/statusicon/wol_error.gif';
				img.alt = '';
				with(img.style) {
					width = '16px';
					height = '16px';
					border = '0';
					verticalAlign = 'top';
					margin = '0 2px';
				}

				div.title = 'This image has been resized.Click to view original image';
				div.className = 'thead mkr_thead';
				with(div.style) {
					width = current_width + 'px';
					height = '16px';
					lineHeight = '16px';
					fontSize = '0.9em';
					padding = '2px 0';
					margin = '0';
					overflow = 'hidden';
					whiteSpace= 'nowrap';
				}
	
				div.appendChild(img);
				div.appendChild(text);

				elem.parentNode.insertBefore(div, elem);
				if (div.clientWidth < Math.floor(div.scrollWidth / 2)) {
					div.removeChild(text);
				}
			}
</script>



