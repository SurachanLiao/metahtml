	tpj=jQuery;
			
tpj(document).ready(function() {
	if(tpj("#rev_slider_32_1").revolution == undefined){
		revslider_showDoubleJqueryError("#rev_slider_32_1");
	}else{
		revapi32 = tpj("#rev_slider_32_1").show().revolution({
			sliderType:"standard",
			jsFileLocation:"//www.ouvry.com/wp-content/plugins/revslider/public/assets/js/",
			sliderLayout:"auto",
			dottedOverlay:"none",
			delay:3000,
			navigation: {
				onHoverStop:"on",
			},
			visibilityLevels:[1240,1024,778,480],
			gridwidth:850,
			gridheight:550,
			lazyType:"none",
			shadow:0,
			spinner:"spinner0",
			stopLoop:"off",
			stopAfterLoops:-1,
			stopAtSlide:-1,
			shuffle:"off",
			autoHeight:"off",
			disableProgressBar:"on",
			hideThumbsOnMobile:"off",
			hideSliderAtLimit:0,
			hideCaptionAtLimit:0,
			hideAllCaptionAtLilmit:0,
			debugMode:false,
			fallbacks: {
				simplifyAll:"off",
				nextSlideOnWindowFocus:"off",
				disableFocusListener:false,
			}
		});
	}
	
});	/*ready*/
