(function() {
  var spotLogParts = {};
  function getSpotLog(o){ try { if (hpUACLogger.debugEnabled) { var r = []; for (var k in o) { if (o.hasOwnProperty(k)) { r.push(k + ': ' + o[k]); } } return r.join(', '); } else { return ''; } } catch(err) { return ''; } }

