           var section = "";  
           section = section || skd_category;
       
          // If on sport use custom lw tags
          if (section && section == "sport") {
            $("#skd_panorama2").attr("id", "skd_panorama2".replace("_", "_sport"));

            skd_ad_lw_ad("Panorama2", "skd_panorama2".replace("_", "_sport"), section);
          } else {
            skd_ad_lw_ad("Panorama2", "skd_panorama2", section);
          }
        </script></div></div></div></div><div class="row align-items-stretch"><div class="col-12 board-col-xs-12 board-col-sm-12 board-col-md-12 board-col-lg-12"><div class="rendered_board_widget content-size-xs-12 content-size-sm-12 content-size-md-12 content-size-lg-12" id="skd_package-2"><div class="base-widget widget-article-package p-2" style="border-color: #000;"><div class="article-package-news-teaser d-block position-relative">
