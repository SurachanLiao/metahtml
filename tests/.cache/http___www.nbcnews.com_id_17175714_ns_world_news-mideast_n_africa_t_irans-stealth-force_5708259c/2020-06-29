    <script type="text/javascript">
        $(function () {

            $('#marquee .adBNM').bind(ijv.evt.AdsBreakingNewsRender, function () {
                if ($('#marquee .AdBnmLeft div').html().trim()) {
                    if ($('body').hasClass('today')) {
                        $('#marquee').css('width', '100%');
                        $('#marquee').css('padding-right', '0px');
                        $('#marquee').css('padding-left', '0px');
                    } else {
                        var bgcolor = $('#marquee div h1').css('background-color');
                        var height = $('#marquee div').css('height');
                        $('#marquee').css('background-color', bgcolor);
                        $('#marquee').css('height', height);
                    }
                }
            });

            $(window).bind(ijv.evt.AdsStoryInitialized, function (e) {
                if ($(window).width() > 1024) {
                    $('#marquee .adBNM').trigger(ijv.evt.AdsBreakingNewsRender);
                } else {
                    $(window).resize(function () {
                        if ($(window).width() > 1024) {
                            $('#marquee .adBNM').trigger(ijv.evt.AdsBreakingNewsEnable);
                        }
                    });

                    $('#marquee .adBNM').bind(ijv.evt.AdsBreakingNewsEnable, function (e) {
                        $('#marquee .adBNM').bind(ijv.evt.EnterViewport, function (e) {
                            $('#marquee .adBNM').unbind(ijv.evt.EnterViewport);
                            $('#marquee .adBNM').unbind(ijv.evt.AdsBreakingNewsEnable);
                            $('#marquee .adBNM').trigger(ijv.evt.AdsBreakingNewsRender);
                        });
                    });
                }
            });
        });
    </script>

     
