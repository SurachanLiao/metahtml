<div class="section__content is-idle" data-js-atom="section-content" role="region"
     id="section-content-ID--1085107661" aria-labelledby="section-title-ID--1085107661" aria-hidden="false">
    <div class="u-grid is-separated-by-line" >
        <div class="grid__col-12 is-tablet-col-6">
                <div
    
    class="c-teaser--default is-compact-view"
    data-css="c-teaser"
