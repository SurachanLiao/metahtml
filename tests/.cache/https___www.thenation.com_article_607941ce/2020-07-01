		<script >
			var pageCounterPaywall ;
			var isMobile = {
			Android: function() {
				return navigator.userAgent.match(/Android/i);
			},
			BlackBerry: function() {
				return navigator.userAgent.match(/BlackBerry/i);
			},
			iOS: function() {
				return navigator.userAgent.match(/iPhone|iPad|iPod/i);
			},
			Opera: function() {
				return navigator.userAgent.match(/Opera Mini/i);
			},
			Windows: function() {
				return navigator.userAgent.match(/IEMobile/i);
			},
			any: function() {
				return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
			}
		};
		</script>
		<!-- Function to call Advertisements on entire website-->
		
				<script>		
			function call_ad_new(ad_type,ad_name,ad_div_id,ad_name_sno,targeting_var){	
												if (tn_ads_on === true && thenationads === true) {
					var w=h=w1=h1=w2=h2=0;	
					googletag.cmd.push(function(){
						var ad_slot = null;
						switch(ad_type){
							case 'leaderboard':									
								w = !isMobile.any() ? 728 : 320;
								h = !isMobile.any() ? 90 : 50;
								w1 = !isMobile.any() ? 970 : 320;
								h1 = !isMobile.any() ? 90 : 50;
								w2 = !isMobile.any() ? 970 : 320 ;
								h2 = !isMobile.any() ? 250 : 50;										
								ad_slot = googletag.defineSlot('/1004953/'+ad_name, [[w, h],[w1,h1],[w2,h2]], ad_div_id);
								break;
							case 'halfpage':
								w = !isMobile.any() ? 300 : 300;
								h = !isMobile.any() ? 250 : 250;
								w1 = !isMobile.any() ? 300 : 300;
								h1 = !isMobile.any() ? 600 : 250;									
								ad_slot = googletag.defineSlot('/1004953/'+ad_name, [[w, h],[w1,h1]], ad_div_id);
								break;	
							case 'halfpage250':
								w = !isMobile.any() ? 300 : 300;
								h = !isMobile.any() ? 250 : 250;
								ad_slot = googletag.defineSlot('/1004953/'+ad_name, [w, h], ad_div_id);
								break;
							case 'pixel':
								w = !isMobile.any() ? 1 : 1;
								h = !isMobile.any() ? 1 : 2;
								ad_slot = googletag.defineSlot('/1004953/'+ad_name, [w, h], ad_div_id);
								break;
							default:
								ad_slot = googletag.defineSlot('/1004953/'+ad_name, [w, h], ad_div_id);
								break;
						}
						
						ad_slot.setTargeting('tn_pos', [ad_name_sno]).setTargeting('tn_loc', ['atf']).addService(googletag.pubads());
						googletag.pubads().enableSingleRequest();
						if (Object.keys(targeting_var).length > 0) {
							for (var key in targeting_var){
								googletag.pubads().setTargeting(key, targeting_var[key]);
							}
							googletag.enableServices();
						}
					});
					googletag.cmd.push(function() {
						googletag.display(ad_div_id);		
					});
				}		
						
			}
		</script>
		
					
