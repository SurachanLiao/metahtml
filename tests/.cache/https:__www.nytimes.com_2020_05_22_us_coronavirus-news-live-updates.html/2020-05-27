    border-bottom: 2px solid;
    padding-bottom: 2px;
  }

  #masthead-bar-one {
    display: none;
  }

  /* No hover state on in app */
  .Hybrid .nytslm_li a:hover,
  .Hybrid .nytslm_li_loud a:hover {
    border-bottom: none;
    padding-bottom: 0;
  }

  h2[id^=link-] {
    scroll-margin-top: 24px;
  }

  .nytslm_baselineAlign {
    align-items: baseline;
  }
  .nytslm_centerAlign {
    align-items: center;
  }

