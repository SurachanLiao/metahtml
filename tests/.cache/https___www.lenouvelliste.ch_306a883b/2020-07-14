        var users_info = [];
                
        $(document).ready(function() {

            $.ajax({
                method: "GET",
                url: "/_private/user-info.php?env=prod"
                /*url: "/_private/user-info/"*/
            })
            .done(function( data ) {


                
                if(data.logged) {

                    users_info = data;

                    var username = '';
                    if(data.firstname) {
                        username += data.firstname;
                    }
                    if(data.lastname) {
                        if(username != '') {
                            username += ' ';
                        }
                        username += data.lastname;
                    }

                    $('.login-form').hide();
                    $('span.user-text').text(username);
                    $('span.user-email').text(data.username);

                    $('.nav.navbar-nav.navbar-right a[data-auth-href]').each(function() {
                        if ($(this).attr('data-auth-href') !== $(this).attr('href')) {
                            $(this)
                                .attr('href', $(this).attr('data-auth-href'))
                                .attr('title', $(this).attr('data-auth-title'))
                                .attr('target', $(this).attr('data-auth-target'))
                                .html($(this).attr('data-auth-title'))
                            ;
                        }
                    });

                    if(data.hasNumericAccess) {

                        if(data.subscriptionName) {
                            $('span.abo-data-title').text(data.subscriptionName);
                        }
                        else {
                            $('span.abo-data-title').parent().hide();
                        }

                        if(data.subscriptionValidity) {
                            $('span.abo-data-validity').text(data.subscriptionValidity)
                            $('span.abo-data-validity').parent().show();
                        }
                        else {
                            $('span.abo-data-validity').parent().hide();
                        }

                        $('div.abo-data').show();
                        $('div.subscription-button').hide();
