            $(".datalayer").click(function(e){
                var id = $(this).attr("dl-id");
                var name = $(this).attr("dl-name");
                var brand = $(this).attr("dl-brand");
                var category = $(this).attr("dl-category");
                var price =  $(this).attr("dl-price");
                var position = 0;

                if(brand == ''){
                    brand = 'na';
                }

                $(this).parent().find($(".datalayer")).each(function(index){
                    if(id == $(this).attr("dl-id")) {
                        position = index+1;
                    }
                });

                var list = 'Homepage_Col1';

                // Check dans quel colonne il se trouve
                if($('div.col-1-1').find($(this)).length == 1 && $('body').hasClass('home')) {
                    list = 'Homepage_Col1';
                }
                else if($('div.col-1-2').find($(this)).length == 1 && $('body').hasClass('home')) {
                    list = 'Homepage_Col2';
                }
                else if($('div.col-1-1').find($(this)).length == 1) {
                    list = 'Article_Col1';
                }
                else if($('div.col-1-2').find($(this)).length == 1) {
                    list = 'Article_Col2';
                }
                else if($('div.tab-content').find($(this)).length == 1) {
                    list = 'Article_Les+Lus';
                }

                dataLayer.push({
                    'event': 'productClick',
                    'ecommerce': {
                        'click': {
                            'actionField': {'list': list},
                            'products': [{
                                'name': name,       // Titre de l'article
                                'id': id, // ID de l'article
                                'price': price, // nombre de mots de l'article
                                'brand': brand, // provenance de l'article : ATS / Région IECG / Communiqué
                                'category': category, // Catégorisation de l'article sur le schéma : Site/Section/Sous-section/Type de contenu (article, infog, photo, video)/Type d’article (gratuit ou payant)
                                'position': position // position de l'article dans la liste
                            }]
                        }
                    }
                });
            });


