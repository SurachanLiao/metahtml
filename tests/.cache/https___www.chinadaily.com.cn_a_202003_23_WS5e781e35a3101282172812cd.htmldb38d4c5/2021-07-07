      //<![CDATA[
      function iFrameScn(){
        var oHref0 = document.getElementById("SCN0").getAttribute("href");
        var oHref1 = document.getElementById("SCN1").getAttribute("href"); 		
        var oDiv =  document.getElementById("SCN");

