<script type="text/javascript">

	$( document ).ready(function() {
		
		var dom = "es";
				
			if ( $("#header-provincia").length > 0 ) { //si no existe este boton, esta conectado, por lo tanto no se muestra mapa

				if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
					if(readCookie('panaNombreProvincia') == null || readCookie('panaNombreProvincia') == ''){
						mostrarSelectProvincias();
					} else {
						existeCookieProvincia(readCookie('panaNombreProvincia'));
						cambiarOfertasProvincia(readCookie('panaNombreProvincia'));
					}
				} else {
					if(readCookie('panaNombreProvincia') == null || readCookie('panaNombreProvincia') == ''){
						mostrarMapaProvincias();
					} else {
						existeCookieProvincia(readCookie('panaNombreProvincia'));
						cambiarOfertasProvincia(readCookie('panaCodProvincia'));
					}
				}
				
			}
			
			
		  $.get( 'http://'+'www.panavision-tours.es'+'/rol2/pag/views/banners.jsp?dom='+dom, function( data ) {
		  $( "#container" ).append( data );
		});
	});
	
	function cambiarOfertasProvincia(codProvincia){
		
		if(codProvincia == 28){
			$("a#oferta").attr('href', '/ofertas/madrid/'); //'Madrid';
		} else if(codProvincia == 2 || codProvincia == 02){
