/* S- Custom DFP For Callback Function */

 googletag.pubads().addEventListener('slotRenderEnded', function(event) {
 try {  
     if(event.slot == gpt_billboard && !event.isEmpty){
          $(document).ready(function(){
             $('#billboard_banner').show();
             Billboard_Sticky();
          })
     }
     if (event.slot == gpt_mediumrectangle2 && !event.isEmpty){
         $(window).scroll(function(){stickyMR2();})
     }
     if (event.slot == gpt_balloon && !event.isEmpty) {
          setTimeout(function(){$('.slideleft').css('display', 'block');}, 500);
      }
      if (event.slot == gpt_topframe && !event.isEmpty ){
            $('.top_banner_bar').height('50px');
            $('.close_tb').show();
      }
      if (event.slot == gpt_parallax1 && !event.isEmpty) {
          var containsAd = !event.isEmpty,
          width = event.size[0],
          height = event.size[1];

          if (height == 250 && width == 300){
               $('#parallax1').show();
               $('.parallax1').css("height", "250px");
               $('.parallax1').css("width", "300px");
               $('.parallax1 .parallax_fix').css({"width": "300", "position": "relative"});
               $('#div-gpt-ad-1572937270585-0 iframe').css("top", "0px");
               $('.parallax1 .parallax_abs').css("width", "300px");
          }
          if (height == 1 && width == 1){
               $('#parallax1').show();
               $('.parallax1').css("height", "300px");
               $('.parallax1').css("width", "430px");
               $('#div-gpt-ad-1572937270585-0 iframe').css({"height": "1280", "width": "430", "top": "0", "position": "fixed"});
               $('.parallax1 .parallax_abs').css("width", "430px");
          }
     }
     if (event.slot == gpt_parallax2 && !event.isEmpty) {
          var containsAd = !event.isEmpty,
          width2 = event.size[0],
          height2 = event.size[1];

          if (height2 == 250 && width2 == 300){
               $('#parallax2').show();
               $('.parallax2').css("height", "250px");
               $('.parallax2').css("width", "300px");
               $('.parallax2 .parallax_fix').css({"width": "300", "position": "relative"});
               $('#div-gpt-ad-1572937414499-0 iframe').css("top", "0px");
               $('.parallax2 .parallax_abs').css("width", "300px");
          }
          if(height2 == 1 && width2 == 1){
               $('#parallax2').show();
               $('.parallax2').css("height", "300px");
               $('.parallax2').css("width", "430px");
               $('#div-gpt-ad-1572937414499-0 iframe').css({"height": "1280", "width": "430", "top": "0", "position": "fixed"});
               $('.parallax2 .parallax_abs').css("width", "430px");
           }
     }
     if (event.slot == gpt_parallaxindetail && !event.isEmpty) {
        var containsAd = !event.isEmpty,
        width = event.size[0],
        height = event.size[1];

        if (height == 250 && width == 300){
            $('.parallax_detail').css("height", "250px");
            $('.parallax_detail').css("width", "300px");
            $('.parallax_detail .parallax_fix').css(
                {"width": "300", "position": "relative"}
            );
            $('.parallax_detail .parallax_abs').css("width", "300px");
        }
        if(height == 1 && width == 1){
            $('.parallax_detail').css("height", "300px");
            $('.parallax_detail').css("width", "430px");
            $('#div-gpt-ad-1572507980488-0 iframe').css(
                {"height": "1280", "width": "430", "top": "0", "position": "fixed"}
            );
            $('.parallax_detail .parallax_abs').css("width", "430px");
        }
     }
     if (event.slot == gpt_bottomframe && !event.isEmpty){
            $('.close_tbot').show();
            $('.bottom_banner_bar').show();
     }
     if(event.slot == gpt_leaderboard && event.isEmpty){
           removeElement('header__leaderboard');
     }
} 
catch (e) {
     console.log(e);
}
});
