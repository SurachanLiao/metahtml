    <script>
        $(document).on('click', '.overlay-blur', function(event){
            event.preventDefault();
            $('#program-dialog').modal('show');
            return;

        });

        $(document).on('click', '.player-video .widescreen .icon', function(event){
            event.preventDefault();
            $('#program-dialog').modal('show');
            return;

        });

    </script>


    <script type="text/javascript">

      $(document).ready(function() {
        $('#btn-plus').click(function(){
          $('#hidden-description').show();
          $('#description').addClass('max');
          $('#btn-plus').hide();
          $('#btn-moins').show();
        });
      });

      $(document).ready(function() {
        $('#btn-moins').click(function(){
          $('#hidden-description').hide();
          $('#description').removeClass('max');
          $('#btn-plus').show();
          $('#btn-moins').hide();
        });
      });

    </script>

