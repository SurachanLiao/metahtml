        <div class="checkbox ">
            <label>
                <input type="checkbox" name="filters" value="1657" >
                Żvilupp reġjonali
            </label>
        </div>

                </div>
                <div class="btn btn-link slide-toggle-agnostic" data-target="slide-toggle-container-0">
                    <span class="glyphicon glyphicon-plus"></span>
                    <a href="javascript:void(0);">
                        Ara s-suġġetti kollha
                    </a>
                </div>
        </fieldset>
        <fieldset class="fieldset-WorkingParties">
