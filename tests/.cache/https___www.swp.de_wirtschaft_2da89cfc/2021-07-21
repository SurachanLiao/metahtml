<script type="application/javascript">
	var trackingkObj = {};
	var statistics = {
		checkout: function (data) {
			window.dataLayer = window.dataLayer || [];
			// Tracking der einzelnen Schritte im Checkout
			if ("load" === data.action) {
				trackingkObj = {
					event: "pageview",
					page: "/checkout/" + data.pageName,
					product: data.product
				};
				window.dataLayer.push(trackingkObj);
			// Abschluss des Kaufs
			} else if ("submit" === data.action) {
				// Submit kommt auch beim 'basket' vor, macht dort aber keinen Sinn
				if ('basket' === data.pageName) {
					return;
				}
				var i = 0, form = [];
				form = data.data || form;
				for (i = 0; i < form.length; i++) {
					if (form[i].name === "paymentMethod") {
						trackingkObj = {
							event: "pageview",
							eventCategory: "PaymentMethod",
