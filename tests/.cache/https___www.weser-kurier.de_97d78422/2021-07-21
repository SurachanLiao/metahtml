  function makeCRETrackingCall() {
    if (typeof cre_client !== 'undefined') {
        console.log('started makeCRETrackingCall...');
        cre_client.debug = 1;
        cre_client.set_page_view();
        cre_client.set_origin('web');
        cre_client.set_service_id('wkr');
        cre_client.set_content_id('/');
        cre_client.set_cms_id('0267-11c3bf60403e-57d05c6be866-1000');
          cre_client.set_channel('/');
        cre_client.set_entitlement('free');
        cre_client.set_doc_type('webpage');				    
        cre_client.set_heading('startseite.dwp');				    				    
        cre_client.addListener(function(response) {
          if (response.data) {
            c1Response = response.data;
            console.log(c1Response);
            //_CUSTOMER['userStatus'] = response.data.user_status;
          }
        });				    
        cre_client.request();
    } else {
        setTimeout(makeCRETrackingCall, 1000);
    }
  }

  makeCRETrackingCall();
</script>







