          var storedData = JSON.parse(storedData);
        }

        if( typeof storedData === 'object' && ( storedData.refId != actualRefId && storedData.refUrl == document.referrer && typeof storedData.version !== 'undefined' && storedData.version == dlhVersion ) ) {
