										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_728x90', [[728,90],[768,90],[468,60]], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_728x90').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_180x300', [180,300], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_180x300').addService(googletag.pubads()); 
										googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_970x250', [[728,90],[970,90],[970,250]], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_970x250').addService(googletag.pubads()); 
											googletag.defineSlot('/1039154/Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_NewsArticle_Native', [650,110], 'Hindi_Moneycontrol/Hindi_Moneycontrol_ROS/Hindi_Moneycontrol_ROS_NewsArticle_Native').addService(googletag.pubads()); 
