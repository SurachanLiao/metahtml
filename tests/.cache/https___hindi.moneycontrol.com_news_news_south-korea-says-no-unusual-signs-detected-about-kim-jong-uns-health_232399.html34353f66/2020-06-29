	<script>
	function chkfeed()
	{
		var strMessage = Quill.getLanguageText('txtMessage');
		strMessage = strMessage.replace(/^\s+/,'');
		strMessage = strMessage.replace(/\s+$/,'');

		var tmp=strMessage;

		tmp.replace(" ","");
		
		if(tmp.length < 1)
		{
			alert("Please input your Message");
			return false;
		}

		else if(tmp.length < 4)
		{
			alert ("Please enter the message more then 4 character");
			return false;
		}

		if(tmp.length > 5000)
		{
			alert("max 5000 characters allowed");
			return false;
		}

		
		var char1=new Array('<','>','&lt;','&gt;','http://');
			
		for(i=0;i<=char1.length;++i)
		{
			if(tmp.indexOf(char1[i],0)!=-1)
			{
			alert("No "+char1[i]+" allowed");
			return false;
			}
				
		}

		var str=tmp;
	   str=str.toLowerCase();
	   var char1=new Array('<','>','www','&lt;','&gt;','http://','.com','.net','.org','.fr','.in','fuck','aswhole','co.uk');
			
		for(i=0;i<=char1.length;++i)
		{
			if(str.indexOf(char1[i],0)!=-1)
			{
			alert("Word "+char1[i]+" not allowed");
			return false;
			}
				
		}

	document.getElementById('f_message_news').value = Quill.getLanguageText('txtMessage');
	}
		function changerhstab(tabname) {
			
			if(tabname == 'relatednews') {
				document.getElementById('topnews_head').className='nact';
				document.getElementById('top_news').style.display='none'; 
				document.getElementById('relatednews_head').className='act';
				document.getElementById('related_news').style.display='block'; 
			}
			if(tabname == 'topnews') {
				document.getElementById('relatednews_head').className='nact';
				document.getElementById('related_news').style.display='none'; 
				document.getElementById('topnews_head').className='act';
				document.getElementById('top_news').style.display='block'; 
			}
			if(tabname == 'budget') {
				document.getElementById('budget_head').className='act';
				document.getElementById('budget').style.display='block'; 
				document.getElementById('budget_expect_head').className='nact';
				document.getElementById('budget_expect').style.display='none'; 
			}
			if(tabname == 'budget_expect') {
				document.getElementById('budget_head').className='nact';
				document.getElementById('budget').style.display='none'; 
				document.getElementById('budget_expect_head').className='act';
				document.getElementById('budget_expect').style.display='block'; 
			}
		}
		function wealthArticle(num) {
			for (tabcnt=1; tabcnt<=5; tabcnt++)
			{
				if(num==tabcnt){
				   document.getElementById("warticle_tab_"+tabcnt).className='active';			
				   document.getElementById("wartcles_div_"+tabcnt).style.display='block';			
				} else {
				   document.getElementById("warticle_tab_"+tabcnt).className='';	
				   document.getElementById("wartcles_div_"+tabcnt).style.display='none';	
				}
			}
		}
