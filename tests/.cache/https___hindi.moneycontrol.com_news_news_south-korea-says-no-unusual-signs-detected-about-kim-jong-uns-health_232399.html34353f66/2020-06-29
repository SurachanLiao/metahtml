/* Common Autosearch */
function getAutosuggesion(){
	var search_str = LTrim(RTrim($('#search_str').val()));
	var topsearch_type = LTrim(RTrim($('#topsearch_type').val()));
	
	if(search_str=='à¤¯à¤¹à¤¾à¤ à¤¢à¥à¤à¤¢à¤¿à¤')
	search_str='';
	
	var url = 'https://hindi.moneycontrol.com/mccode/common/autosuggesion.php?query='+escape(search_str)+'&type='+topsearch_type;
	//alert(url);
	if((topsearch_type==1 || topsearch_type==2) && search_str.length >= 3){
		$.get(url, function(data) {
			if(data.length>1){
				$("#autosugg_mc").html(data); 
				$("#autosugg_mc").show(); 
			}else{
				$("#autosugg_mc").hide(); 
			}
		});
	}else{
		$("#autosugg_mc").hide(); 
	}
}

