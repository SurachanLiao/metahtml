	var i;

	for (i = 0; i < NLHeader.length; i++) {
	  NLHeader[i].addEventListener("click", function() {
	    this.classList.toggle("active");
	    var menu = document.querySelector('.nl__opener');
	    if (menu.style.display === "block") {
	      menu.style.display = "none";
	    } else {
            inscription_newsletter_ajax(addEventListenerForm);
            menu.style.display = "block";
	    }
	  });
	}

    $(document).ready(function() {
        $('.js-close').click(function() {
            $('.nl__opener').slideToggle("fast");
            $( "button" ).removeClass( "active" )
        });
    });

	function addEventListenerForm() {
        var formNL = document.getElementById('NL-formulaire-header');

	    formNL.addEventListener("submit", function(e) {
            submitForm(e);
		});

	    var fieldGrosesse = document.getElementsByClassName('newsletter_grossesse_register')[0];
	    if (fieldGrosesse) {
            fieldGrosesse.addEventListener("change", function () {
                displayOrNotDeliveryDate();
            });
        }
	}

	function displayOrNotDeliveryDate() {
        var menu = document.querySelector('#header .delivery-date');
        if (menu.style.display === "block") {
            menu.style.display = "none";
        } else {
            menu.style.display = "block";
        }
	}

	function showLoader() {
	    var loader = document.getElementsByClassName('loader_subscribe_newsletter--header')[0];
        var buttonSubmit = document.getElementById('submitNlForm-header');
        if (loader && buttonSubmit) {
            loader.style.display = 'block';
            buttonSubmit.style.display = 'none';
        }
    }

	function hideLoader() {
        var loader = document.getElementsByClassName('loader_subscribe_newsletter--header')[0];
        var buttonSubmit = document.getElementById('submitNlForm-header');
        if (loader && buttonSubmit) {
            loader.style.display = 'none';
            buttonSubmit.style.display = 'block';
        }
	}

	function submitForm(e) {
	    e.preventDefault();
        showLoader();
        var form = $('#NL-formulaire-header');
        var action = form.attr('action');
        $.ajax({
            url: action,
            method: 'post',
            data: form.serialize(),
            success: function (s) {
                clearErrorsHeader();
                if (s.result == 0) {
                    hideLoader();
                    for (var key in s.data) {
                        document.getElementById(key + '_errors_header').innerHTML = s.data[key];
                    }
                }
                if (s.success) {
                    $('#ifMessageSuccess').html(s.success);
                    if (s.datalayer) {
                        newslettersDatalayerPush(s.datalayer);
                    }
                }
            }
        })
    }

    function inscription_newsletter_ajax(callback) {
        var button = $('.news-letter');
        var url = button.data('href');
        utils.ajaxGet({
            url: url,
            success: function (s) {
                $('#form-nl-ajax').html(s);
                displayDeliveryDateOnSanteMagazine();
                callback();
            }
        });
	}

	function displayDeliveryDateOnSanteMagazine() {
        if (document.getElementById("delivery_date_on_santemagazine")) {
            document.getElementById('delivery_date_on_santemagazine').style.display = 'none';
        }
    }

    function clearErrorsHeader() {
        var keys = {
            'email':'email',
            'newsletters':'newsletters',
            'delivery_date':'delivery_date'
        };
        for (var key in keys) {
            if (document.getElementById(key + '_errors_header') && document.getElementById(key + '_errors_header') != null) {
                document.getElementById(key + '_errors_header').innerHTML = "";
            }
        }
    }

    function newslettersDatalayerPush(data) {
        if (!data.hasOwnProperty('date_accouchement')) {
            utils.destroyCookie('date_accouchement');
        }
        for (var datum in data) {
            utils.setCookie(datum,data[datum],1);
        }
        dataLayer.push({
            'type_nl': utils.getCookie('type_nl'),
            'inscrit_nl': utils.getCookie('inscrit_nl'),
            'date_accouchement': utils.getCookie('date_accouchement'),
            'event': 'form_nl_submitted'
        });
    }
