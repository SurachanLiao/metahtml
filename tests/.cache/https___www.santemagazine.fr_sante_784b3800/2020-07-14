    var btnSearch = document.getElementById('js-search');
    var btnNewsletter = document.getElementById('newsletterbtn');
    var searchbloc = document.getElementById('searchbloc');

    btnNewsletter.addEventListener("click", function () {
        hideSearchBarWithoutAjax();
    });


    btnSearch.addEventListener("click", function () {
        hideFormNL();
    });

    function hideSearchBarWithoutAjax() {
        var searchbloc = document.getElementById('searchbloc');
        var searchBtn = document.getElementById('js-search');
        if (btnNewsletter && searchbloc.classList.contains('active')) {
            searchbloc.classList.remove('active');
            searchBtn.classList.remove('active');
        }
    }

    function hideFormNL() {
        var nlbloc = document.getElementById('nlbloc');
        var newsletterbtn = document.getElementById('newsletterbtn');
        if (nlbloc.style.display === "block") {
            nlbloc.style.display = "none";
            newsletterbtn.classList.remove('active');
        }
    }
