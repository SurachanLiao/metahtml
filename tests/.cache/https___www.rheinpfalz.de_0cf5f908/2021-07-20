		}

		//initializes the content iframes disabled in publication.php
		function initIFrames(){
			let rhpiframes = document.querySelectorAll('iframe');

			rhpiframes.forEach((rhpiframe) => {
				//console.log(rhpiframe.attributes);

				if (rhpiframe.getAttribute("rhpcmp") !== null) {
					rhpiframe.setAttribute("src", rhpiframe.getAttribute("rhpcmp"));
					console.log("iFrame restored");
				}
			});
		}

		//initializes elements of class rhpcmp
		function initRhpcmpItems(){
			let rhpcmpitems = document.querySelectorAll('.rhpcmp');

			rhpcmpitems.forEach((rhpcmpitem) => {
				//console.log(rhpiframe.attributes);

				//restore src attribute (for js)
				if (rhpcmpitem.getAttribute("rhpcmp") !== null) {
					rhpcmpitem.setAttribute("src", rhpcmpitem.getAttribute("rhpcmp"));
					console.log("rhpcmp item restored");
				}

				//restore data-pym-src attribute (for DPA widget)
				if (rhpcmpitem.getAttribute("data-pym-rhpcmp") !== null) {
					rhpcmpitem.setAttribute("data-pym-src", rhpcmpitem.getAttribute("data-pym-rhpcmp"));
					console.log("rhpcmp item restored");
				}
			});
		}

		// Init Social embeds
		function initSocials(){
			// Twitter embeds
			jQuery.ajax({
				url: "https://platform.twitter.com/widgets.js",
				dataType: 'jsonp',
				crossDomain: true
			});

			// Instagram embeds
			jQuery.ajax({
				url: "https://www.instagram.com/embed.js",
				dataType: 'jsonp',
				crossDomain: true
			});
		}
		function initPodigee(){
			if(typeof podigeeReplacementAction != 'undefined'){
				podigeeReplacementAction();
			}
		}
		//Setzt den Hilfscokie für traffectiv via Strtl
		function initStrtl(){
			// Implement storytile cookie for traffective integration
			var cookieName = 'strytl';
			var cookieValue = RHP.traffectivePageType + '.' + RHP.traffectiveZone + '.' + RHP.traffectivePageKey;
			var myDate = new Date();
			myDate.setMinutes( myDate.getMinutes() + 10 );
			document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate + ";domain=.rheinpfalz.de;path=/";
		}

		// DataLayer Push for cookie consent
		function initPushDataLayerConsent(rhpTrackingConsent) {
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				'event':'cookieConsent'
			});
			// Set cookie with different tracking states, depending on user action on SP CMP layer
			let cookieName = 'rhpConsent';
			document.cookie = cookieName +"=" + rhpTrackingConsent + ";domain=.rheinpfalz.de;path=/";

			$(document).ready(function() {
				if (rhpTrackingConsent === 0) {
					$('.nfy-banner.nfy-billboard').hide();
				}
				else {
					$('.nfy-banner.nfy-billboard').show();
				}
			});

		}

		// Render script-Tag to head for Traffective integration
		function initTraffective() {
			var s   = document.createElement('script');
			s.type  = 'text/javascript';
			s.id  = 'trfAdSetup';
			s.defer = true;
			s.setAttribute('data-traffectiveConf','{"targeting": [{"key":"zone","values":null,"value":"'+ RHP.traffectiveZone +'"},{"key":"pagetype","values":null,"value":"' + RHP.traffectivePageType + '"},{"key":"ort1","values":null,"value":"' + RHP.traffectivePageKey + '"},{"key":"programmatic_ads","values":null,"value":"true"},{"key":"ads","values":null,"value":"true"}],"dfpAdUrl" : "/12254361/rheinpfalz.de"}');
			s.src   = '//cdntrf.com/rheinpfalz.de.js';
			var sc  = document.getElementsByTagName('script')[0];
			sc.parentNode.insertBefore(s, sc);
		}

		// Render script-Tag to head for seeding alliance integration
		function initSeedingAlliance() {
			var s   = document.createElement('script');
			s.type  = 'text/javascript';
			s.async = true;
			s.src   = '//d.nativendo.de/cds/delivery/init?zone=' + RHP.traffectivePageKey;
			var sc  = document.getElementsByTagName('script')[0];
			sc.parentNode.insertBefore(s, sc);
		}

		// Anti-flicker snippet (recommended)
		function initGoogleOptimizeAntiFlicker(){
			var head = document.getElementsByTagName('head')[0];

			// style part of anti-flicker script
			var c =	document.createElement('style');
			var t = document.createTextNode('.async-hide { opacity: 0 !important}');
			c.appendChild(t);
			head.appendChild(c);

			// script part of anti-flicker script
			var s  = document.createElement('script');
			s.text  =	"(function(a,s,y,n,c,h,i,d,e){ \n";
			s.text +=	"\ts.className+=' '+y; \n";
			s.text +=	"\th.start=1*new Date; \n";
			s.text +=	"\th.end=i=function(){ \n";
			s.text +=	"\t\ts.className=s.className.replace(RegExp(' ?'+y),'')\n";
			s.text +=	"\t}\n";
			s.text +=	"\t(a[n]=a[n]||[]);\n";
			s.text +=	"\ta[n].hide=h;\n";
			s.text +=	"\tsetTimeout(function(){ i;h.end=null },c);\n";
			s.text +=	"\th.timeout=c;\n";
			s.text +=	"})\n";
			s.text +=	"(window,document.documentElement,'async-hide','dataLayer',4000,{ 'GTM-M5DLC35': true });";
			head.appendChild(s);
		}

		// Render script-Tag to head for google optimize integration
		function initGoogleOptimize() {
			var s   = document.createElement('script');
			s.src   = 'https://www.googleoptimize.com/optimize.js?id=GTM-M5DLC35';
			var head = document.getElementsByTagName('head')[0];
			head.appendChild(s);
		}

		// Init opinary poll widgets
		function initOpinaryPoll(){
			jQuery.ajax({
				url: "https://widgets.opinary.com/a/rheinpfalz.js",
				dataType: 'jsonp',
				crossDomain: true
			});
			jQuery.ajax({
				url: "https://widgets.opinary.com/embed.js",
				dataType: 'jsonp',
				crossDomain: true
