			snippetClose();

			// Enable link click listener
			if(typeof trakkenC1SnippetLinks != 'undefined'){
				trakkenC1SnippetLinks(templateName);
			}
		}

		/**
		 * setCyberSpecial()
		 * ===================
		 * Triggert die Template-Setter der CMS-Snippets auf Basis des templateName und schreibt generiertes HTML in Container.
		 *
		 * @param cmsSpecialTemplate
		 * @param cmsSpecialOffer
		 * @param cmsSpecialLayout
		 * @param cmsSpecialLink
		 * @param cmsSpecialId
		 * @param cmsSpecialName
		 * @param cmsSpecialCreative
		 * @param cmsSpecialPosition
		 * @returns string cmsSpecialTemplate
		 */
		function setCyberSpecial(cmsSpecialTemplate,cmsSpecialOffer,cmsSpecialLayout,cmsSpecialLink,cmsSpecialId,cmsSpecialName,cmsSpecialCreative,cmsSpecialPosition){
			let base64Image = '';
			let landingUrl = '';

			if(cmsSpecialLayout === 'black_friday_billboard'){
                // Billboard Ad
                base64Image = '';

				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<div id="cmsSnippet" class="cmsSnippetBillboard">';
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				cmsSpecialTemplate += '<!--a href="#" data-url="' + cmsSpecialLink + '" class="btn btn-primary toLanding cmsSnippetButton rhp-trakken-self-promotion rhpSelfPromotion '+ cmsSpecialOffer + '" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '">Sofort bestellen<\/a-->';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippet.cmsSnippetBillboard { position: relative; float: left; width: 100%; padding: 15px 0; text-align: center; } div#cmsSnippet .cmsSnippetButton { margin-top: -45px;}<\/style>';
				cmsSpecialTemplate += '<\/div>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'black_friday_slide'){
				// Mobile ad as default
				base64Image = '';
				// Desktop or Tablet trigger desktop native ad
				if (RHP.isDesktop || RHP.isTablet){
					base64Image = '';
				}
				//HTML Template for SlideIn CMS Snippet
				cmsSpecialTemplate += '<div id="cmsSnippet" class="cmsSnippetSlide">';
				cmsSpecialTemplate += '<span class="cmsSnippetClose">x<\/span>';
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				//cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="btn btn-primary toLanding cmsSnippetButton rhp-trakken-self-promotion rhpSelfPromotion '+ cmsSpecialOffer + '" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '">Sofort bestellen<\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippet.cmsSnippetSlide { position: fixed; bottom: -200px; width: 100%; max-width: 984px; padding: 0; margin-bottom: 0px !important; text-align: center; z-index: 999; animation-name:slideup; animation-delay:0.5s; animation-duration:0.8s; animation-fill-mode:forwards; animation-timing-function:ease-out;} div#cmsSnippet.cmsSnippetSlide img{ width:100%; background:#ffffff; } div#cmsSnippet .cmsSnippetButton { margin-top: -45px;} .cmsSnippetClose { float:right; display:inline-block; padding:0px 10px 10px 10px; margin-right: 0px; color:#005578; font-size:1.4rem; cursor:pointer; background: #B0C80A; } .cmsSnippetClose:hover { color:#fff; } @keyframes slideup { 0%{ bottom:-200px } 100%{ bottom:0; } }<\/style>';
				cmsSpecialTemplate += '<\/div>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'corona_slide'){
				//HTML Template for Corona SlideIn CMS Snippet
				cmsSpecialTemplate += '<div id="cmsSnippet" class="cmsSnippetSlide">';
				cmsSpecialTemplate += '<span class="cmsSnippetClose"> Anzeige schließen x<\/span>';
				cmsSpecialTemplate += '<a href="#" data-url="'+ cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				cmsSpecialTemplate += '<!--a href="#" data-url="'+ cmsSpecialLink + '" class="btn btn-primary toLanding cmsSnippetButton rhp-trakken-self-promotion rhpSelfPromotion '+ cmsSpecialOffer + '" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '">Sofort bestellen<\/a-->';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippet.cmsSnippetSlide { position: fixed; bottom: -220px; width: 100%; max-width: 984px; margin-bottom: 0; text-align: center; z-index: 999; animation-name:slideup; animation-delay:0.5s; animation-duration:0.8s; animation-fill-mode:forwards; animation-timing-function:ease-out;}  div#cmsSnippet.cmsSnippetSlide img{ width:100%; background:#ffffff; } div#cmsSnippet .cmsSnippetButton { margin-top: -45px;} div#cmsSnippet .cmsSnippetClose { width:100%; text-align:right; font-size:1rem; background:#c8dce5; float:right; display:inline-block; padding:2px 5px; color:#005578; cursor:pointer; } .cmsSnippetClose:hover { color:#fff; } #cmsSnippet .toLanding{ position:relative; float:left; width:100%;} #cmsSnippet .cmsSnippetImage{ position:relative; float:left; width:100%; } @keyframes slideup { 0%{ bottom:-220px } 100%{ bottom:0; } }<\/style>';
				cmsSpecialTemplate += '<\/div>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'rhp_billboard_c1'){
				// Billboard Ad
				base64Image = '';

				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '""><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippetBillboard { position: relative; width: 100%; padding: 15px 0; text-align: center; }<\/style>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'rhp_billboard_c1_fruehjahr'){
				// Billboard Ad
				base64Image = '';

				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '""><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippetBillboard { position: relative; width: 100%; padding: 15px 0; text-align: center; }<\/style>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'rhp_rectangle_c1'){
				// Set rectangle explicit
				base64Image = '';
				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippetRectangle { position: relative; width: 100%; padding: 15px 0; text-align: center; }<\/style>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'rhp_native_c1'){
				// Mobile ad as default
				base64Image = '';
				// Desktop or Tablet trigger desktop native ad
				if (RHP.isDesktop || RHP.isTablet){
					base64Image = '';
				}
				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippetNative { position: relative; width: 100%; padding: 15px 0; text-align: center; }<\/style>';

				return cmsSpecialTemplate;
			}else if(cmsSpecialLayout === 'rhp_native_c1_christmas'){
				// Mobile ad as default
				base64Image = '';
				// Desktop or Tablet trigger desktop native ad
				if (RHP.isDesktop || RHP.isTablet){
					base64Image = '';
				}
				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippetNative { position: relative; width: 100%; padding: 15px 0; text-align: center; }<\/style>';

				return cmsSpecialTemplate;
			}else{
				//HTML Template for Billdboard CMS Snippet
				cmsSpecialTemplate += '<div id="cmsSnippet" class="cmsSnippetBillboard">';
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="toLanding rhp-trakken-self-promotion rhpSelfPromotion" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '"><img class="cmsSnippetImage" src="' + base64Image + '" alt="" /><\/a>';
				cmsSpecialTemplate += '<a href="#" data-url="' + cmsSpecialLink + '" class="btn btn-primary toLanding cmsSnippetButton rhp-trakken-self-promotion rhpSelfPromotion '+ cmsSpecialOffer + '" data-offer="'+ cmsSpecialOffer + '" data-promotionid="' + cmsSpecialId + '" data-promotionname="' + cmsSpecialName + '" data-promotioncreative="' + cmsSpecialCreative + '" data-promotionposition="' + cmsSpecialPosition + '">Sofort bestellen<\/a>';
				// Added styles inline, so we don't run into varnish caching problems.
				cmsSpecialTemplate += '<style>div#cmsSnippet.cmsSnippetBillboard { position: relative; float: left; width: 100%; padding: 15px 0; text-align: center; } div#cmsSnippet .cmsSnippetButton { margin-top: -45px;}<\/style>';
				cmsSpecialTemplate += '<\/div>';

				return cmsSpecialTemplate;
			}

		}

		/**
		 * snippetClick()
		 * ============
		 * Ermöglicht Linkverfolgung nach Klick via data-url Attribut.
		 */
		function snippetClick(){
			jQuery('.toLanding').click(function(){
				var offer = jQuery(this).data('offer');
				var url = jQuery(this).data('url');
				if (typeof(cre_client) != "undefined") {
					console.log('SNIPPET-CONVERSION-SET begin');
					//cre_client.set_page_view();
					cre_client.set_service_id( c1ServiceId );
					cre_client.set_site(c1Site);
					cre_client.set_origin( c1Origin );
					cre_client.set_doc_type('paywall');
					cre_client.set_channel('1070');
					cre_client.set_cms_id('/offer/cmsSnippetToLanding/submit');
					cre_client.set_content_id('/offer/cmsSnippetToLanding/submit');
					cre_client.set_offer_ids(offer);
					cre_client.request();
					console.log('SNIPPET-CONVERSION-SET end');
				}
				window.location.href = url;
			});
		}

		/**
		 * landingReviveClick()
		 * ============
		 * Tracks Revive ads in C1.
		 */
		function landingReviveClick(){
			jQuery('.c1SnippetContainer a').click(function(){
				if (typeof(cre_client) != "undefined") {
					console.log('REVIVE-CONVERSION-SET begin');
					//cre_client.set_page_view();
