								date.setTime(date.getTime()+(30*24*60*60*1000));
								let expires = "; expires="+date.toUTCString();
								document.cookie = "consentRHP=true"+expires+"; domain=.rheinpfalz.de; path=/";
								if(adControl === '1') {
									initConsentTools(RHP);
								} else {
									console.log('page configured to not display ads');
								}
								// Share consent information with gtm
								pushConsentState('onPrivacyManagerAction',pmData.vendorConsent);
								break;
							default:
								console.log('vendorList '+ pmData.vendorConsent +' not in some or all.');
								// Share consent information with gtm
								pushConsentState('onPrivacyManagerAction',pmData.vendorConsent);
						}
						// Call Consent dataLayer push
						initPushDataLayerConsent(rhpTrackingConsent);
					},
					onMessageChoiceError: function (err) {
						console.log('onMessageChoiceError', err);
					},
					onMessageChoiceSelect:function(choice_id, choice_type_id){
						let rhpTrackingConsent = 0;
						switch(choice_type_id) {
								//Accepted all consents, trigger advertising and opinary polls
							case 11:
								rhpTrackingConsent = 1;
								let date = new Date();
								date.setTime(date.getTime()+(30*24*60*60*1000));
								let expires = "; expires="+date.toUTCString();
								document.cookie = "consentRHP=true"+expires+"; domain=.rheinpfalz.de; path=/";
								if(adControl === '1') {
									initConsentTools(RHP);
								} else {
									console.log('page configured to not display ads');
								}
								// Share consent information with gtm
								pushConsentState('onMessageChoiceSelect',choice_type_id);
								break;
							default:
								console.log('No Ads. Missing Consent.');
								// Share consent information with gtm
								pushConsentState('onMessageChoiceSelect',choice_type_id);
						}
						// Call Consent dataLayer push
						initPushDataLayerConsent(rhpTrackingConsent);
					},
					onConsentReady:function(consentUUID, euconsent){
						console.log('onConsentReady');
						//console.log('consentUUID', consentUUID);
						//console.log('euconsent', euconsent);

						// Define rhp tracking and ad consent as disabled
						let rhpTrackingConsent = 0;
						let storingConsent = 0;

						// Get consents from sourcepoint api, depending on UUID
						jQuery.ajax({
							url: 'https://sourcepoint.mgr.consensu.org/tcfv2/consent/v3/history/5388?consentUUID='+consentUUID,
							contentType: "application/json",
							dataType: 'json',
							async: false,
							success: function(result){
								// Get CMP categories (tracking, ads, analysis, etc.)
								let categories = result[0]['categories'];
								console.warn(categories);

								categories.forEach(function(item){
									// Define which categories trigger ads
									switch (item['iabPurposeRef']['iabId']) {
										// Deliver capsuled tools if consent is given
										case 1:
											storingConsent = 1;
											//User accepted storing, so we set consentRHP cookie to allow lazy loading of polls and surveys
											let date = new Date();
											date.setTime(date.getTime()+(30*24*60*60*1000));
											let expires = "; expires="+date.toUTCString();
											document.cookie = "consentRHP=true"+expires+"; domain=.rheinpfalz.de; path=/";
											break;
										// Define rhp tracking as enabled, if consent is given
										case 8:
										case 9:
										case 10:
											rhpTrackingConsent = 1;
											break;
									}
								});

								// Init capsuled tools
								if (storingConsent){
									if(adControl === '1') {
										initConsentTools(RHP);
									} else {
										console.log('page configured to not display ads');
									}
								}
								// Share consent information with gtm
								pushConsentState('onConsentReady',rhpTrackingConsent);

								// Call Consent dataLayer push
								initPushDataLayerConsent(rhpTrackingConsent);
							}
						});
					},
					onPMCancel: function () {
						console.log('onPMCancel');
					},
					onSPPMObjectReady: function () {
						console.log('onSPPMObjectReady', window._sp_.loadPrivacyManagerModal);
					}
				}
			}
		};
	</script><script src=https://cdn.privacy-mgmt.com/wrapperMessagingWithoutDetection.js async></script><script src=https://static-eu.cleverpush.com/channel/loader/zuiP3sW93KZL68MGt.js async></script><script src=https://cdnjs.cloudflare.com/ajax/libs/mobile-detect/1.4.3/mobile-detect.min.js></script><script src="https://track.rheinpfalz.de/cre-1.0/api/service/rhp/templatesets.js"></script><script src="https://track.rheinpfalz.de/cre-1.0/api/tracking/service/rhp/templateclient.js"></script><script src="https://track.rheinpfalz.de/cam-1.0/static/jssdk.js?lang=de&amp;service=rhp_rhp.de"></script><script>		

		// RHP object usse the uevice detection 'mobile-detect.js'
		RHP.getC1Origin = function () {
			var md = new MobileDetect(window.navigator.userAgent);
			if ( md.mobile() || md.tablet() ) {
				console.log('mobile');
				return 'mobile';
			}
			//console.log('web');
			return 'web';
		};

		// Assign that C1 data (object) to as syntax in PHPTAL
		var c1ServiceId = 'rhp_rhp.de';
		var c1Site = 'rheinpfalz.de';
		var c1Origin = RHP.getC1Origin();
		var c1Content = decodeURIComponent('/');
		var c1DocType = 'homepage';
		var c1Kicker = 'RHP';
		var c1Channel = '100';
		var c1SubChannel = '';
		var c1SubSubChannel = '';
		var c1CmsId = '1';
		var c1Heading = 'startseite';
		var c1Entitlement = '';
		var c1Conversion = false;
		var c1Tag = '';

		// Assign that C1 data (JSON) to as syntax in javascript
		var c1Values = JSON.parse(('{"serviceId":"rhp_rhp.de","site":"rheinpfalz.de","origin":"dummy","contentId":"\/","docType":"homepage","kicker":"RHP","channel":"100","subChannel":"","subSubChannel":"","cmsId":1,"heading":"startseite","entitlement":"","conversion":"false","tag":""}').replace(/&quot;/g, '"'));

		/**
		 * downloadScript(url, callback)
		 * ==============
		 * Allows C1 to dynamically reload scripts.
		 *
		 * @param url
		 * @param callback
		 */
		var downloadScript = function (url, callback) {
			jQuery.ajax({
				url: url,
				dataType: "script",
				cache: true,
				success: callback
			})
		};

		/**
		 * getUserType(products)
		 * =========================
		 * Gibt als Rückgabe den UserType des aktuellen Users zurück.
		 *
		 * @param products
		 * @return userType
		 */
		function getUserType(products) {
			var userType = 'Anonym';

			if(products.indexOf('product_rhp_print') >= 0){
				//Abonnenten mit Printkomponente
				if(products.indexOf('product_rhp_tagesticket_archiv') >= 0){
					userType = 'PrintArchivTag';
				}else if(products.indexOf('product_rhp_wochenticket_archiv') >= 0){
					userType = 'PrintArchivWoche';
				}else if(products.indexOf('product_rhp_flatrate_archiv') >= 0){
					userType = 'PrintArchivFlat';
				}else if(products.indexOf('product_rhp_tagesticket_rheinpfalz.de') >= 0){
					userType = 'PrintWebPass';
				}else if(products.indexOf('product_rhp_tagesticket_premium') >= 0){
					userType = 'PrintPremiumPass';
				}else if(products.indexOf('product_rhp_epaper-app-web') >= 0){
					userType = 'Premium';
				}else if(products.indexOf('product_rhp_app-web') >= 0 && products.indexOf('product_rhp_epaper') >= 0){
					userType = 'Premium';
				}else{
					userType = 'Print';
				}
			}else if(products.indexOf('product_ras_print') >= 0){
				//Abonnenten mit RaS-Printkomponente
				if(products.indexOf('product_rhp_tagesticket_archiv') >= 0){
					userType = 'PrintSoArchivTag';
				}else if(products.indexOf('product_rhp_wochenticket_archiv') >= 0){
					userType = 'PrintSoArchivWoche';
				}else if(products.indexOf('product_rhp_flatrate_archiv') >= 0){
					userType = 'PrintSoArchivFlat';
				}else if(products.indexOf('product_rhp_tagesticket_rheinpfalz.de') >= 0){
					userType = 'PrintSoWebPass';
				}else if(products.indexOf('product_rhp_tagesticket_premium') >= 0){
					userType = 'PrintSoPremiumPass';
				}else if(products.indexOf('product_rhp_epaper-app-web') >= 0){
					userType = 'PremiumSo';
				}else if(products.indexOf('product_rhp_app-web') >= 0 && products.indexOf('product_rhp_epaper') >= 0){
					userType = 'PremiumSo';
				}else{
					userType = 'PrintSo';
				}
			}else if(products.indexOf('product_rhp_print') < 0 && products.indexOf('product_ras_print') < 0){
				//Gastzugänge und Digitalabonnenten
				if(products.indexOf('product_rhp_tagesticket_archiv') >= 0){
					userType = 'GastArchivTag';
				}else if(products.indexOf('product_rhp_wochenticket_archiv') >= 0){
					userType = 'GastArchivWoche';
				}else if(products.indexOf('product_rhp_flatrate_archiv') >= 0){
					userType = 'GastArchivFlat';
				}else if(products.indexOf('product_rhp_tagesticket_rheinpfalz.de') >= 0){
					userType = 'GastWebPass';
				}else if(products.indexOf('product_rhp_tagesticket_premium') >= 0){
					userType = 'GastPremiumPass';
				}else if(products.indexOf('product_rhp_epaper-app-web') >= 0){
					userType = 'Digital';
				}else if(products.indexOf('product_rhp_app-web') >= 0 && products.indexOf('product_rhp_epaper') >= 0){
					userType = 'Digital';
				}
			}

			return userType;
		}

		/**
		 * content_manager(response)
		 * =========================
		 * Listener für C1 Callback. Aktiviert das Template.
		 *
		 * @param response
		 */
		function content_manager(response) {
			var userType = '';
			var data = response.data;
			var tracking_id = data.tracking_id;
			var products = data.product_ids;
			jQuery(document).ready(function () {
				template_activation(data, tracking_id);
				userType = getUserType(products);
				//add user_status to gtm dataLayer
				window.dataLayer = window.dataLayer || [];
				dataLayer.push({
					'userType': userType,
					'userStatus': data.user_status,
					'pageStatus': data.status,
					'userArticleCount': data.count,
					'pageInfo': data.info,
					'productIds': products
				});

				// Add template id as class to widgetContainer
				// Add nativendo paid container after blurred text
				if ( data.template_id !== undefined){
					jQuery('#widgetContainer').addClass(data.template_id).data('trakken',data.template_id);
					jQuery('#widgetContainer').append('<div class="nativendo-paid" style="position:relative;float:left;background:white;width:100%;clear:both;"><\/div>');
				}

				//add dataPushed as event
				window.dataLayer = window.dataLayer || [];
				dataLayer.push({
					'event' : 'dataPushed'
				});

				// Trakken paywall visibility
				trakkenPaywallVisible();

				// Trakken self promo visibility
				trakkenSelfPromotionVisible();

				// Init Drive Funnel Entry Listener
				driveFunnelInit();
			});
		}

		/**
		 * showOfferPage(conversionType)
		 * ===============
		 * Rendert die OfferPage auf Basis der in C1 hinterlegten Offers.
		 *
		 * @param conversionType
		 */
		function showOfferPage(conversionType) {
			let creid = RHP.tracking_id;
			jQuery.ajax({
				url: creClientHost + "cre-1.0/api/service/" + c1ServiceId + "/offers.js?tracking_id=" + creid,
				dataType: "jsonp",
				jsonpCallback: function(){ return "cre_callback_" + Math.floor(Math.random() * 100000);},
				jsonp: '_c',
				success:function(response){
					jQuery('#paywall-reminder').fadeOut();
					let conversionOffers = '';
					let offers = '';
					let impressions = '';
					let abTest = false;
					let abTestId = '';
					let abTestVariantId = '';
					let data = response.data;
					if (!data) {
						return;
					}
					let groups = data.groups;
					if (!groups) {
						return;
					}
					// Get A/B testing attributes from C1 offers
					let attributes = data.config.attributes;
					if (!attributes) {
						return;
					}else{
						abTest = true;
						abTestId = attributes.abtestid;
						abTestVariantId = attributes.abtestvariantid;
					}
					let container = jQuery('#c1_rhpOffers');
					container.html('<div class="offers"><div class="row"><\/div><\/div>');
					jQuery.map(groups, function(group) {
						let disrupter = group.config.attributes.disrupter;
						let offers = group.offers;
						console.log(offers);
						jQuery.map(offers, function(offer) {
							conversionOffers += offer.offer_id + ',';
							let template = setTemplate(offer, disrupter, conversionType);
							jQuery('#c1_rhpOffers .offers .row').append(template);
						});
					});
					// Check if PlusAbo widget is triggered, as we need to add offer_rhp_plus manually, because the main offer is not delivered by C1 call
					if ( conversionType.indexOf('PlusAbo') >= 0 ){
						conversionOffers += 'offer_rhp_plus,';
					}
					impressions = pushProductImpressions(data,groups);
					container.attr('id', 'offers');
					jQuery('.offerWidget').fadeIn();
					container.fadeIn();
					/* ### ### Collapse begin ### ###*/
					jQuery('button[data-toggle="collapse"]').click(function(){
						jQuery(this).toggleClass('fadeIn');
						jQuery(this).next('.collapse').toggleClass('fadeIn');
					});
					jQuery('button.rhp-collapsible').click(function () {
						jQuery('#offers .offers').slideToggle(500, function () {
							jQuery('button.rhp-collapsible h4').html(function () {
								return jQuery('#offers .offers').is(":visible") ? 'Weitere Angebote <i class="fa fa-chevron-up"><\/i>' : 'Weitere Angebote <i class="fa fa-chevron-down"><\/i>';
							});
						});
					});
					/* ### ### Collapse end ### ###*/
					if (typeof(cre_client) != "undefined") {
						console.log('CONVERSION-START begin');
						//cre_client.set_page_view();
						cre_client.set_service_id(c1ServiceId);
						cre_client.set_site(c1Site);
						cre_client.set_origin(c1Origin);
						cre_client.set_doc_type('paywall');
						cre_client.set_channel('1070');
						cre_client.set_cms_id('/offer/view');
						cre_client.set_content_id('/offer/view');
						cre_client.set_offer_ids(conversionOffers);
						// Trigger A/B testing calls, if A/B testing is active
						if ( abTest ){
							cre_client.set_ab_test_id(abTestId);
							cre_client.set_ab_test_variant_id(abTestVariantId);
							cre_client.set_site('offers.rheinpfalz.de');
							cre_client.set_channel('offer');
						}
						cre_client.request();
						console.log('CONVERSION-START end');
						startConversion(conversionType);

						// Add referers to allow jump back to article
						if(typeof addWidgetReferer != 'undefined'){
							addWidgetReferer();
						}
					}
					offerClick(data,offers,impressions);
				}
			})
		}

		/**
		 * pushProductImpressions(data,groups)
		 * =========================
		 * Angezeigte Produkte einer Paywall in GTM zur Verfügung stellen.
		 *
		 * @param data
		 * @param groups
		 * @return impressions
		 */
		function pushProductImpressions(data,groups) {
			var price = 0.00;
			var category = 'Paywall';
			var position = 0;
			var list = 'Artikel';
			var impressions = [];
			var offers = [];

			groups.forEach(function(group){
				offers[position] = group.offers;
				position++;
			});

			position = 0;
			offers.forEach(function(offer) {
				if(offer.length > 0){
					position++;
					console.log(offer);
					price = (offer[0].stores && offer[0].stores[0].price) / 100;
					impressions.push({
						'id' : offer[0].offer_id,
						'name' : offer[0].name,
						'price' : price,
						'category' : category,
						'position' : position,
						'list' : list
					});
				}
			});

			// push product impressions from paywall to gtm layer
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				"ecommerce": {
					"currencyCode": "EUR",
					"impressions": [
						impressions
					]
				}
			});

			return impressions;
		}

		/**
		 * pushProductClick(data,impressions,offer)
		 * =========================
		 * Klick auf Angebot einer Paywall an GTM DataLayer übermitteln.
		 *
		 * @param data
		 * @param impressions
		 * @param offer
		 */
		function pushProductClick(data,impressions,offer) {
			var id = offer;
			var name = '';
			var price = 0.00;
			var category = 'Paywall';
			var list = 'Artikel';
			var position = 0;

			impressions.forEach(function(impression) {
				if(impression.id === id){
					name = impression.name;
					price = impression.price;
					position = impression.position;
				}
			});

			// push product click from paywall to gtm layer
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				"event":"EEproductClick",
				"ecommerce" : {
					"currencyCode":"EUR",
					"click" : {
						"actionField" : [{
							"list": list
						}],
						"products" : [{
							"id" : offer,
							"name" : name,
							"price" : price,
							"category" : category,
							"position" : position
						}]
					}
				}
			});
		}

		/**
		 * setTemplate
		 * ===========
		 * HTML Konstruktor für OfferPage
		 *
		 * @param offer
		 * @param disrupter
		 * @param conversionType
		 * @return template
		 */
		function setTemplate(offer, disrupter, conversionType) {
			let offerHost = '';
			let offerLink = '';
			var name = offer.name;
			var description = offer.description;
			var price = (offer.stores && offer.stores[0].price) / 100;
			var text = '';
			var template = '';

			// Switch hosts, if offer is a ticket and needs a paypal checkout
			switch(offer.offer_id.trim()) {

				case 'offer_rhp_digital_akti_6w':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=62';
					break;
				case 'offer_rhp_print_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=8';
					break;
				case 'offer_rhp_print_geschenk_12':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=30';
					break;
				case 'offer_rhp_print_geschenk_6':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=28';
					break;
				case 'offer_rhp_print_geschenk_3':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=26';
					break;
				case 'offer_rhp_print_card_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=23';
					break;
				case 'offer_rhp_digital_card':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=5';
					break;
				case 'offer_rhp_digital_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=9';
					break;
				case 'offer_rhp_digital_card_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=14';
					break;
				case 'offer_rhp_upgrade_digital_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=37';
					break;
				case 'offer_rhp_premium':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=6';
					break;
				case 'offer_rhp_premium_probe':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=10';
					break;
				case 'offer_rhp_premium_student':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=19';
					break;
					case 'offer_rhp_premium_upgrade_print':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=37';
					break;
				case 'offer_rhp_digital_6m_fck':
					offerHost = creAboHost;
					offerLink = 'checkout/index/onestepcheckout/?product_id=15';
					break;
				case 'offer_rhp_tagesticket_premium':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=284';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=242';
					}
					break;
				case 'offer_rhp_print_akti_3m_NMA_13000279':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=146';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=125';
					}
					break;
				case 'offer_rhp_plus':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=226';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=202';
					}
					break;
				case 'offer_rhp_digital_akti_3m_NMA_13000279':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=148';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=126';
					}
					break;
				case 'offer_rhp_premium_akti_3m_NMA_13000213':
					offerHost = creAboHost;
					if(systemEnv == 'development') {
						offerLink = 'checkout/index/onestepcheckout/?product_id=149';
					}else{
						offerLink = 'checkout/index/onestepcheckout/?product_id=127';
					}
					break;
				default:
					offerHost = creAboHost;
			}

			if (conversionType === 'RegPlusAboWall' || conversionType === 'RegPlusAboWallFree'){
				text = '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn rhp-btn-text rhp-blue-text rhp-bold toOffer" data-offer="'+ offer.offer_id.trim() + '"><h4>' + name + ' - ' + price.toFixed(2) + '&euro;<\/h4><\/a>';

				// Linkchange for paypal checkout
				if( offerLink !== '' ){
					text = '<a href="#" data-url="'+ offerHost + offerLink + '" class="btn rhp-btn-text rhp-blue-text rhp-bold toOffer" data-offer="'+ offer.offer_id.trim() + '"><h4>' + name + ' - ' + price.toFixed(2) + '&euro;<\/h4><\/a>';
				}

				template = jQuery('<div class="offer rhp-plus-abo-offer col-sm-12"><\/div>');
				template.html(text);
			}else{
				text = '<header><h4>' + name + '<span class="float-right w-auto text-right">' + price.toFixed(2) + '&euro;<\/span><\/h4><\/header>';
				text += '<p><small>' + description + '<\/small><\/p>';

				if (disrupter) {
					text += '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn btn-primary btn-success toOffer" data-offer="'+ offer.offer_id.trim() + '">Auswählen<\/a>';
				}else{
					text += '<a href="#" data-url="'+ offerHost + offer.offer_id.trim() + '" class="btn btn-light btn-success toOffer" data-offer="'+ offer.offer_id.trim() + '">Auswählen<\/a>';
				}

				text += '<hr class="sand">';

				template = jQuery('<div class="offer col-sm-12"><\/div>');
				template.html(text);

				if (disrupter) {
					var title;
					var color;
					if (disrupter === 'topoffer') {
						title = 'Unsere Empfehlung';
						color = 'red';
					} else if (disrupter === 'bestselling') {
						title = 'Meistverkauft';
						color = 'green';
					} else {
						title = '';
						color = 'blue';
					}
					var ribbon = jQuery('<span class="highlight ' + color + '">' + title + '<\/span>');
					template.prepend(ribbon);
				}
			}
			return template;
		}

		/**
		 * getAdvertOptin()
		 * ================
		 * API Call um aktuellen WerbeOptin-Status zurück zu liefern.
		 */
		function getAdvertOptin(){
			window.getCookie = function(name) {
				match = document.cookie.match(new RegExp(name + '=([^;]+)'));
				if (match) return match[1];
			};
			var cresid = getCookie('cresid');
			if(cresid){
				jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/get', function(response) {
					var status = response.indexOf('checked');
					if(status !== -1){
						jQuery('input#werbeOptin').prop('checked',true);
					}else{
						jQuery('input#werbeOptin').removeAttr('checked');
					}
				});
			}
		}

		/**
		 * setAdvertOptin()
		 * ================
		 * API-Call um WerbeOptin zu setzen.
		 */
		function setAdvertOptin(){
			window.getCookie = function(name) {
				match = document.cookie.match(new RegExp(name + '=([^;]+)'));
				if (match) return match[1];
			};
			var cresid = getCookie('cresid');
			if(cresid){
				jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/delete', function(response) {
					var status = response.indexOf('true');
					jQuery.get(creProfilHost + 'ajax/'+cresid+'/optins/advertising', function(response) {
						var status = response.indexOf('true');
						if(status !== -1){
							jQuery('.featherlight').hide();
						}
					});
				});
			}
		}

		/**
		 * startConversion
		 * ===========
		 * C1 Request Sender für Conversion Funnel Tracking
		 *
		 * @param conversionType
		 */
		function startConversion(conversionType){
			//cre_client.set_page_view();
			cre_client.set_service_id(c1ServiceId);
			cre_client.set_site(c1Site);
			cre_client.set_origin(c1Origin);
			cre_client.set_channel('1070');
			cre_client.set_doc_type('paywall');
			cre_client.set_content_id('/offer/view/'+conversionType);
			cre_client.request();
		}

		/**
		 * template_activation(data)
		 * ===================
		 * Aktiviert das Template, wenn die PayWall aktiv ist, oder das Template Initialer Login aktiv ist.
		 *
		 * @param data
		 */
		function template_activation(data,tracking_id) {
			console.log(data);
			console.log(data.template_set);
			console.log(data.template_id);
			var tracking_id = tracking_id;
			RHP.tracking_id = tracking_id;
			var isPaywallActive = (data.status === 'paywall' || data.status === 'always_paid');
			if(data.template_id !== undefined){
				if (isPaywallActive || data.template_id === 'rhp_initial_login') {
                    cre_templateclient.show({
						"container_id":"widgetContainer",
						"templateset_name":data.template_set,
						"template_name":data.template_id,
					});
				}
			}
			// Get action lanes from c1 backend and start snippet rendering
			if(data.action_groups != undefined){
				let snippetParams = data.action_groups.cms_snippet.parameters;
				snippetParams.forEach(function(snippetParam){
					// Check if snippet parameter is of type adserver
					if(snippetParam.indexOf('adserver_') < 0){
						startConversion('cmsSnippet');
						showCmsSpecials(snippetParam);
					}else{
						// Set user group information on ad zone
						snippetParam = snippetParam.replace('adserver_','');

						// Iterate through c1 rectangle places and set data attribute for targeting
						let c1Rectangles = document.querySelectorAll('ins.c1SnippetRectangle');
						c1Rectangles.forEach(function(c1Rectangle){
							c1Rectangle.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 rectangle places on article detail and set data attribute for targeting
						let c1RectanglesSixBlocks = document.querySelectorAll('ins.c1SnippetRectangleSixBlocks');
						c1RectanglesSixBlocks.forEach(function(c1RectangleSixBlocks){
							c1RectangleSixBlocks.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 billboard places and set data attribute for targeting
						let c1Billboards = document.querySelectorAll('ins.c1SnippetBillboard');
						c1Billboards.forEach(function(c1Billboard){
							c1Billboard.setAttribute('data-revive-c1usergroup',snippetParam)
						});

						// Iterate through c1 native places and set data attribute for targeting
						let c1Natives = document.querySelectorAll('ins.c1SnippetNative');
						c1Natives.forEach(function(c1Native){
							c1Native.setAttribute('data-revive-c1usergroup',snippetParam)
						});
					}

				});
			}
		}

		// DataLayer Push for c1 user groups
		function pushSapDataLayer(sapParameter){
			window.dataLayer = window.dataLayer || [];
			dataLayer.push({
				'c1UserGroups':sapParameter
			});
		}

		/**
