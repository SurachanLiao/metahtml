  <script>
  (function($) {
    'use strict'
    const headerType = $('.js-variableHeader').data('header-type'),
          $hiddenHeader = $('.js-variableHeader'),
          $profileImg = $('.js-variableHeader__authorImg'),
          $profileAlterImg = $('.js-variableHeader__alterImg'),
          $labelTitle = $('.js-variableHeader__labelTitle');

    $(window).scroll(function() {
      let scrollTop = $(window).scrollTop();

      if (headerType == 'author') {
        const switchingPoint = 55,
              beforeImgSize = 80,
              beforeAlterImgSize = 7.0,
              afterImgSize = 40,
              afterAlterImgSize = 3.2;

        if (scrollTop > switchingPoint + afterImgSize) {
          $profileImg.width(afterImgSize);
          $profileImg.height(afterImgSize);
          $profileAlterImg.css({'font-size': afterAlterImgSize + 'rem'});
        } else if (scrollTop > switchingPoint) {
          let count = scrollTop - switchingPoint;
          $hiddenHeader.css({'display': 'flex'});
          $profileImg.width(beforeImgSize - count);
          $profileImg.height(beforeImgSize - count);
          $profileAlterImg.css({'font-size': beforeAlterImgSize - count / 10 + 'rem'});
        } else {
          $hiddenHeader.hide();
        }
      }

      if (headerType == 'label') {
        let labelTitlePosition = $labelTitle.position().top;
        const switchingPoint = labelTitlePosition - 11;
        if (scrollTop > switchingPoint) {
          $hiddenHeader.css({'display': 'flex'});
        } else {
          $hiddenHeader.hide();
        }
      }
    });
  })(jQuery);
  </script>



