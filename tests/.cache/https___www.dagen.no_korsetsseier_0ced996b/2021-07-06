<script>
function appear(options) {
  var intersectionObserver = new IntersectionObserver(function (entries) {
    entries.forEach(function (entry) {
      if (entry.isIntersecting) {
        //console.log('appear', entry.target);
        options.appear(entry.target);
        intersectionObserver.unobserve(entry.target);
      }
    });
  }, {
    rootMargin: (options.bounds ? (options.bounds + 'px') : '0px'),
    threshold: options.threshold || 0
  });

  var lazies = (typeof options.elements === 'function') ? options.elements() : [options.elements];
  lazies.forEach(function (node) { return intersectionObserver.observe(node)});
}
</script>
