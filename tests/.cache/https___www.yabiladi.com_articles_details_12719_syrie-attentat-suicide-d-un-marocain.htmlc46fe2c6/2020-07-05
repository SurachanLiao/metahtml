	$j('html, body').animate({scrollTop:0}, 'slow');
	WaitingContact();
	$j.get('/annonces/callSend', {id: id, type: type}, function(data) {
		recalculatePosition("#dialog2", 425, 424);
		$j("#dialog2").html(data);
	});
	return false;
}
  

