<script type="text/javascript">
function afterlogin_12719() {
	this.j = 1;
	repondre(12719, 1);
}

function VerifMail(email) {
	a = email;
	valide1 = false;
	
	for(var j=1;j<(a.length);j++){
		if(a.charAt(j)=='@'){
			if(j<(a.length-4)){
				for(var k=j;k<(a.length-2);k++){
					if(a.charAt(k)=='.') valide1=true;
				}
			}
		}
	}
	if(valide1==false) afficheNotification("Le format de votre adresse email est invalide !!");
	return valide1;
}

function verifier_12719() {
	if ($j('#LoginTitle_12719').val() == '' || $j('#LoginComment_12719').val() == '') 
		return false;
	else
		return true;
}
function submitLoginAnnonce12719() {
	$j('#LoginReturn').val($j('#requestURI').val());

		
	WaitingEffect();
	$j('#Loginform12719').ajaxSubmit({ 
		inlineValidation: false,
		url:'/login/index',
		success:  function(responseText, responseStatus) {
			$j('.login_12719').html(responseText);
			afterlogin_12719();
		}
	});	
	}



function identifier(data) {
	refreche();
}
var clicked = false;
function checkemail_12719() {
	
	if(verifier_12719()) {
		if(VerifMail($j("#LoginUsername_12719").val())) {
			$j('#btclick_12719').hide();
			$j('#wait_12719').html('<img src="/images/spinner.gif" title="chargement..." alt="chargement..." />');
			$j('#Loginform12719').ajaxSubmit({ 
				inlineValidation: false,
				url:'/register/checkemail',
				success:  function(responseText, responseStatus) {
					$j('#btclick_12719').show();
				if(responseText == 4) {
					afficheNotification("Le format de votre adresse email est invalide !!");
					$j('#wait').html('');
				} else if(responseText == 1) {
						$j("#mdp").show();
						$j("#mdpl").show();
						$j("#btclick_12719").removeAttr("onclick");
						$j("#btclick_12719").removeAttr("ondblclick");
						$j("#btclick_12719").click(connexionAccount);
						$j('#wait_12719').html('');
						
					} else if(responseText == 0) {
						$j("#add_newsletter_12719").html("Vous n'avez pas encore validé votre compte, cliquez <a href='javascript://' onclick='revalidate();'>ici</a> pour demander un nouveau mail de validation").show();
						$j('#wait_12719').html('');
						
					} else if(responseText == 5) {
						$j("#add_newsletter_12719").html("Désolé, Vous ne pouvez pas ajouter de commentaire, Contactez l'administrateur (webmaster@yabiladi.com) par mail pour plus d'informations.").show();
						$j('#wait_12719').html('');
						
					} else {
						$j("#pseudo_12719").show();
						$j("#pseudol_12719").show();
						$j("#btclick_12719").unbind();
						$j("#btclick_12719").removeAttr("onclick");
						$j("#btclick_12719").removeAttr("ondblclick");
						$j("#btclick_12719").bind("click", createAccount);
						$j('#wait_12719').html('');
					}
				}
			});
		}
			} else {
				afficheNotification("Merci de spécifier le sujet et le texte de votre commentaire avant de valider");
			}
	
}

function createAccount() {
	
	if(verifier_12719()) {
		$j('#btclick_12719').hide();
	$j('#Loginform12719').ajaxSubmit({ 
		inlineValidation: false,
		url:'/register/createAccount',
		success:  function(responseText, responseStatus) {
			$j('#btclick_12719').show();
			if(responseText == "true") {
				$j(".warning_12719").html("Votre commentaire a été ajouté avec succès");
				quitterPage = false;
				return false;
			} else if(responseText == "empty_field") {
					$j(".warning_12719").html("Merci de spécifier le sujet et le texte de votre commentaire avant de valider");
					quitterPage = false;
					return false;
			} else if(responseText == "double_content") {
				$j(".warning_12719").html("Désolé, Vous ne pouvez pas ajouter de commentaire, Contactez l'administrateur (webmaster@yabiladi.com) par mail pour plus d'informations. Error #005");
				closeEffect();
				quitterPage = false;
				
				return false;
			
			} else if(responseText == "ErrBodyTooLarge") {
				afficheNotification("Réduisez votre messages, il est trop long.");
				closeEffect();
				quitterPage = false;
				return false;
			} else if(responseText == "plus") {
				$j(".warning_12719").html("Désolé, vous avez atteint le nombre maximum de commentaires autorisés pour aujourd'hui");
				quitterPage = false;
				return false;
			}else if(responseText == "error") {
				$j(".warning_12719").html("Le login ou l'email que vous avez saisi sont déjà utilisés").show();
			}
			else if(responseText == "empty") {
				alert("Merci de spécifier le sujet et le texte de votre commentaire avant de valider").show();
				
			} else if(responseText == "ErrLength") {
				alert("Le pseudo est limité à 20 caractères").show();
				
			}
			 else {
				$j("#add_comment_12719").html(responseText);
			}
			
		}
	});
	} else {
		afficheNotification("Merci de spécifier le sujet et le texte de votre commentaire avant de valider");
		}
}

function connexionAccount() {
	$j('#btclick_12719').hide();
	if(verifier_12719()) {
	WaitingEffect();
$j('#Loginform12719').ajaxSubmit({ 
	inlineValidation: false,
	url:'/register/connexionAccount',
	success:  function(responseText, responseStatus) {
		$j('#btclick_12719').show();
		if(responseText == "false") {
			$j(".warning_12719").html("Désolé, Vous ne pouvez pas ajouter de commentaire, Contactez l'administrateur (webmaster@yabiladi.com) par mail pour plus d'informations.");
			closeEffect();
		}  else if(responseText == "plus") {
			$j(".warning_12719").html("Désolé, vous avez atteint le nombre maximum de commentaires autorisés pour aujourd'hui");
			closeEffect();
			} else if(responseText == "error") {
			$j(".warning_12719").html("Merci de vérifier vos informations de connexion").show();
			closeEffect();
		} else if(responseText == "empty_field") {
			$j(".warning_12719").html("Merci de spécifier le sujet et le texte de votre commentaire avant de valider.");
			closeEffect();
		} 
		else if(responseText == "empty") {
			$j(".warning_12719").html("Merci de renseigner vos informations de connexion");
			closeEffect();
		} else if(responseText == "length") {
			$j(".warning_12719").html("Votre sujet ne doit pas faire plus de 120 caractères");
			closeEffect();
		}  else if(responseText == "ErrBodyTooLarge") {
			afficheNotification("Réduisez votre messages, il est trop long.");
			closeEffect();
			quitterPage = false;
			return false;
		} else if(responseText == "double_content") {
			$j(".warning_12719").html("Désolé, Vous ne pouvez pas ajouter de commentaire, Contactez l'administrateur (webmaster@yabiladi.com) par mail pour plus d'informations. Error #005");
			closeEffect();
		} else {
			afficheNotification('Votre commentaire a été ajouté avec succés');
			$j('html, body').animate({scrollTop:0}, 'slow');
			$j('.login_12719').html(responseText);
			refreche();
		}
		
	}
});
	} else {
		afficheNotification("Merci de spécifier le sujet et le texte de votre commentaire avant de valider");
	}
}

function refreche() {
	$j.get('/articles/details/12719/syrie-attentat-suicide-d-un-marocain.html', {noLayout: true}, function (data) { 
	 	$j(".all_content").html(data);
	 	closeEffect();
	 	
 	}); 
}
</script>
