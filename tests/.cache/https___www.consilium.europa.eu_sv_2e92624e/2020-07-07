            at.type = 'text/javascript';
            at.async = true;
            at.src = '/js/smarttag-production.js';
            (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0] || document.getElementsByTagName('script')[0].parentNode).insertBefore(at, null);
