<script type="text/javascript">

        $('#btn-mobile-find').click(function (e) {
            $("#hd_nav_uno").hide();
            $("#hd_nav_dos").hide();
            if ($("#hd_frm").is(':visible')) {
                $("#hd_frm").hide();
            }
            else {
                $("#hd_frm").show();
            }
        });

        $('#btn-mobile-menu').click(function (e) {
            $("#hd_frm").hide();
            if ($("#hd_nav_uno").is(':visible')) {
                $("#hd_nav_uno").hide();
                $("#hd_nav_dos").hide();

            }
            else {
                $("#hd_nav_uno").show();
                $("#hd_nav_dos").show();
                $('html, body').animate({ scrollTop: 0 }, 'fast');
            }

        });
        $(window).resize(function () {
            if ($(window).width() > 1023) {
                $("#hd_nav_uno").show();
                $("#hd_nav_dos").show();
            }
        });


    </script>
